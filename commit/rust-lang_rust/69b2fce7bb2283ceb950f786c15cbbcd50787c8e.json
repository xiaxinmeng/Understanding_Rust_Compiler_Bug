sha": "c505af5c4d3f0ba9237ba5c9aa3c6530202ff5b5", "filename": "configure", "status": "modified", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/69b2fce7bb2283ceb950f786c15cbbcd50787c8e/configure", "raw_url": "https://github.com/rust-lang/rust/raw/69b2fce7bb2283ceb950f786c15cbbcd50787c8e/configure", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/configure?ref=69b2fce7bb2283ceb950f786c15cbbcd50787c8e", "patch": "@@ -541,6 +541,16 @@ CFG_BUILD_DIR=\"$(pwd)/\"\n CFG_SELF=\"$0\"\n CFG_CONFIGURE_ARGS=\"$@\"\n \n+\n+case \"${CFG_SRC_DIR}\" in  \n+    *\\ * )\n+        err \"The path to the rust source directory contains spaces, which is not supported\"\n+        ;;\n+    *)\n+        ;;\n+esac\n+\n+\n OPTIONS=\"\"\n HELP=0\n if [ \"$1\" = \"--help\" ]"}]}