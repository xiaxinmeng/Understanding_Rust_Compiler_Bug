ust-lang/rust/contents/src%2Ftest%2Fauxiliary%2Fcci_class_iface.rs?ref=d7ba59eb6e4b9ff99e94b0ca9c06ad47968c67ea", "patch": "@@ -0,0 +1,7 @@\n+mod animals {\n+\n+iface noisy {\n+  fn speak();\n+}\n+\n+}"}, {"sha": "2c845fa1c03db5f09faa3aa4ce1d107716dd700e", "filename": "src/test/run-pass/class-implement-iface-cross-crate.rs", "status": "added", "additions": 45, "deletions": 0, "changes": 45, "blob_url": "https://github.com/rust-lang/rust/blob/d7ba59eb6e4b9ff99e94b0ca9c06ad47968c67ea/src%2Ftest%2Frun-pass%2Fclass-implement-iface-cross-crate.rs", "raw_url": "https://github.com/rust-lang/rust/raw/d7ba59eb6e4b9ff99e94b0ca9c06ad47968c67ea/src%2Ftest%2Frun-pass%2Fclass-implement-iface-cross-crate.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fclass-implement-iface-cross-crate.rs?ref=d7ba59eb6e4b9ff99e94b0ca9c06ad47968c67ea", "patch": "@@ -0,0 +1,45 @@\n+// xfail-fast\n+// aux-build:cci_class_iface.rs\n+use cci_class_iface;\n+import cci_class_iface::animals::*;\n+\n+class cat implements noisy {\n+  priv {\n+    let mut meows : uint;\n+    fn meow() {\n+      #error(\"Meow\");\n+      self.meows += 1u;\n+      if self.meows % 5u == 0u {\n+          self.how_hungry += 1;\n+      }\n+    }\n+  }\n+\n+  let mutable how_hungry : int;\n+  let name : str;\n+\n+  new(in_x : uint, in_y : int, in_name: str)\n+    { self.meows = in_x; self.how_hungry = in_y; self.name = in_name; }\n+\n+  fn speak() { self.meow(); }\n+\n+  fn eat() -> bool {\n+    if self.how_hungry > 0 {\n+        #error(\"OM NOM NOM\");\n+        self.how_hungry -= 2;\n+        ret true;\n+    }\n+    else {\n+        #error(\"Not hungry!\");\n+        ret false;\n+    }\n+  }\n+}\n+\n+fn main() {\n+  let nyan = cat(0u, 2, \"nyan\");\n+  nyan.eat();\n+  assert(!nyan.eat());\n+  uint::range(1u, 10u, {|_i| nyan.speak(); });\n+  assert(nyan.eat());\n+}\n\\ No newline at end of file"}]}