n+if [ $CFG_OSTYPE = Darwin -a $CFG_CPUTYPE = i386 ]\n+then\n+    # Darwin's `uname -s` lies and always returns i386. We have to use sysctl\n+    # instead.\n+    if sysctl hw.optional.x86_64 | grep ': 1'\n+    then\n+        CFG_CPUTYPE=x86_64\n+    fi\n+fi\n \n CFG_SELF=$(echo $0 | tr '\\\\' '/')\n CFG_SRC_DIR=${CFG_SELF%${CFG_SELF##*/}}"}]}