etions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/1536ab1b383f21b38f8d49230a2aecc51daffa3d/compiler%2Frustc_resolve%2Fsrc%2Fimports.rs", "raw_url": "https://github.com/rust-lang/rust/raw/1536ab1b383f21b38f8d49230a2aecc51daffa3d/compiler%2Frustc_resolve%2Fsrc%2Fimports.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/compiler%2Frustc_resolve%2Fsrc%2Fimports.rs?ref=1536ab1b383f21b38f8d49230a2aecc51daffa3d", "patch": "@@ -252,7 +252,7 @@ impl<'a> Resolver<'a> {\n         self.set_binding_parent_module(binding, module);\n         self.update_resolution(module, key, |this, resolution| {\n             if let Some(old_binding) = resolution.binding {\n-                if res == Res::Err {\n+                if res == Res::Err && old_binding.res() != Res::Err {\n                     // Do not override real bindings with `Res::Err`s from error recovery.\n                     return Ok(());\n                 }"}, {"sha": "9e9cba13a22f3fdb005cd21959e7d3ddfd58f658", "filename": "src/test/rustdoc/issue-100241.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/1536ab1b383f21b38f8d49230a2aecc51daffa3d/src%2Ftest%2Frustdoc%2Fissue-100241.rs", "raw_url": "https://github.com/rust-lang/rust/raw/1536ab1b383f21b38f8d49230a2aecc51daffa3d/src%2Ftest%2Frustdoc%2Fissue-100241.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc%2Fissue-100241.rs?ref=1536ab1b383f21b38f8d49230a2aecc51daffa3d", "patch": "@@ -0,0 +1,12 @@\n+//! See [`S`].\n+\n+// Check that this isn't an ICE\n+// should-fail\n+\n+mod foo {\n+    pub use inner::S;\n+    //~^ ERROR unresolved imports `inner`, `foo::S`\n+}\n+\n+use foo::*;\n+use foo::S;"}]}