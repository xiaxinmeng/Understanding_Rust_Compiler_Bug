lse}, "committer": {"login": "luqmana", "id": 287063, "node_id": "MDQ6VXNlcjI4NzA2Mw==", "avatar_url": "https://avatars.githubusercontent.com/u/287063?v=4", "gravatar_id": "", "url": "https://api.github.com/users/luqmana", "html_url": "https://github.com/luqmana", "followers_url": "https://api.github.com/users/luqmana/followers", "following_url": "https://api.github.com/users/luqmana/following{/other_user}", "gists_url": "https://api.github.com/users/luqmana/gists{/gist_id}", "starred_url": "https://api.github.com/users/luqmana/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/luqmana/subscriptions", "organizations_url": "https://api.github.com/users/luqmana/orgs", "repos_url": "https://api.github.com/users/luqmana/repos", "events_url": "https://api.github.com/users/luqmana/events{/privacy}", "received_events_url": "https://api.github.com/users/luqmana/received_events", "type": "User", "site_admin": false}, "parents": [{"sha": "48af94c080d37a5396dd4452c21cd8006f38fba4", "url": "https://api.github.com/repos/rust-lang/rust/commits/48af94c080d37a5396dd4452c21cd8006f38fba4", "html_url": "https://github.com/rust-lang/rust/commit/48af94c080d37a5396dd4452c21cd8006f38fba4"}], "stats": {"total": 14, "additions": 14, "deletions": 0}, "files": [{"sha": "3376449598cac51ddc66c29e94b0f4349fad997a", "filename": "src/test/ui/simd/issue-105439.rs", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/rust-lang/rust/blob/75f3fafa72a4149e730d17e755c3ad96aa40ad9a/src%2Ftest%2Fui%2Fsimd%2Fissue-105439.rs", "raw_url": "https://github.com/rust-lang/rust/raw/75f3fafa72a4149e730d17e755c3ad96aa40ad9a/src%2Ftest%2Fui%2Fsimd%2Fissue-105439.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fsimd%2Fissue-105439.rs?ref=75f3fafa72a4149e730d17e755c3ad96aa40ad9a", "patch": "@@ -0,0 +1,14 @@\n+// This is used to ICE with MIR inlining enabled due to an invalid bitcast.\n+// run-pass\n+// compile-flags: -O -Zmir-opt-level=3\n+#![feature(portable_simd)]\n+\n+use std::simd::Simd;\n+\n+fn main() {\n+    let a = Simd::from_array([0, 4, 1, 5]);\n+    let b = Simd::from_array([2, 6, 3, 7]);\n+    let (x, y) = a.deinterleave(b);\n+    assert_eq!(x.to_array(), [0, 1, 2, 3]);\n+    assert_eq!(y.to_array(), [4, 5, 6, 7]);\n+}"}]}