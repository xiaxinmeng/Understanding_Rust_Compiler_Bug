ps://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc-gui%2Fitem-info-width.goml?ref=57f7501b1d0e276415cefdc1b8fba76563387744", "patch": "@@ -0,0 +1,7 @@\n+// This test ensures that the item information don't take 100% of the width if unnecessary.\n+goto: file://|DOC_PATH|/lib2/struct.Foo.html\n+// We set a fixed size so there is no chance of \"random\" resize.\n+size: (1100, 800)\n+// We check that \".item-info\" is bigger than its content.\n+assert-css: (\".item-info\", {\"width\": \"807px\"})\n+assert-css: (\".item-info .stab\", {\"width\": \"343px\"})"}, {"sha": "cd00348cad3d1b5b81ce24ea8706fcce9567fb3f", "filename": "src/test/rustdoc-gui/src/lib2/lib.rs", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/rust-lang/rust/blob/57f7501b1d0e276415cefdc1b8fba76563387744/src%2Ftest%2Frustdoc-gui%2Fsrc%2Flib2%2Flib.rs", "raw_url": "https://github.com/rust-lang/rust/raw/57f7501b1d0e276415cefdc1b8fba76563387744/src%2Ftest%2Frustdoc-gui%2Fsrc%2Flib2%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc-gui%2Fsrc%2Flib2%2Flib.rs?ref=57f7501b1d0e276415cefdc1b8fba76563387744", "patch": "@@ -1,5 +1,7 @@\n // ignore-tidy-linelength\n \n+#![feature(doc_cfg)]\n+\n pub mod module {\n     pub mod sub_module {\n         pub mod sub_sub_module {\n@@ -14,6 +16,7 @@ pub fn foobar() {}\n \n pub type Alias = u32;\n \n+#[doc(cfg(feature = \"foo-method\"))]\n pub struct Foo {\n     pub x: Alias,\n }"}]}