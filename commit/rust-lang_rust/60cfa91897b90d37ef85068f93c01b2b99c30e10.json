 {\n+    assert i == j;\n+    assert j != k;\n+}\n+\n+fn main() {\n+    // Binding a bare function turns it into a shared closure\n+    let g: fn() = bind f(10, 10, 20);\n+    g();\n+}\n\\ No newline at end of file"}]}