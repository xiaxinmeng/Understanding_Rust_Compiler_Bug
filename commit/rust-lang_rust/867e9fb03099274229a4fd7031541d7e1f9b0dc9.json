inc_by(int incr) -> int {\n+          i += incr;\n+          ret i;\n+      }\n+\n+      fn inc_by_5() -> int {\n+          ret self.inc_by(5);\n+      }\n+\n+      // A test case showing that issue #324 is resolved.  (It used to\n+      // be that commenting out this (unused!) function produced a\n+      // type error.)\n+      // fn wrapper(int incr) -> int {\n+      //     ret self.inc_by(incr);\n+      // }\n+\n+      fn get() -> int {\n+          ret i;\n+      }\n+  }\n+  \n+  let int res;\n+  auto o = foo(5);\n+  \n+  res = o.get();\n+  assert (res == 5);\n+\n+  res = o.inc_by(3);\n+  assert (res == 8);\n+  \n+  res = o.get();\n+  assert (res == 8);\n+\n+}\n+"}]}