ars,\n char_len, char_at, bytes, is_ascii, shift_byte, pop_byte;\n \n+export from_str, to_str;\n+\n+fn from_str(s: &str) -> istr {\n+    let s2 = ~\"\";\n+    for u in s {\n+        push_byte(s2, u);\n+    }\n+    ret s2;\n+}\n+\n+fn to_str(s: &istr) -> str {\n+    let s2 = \"\";\n+    for u in s {\n+        str::push_byte(s2, u);\n+    }\n+    ret s2;\n+}\n+\n fn eq(a: &istr, b: &istr) -> bool { a == b }\n \n fn lteq(a: &istr, b: &istr) -> bool { a <= b }"}]}