 test.\n+\n+impure fn main() {\n+  let port[()] po = port();\n+  let chan[()] ch = chan(po);\n+\n+  ch <| ();\n+\n+  let () n;\n+  n <- po;\n+\n+  check (n == ());\n+}"}]}