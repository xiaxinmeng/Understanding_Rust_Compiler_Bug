false}, "parents": [{"sha": "4083c698e5efdfbe7df69fd5103ce790b3fb0800", "url": "https://api.github.com/repos/rust-lang/rust/commits/4083c698e5efdfbe7df69fd5103ce790b3fb0800", "html_url": "https://github.com/rust-lang/rust/commit/4083c698e5efdfbe7df69fd5103ce790b3fb0800"}, {"sha": "b129de47a0a1fbb387510da72b90d9035fd2e4fe", "url": "https://api.github.com/repos/rust-lang/rust/commits/b129de47a0a1fbb387510da72b90d9035fd2e4fe", "html_url": "https://github.com/rust-lang/rust/commit/b129de47a0a1fbb387510da72b90d9035fd2e4fe"}], "stats": {"total": 17, "additions": 17, "deletions": 0}, "files": [{"sha": "94e2b2563df59baae386022e53c4752a6b152960", "filename": "src/test/run-pass/issues/issue-58435-ice-with-assoc-const.rs", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/rust-lang/rust/blob/c789291d485082627b29c163cf4df458f105b7b8/src%2Ftest%2Frun-pass%2Fissues%2Fissue-58435-ice-with-assoc-const.rs", "raw_url": "https://github.com/rust-lang/rust/raw/c789291d485082627b29c163cf4df458f105b7b8/src%2Ftest%2Frun-pass%2Fissues%2Fissue-58435-ice-with-assoc-const.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissues%2Fissue-58435-ice-with-assoc-const.rs?ref=c789291d485082627b29c163cf4df458f105b7b8", "patch": "@@ -0,0 +1,17 @@\n+// The const-evaluator was at one point ICE'ing while trying to\n+// evaluate the body of `fn id` during the `s.id()` call in main.\n+\n+struct S<T>(T);\n+\n+impl<T> S<T> {\n+    const ID: fn(&S<T>) -> &S<T> = |s| s;\n+    pub fn id(&self) -> &Self {\n+        Self::ID(self) // This, plus call below ...\n+    }\n+}\n+\n+fn main() {\n+    let s = S(10u32);\n+    assert!(S::<u32>::ID(&s).0 == 10); // Works fine\n+    assert!(s.id().0 == 10); // ... causes compiler to panic\n+}"}]}