,8 +1,14 @@\n+use std::sync::atomic::{Ordering, AtomicUsize};\n+\n static mut X: usize = 5;\n+static Y: AtomicUsize = AtomicUsize::new(5);\n \n fn main() {\n     unsafe {\n         X = 6;\n         assert_eq!(X, 6);\n     }\n+\n+    Y.store(6, Ordering::Relaxed);\n+    assert_eq!(Y.load(Ordering::Relaxed), 6);\n }"}]}