y(s, m);\n+        // Avoid `& &e` => `&&e`.\n+        match (m, &expr.node) {\n+            (ast::m_imm, &ast::expr_addr_of(*)) => space(s.s),\n+            _ => { }\n+        }\n         print_expr(s, expr);\n       }\n       ast::expr_lit(lit) => print_literal(s, lit),"}]}