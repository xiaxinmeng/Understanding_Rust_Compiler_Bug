api.github.com/repos/rust-lang/rust/contents/tests%2Fcompile-fail%2Funneeded_field_pattern.rs?ref=4b9912c2b00a047c6c19aa62ba90bb7d252f07d4", "patch": "@@ -0,0 +1,26 @@\n+#![feature(plugin)]\n+#![plugin(clippy)]\n+\n+#![deny(unneeded_field_pattern)]\n+#[allow(dead_code, unused)]\n+\n+struct Foo {\n+    a: i32,\n+    b: i32,\n+    c: i32,\n+}\n+\n+fn main() {\n+    let f = Foo { a: 0, b: 0, c: 0 };\n+\n+    match f {\n+        Foo { a: _, b: 0, .. } => {} //~ERROR You matched a field with a wildcard pattern\n+                                     //~^ HELP Try with `Foo { b: 0, .. }`\n+        Foo { a: _, b: _, c: _ } => {} //~ERROR All the struct fields are matched to a\n+                                       //~^ HELP Try with `Foo { .. }`\n+    }\n+    match f {\n+        Foo { b: 0, .. } => {} // should be OK\n+        Foo { .. } => {} // and the Force might be with this one\n+    }\n+}\n\\ No newline at end of file"}]}