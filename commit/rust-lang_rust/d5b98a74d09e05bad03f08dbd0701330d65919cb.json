_in_place2\n+#[no_mangle]\n+pub fn trim_in_place2(a: &mut &[u8]) {\n+    while let Some(&42) = a.first() {\n+        // CHECK-NOT: slice_index_order_fail\n+        *a = &a[2..];\n+    }\n+}"}]}