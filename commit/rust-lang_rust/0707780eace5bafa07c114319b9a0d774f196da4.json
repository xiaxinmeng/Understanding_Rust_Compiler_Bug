\n+\n+fn foo() {\n+    let mut i = 100u;\n+    while i >= 0 { //~ ERROR comparison is useless due to type limits\n+        i -= 1;\n+    }\n+}\n+\n+fn bar() -> i8 {\n+    return 123;\n+}\n+\n+fn baz() -> bool {\n+    128 > bar() //~ ERROR comparison is useless due to type limits\n+}\n+\n+fn qux() {\n+    let mut i = 1i8;\n+    while 200 != i { //~ ERROR comparison is useless due to type limits\n+        i += 1;\n+    }\n+}\n+"}]}