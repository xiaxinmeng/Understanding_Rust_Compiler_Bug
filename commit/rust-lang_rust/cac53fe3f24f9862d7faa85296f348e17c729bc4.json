thub.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fcross-crate%2Fanon-extern-mod-cross-crate-2.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,14 @@\n+// run-pass\n+// aux-build:anon-extern-mod-cross-crate-1.rs\n+// pretty-expanded FIXME #23616\n+// ignore-wasm32-bare no libc to test ffi with\n+\n+extern crate anonexternmod;\n+\n+use anonexternmod::rust_get_test_int;\n+\n+pub fn main() {\n+    unsafe {\n+        rust_get_test_int();\n+    }\n+}"}, {"sha": "f2c1e1f654016fd09c37b73d74a6a1ac891d751c", "filename": "src/test/run-pass/abi/duplicated-external-mods.rs", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fduplicated-external-mods.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fduplicated-external-mods.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fduplicated-external-mods.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,8 @@\n+// aux-build:anon-extern-mod-cross-crate-1.rs\n+// aux-build:anon-extern-mod-cross-crate-1.rs\n+// pretty-expanded FIXME #23616\n+// ignore-wasm32-bare no libc to test ffi with\n+\n+extern crate anonexternmod;\n+\n+pub fn main() { }"}, {"sha": "123ce20ca262f3d2d02458b2f17e132c1a4ac457", "filename": "src/test/run-pass/abi/extern/extern-crosscrate.rs", "status": "added", "additions": 21, "deletions": 0, "changes": 21, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-crosscrate.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-crosscrate.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-crosscrate.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,21 @@\n+// run-pass\n+// aux-build:extern-crosscrate-source.rs\n+// ignore-wasm32-bare no libc to test ffi with\n+\n+#![feature(rustc_private)]\n+\n+extern crate externcallback;\n+extern crate libc;\n+\n+fn fact(n: libc::uintptr_t) -> libc::uintptr_t {\n+    unsafe {\n+        println!(\"n = {}\", n);\n+        externcallback::rustrt::rust_dbg_call(externcallback::cb, n)\n+    }\n+}\n+\n+pub fn main() {\n+    let result = fact(10);\n+    println!(\"result = {}\", result);\n+    assert_eq!(result, 3628800);\n+}"}, {"sha": "3c9f0f583d48779c9944931eea4c33f72692c1d1", "filename": "src/test/run-pass/abi/foreign/foreign-dupe.rs", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fforeign%2Fforeign-dupe.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fforeign%2Fforeign-dupe.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fforeign%2Fforeign-dupe.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,17 @@\n+// run-pass\n+// aux-build:foreign_lib.rs\n+// ignore-wasm32-bare no libc to test ffi with\n+\n+// Check that we can still call duplicated extern (imported) functions\n+// which were declared in another crate. See issues #32740 and #32783.\n+\n+\n+extern crate foreign_lib;\n+\n+pub fn main() {\n+    unsafe {\n+        let x = foreign_lib::rustrt::rust_get_test_int();\n+        assert_eq!(x, foreign_lib::rustrt2::rust_get_test_int());\n+        assert_eq!(x as *const _, foreign_lib::rustrt3::rust_get_test_int());\n+    }\n+}"}, {"sha": "d34933cde4246e1ef04709c4eb87f8dfaf4ff868", "filename": "src/test/run-pass/abi/invoke-external-foreign.rs", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Finvoke-external-foreign.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Finvoke-external-foreign.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Finvoke-external-foreign.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,16 @@\n+// aux-build:foreign_lib.rs\n+// ignore-wasm32-bare no libc to test ffi with\n+\n+// The purpose of this test is to check that we can\n+// successfully (and safely) invoke external, cdecl\n+// functions from outside the crate.\n+\n+// pretty-expanded FIXME #23616\n+\n+extern crate foreign_lib;\n+\n+pub fn main() {\n+    unsafe {\n+        let _foo = foreign_lib::rustrt::rust_get_test_int();\n+    }\n+}"}, {"sha": "7ce3df255a3314a6be4b640db589ac71cda8ce55", "filename": "src/test/run-pass/abi/issues/auxiliary/issue-25185-2.rs", "status": "added", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fauxiliary%2Fissue-25185-2.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fauxiliary%2Fissue-25185-2.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fauxiliary%2Fissue-25185-2.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,3 @@\n+extern crate issue_25185_1;\n+\n+pub use issue_25185_1::rust_dbg_extern_identity_u32;"}, {"sha": "383c9a1e9c4ab9aeffea22f98970fa0b6e76628c", "filename": "src/test/run-pass/abi/issues/issue-25185.rs", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fissue-25185.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fissue-25185.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fissues%2Fissue-25185.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,13 @@\n+// run-pass\n+// aux-build:issue-25185-1.rs\n+// aux-build:issue-25185-2.rs\n+// ignore-wasm32-bare no libc for ffi testing\n+\n+extern crate issue_25185_2;\n+\n+fn main() {\n+    let x = unsafe {\n+        issue_25185_2::rust_dbg_extern_identity_u32(1)\n+    };\n+    assert_eq!(x, 1);\n+}"}, {"sha": "15fe3804f9b4ff73c46a2784207715e90a471026", "filename": "src/test/run-pass/abi/proc_macro/auxiliary/test-macros.rs", "status": "added", "additions": 26, "deletions": 0, "changes": 26, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fproc_macro%2Fauxiliary%2Ftest-macros.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fproc_macro%2Fauxiliary%2Ftest-macros.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fproc_macro%2Fauxiliary%2Ftest-macros.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,26 @@\n+// force-host\n+// no-prefer-dynamic\n+\n+#![crate_type = \"proc-macro\"]\n+\n+extern crate proc_macro;\n+\n+use proc_macro::TokenStream;\n+\n+#[proc_macro_attribute]\n+pub fn nop_attr(_attr: TokenStream, input: TokenStream) -> TokenStream {\n+    assert!(_attr.to_string().is_empty());\n+    input\n+}\n+\n+#[proc_macro_attribute]\n+pub fn no_output(_attr: TokenStream, _input: TokenStream) -> TokenStream {\n+    assert!(_attr.to_string().is_empty());\n+    assert!(!_input.to_string().is_empty());\n+    \"\".parse().unwrap()\n+}\n+\n+#[proc_macro]\n+pub fn emit_input(input: TokenStream) -> TokenStream {\n+    input\n+}"}, {"sha": "1274f032a3e298fdaac584eecdd6c1dbc7a83d09", "filename": "src/test/run-pass/abi/stack-probes-lto.rs", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/rust-lang/rust/blob/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fstack-probes-lto.rs", "raw_url": "https://github.com/rust-lang/rust/raw/cac53fe3f24f9862d7faa85296f348e17c729bc4/src%2Ftest%2Frun-pass%2Fabi%2Fstack-probes-lto.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fstack-probes-lto.rs?ref=cac53fe3f24f9862d7faa85296f348e17c729bc4", "patch": "@@ -0,0 +1,18 @@\n+// ignore-arm\n+// ignore-aarch64\n+// ignore-mips\n+// ignore-mips64\n+// ignore-powerpc\n+// ignore-s390x\n+// ignore-sparc\n+// ignore-sparc64\n+// ignore-wasm\n+// ignore-cloudabi no processes\n+// ignore-emscripten no processes\n+// ignore-sgx no processes\n+// ignore-musl FIXME #31506\n+// ignore-pretty\n+// compile-flags: -C lto\n+// no-prefer-dynamic\n+\n+include!(\"stack-probes.rs\");"}]}