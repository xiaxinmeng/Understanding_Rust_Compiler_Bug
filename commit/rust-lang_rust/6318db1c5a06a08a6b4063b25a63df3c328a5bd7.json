a5bd7", "patch": "@@ -0,0 +1,21 @@\n+// check-pass\n+#![crate_type = \"lib\"]\n+\n+#[cfg(target_arch = \"wasm32\")]\n+mod wasm_non_clash {\n+    mod a {\n+        #[link(wasm_import_module = \"a\")]\n+        extern \"C\" {\n+            pub fn foo();\n+        }\n+    }\n+\n+    mod b {\n+        #[link(wasm_import_module = \"b\")]\n+        extern \"C\" {\n+            pub fn foo() -> usize;\n+            // #79581: These declarations shouldn't clash because foreign fn names are mangled\n+            // on wasm32.\n+        }\n+    }\n+}"}]}