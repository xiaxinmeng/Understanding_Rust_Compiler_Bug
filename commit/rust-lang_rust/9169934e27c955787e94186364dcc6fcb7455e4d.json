 for SmallVec<A> {\n \n impl<A: Array> Extend<A::Element> for SmallVec<A> {\n     fn extend<I: IntoIterator<Item=A::Element>>(&mut self, iter: I) {\n-        let iter = iter.into_iter();\n-        self.reserve(iter.size_hint().0);\n-        for el in iter {\n-            self.push(el);\n+        if self.is_array() {\n+            let iter = iter.into_iter();\n+            self.reserve(iter.size_hint().0);\n+\n+            for el in iter {\n+                self.push(el);\n+            }\n+        } else {\n+            match self.0 {\n+                AccumulateVec::Heap(ref mut vec) => vec.extend(iter),\n+                _ => unreachable!()\n+            }\n         }\n     }\n }"}]}