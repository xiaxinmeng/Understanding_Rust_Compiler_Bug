om/rust-lang/rust/raw/9430423cab6909792fb1b3a850f1c3c8974a5111/src%2Fetc%2Fgdb_rust_pretty_printing.py", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fetc%2Fgdb_rust_pretty_printing.py?ref=9430423cab6909792fb1b3a850f1c3c8974a5111", "patch": "@@ -16,7 +16,7 @@\n # This fix went in 8.1, so check for that.\n # See https://github.com/rust-lang/rust/issues/56730\n gdb_81 = False\n-_match = re.match('([0-9]+)\\\\.([0-9]+)', gdb.VERSION)\n+_match = re.search('([0-9]+)\\\\.([0-9]+)', gdb.VERSION)\n if _match:\n     if int(_match.group(1)) > 8 or (int(_match.group(1)) == 8 and int(_match.group(2)) >= 1):\n         gdb_81 = True"}]}