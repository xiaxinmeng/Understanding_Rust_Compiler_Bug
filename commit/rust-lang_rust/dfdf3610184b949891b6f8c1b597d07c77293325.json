\n+#[test]\n+fn test_ord() {\n+    macro_rules! ord(\n+        ($ord:ident, $left:expr, $right:expr) => ( {\n+            assert_eq!(Path::new($left).cmp(&Path::new($right)), core::cmp::Ordering::$ord);\n+        });\n+    );\n+\n+    ord!(Less, \"1\", \"2\");\n+    ord!(Less, \"/foo/bar\", \"/foo./bar\");\n+    ord!(Less, \"foo/bar\", \"foo/bar.\");\n+    ord!(Equal, \"foo/./bar\", \"foo/bar/\");\n+    ord!(Equal, \"foo/bar\", \"foo/bar/\");\n+    ord!(Equal, \"foo/bar\", \"foo/bar/.\");\n+    ord!(Equal, \"foo/bar\", \"foo/bar//\");\n+}\n+\n #[bench]\n fn bench_path_cmp_fast_path_buf_sort(b: &mut test::Bencher) {\n     let prefix = \"my/home\";"}]}