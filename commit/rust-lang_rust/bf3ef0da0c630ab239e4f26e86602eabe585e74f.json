w_url": "https://github.com/rust-lang/rust/raw/bf3ef0da0c630ab239e4f26e86602eabe585e74f/compiler%2Frustc_codegen_llvm%2Fsrc%2Fasm.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/compiler%2Frustc_codegen_llvm%2Fsrc%2Fasm.rs?ref=bf3ef0da0c630ab239e4f26e86602eabe585e74f", "patch": "@@ -290,6 +290,11 @@ impl<'ll, 'tcx> AsmBuilderMethods<'tcx> for Builder<'_, 'll, 'tcx> {\n         }\n         attributes::apply_to_callsite(result, llvm::AttributePlace::Function, &{ attrs });\n \n+        // Switch to the 'normal' basic block if we did an `invoke` instead of a `call`\n+        if let Some((dest, _, _)) = dest_catch_funclet {\n+            self.switch_to_block(dest);\n+        }\n+\n         // Write results to outputs\n         for (idx, op) in operands.iter().enumerate() {\n             if let InlineAsmOperandRef::Out { reg, place: Some(place), .. }"}]}