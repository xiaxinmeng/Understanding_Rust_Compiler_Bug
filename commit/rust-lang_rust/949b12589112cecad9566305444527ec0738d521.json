lippy::for_loops_over_options)]\n+\n+fn main() {\n+    let x = vec![Some(1), Some(2), Some(3)];\n+    for n in x {\n+        if let Some(n) = n {\n+            println!(\"{}\", n);\n+        }\n+    }\n+\n+    let y: Vec<Result<i32, i32>> = vec![];\n+    for n in y.clone() {\n+        if let Ok(n) = n {\n+            println!(\"{}\", n);\n+        }\n+    }\n+\n+    // This should not trigger the lint\n+    for n in y.clone() {\n+        if let Ok(n) = n {\n+            println!(\"{}\", n);\n+        } else {\n+            println!(\"Oops!\");\n+        }\n+    }\n+\n+    // This should not trigger the lint\n+    for n in vec![Some(1), Some(2), Some(3)].iter().flatten() {\n+        println!(\"{}\", n);\n+    }\n+}"}]}