atus": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/rust-lang/rust/blob/758c00ea4088e1ca2714890f00c791c24f001536/compiler%2Frustc_session%2Fsrc%2Foutput.rs", "raw_url": "https://github.com/rust-lang/rust/raw/758c00ea4088e1ca2714890f00c791c24f001536/compiler%2Frustc_session%2Fsrc%2Foutput.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/compiler%2Frustc_session%2Fsrc%2Foutput.rs?ref=758c00ea4088e1ca2714890f00c791c24f001536", "patch": "@@ -127,6 +127,11 @@ pub fn filename_for_metadata(\n     crate_name: &str,\n     outputs: &OutputFilenames,\n ) -> PathBuf {\n+    // If the command-line specified the path, use that directly.\n+    if let Some(Some(out_filename)) = sess.opts.output_types.get(&OutputType::Metadata) {\n+        return out_filename.clone();\n+    }\n+\n     let libname = format!(\"{}{}\", crate_name, sess.opts.cg.extra_filename);\n \n     let out_filename = outputs"}, {"sha": "03eb83b97e38343f2c16b6a658acfa70fb576f71", "filename": "src/test/run-make/emit-named-files/Makefile", "status": "added", "additions": 33, "deletions": 0, "changes": 33, "blob_url": "https://github.com/rust-lang/rust/blob/758c00ea4088e1ca2714890f00c791c24f001536/src%2Ftest%2Frun-make%2Femit-named-files%2FMakefile", "raw_url": "https://github.com/rust-lang/rust/raw/758c00ea4088e1ca2714890f00c791c24f001536/src%2Ftest%2Frun-make%2Femit-named-files%2FMakefile", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-make%2Femit-named-files%2FMakefile?ref=758c00ea4088e1ca2714890f00c791c24f001536", "patch": "@@ -0,0 +1,33 @@\n+-include ../../run-make-fulldeps/tools.mk\n+\n+OUT=$(TMPDIR)/emit\n+\n+all: asm llvm-bc llvm-ir obj metadata link dep-info mir\n+\n+asm: $(OUT)\n+\t$(RUSTC) --emit asm=$(OUT)/libfoo.s foo.rs\n+\ttest -f $(OUT)/libfoo.s\n+llvm-bc: $(OUT)\n+\t$(RUSTC) --emit llvm-bc=$(OUT)/libfoo.bc foo.rs\n+\ttest -f $(OUT)/libfoo.bc\n+llvm-ir: $(OUT)\n+\t$(RUSTC) --emit llvm-ir=$(OUT)/libfoo.ll foo.rs\n+\ttest -f $(OUT)/libfoo.ll\n+obj: $(OUT)\n+\t$(RUSTC) --emit obj=$(OUT)/libfoo.o foo.rs\n+\ttest -f $(OUT)/libfoo.o\n+metadata: $(OUT)\n+\t$(RUSTC) --emit metadata=$(OUT)/libfoo.rmeta foo.rs\n+\ttest -f $(OUT)/libfoo.rmeta\n+link: $(OUT)\n+\t$(RUSTC) --emit link=$(OUT)/libfoo.rlib foo.rs\n+\ttest -f $(OUT)/libfoo.rlib\n+dep-info: $(OUT)\n+\t$(RUSTC) --emit dep-info=$(OUT)/libfoo.d foo.rs\n+\ttest -f $(OUT)/libfoo.d\n+mir: $(OUT)\n+\t$(RUSTC) --emit mir=$(OUT)/libfoo.mir foo.rs\n+\ttest -f $(OUT)/libfoo.mir\n+\n+$(OUT):\n+\tmkdir -p $(OUT)"}, {"sha": "c1bfaa6cab5d9be9e67e4cfacc8eb4e6110f6358", "filename": "src/test/run-make/emit-named-files/foo.rs", "status": "added", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/758c00ea4088e1ca2714890f00c791c24f001536/src%2Ftest%2Frun-make%2Femit-named-files%2Ffoo.rs", "raw_url": "https://github.com/rust-lang/rust/raw/758c00ea4088e1ca2714890f00c791c24f001536/src%2Ftest%2Frun-make%2Femit-named-files%2Ffoo.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-make%2Femit-named-files%2Ffoo.rs?ref=758c00ea4088e1ca2714890f00c791c24f001536", "patch": "@@ -0,0 +1 @@\n+#![crate_type = \"rlib\"]"}]}