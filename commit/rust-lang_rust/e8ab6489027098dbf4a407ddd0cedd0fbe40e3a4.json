tc_trait_selection%2Fsrc%2Ftraits%2Fselect%2Fconfirmation.rs", "raw_url": "https://github.com/rust-lang/rust/raw/e8ab6489027098dbf4a407ddd0cedd0fbe40e3a4/compiler%2Frustc_trait_selection%2Fsrc%2Ftraits%2Fselect%2Fconfirmation.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/compiler%2Frustc_trait_selection%2Fsrc%2Ftraits%2Fselect%2Fconfirmation.rs?ref=e8ab6489027098dbf4a407ddd0cedd0fbe40e3a4", "patch": "@@ -812,7 +812,7 @@ impl<'cx, 'tcx> SelectionContext<'cx, 'tcx> {\n     fn confirm_poly_trait_refs(\n         &mut self,\n         obligation: &TraitObligation<'tcx>,\n-        expected_trait_ref: ty::PolyTraitRef<'tcx>,\n+        self_ty_trait_ref: ty::PolyTraitRef<'tcx>,\n     ) -> Result<Vec<PredicateObligation<'tcx>>, SelectionError<'tcx>> {\n         let obligation_trait_ref = obligation.predicate.to_poly_trait_ref();\n         // Normalize the obligation and expected trait refs together, because why not\n@@ -823,7 +823,7 @@ impl<'cx, 'tcx> SelectionContext<'cx, 'tcx> {\n                     obligation.param_env,\n                     obligation.cause.clone(),\n                     obligation.recursion_depth + 1,\n-                    (obligation_trait_ref, expected_trait_ref),\n+                    (obligation_trait_ref, self_ty_trait_ref),\n                 )\n             });\n "}]}