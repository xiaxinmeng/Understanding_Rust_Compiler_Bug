 => {\n+        stringify_item! {\n+            pub fn repro() -> bool {\n+                $expr\n+            }\n+        }\n+    };\n+}\n+\n+fn main() {\n+    assert_eq!(\n+        repro!(match () { () => true } | true),\n+        \"pub fn repro() -> bool { (match () { () => true, }) | true }\"\n+    );\n+}"}]}