otest)]\n #[lang=\"str_eq\"]\n pure fn eq_slice(a: &str, b: &str) -> bool {\n     do as_buf(a) |ap, alen| {\n@@ -729,6 +730,22 @@ pure fn eq_slice(a: &str, b: &str) -> bool {\n     }\n }\n \n+#[cfg(test)]\n+pure fn eq_slice(a: &str, b: &str) -> bool {\n+    do as_buf(a) |ap, alen| {\n+        do as_buf(b) |bp, blen| {\n+            if (alen != blen) { false }\n+            else {\n+                unsafe {\n+                    libc::memcmp(ap as *libc::c_void,\n+                                 bp as *libc::c_void,\n+                                 (alen - 1) as libc::size_t) == 0\n+                }\n+            }\n+        }\n+    }\n+}\n+\n /// Bytewise string equality\n pure fn eq(a: &~str, b: &~str) -> bool {\n     eq_slice(*a, *b)"}]}