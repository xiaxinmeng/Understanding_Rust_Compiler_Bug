s\n+\n+#![feature(track_caller)]\n+\n+fn pass_to_ptr_call<T>(f: fn(T), x: T) {\n+    f(x);\n+}\n+\n+#[track_caller]\n+fn tracked_unit(_: ()) {\n+    assert_eq!(std::panic::Location::caller().file(), file!());\n+}\n+\n+fn main() {\n+    pass_to_ptr_call(tracked_unit, ());\n+}"}, {"sha": "1ce8f678b60f7e4aeb58d66743bcbf9159000d7d", "filename": "src/test/ui/rfc-2091-track-caller/tracked-fn-ptr.rs", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/rust-lang/rust/blob/fd90e56120c9fec88808f9464e27283b0c6954ad/src%2Ftest%2Fui%2Frfc-2091-track-caller%2Ftracked-fn-ptr.rs", "raw_url": "https://github.com/rust-lang/rust/raw/fd90e56120c9fec88808f9464e27283b0c6954ad/src%2Ftest%2Fui%2Frfc-2091-track-caller%2Ftracked-fn-ptr.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Frfc-2091-track-caller%2Ftracked-fn-ptr.rs?ref=fd90e56120c9fec88808f9464e27283b0c6954ad", "patch": "@@ -0,0 +1,16 @@\n+// run-pass\n+\n+#![feature(track_caller)]\n+\n+fn ptr_call(f: fn()) {\n+    f();\n+}\n+\n+#[track_caller]\n+fn tracked() {\n+    assert_eq!(std::panic::Location::caller().file(), file!());\n+}\n+\n+fn main() {\n+    ptr_call(tracked);\n+}"}]}