"@@ -206,6 +206,11 @@ impl Inliner<'tcx> {\n         debug!(\"should_inline({:?})\", callsite);\n         let tcx = self.tcx;\n \n+        if callsite.fn_sig.c_variadic() {\n+            debug!(\"callee is variadic - not inlining\");\n+            return false;\n+        }\n+\n         let codegen_fn_attrs = tcx.codegen_fn_attrs(callsite.callee.def_id());\n \n         let self_features = &self.codegen_fn_attrs.target_features;"}, {"sha": "30aff0a64efb91ff24c9c1cf24e74531e4502080", "filename": "src/test/mir-opt/inline/inline-compatibility.rs", "status": "modified", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/rust-lang/rust/blob/79d853eccebf6d9704adeea45f20f11bae0d7396/src%2Ftest%2Fmir-opt%2Finline%2Finline-compatibility.rs", "raw_url": "https://github.com/rust-lang/rust/raw/79d853eccebf6d9704adeea45f20f11bae0d7396/src%2Ftest%2Fmir-opt%2Finline%2Finline-compatibility.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fmir-opt%2Finline%2Finline-compatibility.rs?ref=79d853eccebf6d9704adeea45f20f11bae0d7396", "patch": "@@ -5,6 +5,7 @@\n #![crate_type = \"lib\"]\n #![feature(no_sanitize)]\n #![feature(target_feature_11)]\n+#![feature(c_variadic)]\n \n // EMIT_MIR inline_compatibility.inlined_target_feature.Inline.diff\n #[target_feature(enable = \"sse2\")]\n@@ -35,3 +36,20 @@ pub unsafe fn target_feature() {}\n #[inline]\n #[no_sanitize(address)]\n pub unsafe fn no_sanitize() {}\n+\n+// EMIT_MIR inline_compatibility.not_inlined_c_variadic.Inline.diff\n+pub unsafe fn not_inlined_c_variadic() {\n+    let s = sum(4u32, 4u32, 30u32, 200u32, 1000u32);\n+}\n+\n+#[no_mangle]\n+#[inline(always)]\n+unsafe extern \"C\" fn sum(n: u32, mut vs: ...) -> u32 {\n+    let mut s = 0;\n+    let mut i = 0;\n+    while i != n {\n+        s += vs.arg::<u32>();\n+        i += 1;\n+    }\n+    s\n+}"}]}