>(x: Foo<for<'a> fn(&'a ())>) {\n+    let Foo(y): Foo<fn(&'b ())> = x;\n+}\n+\n+fn foo_nested<'b>(x: Foo<Foo<for<'a> fn(&'a ())>>) {\n+    let Foo(Foo(y)): Foo<Foo<fn(&'b ())>> = x;\n+}\n+\n+fn tuple<'b>(x: (u32, for<'a> fn(&'a ()))) {\n+    let (_, y): (u32, fn(&'b ())) = x;\n+}\n+\n+fn tuple_nested<'b>(x: (u32, (u32, for<'a> fn(&'a ())))) {\n+    let (_, (_, y)): (u32, (u32, fn(&'b ()))) = x;\n+}\n+\n+fn main() {}"}]}