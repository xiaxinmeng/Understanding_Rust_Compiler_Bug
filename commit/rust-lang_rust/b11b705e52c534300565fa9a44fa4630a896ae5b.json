 "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fgeneric-associated-types%2Fissue-74824.rs?ref=b11b705e52c534300565fa9a44fa4630a896ae5b", "patch": "@@ -0,0 +1,27 @@\n+#![feature(generic_associated_types)]\n+#![feature(associated_type_defaults)]\n+#![allow(incomplete_features)]\n+\n+use std::ops::Deref;\n+\n+trait UnsafeCopy {\n+    type Copy<T>: Copy = Box<T>;\n+    //~^ ERROR the trait bound `Box<T>: Copy` is not satisfied\n+    //~^^ ERROR the trait bound `T: Clone` is not satisfied\n+    fn copy<T>(x: &Self::Copy<T>) -> Self::Copy<T> {\n+        *x\n+    }\n+}\n+\n+impl<T> UnsafeCopy for T {}\n+\n+fn main() {\n+    let b = Box::new(42usize);\n+    let copy = <()>::copy(&b);\n+\n+    let raw_b = Box::deref(&b) as *const _;\n+    let raw_copy = Box::deref(&copy) as *const _;\n+\n+    // assert the addresses.\n+    assert_eq!(raw_b, raw_copy);\n+}"}, {"sha": "34a2c1932ebcccff2ef86b2d1c533eda6de463d4", "filename": "src/test/ui/generic-associated-types/issue-74824.stderr", "status": "added", "additions": 27, "deletions": 0, "changes": 27, "blob_url": "https://github.com/rust-lang/rust/blob/b11b705e52c534300565fa9a44fa4630a896ae5b/src%2Ftest%2Fui%2Fgeneric-associated-types%2Fissue-74824.stderr", "raw_url": "https://github.com/rust-lang/rust/raw/b11b705e52c534300565fa9a44fa4630a896ae5b/src%2Ftest%2Fui%2Fgeneric-associated-types%2Fissue-74824.stderr", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fgeneric-associated-types%2Fissue-74824.stderr?ref=b11b705e52c534300565fa9a44fa4630a896ae5b", "patch": "@@ -0,0 +1,27 @@\n+error[E0277]: the trait bound `Box<T>: Copy` is not satisfied\n+  --> $DIR/issue-74824.rs:8:5\n+   |\n+LL |     type Copy<T>: Copy = Box<T>;\n+   |     ^^^^^^^^^^^^^^----^^^^^^^^^^\n+   |     |             |\n+   |     |             required by this bound in `UnsafeCopy::Copy`\n+   |     the trait `Copy` is not implemented for `Box<T>`\n+\n+error[E0277]: the trait bound `T: Clone` is not satisf