f7fed3acc227c", "patch": "@@ -16,7 +16,16 @@ macro_rules! higher_order {\n     });\n }\n \n+macro_rules! outer {\n+    ($x:expr; $fragment:ident) => {\n+        macro_rules! inner { ($y:$fragment) => { $x + $y } }\n+    }\n+}\n+\n fn main() {\n     let val = higher_order!(subst ($x:expr, $y:expr, $foo:expr) => (($x + $y, $foo)));\n     assert_eq!(val, (3, \"foo\"));\n+\n+    outer!(2; expr);\n+    assert_eq!(inner!(3), 5);\n }"}]}