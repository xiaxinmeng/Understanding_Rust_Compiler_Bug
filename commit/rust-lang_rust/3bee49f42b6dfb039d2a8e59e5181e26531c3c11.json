a8e59e5181e26531c3c11/src%2Flibstd%2Fsys%2Funix%2Ffs.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3bee49f42b6dfb039d2a8e59e5181e26531c3c11/src%2Flibstd%2Fsys%2Funix%2Ffs.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fsys%2Funix%2Ffs.rs?ref=3bee49f42b6dfb039d2a8e59e5181e26531c3c11", "patch": "@@ -526,8 +526,15 @@ impl File {\n     }\n \n     pub fn fsync(&self) -> io::Result<()> {\n-        cvt_r(|| unsafe { libc::fsync(self.0.raw()) })?;\n-        Ok(())\n+        cvt_r(|| unsafe { os_fsync(self.0.raw()) })?;\n+        return Ok(());\n+\n+        #[cfg(any(target_os = \"macos\", target_os = \"ios\"))]\n+        unsafe fn os_fsync(fd: c_int) -> c_int {\n+            libc::fcntl(fd, libc::F_FULLFSYNC)\n+        }\n+        #[cfg(not(any(target_os = \"macos\", target_os = \"ios\")))]\n+        unsafe fn os_fsync(fd: c_int) -> c_int { libc::fsync(fd) }\n     }\n \n     pub fn datasync(&self) -> io::Result<()> {"}]}