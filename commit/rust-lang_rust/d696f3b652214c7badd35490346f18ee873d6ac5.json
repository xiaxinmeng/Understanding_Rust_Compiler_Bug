g.\n+\n+fn foofoo() {}\n+\n+mod outer {\n+    mod inner {\n+        use super::super::*;\n+        fn barbar() {\n+            let _ = foofoo();\n+        }\n+    }\n+}\n+\n+fn main() {}"}]}