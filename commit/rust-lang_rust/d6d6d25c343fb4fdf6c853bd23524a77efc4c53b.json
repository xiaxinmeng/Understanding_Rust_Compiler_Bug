c%2Ftest%2Fui%2Frfc-2632-const-trait-impl%2Fcall-const-trait-method-pass.rs", "raw_url": "https://github.com/rust-lang/rust/raw/d6d6d25c343fb4fdf6c853bd23524a77efc4c53b/src%2Ftest%2Fui%2Frfc-2632-const-trait-impl%2Fcall-const-trait-method-pass.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Frfc-2632-const-trait-impl%2Fcall-const-trait-method-pass.rs?ref=d6d6d25c343fb4fdf6c853bd23524a77efc4c53b", "patch": "@@ -0,0 +1,41 @@\n+// run-pass\n+\n+#![allow(incomplete_features)]\n+#![feature(const_trait_impl)]\n+#![feature(const_fn)]\n+\n+struct Int(i32);\n+\n+impl const std::ops::Add for Int {\n+    type Output = Int;\n+\n+    fn add(self, rhs: Self) -> Self {\n+        Int(self.0.plus(rhs.0))\n+    }\n+}\n+\n+impl const PartialEq for Int {\n+    fn eq(&self, rhs: &Self) -> bool {\n+        self.0 == rhs.0\n+    }\n+}\n+\n+pub trait Plus {\n+    fn plus(self, rhs: Self) -> Self;\n+}\n+\n+impl const Plus for i32 {\n+    fn plus(self, rhs: Self) -> Self {\n+        self + rhs\n+    }\n+}\n+\n+pub const fn add_i32(a: i32, b: i32) -> i32 {\n+    a.plus(b)\n+}\n+\n+const ADD_INT: Int = Int(1i32) + Int(2i32);\n+\n+fn main() {\n+    assert!(ADD_INT == Int(3i32));\n+}"}]}