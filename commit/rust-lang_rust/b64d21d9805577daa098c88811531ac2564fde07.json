/github.com/rust-lang/rust/raw/b64d21d9805577daa098c88811531ac2564fde07/clippy_lints%2Fsrc%2Fwrite.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_lints%2Fsrc%2Fwrite.rs?ref=b64d21d9805577daa098c88811531ac2564fde07", "patch": "@@ -235,8 +235,19 @@ impl EarlyLintPass for Write {\n     }\n \n     fn check_mac(&mut self, cx: &EarlyContext<'_>, mac: &MacCall) {\n+        fn is_build_script(cx: &EarlyContext<'_>) -> bool {\n+            // Cargo sets the crate name for build scripts to `build_script_build`\n+            cx.sess\n+                .opts\n+                .crate_name\n+                .as_ref()\n+                .map_or(false, |crate_name| crate_name == \"build_script_build\")\n+        }\n+\n         if mac.path == sym!(println) {\n-            span_lint(cx, PRINT_STDOUT, mac.span(), \"use of `println!`\");\n+            if !is_build_script(cx) {\n+                span_lint(cx, PRINT_STDOUT, mac.span(), \"use of `println!`\");\n+            }\n             if let (Some(fmt_str), _) = self.check_tts(cx, mac.args.inner_tokens(), false) {\n                 if fmt_str.symbol == Symbol::intern(\"\") {\n                     span_lint_and_sugg(\n@@ -251,7 +262,9 @@ impl EarlyLintPass for Write {\n                 }\n             }\n         } else if mac.path == sym!(print) {\n-            span_lint(cx, PRINT_STDOUT, mac.span(), \"use of `print!`\");\n+            if !is_build_script(cx) {\n+                span_lint(cx, PRINT_STDOUT, mac.span(), \"use of `print!`\");\n+            }\n             if let (Some(fmt_str), _) = self.check_tts(cx, mac.args.inner_tokens(), false) {\n                 if check_newlines(&fmt_str) {\n                     span_lint_and_then("}, {"sha": "3507168a3a96452abe013ab42094028e10d8db6a", "filename": "clippy_workspace_tests/build.rs", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/b64d21d9805577daa098c88811531ac2564fde07/clippy_workspace_tests%2Fbuild.rs", "raw_url": "https://github.com/rust-lang/rust/raw/b64d21d9805577daa098c88811531ac2564fde07/clippy_workspace_tests%2Fbuild.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_workspace_tests%2Fbuild.rs?ref=b64d21d9805577daa098c88811531ac2564fde07", "patch": "@@ -0,0 +1,7 @@\n+#![deny(clippy::print_stdout)]\n+\n+fn main() {\n+    // Test for #6041\n+    println!(\"Hello\");\n+    print!(\"Hello\");\n+}"}, {"sha": "997ebef8a699226eb46fe04ba72c078534a17adf", "filename": "tests/ui/print_stdout_build_script.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/b64d21d9805577daa098c88811531ac2564fde07/tests%2Fui%2Fprint_stdout_build_script.rs", "raw_url": "https://github.com/rust-lang/rust/raw/b64d21d9805577daa098c88811531ac2564fde07/tests%2Fui%2Fprint_stdout_build_script.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/tests%2Fui%2Fprint_stdout_build_script.rs?ref=b64d21d9805577daa098c88811531ac2564fde07", "patch": "@@ -0,0 +1,12 @@\n+// compile-flags: --crate-name=build_script_build\n+\n+#![warn(clippy::print_stdout)]\n+\n+fn main() {\n+    // Fix #6041\n+    //\n+    // The `print_stdout` lint shouldn't emit in `build.rs`\n+    // as these methods are used for the build script.\n+    println!(\"Hello\");\n+    print!(\"Hello\");\n+}"}]}