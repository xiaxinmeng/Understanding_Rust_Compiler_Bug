: RefMut<[i32; 3]> = cell.borrow_mut();\n+        cellref[0] = 4;\n+        let mut coerced: RefMut<[i32]> = cellref;\n+        coerced[2] = 5;\n+    }\n+    {\n+        let comp: &mut [i32] = &mut [4, 2, 5];\n+        let cellref: Ref<[i32; 3]> = cell.borrow();\n+        assert_eq!(&*cellref, comp);\n+        let coerced: Ref<[i32]> = cellref;\n+        assert_eq!(&*coerced, comp);\n+    }\n+}\n+\n+"}]}