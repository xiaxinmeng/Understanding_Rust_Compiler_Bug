/rust/raw/dda53383d9d932d5d55543c9f0bda7abc6b3241a/src%2Fbootstrap%2Fcompile.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fbootstrap%2Fcompile.rs?ref=dda53383d9d932d5d55543c9f0bda7abc6b3241a", "patch": "@@ -651,6 +651,12 @@ impl Step for Rustc {\n             panic!(\"Cannot use and generate PGO profiles at the same time\");\n         }\n \n+        // With LLD, we can use ICF (identical code folding) to reduce the executable size\n+        // of librustc_driver/rustc and to improve i-cache utilization.\n+        if builder.config.use_lld {\n+            cargo.rustflag(\"-Clink-args=-Wl,--icf=all\");\n+        }\n+\n         let is_collecting = if let Some(path) = &builder.config.rust_profile_generate {\n             if compiler.stage == 1 {\n                 cargo.rustflag(&format!(\"-Cprofile-generate={}\", path));"}, {"sha": "f262bc3c7d8d2112f04555861a5cccfec888a663", "filename": "src/ci/docker/host-x86_64/dist-x86_64-linux/Dockerfile", "status": "modified", "additions": 2, "deletions": 1, "changes": 3, "blob_url": "https://github.com/rust-lang/rust/blob/dda53383d9d932d5d55543c9f0bda7abc6b3241a/src%2Fci%2Fdocker%2Fhost-x86_64%2Fdist-x86_64-linux%2FDockerfile", "raw_url": "https://github.com/rust-lang/rust/raw/dda53383d9d932d5d55543c9f0bda7abc6b3241a/src%2Fci%2Fdocker%2Fhost-x86_64%2Fdist-x86_64-linux%2FDockerfile", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fci%2Fdocker%2Fhost-x86_64%2Fdist-x86_64-linux%2FDockerfile?ref=dda53383d9d932d5d55543c9f0bda7abc6b3241a", "patch": "@@ -122,7 +122,8 @@ ENV RUST_CONFIGURE_ARGS \\\n       --set target.x86_64-unknown-linux-gnu.ranlib=/rustroot/bin/llvm-ranlib \\\n       --set llvm.thin-lto=true \\\n       --set llvm.ninja=false \\\n-      --set rust.jemalloc\n+      --set rust.jemalloc \\\n+      --set rust.use-lld=true\n ENV SCRIPT ../src/ci/pgo.sh python3 ../x.py dist \\\n     --host $HOSTS --target $HOSTS \\\n     --include-default-paths \\"}, {"sha": "4773dc09960a00a599604b4f6dd987c3343dbdfd", "filename": "src/ci/pgo.sh", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/rust-lang/rust/blob/dda53383d9d932d5d55543c9f0bda7abc6b3241a/src%2Fci%2Fpgo.sh", "raw_url": "https://github.com/rust-lang/rust/raw/dda53383d9d932d5d55543c9f0bda7abc6b3241a/src%2Fci%2Fpgo.sh", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fci%2Fpgo.sh?ref=dda53383d9d932d5d55543c9f0bda7abc6b3241a", "patch": "@@ -195,3 +195,7 @@ rm -r $BUILD_ARTIFACTS/llvm $BUILD_ARTIFACTS/lld\n $@ \\\n     --rust-profile-use=${RUSTC_PROFILE_MERGED_FILE} \\\n     --llvm-profile-use=${LLVM_PROFILE_MERGED_FILE}\n+\n+echo \"Rustc binary size\"\n+ls -la ./build/$PGO_HOST/stage2/bin\n+ls -la ./build/$PGO_HOST/stage2/lib"}]}