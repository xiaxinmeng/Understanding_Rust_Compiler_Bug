o(Option<Box<Foo>>, Option<Box<Foo>>);\n+\n+fn test(f: &mut Foo) {\n+  match *f {\n+    Foo(Some(ref mut left), Some(ref mut right)) => match **left {\n+      Foo(Some(ref mut left), Some(ref mut right)) => panic!(),\n+      _ => panic!(),\n+    },\n+    _ => panic!(),\n+  }\n+}\n+\n+fn main() {\n+}"}]}