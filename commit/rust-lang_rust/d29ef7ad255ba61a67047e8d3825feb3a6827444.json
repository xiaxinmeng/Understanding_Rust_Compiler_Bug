tents/src%2Flibcore%2Fiterator.rs?ref=d29ef7ad255ba61a67047e8d3825feb3a6827444", "patch": "@@ -22,6 +22,7 @@ pub trait IteratorUtil<A> {\n     // FIXME: #5898: should be called map\n     fn transform<'r, B>(self, f: &'r fn(A) -> B) -> MapIterator<'r, A, B, Self>;\n     fn filter<'r>(self, predicate: &'r fn(&A) -> bool) -> FilterIterator<'r, A, Self>;\n+    fn enumerate(self) -> EnumerateIterator<Self>;\n     fn advance(&mut self, f: &fn(A) -> bool);\n }\n \n@@ -42,6 +43,11 @@ impl<A, T: Iterator<A>> IteratorUtil<A> for T {\n         FilterIterator{iter: self, predicate: predicate}\n     }\n \n+    #[inline(always)]\n+    fn enumerate(self) -> EnumerateIterator<T> {\n+        EnumerateIterator{iter: self, count: 0}\n+    }\n+\n     /// A shim implementing the `for` loop iteration protocol for iterator objects\n     #[inline]\n     fn advance(&mut self, f: &fn(A) -> bool) {\n@@ -104,3 +110,22 @@ impl<'self, A, B, T: Iterator<A>> Iterator<B> for MapIterator<'self, A, B, T> {\n         }\n     }\n }\n+\n+pub struct EnumerateIterator<T> {\n+    priv iter: T,\n+    priv count: uint\n+}\n+\n+impl<A, T: Iterator<A>> Iterator<(uint, A)> for EnumerateIterator<T> {\n+    #[inline]\n+    fn next(&mut self) -> Option<(uint, A)> {\n+        match self.iter.next() {\n+            Some(a) => {\n+                let ret = Some((self.count, a));\n+                self.count += 1;\n+                ret\n+            }\n+            _ => None\n+        }\n+    }\n+}"}, {"sha": "eebe6a7a37f32497ae6cbe945b4edf134bd56b7c", "filename": "src/libcore/vec.rs", "status": "modified", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/d29ef7ad255ba61a67047e8d3825feb3a6827444/src%2Flibcore%2Fvec.rs", "raw_url": "https://github.com/rust-lang/rust/raw/d29ef7ad255ba61a67047e8d3825feb3a6827444/src%2Flibcore%2Fvec.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibcore%2Fvec.rs?ref=d29ef7ad255ba61a67047e8d3825feb3a6827444", "patch": "@@ -4474,6 +4474,16 @@ mod tests {\n             i += 1;\n         }\n     }\n+\n+    #[test]\n+    fn test_iterator_enumerate() {\n+        use iterator::*;\n+        let xs = [0u,1,2,3,4,5];\n+        let mut it = xs.iter().enumerate();\n+        for it.advance |(i, &x): (uint, &uint)| {\n+            assert_eq!(i, x);\n+        }\n+    }\n }\n \n // Local Variables:"}]}