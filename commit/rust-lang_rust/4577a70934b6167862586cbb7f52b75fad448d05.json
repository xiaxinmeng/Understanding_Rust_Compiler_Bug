             tidy_error!(bad, \"{}: incorrect license\", file.display());\n         }\n+        match trailing_new_lines {\n+            0 => tidy_error!(bad, \"{}: missing trailing newline\", file.display()),\n+            1 | 2 => {}\n+            n => tidy_error!(bad, \"{}: too many trailing newlines ({})\", file.display(), n),\n+        };\n     })\n }\n "}]}