\n-fn main() {\n+use std;\n+import std._str;\n+\n+fn test1() {\n   let str s = \"hello\";\n   s += \"world\";\n   log s;\n   check(s.(9) == ('d' as u8));\n }\n+\n+fn test2() {\n+  // This tests for issue #163\n+\n+  let str ff = \"abc\";\n+  let str a = ff + \"ABC\" + ff;\n+  let str b = \"ABC\" + ff + \"ABC\";\n+\n+  log a;\n+  log b;\n+\n+  check (_str.eq(a, \"abcABCabc\"));\n+  check (_str.eq(b, \"ABCabcABC\"));\n+}\n+\n+fn main() {\n+  test1();\n+  test2();\n+}"}]}