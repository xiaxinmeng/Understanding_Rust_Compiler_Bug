\n+    let int i = _str.rindex(p, sep.(0));\n+    if (i == -1) {\n+        ret p;\n+    }\n+    ret _str.substr(p, 0u, i as uint);\n+}\n+\n+// Local Variables:\n+// mode: rust;\n+// fill-column: 78;\n+// indent-tabs-mode: nil\n+// c-basic-offset: 4\n+// buffer-file-coding-system: utf-8-unix\n+// compile-command: \"make -k -C .. 2>&1 | sed -e 's/\\\\/x\\\\//x:\\\\//g'\";\n+// End:"}, {"sha": "102aa4d15161cd3b5875787c17f275857b91c333", "filename": "src/lib/std.rc", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/11cbbcf79e0de904eb7f176db7f7587b40637ec8/src%2Flib%2Fstd.rc", "raw_url": "https://github.com/rust-lang/rust/raw/11cbbcf79e0de904eb7f176db7f7587b40637ec8/src%2Flib%2Fstd.rc", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flib%2Fstd.rc?ref=11cbbcf79e0de904eb7f176db7f7587b40637ec8", "patch": "@@ -56,6 +56,7 @@ mod rand;\n mod dbg;\n mod bitv;\n mod sort;\n+mod path;\n \n // Local Variables:\n // mode: rust;"}]}