/rust-lang/rust/contents/src%2Flibstd%2Fffi%2Fos_str.rs?ref=295bcdb715871ef3ba0258c75dad885b7315a162", "patch": "@@ -677,7 +677,13 @@ impl Borrow<OsStr> for OsString {\n #[stable(feature = \"rust1\", since = \"1.0.0\")]\n impl ToOwned for OsStr {\n     type Owned = OsString;\n-    fn to_owned(&self) -> OsString { self.to_os_string() }\n+    fn to_owned(&self) -> OsString {\n+        self.to_os_string()\n+    }\n+    fn clone_into(&self, target: &mut OsString) {\n+        target.clear();\n+        target.push(self);\n+    }\n }\n \n #[stable(feature = \"rust1\", since = \"1.0.0\")]\n@@ -863,4 +869,14 @@ mod tests {\n         let boxed = <Box<OsStr>>::default();\n         assert!(boxed.is_empty());\n     }\n+\n+    #[test]\n+    fn test_os_str_clone_into() {\n+        let mut os_string = OsString::with_capacity(123);\n+        os_string.push(\"hello\");\n+        let os_str = OsStr::new(\"bonjour\");\n+        os_str.clone_into(&mut os_string);\n+        assert_eq!(os_str, os_string);\n+        assert!(os_string.capacity() >= 123);\n+    }\n }"}, {"sha": "367779bb701c858f95f29fc28d39c510962f33e6", "filename": "src/libstd/lib.rs", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/295bcdb715871ef3ba0258c75dad885b7315a162/src%2Flibstd%2Flib.rs", "raw_url": "https://github.com/rust-lang/rust/raw/295bcdb715871ef3ba0258c75dad885b7315a162/src%2Flibstd%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Flib.rs?ref=295bcdb715871ef3ba0258c75dad885b7315a162", "patch": "@@ -311,6 +311,7 @@\n #![feature(str_utf16)]\n #![feature(test, rustc_private)]\n #![feature(thread_local)]\n+#![feature(toowned_clone_into)]\n #![feature(try_from)]\n #![feature(unboxed_closures)]\n #![feature(unicode)]"}, {"sha": "fcbd3705e881f56267990f0fc48913b79de8e50e", "filename": "src/libstd/path.rs", "status": "modified", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/295bcdb715871ef3ba0258c75dad885b7315a162/src%2Flibstd%2Fpath.rs", "raw_url": "https://github.com/rust-lang/rust/raw/295bcdb715871ef3ba0258c75dad885b7315a162/src%2Flibstd%2Fpath.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fpath.rs?ref=295bcdb715871ef3ba0258c75dad885b7315a162", "patch": "@@ -1322,6 +1322,9 @@ impl ToOwned for Path {\n     fn to_owned(&self) -> PathBuf {\n         self.to_path_buf()\n     }\n+    fn clone_into(&self, target: &mut PathBuf) {\n+        self.inner.clone_into(&mut target.inner);\n+    }\n }\n \n #[stable(feature = \"rust1\", since = \"1.0.0\")]\n@@ -3722,4 +3725,13 @@ mod tests {\n         assert_eq!(&*boxed, &*path_buf);\n         assert_eq!(&*path_buf, path);\n     }\n+\n+    #[test]\n+    fn test_clone_into() {\n+        let mut path_buf = PathBuf::from(\"supercalifragilisticexpialidocious\");\n+        let path = Path::new(\"short\");\n+        path.clone_into(&mut path_buf);\n+        assert_eq!(path, path_buf);\n+        assert!(path_buf.into_os_string().capacity() >= 15);\n+    }\n }"}]}