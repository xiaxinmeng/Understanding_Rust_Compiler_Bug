pied, modified, or distributed\n+// except according to those terms.\n+\n+fn f(mut y: ~int) {\n+    *y = 5;\n+    assert *y == 5;\n+}\n+\n+fn g() {\n+    let frob: fn(~int) = |mut q| { *q = 2; assert *q == 2; };\n+    let w = ~37;\n+    frob(w);\n+\n+}\n+\n+fn main() {\n+    let z = ~17;\n+    f(z);\n+    g();\n+}\n\\ No newline at end of file"}]}