         */\n+    test %rax,%rax\n+    jle .Lelf_exit                    /* exit on error          */\n+    add %rax,%rsi\n+    sub %rax,%rdx                     /* all chars written?     */\n+    jnz .Lelf_entry_call\n+\n+.Lelf_exit:    \n+    movq $60,%rax                     /* exit() syscall         */\n+    movq $1,%rdi                      /* exit code 1            */\n     syscall\n+    ud2                               /* should not be reached  */\n /*  end elf_entry */\n \n .text"}]}