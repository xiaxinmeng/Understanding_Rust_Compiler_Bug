ontents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissue-2718.rs?ref=91b69aeb63440d0d8ba09fdff57be2e343e6b2e8", "patch": "@@ -0,0 +1,38 @@\n+fn sender_terminate<T:send>(p: *packet<T>) {\n+}\n+\n+class send_packet<T: send> {\n+        let mut p: option<*packet<T>>;\n+        new(p: *packet<T>) { self.p = some(p); }\n+        drop {\n+            if self.p != none {\n+                let mut p = none;\n+                p <-> self.p;\n+                sender_terminate(option::unwrap(p))\n+            }\n+        }\n+        fn unwrap() -> *packet<T> {\n+            let mut p = none;\n+            p <-> self.p;\n+            option::unwrap(p)\n+        }\n+}\n+\n+enum state {\n+        empty,\n+        full,\n+        blocked,\n+        terminated\n+}\n+\n+type packet<T: send> = {\n+        mut state: state,\n+        mut blocked_task: option<task::task>,\n+        mut payload: option<T>\n+};\n+\n+fn main() {\n+  let _s: send_packet<int> = send_packet(ptr::addr_of({mut state: empty,\n+        mut blocked_task: none,\n+        mut payload: some(42)}));\n+}"}]}