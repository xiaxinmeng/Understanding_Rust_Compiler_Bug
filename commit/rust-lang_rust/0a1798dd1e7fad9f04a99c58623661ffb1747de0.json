> State<'a> {\n     fn print_poly_trait_ref(&mut self, t: &ast::PolyTraitRef) -> IoResult<()> {\n         if !t.bound_lifetimes.is_empty() {\n             try!(word(&mut self.s, \"for<\"));\n+            let mut comma = false;\n             for lifetime_def in t.bound_lifetimes.iter() {\n+                if comma {\n+                    try!(self.word_space(\",\"))\n+                }\n                 try!(self.print_lifetime_def(lifetime_def));\n+                comma = true;\n             }\n             try!(word(&mut self.s, \">\"));\n         }"}]}