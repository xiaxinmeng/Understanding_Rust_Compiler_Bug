?ref=768247f393a18ec56d7a92cded2545e6f7d92649", "patch": "@@ -0,0 +1,8 @@\n+type t<T> = { f: fn() -> T };\n+\n+fn f<T>(_x: t<T>) {}\n+\n+fn main() {\n+    let x: t<()> = { f: { || () } };\n+    f(x); //~ ERROR copying a noncopyable value\n+}\n\\ No newline at end of file"}, {"sha": "bee92bc9c9abbeb02c9a84ec542bc30188f81522", "filename": "src/test/run-pass/issue-1896-1.rs", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/rust-lang/rust/blob/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-1.rs", "raw_url": "https://github.com/rust-lang/rust/raw/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-1.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissue-1896-1.rs?ref=768247f393a18ec56d7a92cded2545e6f7d92649", "patch": "@@ -0,0 +1,13 @@\n+type boxedFn = { theFn: fn () -> uint };\n+\n+fn createClosure (closedUint: uint) -> boxedFn {\n+    { theFn: fn@ () -> uint { closedUint } }\n+}\n+\n+fn main () {\n+    let aFn: boxedFn = createClosure(10);\n+\n+    let myInt: uint = aFn.theFn();\n+\n+    assert myInt == 10;\n+}\n\\ No newline at end of file"}, {"sha": "671d80a1227027f8dd510d845e4d93f8d43ad2e5", "filename": "src/test/run-pass/issue-1896-2.rs", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-2.rs", "raw_url": "https://github.com/rust-lang/rust/raw/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-2.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissue-1896-2.rs?ref=768247f393a18ec56d7a92cded2545e6f7d92649", "patch": "@@ -0,0 +1,10 @@\n+fn add(n: int) -> fn@(int) -> int {\n+      fn@(m: int) -> int { m + n }\n+}\n+\n+fn main()\n+{\n+      assert add(3)(4) == 7;\n+        let add3 : fn(int)->int = add(3);\n+          assert add3(4) == 7;\n+}\n\\ No newline at end of file"}, {"sha": "7cc254d7c19a9ea3a90afb7eb7282adda604a9f3", "filename": "src/test/run-pass/issue-1896-3.rs", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/rust-lang/rust/blob/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-3.rs", "raw_url": "https://github.com/rust-lang/rust/raw/768247f393a18ec56d7a92cded2545e6f7d92649/src%2Ftest%2Frun-pass%2Fissue-1896-3.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissue-1896-3.rs?ref=768247f393a18ec56d7a92cded2545e6f7d92649", "patch": "@@ -0,0 +1,17 @@\n+fn add(n: int) -> fn@(int) -> int {\n+    fn@(m: int) -> int { m + n }\n+}\n+\n+fn main()\n+{\n+    assert add(3)(4) == 7;\n+\n+    let add1 : fn@(int)->int = add(1);\n+    assert add1(6) == 7;\n+\n+    let add2 : &(fn@(int)->int) = &add(2);\n+    assert (*add2)(5) == 7;\n+\n+    let add3 : fn(int)->int = add(3);\n+    assert add3(4) == 7;\n+}"}]}