ub.com/rust-lang/rust/blob/a2fab457dcb6e65e119694acacc8d92fde2569c2/src%2Ftest%2Frun-pass%2Ftemporary-lifetime-for-conditions.rs", "raw_url": "https://github.com/rust-lang/rust/raw/a2fab457dcb6e65e119694acacc8d92fde2569c2/src%2Ftest%2Frun-pass%2Ftemporary-lifetime-for-conditions.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Ftemporary-lifetime-for-conditions.rs?ref=a2fab457dcb6e65e119694acacc8d92fde2569c2", "patch": "@@ -23,7 +23,7 @@ impl Drop for Temporary {\n }\n \n impl Temporary {\n-    fn do(&self) -> bool {true}\n+    fn do_stuff(&self) -> bool {true}\n }\n \n fn borrow() -> ~Temporary { ~Temporary }\n@@ -35,7 +35,7 @@ pub fn main() {\n     // This loop's condition\n     // should call `Temporary`'s\n     // `drop` 6 times.\n-    while borrow().do() {\n+    while borrow().do_stuff() {\n         i += 1;\n         if i > 5 {\n             break;\n@@ -44,7 +44,7 @@ pub fn main() {\n \n     // This if condition should\n     // call it 1 time\n-    if borrow().do() {\n+    if borrow().do_stuff() {\n         unsafe { assert_eq!(DROPPED, 7) }\n     }\n }"}]}