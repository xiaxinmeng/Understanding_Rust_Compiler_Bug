ata;\n \n struct Drain<'a, T: 'a> {\n+    // Need to bound the lifetime here, so we do it with `&'a mut Vec<T>`\n+    // because that's semantically what we contain. We're \"just\" calling\n+    // `pop()` and `remove(0)`.\n     vec: PhantomData<&'a mut Vec<T>>\n     start: *const T,\n     end: *const T,"}]}