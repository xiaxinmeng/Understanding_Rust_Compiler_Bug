tch": "@@ -0,0 +1,19 @@\n+// compile-pass\n+\n+#![deny(unreachable_code)]\n+\n+mod foo {\n+    enum Bar {}\n+\n+    #[allow(dead_code)]\n+    pub struct Foo {\n+        value: Bar, // \"privately\" uninhabited\n+    }\n+\n+    pub fn give_foo() -> Foo { panic!() }\n+}\n+\n+fn main() {\n+    foo::give_foo();\n+    println!(\"Hello, world!\"); // ok: we can't tell that this code is dead\n+}"}]}