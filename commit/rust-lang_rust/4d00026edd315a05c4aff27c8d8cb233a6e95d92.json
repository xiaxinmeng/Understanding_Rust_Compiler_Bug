 mut &'a ());\n+enum Foo<T> {\n+    Bar,\n+    Var(T),\n+}\n+type Supertype = Foo<for<'a> fn(Inv<'a>, Inv<'a>)>;\n+\n+fn foo(x: Foo<for<'a, 'b> fn(Inv<'a>, Inv<'b>)>) {\n+    match x {\n+        Supertype::Bar => {}\n+        Supertype::Var(x) => {}\n+    }\n+}\n+\n+fn foo_nested(x: Foo<Foo<for<'a, 'b> fn(Inv<'a>, Inv<'b>)>>) {\n+    match x {\n+        Foo::Bar => {}\n+        Foo::Var(Supertype::Bar) => {}\n+        Foo::Var(Supertype::Var(x)) => {}\n+    }\n+}\n+\n+fn main() {}"}]}