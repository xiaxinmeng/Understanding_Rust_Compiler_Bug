\n }\n \n+/*\n+Function: from_cstr_len\n+\n+Create a Rust string from a C string of the given length\n+*/\n+unsafe fn from_cstr_len(cstr: sbuf, len: uint) -> str {\n+    let res = [];\n+    let start = cstr;\n+    let curr = start;\n+    let i = 0u;\n+    while i < len {\n+        vec::push(res, *curr);\n+        i += 1u;\n+        curr = ptr::offset(start, i);\n+    }\n+    ret from_bytes(res);\n+}\n+\n /*\n Function: concat\n "}]}