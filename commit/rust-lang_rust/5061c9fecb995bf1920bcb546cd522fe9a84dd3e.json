s": 4, "blob_url": "https://github.com/rust-lang/rust/blob/5061c9fecb995bf1920bcb546cd522fe9a84dd3e/src%2Flibcore%2Fnum%2Fmod.rs", "raw_url": "https://github.com/rust-lang/rust/raw/5061c9fecb995bf1920bcb546cd522fe9a84dd3e/src%2Flibcore%2Fnum%2Fmod.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibcore%2Fnum%2Fmod.rs?ref=5061c9fecb995bf1920bcb546cd522fe9a84dd3e", "patch": "@@ -2331,11 +2331,11 @@ impl u8 {\n     /// let lowercase_a = 97u8;\n     /// let uppercase_a = 65u8;\n     ///\n-    /// assert!(lowercase_a.eq_ignore_ascii_case(uppercase_a));\n+    /// assert!(lowercase_a.eq_ignore_ascii_case(&uppercase_a));\n     /// ```\n     #[stable(feature = \"ascii_methods_on_intrinsics\", since = \"1.21.0\")]\n     #[inline]\n-    pub fn eq_ignore_ascii_case(&self, other: u8) -> bool {\n+    pub fn eq_ignore_ascii_case(&self, other: &u8) -> bool {\n         self.to_ascii_lowercase() == other.to_ascii_lowercase()\n     }\n "}, {"sha": "327deb9b419bee6c93ead3b6bf3554c152dfaace", "filename": "src/libstd/ascii.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/5061c9fecb995bf1920bcb546cd522fe9a84dd3e/src%2Flibstd%2Fascii.rs", "raw_url": "https://github.com/rust-lang/rust/raw/5061c9fecb995bf1920bcb546cd522fe9a84dd3e/src%2Flibstd%2Fascii.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fascii.rs?ref=5061c9fecb995bf1920bcb546cd522fe9a84dd3e", "patch": "@@ -685,7 +685,7 @@ impl AsciiExt for [u8] {\n     #[inline]\n     fn eq_ignore_ascii_case(&self, other: &[u8]) -> bool {\n         self.len() == other.len() &&\n-        self.iter().zip(other).all(|(a, &b)| {\n+        self.iter().zip(other).all(|(a, b)| {\n             a.eq_ignore_ascii_case(b)\n         })\n     }"}]}