+\n+fn main() {\n+    let i = ~@1;\n+    let j = ~@2;\n+    let rc1 = refcount(*i);\n+    let j = i;\n+    let rc2 = refcount(*i);\n+    log_err #fmt(\"rc1: %u rc2: %u\", rc1, rc2);\n+    assert rc1 + 1u == rc2;\n+}\n\\ No newline at end of file"}]}