ttern: reference is not valid outside of its lifetime\n+use std;\n+import std::arc;\n+fn main() {\n+    let x = ~arc::rw_arc(1);\n+    let mut y = none;\n+    do x.write |one| {\n+        y = some(one);\n+    }\n+    *option::unwrap(y) = 2;\n+}"}]}