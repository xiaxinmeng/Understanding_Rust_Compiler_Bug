", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fffi%2Fc_str.rs?ref=77987ba89894997c174ac8b01185c0dc12913fa5", "patch": "@@ -326,6 +326,22 @@ impl fmt::Debug for CStr {\n     }\n }\n \n+#[stable(feature = \"cstr_default\", since = \"1.10.0\")]\n+impl<'a> Default for &'a CStr {\n+    fn default() -> &'a CStr {\n+        static SLICE: &'static [c_char] = &[0];\n+        unsafe { CStr::from_ptr(SLICE.as_ptr()) }\n+    }\n+}\n+\n+#[stable(feature = \"cstr_default\", since = \"1.10.0\")]\n+impl Default for CString {\n+    fn default() -> CString {\n+        let a: &CStr = Default::default();\n+        a.to_owned()\n+    }\n+}\n+\n #[stable(feature = \"cstr_borrow\", since = \"1.3.0\")]\n impl Borrow<CStr> for CString {\n     fn borrow(&self) -> &CStr { self }"}]}