_ctypes)]\n+\n+// ignore-wasm32-bare no libc for ffi testing\n+\n+// Test a foreign function that accepts and returns a struct\n+// by value.\n+\n+#[derive(Copy, Clone, PartialEq, Debug)]\n+pub struct TwoU32s {\n+    one: u32, two: u32\n+}\n+\n+#[link(name = \"rust_test_helpers\", kind = \"static\")]\n+extern {\n+    pub fn rust_dbg_extern_identity_TwoU32s(v: TwoU32s) -> TwoU32s;\n+}\n+\n+pub fn main() {\n+    unsafe {\n+        let x = TwoU32s {one: 22, two: 23};\n+        let y = rust_dbg_extern_identity_TwoU32s(x);\n+        assert_eq!(x, y);\n+    }\n+}"}, {"sha": "419648263aa9baecebb83f774ed2a3f87ba2761f", "filename": "src/test/run-pass/abi/extern/extern-pass-TwoU64s.rs", "status": "added", "additions": 25, "deletions": 0, "changes": 25, "blob_url": "https://github.com/rust-lang/rust/blob/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-TwoU64s.rs", "raw_url": "https://github.com/rust-lang/rust/raw/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-TwoU64s.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-TwoU64s.rs?ref=98325eb592f537e87100199c0c67682306df3fa4", "patch": "@@ -0,0 +1,25 @@\n+// run-pass\n+#![allow(improper_ctypes)]\n+\n+// ignore-wasm32-bare no libc for ffi testing\n+\n+// Test a foreign function that accepts and returns a struct\n+// by value.\n+\n+#[derive(Copy, Clone, PartialEq, Debug)]\n+pub struct TwoU64s {\n+    one: u64, two: u64\n+}\n+\n+#[link(name = \"rust_test_helpers\", kind = \"static\")]\n+extern {\n+    pub fn rust_dbg_extern_identity_TwoU64s(v: TwoU64s) -> TwoU64s;\n+}\n+\n+pub fn main() {\n+    unsafe {\n+        let x = TwoU64s {one: 22, two: 23};\n+        let y = rust_dbg_extern_identity_TwoU64s(x);\n+        assert_eq!(x, y);\n+    }\n+}"}, {"sha": "f2efdb7d3664ea7964d75674825179f8cb857db2", "filename": "src/test/run-pass/abi/extern/extern-pass-u32.rs", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/rust-lang/rust/blob/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u32.rs", "raw_url": "https://github.com/rust-lang/rust/raw/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u32.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u32.rs?ref=98325eb592f537e87100199c0c67682306df3fa4", "patch": "@@ -0,0 +1,16 @@\n+// run-pass\n+// ignore-wasm32-bare no libc for ffi testing\n+\n+// Test a function that takes/returns a u32.\n+\n+\n+#[link(name = \"rust_test_helpers\", kind = \"static\")]\n+extern {\n+    pub fn rust_dbg_extern_identity_u32(v: u32) -> u32;\n+}\n+\n+pub fn main() {\n+    unsafe {\n+        assert_eq!(22, rust_dbg_extern_identity_u32(22));\n+    }\n+}"}, {"sha": "975446d430c2869b56619c915e73d3882b2e89fd", "filename": "src/test/run-pass/abi/extern/extern-pass-u64.rs", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/rust-lang/rust/blob/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u64.rs", "raw_url": "https://github.com/rust-lang/rust/raw/98325eb592f537e87100199c0c67682306df3fa4/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u64.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fabi%2Fextern%2Fextern-pass-u64.rs?ref=98325eb592f537e87100199c0c67682306df3fa4", "patch": "@@ -0,0 +1,16 @@\n+// run-pass\n+// ignore-wasm32-bare no libc for ffi testing\n+\n+// Test a call to a function that takes/returns a u64.\n+\n+\n+#[link(name = \"rust_test_helpers\", kind = \"static\")]\n+extern {\n+    pub fn rust_dbg_extern_identity_u64(v: u64) -> u64;\n+}\n+\n+pub fn main() {\n+    unsafe {\n+        assert_eq!(22, rust_dbg_extern_identity_u64(22));\n+    }\n+}"}]}