i.github.com/repos/rust-lang/rust/contents/src%2Flibrustc_mir%2Fborrow_check%2Fnll%2Ftype_check%2Fmod.rs?ref=73a09f35b114d9ddf10e29df4e730d15e64f58c7", "patch": "@@ -759,6 +759,11 @@ impl<'a, 'gcx, 'tcx> TypeChecker<'a, 'gcx, 'tcx> {\n         sup: Ty<'tcx>,\n         locations: Locations,\n     ) -> UnitResult<'tcx> {\n+        // Micro-optimization.\n+        if sub == sup {\n+            return Ok(());\n+        }\n+\n         self.fully_perform_op(\n             locations,\n             || format!(\"sub_types({:?} <: {:?})\", sub, sup),\n@@ -772,6 +777,11 @@ impl<'a, 'gcx, 'tcx> TypeChecker<'a, 'gcx, 'tcx> {\n \n     #[inline(never)]\n     fn eq_types(&mut self, a: Ty<'tcx>, b: Ty<'tcx>, locations: Locations) -> UnitResult<'tcx> {\n+        // Micro-optimization.\n+        if a == b {\n+            return Ok(());\n+        }\n+\n         self.fully_perform_op(\n             locations,\n             || format!(\"eq_types({:?} = {:?})\", a, b),"}]}