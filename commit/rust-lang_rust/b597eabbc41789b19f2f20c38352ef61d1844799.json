;\n+            }\n \n             // Some things needed for `sys::thread` initialization to go through.\n             \"signal\" | \"sigaction\" | \"sigaltstack\" => {"}, {"sha": "504ef47897953862ed0820cdf18c55895e95380a", "filename": "tests/run-pass/intrinsics-math.rs", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/rust-lang/rust/blob/b597eabbc41789b19f2f20c38352ef61d1844799/tests%2Frun-pass%2Fintrinsics-math.rs", "raw_url": "https://github.com/rust-lang/rust/raw/b597eabbc41789b19f2f20c38352ef61d1844799/tests%2Frun-pass%2Fintrinsics-math.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/tests%2Frun-pass%2Fintrinsics-math.rs?ref=b597eabbc41789b19f2f20c38352ef61d1844799", "patch": "@@ -66,4 +66,7 @@ pub fn main() {\n \n     assert_approx_eq!(0.1f32.trunc(), 0.0f32);\n     assert_approx_eq!((-0.1f64).trunc(), 0.0f64);\n+\n+    assert_approx_eq!(27f64.cbrt(), 3.0f64);\n+    assert_approx_eq!(3f64.hypot(4f64), 5.0f64);\n }"}]}