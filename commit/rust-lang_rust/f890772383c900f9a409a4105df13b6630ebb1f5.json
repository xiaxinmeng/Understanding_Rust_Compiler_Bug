_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fcollections%2Fhash%2Ftable.rs?ref=f890772383c900f9a409a4105df13b6630ebb1f5", "patch": "@@ -12,6 +12,7 @@ use alloc::heap::{allocate, deallocate, EMPTY};\n \n use cmp;\n use hash::{Hash, Hasher, BuildHasher};\n+use intrinsics::needs_drop;\n use marker;\n use mem::{align_of, size_of};\n use mem;\n@@ -1009,7 +1010,9 @@ impl<K, V> Drop for RawTable<K, V> {\n         // dropping empty tables such as on resize.\n         // Also avoid double drop of elements that have been already moved out.\n         unsafe {\n-            for _ in self.rev_move_buckets() {}\n+            if needs_drop::<(K, V)>() { // avoid linear runtime for types that don't need drop\n+                for _ in self.rev_move_buckets() {}\n+            }\n         }\n \n         let hashes_size = self.capacity * size_of::<u64>();"}]}