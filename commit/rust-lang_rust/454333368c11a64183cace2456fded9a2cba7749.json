n \n pure fn add(x: int, y: int) -> int { ret x + y; }"}, {"sha": "84b6bc5b1e07e9a31b44424ad7a2a1f387d34693", "filename": "src/lib/uint.rs", "status": "modified", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/rust-lang/rust/blob/454333368c11a64183cace2456fded9a2cba7749/src%2Flib%2Fuint.rs", "raw_url": "https://github.com/rust-lang/rust/raw/454333368c11a64183cace2456fded9a2cba7749/src%2Flib%2Fuint.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flib%2Fuint.rs?ref=454333368c11a64183cace2456fded9a2cba7749", "patch": "@@ -1,5 +1,19 @@\n+/**\n+ * Return the minimal value for an uint.\n+ *\n+ * This is always 0\n+ */\n pure fn min_value() -> uint { ret 0u; }\n \n+/**\n+ * Return the maximal value for an uint.\n+ *\n+ * This is 2^wordsize - 1\n+ */\n+pure fn max_value() -> uint {\n+     ret 0u - 1u;\n+}\n+\n fn add(x: uint, y: uint) -> uint { ret x + y; }\n \n fn sub(x: uint, y: uint) -> uint { ret x - y; }"}, {"sha": "4a87908e5a64f6583debc1414a4ed5c7db74e1fc", "filename": "src/test/stdtest/int.rs", "status": "modified", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/454333368c11a64183cace2456fded9a2cba7749/src%2Ftest%2Fstdtest%2Fint.rs", "raw_url": "https://github.com/rust-lang/rust/raw/454333368c11a64183cace2456fded9a2cba7749/src%2Ftest%2Fstdtest%2Fint.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fstdtest%2Fint.rs?ref=454333368c11a64183cace2456fded9a2cba7749", "patch": "@@ -23,3 +23,10 @@ fn test_pow() {\n     assert (int::pow(-3, 3u) == -27);\n     assert (int::pow(4, 9u) == 262144);\n }\n+\n+#[test]\n+fn test_overflows() {\n+   assert (int::max_value() > 0);\n+   assert (int::min_value() <= 0);\n+   assert (int::min_value() + int::max_value() + 1 == 0);\n+}\n\\ No newline at end of file"}, {"sha": "141b59ace82a871e80ac8d78e6d38f023e20cf25", "filename": "src/test/stdtest/uint.rs", "status": "modified", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/454333368c11a64183cace2456fded9a2cba7749/src%2Ftest%2Fstdtest%2Fuint.rs", "raw_url": "https://github.com/rust-lang/rust/raw/454333368c11a64183cace2456fded9a2cba7749/src%2Ftest%2Fstdtest%2Fuint.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fstdtest%2Fuint.rs?ref=454333368c11a64183cace2456fded9a2cba7749", "patch": "@@ -47,3 +47,10 @@ fn test_next_power_of_two() {\n     assert (uint::next_power_of_two(38u) == 64u);\n     assert (uint::next_power_of_two(39u) == 64u);\n }\n+\n+#[test]\n+fn test_overflows() {\n+   assert (uint::max_value() > 0u);\n+   assert (uint::min_value() <= 0u);\n+   assert (uint::min_value() + uint::max_value() + 1u == 0u);\n+}\n\\ No newline at end of file"}]}