   ///     let mut f = File::options().read(true).open(\"foo.txt\")?;\n     ///     Ok(())\n     /// }\n     /// ```\n     #[must_use]\n-    #[unstable(feature = \"with_options\", issue = \"65439\")]\n-    pub fn with_options() -> OpenOptions {\n+    #[stable(feature = \"with_options\", since = \"1.58.0\")]\n+    pub fn options() -> OpenOptions {\n         OpenOptions::new()\n     }\n "}]}