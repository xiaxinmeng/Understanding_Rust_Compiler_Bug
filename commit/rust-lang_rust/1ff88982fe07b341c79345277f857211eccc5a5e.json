ult {\n     for (@ast.stmt s in b.node.stmts) {\n         r = trans_stmt(bcx, *s);\n         bcx = r.bcx;\n+        // If we hit a terminator, control won't go any further so\n+        // we're in dead-code land. Stop here.\n+        if (is_terminated(bcx)) {\n+            ret r;\n+        }\n     }\n \n     bcx = trans_block_cleanups(bcx);"}]}