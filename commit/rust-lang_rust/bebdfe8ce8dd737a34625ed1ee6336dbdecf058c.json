xiliary%2Fcci_class_3.rs?ref=bebdfe8ce8dd737a34625ed1ee6336dbdecf058c", "patch": "@@ -0,0 +1,17 @@\n+mod kitties {\n+\n+class cat {\n+  priv {\n+    let mutable meows : uint;\n+  }\n+\n+  let how_hungry : int;\n+\n+  new(in_x : uint, in_y : int) { meows = in_x; how_hungry = in_y; }\n+\n+  fn speak() { meows += 1u; }\n+  fn meow_count() -> uint { meows }\n+\n+}\n+\n+}"}, {"sha": "d6fb30dc7b2f9df444f0ea4e9d351a8a23e44eae", "filename": "src/test/run-pass/class-methods-cross-crate.rs", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/rust-lang/rust/blob/bebdfe8ce8dd737a34625ed1ee6336dbdecf058c/src%2Ftest%2Frun-pass%2Fclass-methods-cross-crate.rs", "raw_url": "https://github.com/rust-lang/rust/raw/bebdfe8ce8dd737a34625ed1ee6336dbdecf058c/src%2Ftest%2Frun-pass%2Fclass-methods-cross-crate.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fclass-methods-cross-crate.rs?ref=bebdfe8ce8dd737a34625ed1ee6336dbdecf058c", "patch": "@@ -0,0 +1,13 @@\n+// xfail-fast\n+// aux-build:cci_class_3.rs\n+use cci_class_3;\n+import cci_class_3::kitties::*;\n+\n+fn main() {\n+  let nyan : cat = cat(52u, 99);\n+  let kitty = cat(1000u, 2);\n+  assert(nyan.how_hungry == 99);\n+  assert(kitty.how_hungry == 2);\n+  nyan.speak();\n+  assert(nyan.meow_count() == 53u);\n+}"}]}