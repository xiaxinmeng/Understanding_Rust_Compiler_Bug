rl -w\n+\n+$ident = 0;\n+while (<>) {\n+    if (/^rust: \">>/) {\n+        $indent += 1;\n+    } elsif (/^rust: \"<</) {\n+        $indent -= 1;\n+    }\n+\n+    printf \"%03d  \", $indent;\n+    for ($i = 0; $i < $indent; $i++) {\n+        printf(\"  \");\n+    }\n+    print;\n+}\n+"}, {"sha": "9c666beb54dd902129e0022441f7a061af6704f7", "filename": "src/rustc/util/common.rs", "status": "modified", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/rust-lang/rust/blob/f1afb0b3e226164f551f3e2fc1270c6dba007281/src%2Frustc%2Futil%2Fcommon.rs", "raw_url": "https://github.com/rust-lang/rust/raw/f1afb0b3e226164f551f3e2fc1270c6dba007281/src%2Frustc%2Futil%2Fcommon.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Frustc%2Futil%2Fcommon.rs?ref=f1afb0b3e226164f551f3e2fc1270c6dba007281", "patch": "@@ -5,6 +5,24 @@ import syntax::codemap::{span};\n import syntax::visit;\n import syntax::print;\n \n+fn indent<R>(op: fn() -> R) -> R {\n+    // Use in conjunction with the log post-processor like `src/etc/indenter`\n+    // to make debug output more readable.\n+    #debug[\">>\"];\n+    let r <- op();\n+    #debug[\"<< (Result = %?)\", r];\n+    ret r;\n+}\n+\n+resource _indenter(_i: ()) {\n+    #debug[\"<<\"];\n+}\n+\n+fn indenter() -> _indenter {\n+    #debug[\">>\"];\n+    _indenter(())\n+}\n+\n type flag = hashmap<str, ()>;\n \n fn def_eq(a: ast::def_id, b: ast::def_id) -> bool {"}]}