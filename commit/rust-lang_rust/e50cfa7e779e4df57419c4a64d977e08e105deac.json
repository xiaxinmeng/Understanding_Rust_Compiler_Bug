ng/rust/contents/tests%2Fcompile-fail%2Fshadow.rs?ref=e50cfa7e779e4df57419c4a64d977e08e105deac", "patch": "@@ -20,6 +20,9 @@ fn main() {\n     let y = 1;\n     let x = y; //~ERROR `x` is shadowed by `y`\n \n+    let x; //~ERROR `x` shadows a previous declaration\n+    x = 42;\n+\n     let o = Some(1_u8);\n \n     if let Some(p) = o { assert_eq!(1, p); }"}]}