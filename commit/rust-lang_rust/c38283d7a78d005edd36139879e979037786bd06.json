contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fconsts%2Fmiri_unleashed%2Fnrvo.rs?ref=c38283d7a78d005edd36139879e979037786bd06", "patch": "@@ -0,0 +1,23 @@\n+// compile-flags: -Zunleash-the-miri-inside-of-you\n+// run-pass\n+\n+const fn init(buf: &mut [u8; 1024]) {\n+    buf[33] = 3;\n+    buf[444] = 4;\n+}\n+\n+const fn nrvo(init: fn(&mut [u8; 1024])) -> [u8; 1024] {\n+    let mut buf = [0; 1024];\n+    init(&mut buf);\n+    buf\n+}\n+\n+// When the NRVO is applied, the return place (`_0`) gets treated like a normal local. For example,\n+// its address may be taken and it may be written to indirectly. Ensure that MIRI can handle this.\n+const BUF: [u8; 1024] = nrvo(init);\n+\n+fn main() {\n+    assert_eq!(BUF[33], 3);\n+    assert_eq!(BUF[19], 0);\n+    assert_eq!(BUF[444], 4);\n+}"}, {"sha": "d38f869181b29d53ec2273c07eb892ebcdca6172", "filename": "src/test/ui/consts/miri_unleashed/nrvo.stderr", "status": "added", "additions": 27, "deletions": 0, "changes": 27, "blob_url": "https://github.com/rust-lang/rust/blob/c38283d7a78d005edd36139879e979037786bd06/src%2Ftest%2Fui%2Fconsts%2Fmiri_unleashed%2Fnrvo.stderr", "raw_url": "https://github.com/rust-lang/rust/raw/c38283d7a78d005edd36139879e979037786bd06/src%2Ftest%2Fui%2Fconsts%2Fmiri_unleashed%2Fnrvo.stderr", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fconsts%2Fmiri_unleashed%2Fnrvo.stderr?ref=c38283d7a78d005edd36139879e979037786bd06", "patch": "@@ -0,0 +1,27 @@\n+warning: skipping const checks\n+   |\n+help: skipping check for `const_mut_refs` feature\n+  --> $DIR/nrvo.rs:5:5\n+   |\n+LL |     buf[33] = 3;\n+   |     ^^^^^^^^^^^\n+help: skipping check for `const_mut_refs` feature\n+  --> $DIR/nrvo.rs:6:5\n+   |\n+LL |     buf[444] = 4;\n+   |     ^^^^^^^^^^^^\n+help: skipping check for `const_mut_refs` feature\n+  --> $DIR/nrvo.rs:11:10\n+   |\n+LL |     init(&mut buf);\n+   |          ^^^^^^^^\n+help: skipping check that does not even 