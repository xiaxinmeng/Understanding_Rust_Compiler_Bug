ents/library%2Falloc%2Fbenches%2Fvec_deque.rs?ref=84b8898d6357810472a01038bde7b1788615aa8a", "patch": "@@ -67,3 +67,27 @@ fn bench_from_array_1000(b: &mut Bencher) {\n         black_box(deq);\n     })\n }\n+\n+#[bench]\n+fn bench_extend_bytes(b: &mut Bencher) {\n+    let mut ring: VecDeque<u8> = VecDeque::with_capacity(1000);\n+    let input: &[u8] = &[128; 512];\n+\n+    b.iter(|| {\n+        ring.clear();\n+        ring.extend(black_box(input));\n+    });\n+}\n+\n+#[bench]\n+fn bench_extend_vec(b: &mut Bencher) {\n+    let mut ring: VecDeque<u8> = VecDeque::with_capacity(1000);\n+    let input = vec![128; 512];\n+\n+    b.iter(|| {\n+        ring.clear();\n+\n+        let input = input.clone();\n+        ring.extend(black_box(input));\n+    });\n+}"}]}