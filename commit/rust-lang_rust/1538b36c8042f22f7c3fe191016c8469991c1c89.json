 +1,20 @@\n+static mut LEAK: usize = 0;\n+\n+fn fill(v: &mut i32) {\n+    unsafe { LEAK = v as *mut _ as usize; }\n+}\n+\n+fn evil() {\n+    let v = unsafe { &mut *(LEAK as *mut i32) };\n+    let _x = *v; //~ ERROR dangling pointer was dereferenced\n+}\n+\n+fn main() {\n+    let _y;\n+    {\n+        let mut x = 0i32;\n+        fill(&mut x);\n+        _y = x;\n+    }\n+    evil();\n+}"}]}