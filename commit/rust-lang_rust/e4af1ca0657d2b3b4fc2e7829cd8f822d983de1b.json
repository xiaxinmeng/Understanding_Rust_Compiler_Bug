const t] + v;\n+}\n+\n #[doc = \"Remove the last element from a vector and return it\"]\n fn pop<T>(&v: [const T]) -> T unsafe {\n     let ln = len(v);\n@@ -1734,6 +1739,13 @@ mod tests {\n         let addr_imm = unsafe::to_ptr(x_imm);\n         assert addr == addr_imm;\n     }\n+\n+    #[test]\n+    fn test_unshift() {\n+        let x = [1, 2, 3];\n+        unshift(x, 0);\n+        assert x == [0, 1, 2, 3];\n+    }\n }\n \n // Local Variables:"}]}