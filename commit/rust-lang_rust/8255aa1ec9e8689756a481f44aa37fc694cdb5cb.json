ue-2828.rs?ref=8255aa1ec9e8689756a481f44aa37fc694cdb5cb", "patch": "@@ -0,0 +1,21 @@\n+struct NoCopy {\n+    n: int\n+}\n+fn NoCopy() -> NoCopy {\n+    NoCopy { n: 0 }\n+}\n+\n+impl NoCopy: Drop {\n+    fn finalize(&self) {\n+        log(error, \"running destructor\");\n+    }\n+}\n+\n+fn main() {\n+    let x = NoCopy();\n+\n+    let f = fn~() { assert x.n == 0; }; //~ ERROR copying a noncopyable value\n+    let g = copy f;\n+\n+    f(); g();\n+}\n\\ No newline at end of file"}]}