@\n+// run-pass\n+\n+use std::ops::Deref;\n+\n+trait MyTrait {\n+    fn do_something(&self);\n+    fn as_str(&self) -> &str;\n+}\n+\n+impl Deref for dyn MyTrait {\n+    type Target = str;\n+    fn deref(&self) -> &Self::Target {\n+        self.as_str()\n+    }\n+}\n+\n+fn trait_object_does_something(t: &dyn MyTrait) {\n+    t.do_something()\n+}\n+\n+fn main() {}"}]}