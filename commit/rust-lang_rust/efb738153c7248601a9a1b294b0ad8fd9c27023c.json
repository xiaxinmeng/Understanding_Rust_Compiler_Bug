\n+\n+fn test() {\n+  let bool res = alt (true) {\n+    case (true) {\n+      true\n+    }\n+    case (false) {\n+      false\n+    }\n+  };\n+  check (res);\n+\n+  res = alt(false) {\n+    case (true) {\n+      false\n+    }\n+    case (false) {\n+      true\n+    }\n+  };\n+  check (res);\n+}\n+\n+fn main() {\n+  test();\n+}\n\\ No newline at end of file"}]}