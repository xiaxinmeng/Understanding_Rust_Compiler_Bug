nt {\n+    if (_vec.len[u8](buf) == 0u) {\n+        log_err \"parse_buf(): buf is empty\";\n+        fail;\n+    }\n+\n     auto i = _vec.len[u8](buf) - 1u;\n     auto power = 1u;\n     auto n = 0u;\n     while (true) {\n         n += (((buf.(i)) - ('0' as u8)) as uint) * power;\n         power *= radix;\n         if (i == 0u) { ret n; }\n+        i -= 1u;\n     }\n+\n+    fail;\n }\n \n fn to_str(uint num, uint radix) -> str"}]}