nction `f`\n+*/\n+fn iter<T>(v: [mutable? T], it: block(T)) {\n+    iter2(v) { |_i, v| it(v) }\n+}\n+\n /*\n Function: iter2\n \n FIXME: This is exactly the same as eachi\n */\n-fn iter2<T>(v: [T], it: block(uint, T)) {\n+fn iter2<T>(v: [mutable? T], it: block(uint, T)) {\n     let i = 0u;\n     for x in v { it(i, x); i += 1u; }\n }"}, {"sha": "4015899acec47b87b206e696255aa5046cfcd19c", "filename": "src/test/stdtest/vec.rs", "status": "modified", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/rust-lang/rust/blob/b5ed1c46c0407c6a6c3354d1c8da8531d992064c/src%2Ftest%2Fstdtest%2Fvec.rs", "raw_url": "https://github.com/rust-lang/rust/raw/b5ed1c46c0407c6a6c3354d1c8da8531d992064c/src%2Ftest%2Fstdtest%2Fvec.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fstdtest%2Fvec.rs?ref=b5ed1c46c0407c6a6c3354d1c8da8531d992064c", "patch": "@@ -292,6 +292,20 @@ fn test_foldl() {\n     assert (sum == 15u);\n }\n \n+#[test]\n+fn iter_empty() {\n+    let i = 0;\n+    vec::iter::<int>([], { |_v| i += 1 });\n+    assert i == 0;\n+}\n+\n+#[test]\n+fn iter_nonempty() {\n+    let i = 0;\n+    vec::iter([1, 2, 3], { |v| i += v });\n+    assert i == 6;\n+}\n+\n #[test]\n fn test_any_and_all() {\n     assert (vec::any(is_three, [1u, 2u, 3u]));"}]}