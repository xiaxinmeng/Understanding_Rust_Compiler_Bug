#![crate_type = \"lib\"]\n+\n+pub trait Corge<T> {\n+    type Fred;\n+}\n+\n+impl Corge<u8> for () {\n+    type Fred = u32;\n+}\n+\n+pub trait Waldo {\n+    type Quax;\n+}\n+\n+impl Waldo for u32 {\n+    type Quax = u8;\n+}\n+\n+pub trait Grault\n+where\n+    (): Corge<Self::Thud>,\n+{\n+    type Thud;\n+    fn bar(_: <() as Corge<Self::Thud>>::Fred) {}\n+}\n+\n+impl<T> Grault for T\n+where\n+    T: Waldo,\n+    (): Corge<T::Quax>,\n+    <() as Corge<T::Quax>>::Fred: Waldo,\n+{\n+    type Thud = u8;\n+}\n+\n+pub trait Plugh<I> {\n+    fn baz();\n+}\n+\n+#[derive(Copy, Clone, Debug)]\n+pub struct Qiz<T> {\n+    foo: T,\n+}\n+\n+impl<T> Plugh<<() as Corge<T::Thud>>::Fred> for Qiz<T>\n+where\n+    T: Grault,\n+    (): Corge<T::Thud>,\n+{\n+    fn baz() {}\n+}\n+\n+pub fn test() {\n+    <Qiz<u32> as Plugh<u32>>::baz();\n+}"}]}