ibserialize%2Fjson.rs?ref=fc30518be9774e7661739eb29a9bca38f31505c5", "patch": "@@ -331,21 +331,22 @@ pub fn escape_bytes(wr: &mut io::Writer, bytes: &[u8]) -> Result<(), io::IoError\n \n     for (i, byte) in bytes.iter().enumerate() {\n         let escaped = match *byte {\n-            b'\"' => \"\\\\\\\"\",\n-            b'\\\\' => \"\\\\\\\\\",\n-            b'\\x08' => \"\\\\b\",\n-            b'\\x0c' => \"\\\\f\",\n-            b'\\n' => \"\\\\n\",\n-            b'\\r' => \"\\\\r\",\n-            b'\\t' => \"\\\\t\",\n+            b'\"' => \"\\\\\\\"\".into_cow(),\n+            b'\\\\' => \"\\\\\\\\\".into_cow(),\n+            b'\\x08' => \"\\\\b\".into_cow(),\n+            b'\\x0c' => \"\\\\f\".into_cow(),\n+            b'\\n' => \"\\\\n\".into_cow(),\n+            b'\\r' => \"\\\\r\".into_cow(),\n+            b'\\t' => \"\\\\t\".into_cow(),\n+            b'\\x00'...b'\\x1f' | b'\\x7f' => format!(\"\\\\u00{:0>2x}\", *byte).into_cow(),\n             _ => { continue; }\n         };\n \n         if start < i {\n             try!(wr.write(bytes[start..i]));\n         }\n \n-        try!(wr.write_str(escaped));\n+        try!(wr.write_str(escaped.deref()));\n \n         start = i + 1;\n     }\n@@ -2731,6 +2732,9 @@ mod tests {\n     fn test_write_char() {\n         check_encoder_for_simple!('a', \"\\\"a\\\"\");\n         check_encoder_for_simple!('\\t', \"\\\"\\\\t\\\"\");\n+        check_encoder_for_simple!('\\u{0000}', \"\\\"\\\\u0000\\\"\");\n+        check_encoder_for_simple!('\\u{001b}', \"\\\"\\\\u001b\\\"\");\n+        check_encoder_for_simple!('\\u{007f}', \"\\\"\\\\u007f\\\"\");\n         check_encoder_for_simple!('\\u{00a0}', \"\\\"\\u{00a0}\\\"\");\n         check_encoder_for_simple!('\\u{abcd}', \"\\\"\\u{abcd}\\\"\");\n         check_encoder_for_simple!('\\u{10ffff}', \"\\\"\\u{10ffff}\\\"\");"}]}