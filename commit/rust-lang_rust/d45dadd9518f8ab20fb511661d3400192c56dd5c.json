hains%2Fprotect-precedences.rs?ref=d45dadd9518f8ab20fb511661d3400192c56dd5c", "patch": "@@ -0,0 +1,18 @@\n+// run-pass\n+\n+#![allow(irrefutable_let_patterns)]\n+\n+use std::ops::Range;\n+\n+fn main() {\n+    let x: bool;\n+    // This should associate as: `(x = (true && false));`.\n+    x = true && false;\n+    assert!(!x);\n+\n+    fn _f1() -> bool {\n+        // Should associate as `(let _ = (return (true && false)))`.\n+        if let _ = return true && false {};\n+    }\n+    assert!(!_f1());\n+}"}]}