 }\n \n+fn eachi<@T>(f: block(T, uint) -> (), v: [mutable? T]) {\n+    let i = 0u;\n+    let l = len(v);\n+    while (i < l) {\n+        let elem = v[i];    // Satisfy alias checker\n+        f(elem, i);\n+        i += 1u;\n+    }\n+}\n+\n // Iterate over a list with with the indexes\n iter iter2<@T>(v: [T]) -> (uint, T) {\n     let i = 0u;"}]}