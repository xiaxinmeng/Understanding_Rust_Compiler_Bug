| writeln!(stdout(), \"{}\", x))\n+///     .collect();\n+/// assert!(res.is_ok());\n+/// ```\n+#[stable(feature = \"unit_from_iter\", since = \"1.23.0\")]\n+impl FromIterator<()> for () {\n+    fn from_iter<I: IntoIterator<Item=()>>(iter: I) -> Self {\n+        iter.into_iter().for_each(|()| {})\n+    }\n+}"}]}