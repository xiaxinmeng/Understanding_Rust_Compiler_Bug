}\n+      ast::def_fn(id, ast::crust_fn) {\n+        // Crust functions are just u8 pointers\n+        ret {\n+            bounds: @[],\n+            ty: ty::mk_ptr(\n+                fcx.ccx.tcx,\n+                {\n+                    ty: ty::mk_mach_uint(fcx.ccx.tcx, ast::ty_u8),\n+                    mut: ast::imm\n+                })\n+        };\n+      }\n       ast::def_fn(id, _) | ast::def_const(id) |\n       ast::def_variant(_, id) | ast::def_class(id)\n           | ast::def_class_method(_, id) | ast::def_class_field(_, id)"}, {"sha": "66ba4e4dad957dbbdfde5e694322c7813ed7dd7d", "filename": "src/test/compile-fail/crust-no-bind.rs", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-no-bind.rs", "raw_url": "https://github.com/rust-lang/rust/raw/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-no-bind.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fcrust-no-bind.rs?ref=0a503228f6b7a831418f0a247944e20c01c1b84e", "patch": "@@ -0,0 +1,7 @@\n+// error-pattern:expected function or native function but found *u8\n+crust fn f() {\n+}\n+\n+fn main() {\n+    let x = bind f();\n+}\n\\ No newline at end of file"}, {"sha": "d6a3fffb754d39d532602d5d26100fb00b007e7a", "filename": "src/test/compile-fail/crust-no-call.rs", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-no-call.rs", "raw_url": "https://github.com/rust-lang/rust/raw/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-no-call.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fcrust-no-call.rs?ref=0a503228f6b7a831418f0a247944e20c01c1b84e", "patch": "@@ -0,0 +1,7 @@\n+// error-pattern:expected function or native function but found *u8\n+crust fn f() {\n+}\n+\n+fn main() {\n+    f();\n+}\n\\ No newline at end of file"}, {"sha": "841497bc7353a5901500b94fcec05f00cf72fa2a", "filename": "src/test/compile-fail/crust-wrong-value-type.rs", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/rust-lang/rust/blob/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-wrong-value-type.rs", "raw_url": "https://github.com/rust-lang/rust/raw/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Fcompile-fail%2Fcrust-wrong-value-type.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fcrust-wrong-value-type.rs?ref=0a503228f6b7a831418f0a247944e20c01c1b84e", "patch": "@@ -0,0 +1,8 @@\n+// error-pattern:expected `fn()` but found `*u8`\n+crust fn f() {\n+}\n+\n+fn main() {\n+    // Crust functions are *u8 types\n+    let _x: fn() = f;\n+}\n\\ No newline at end of file"}, {"sha": "f9248704e1d9035e326a4d7da6939b0b95cda476", "filename": "src/test/run-pass/crust-take-value.rs", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Frun-pass%2Fcrust-take-value.rs", "raw_url": "https://github.com/rust-lang/rust/raw/0a503228f6b7a831418f0a247944e20c01c1b84e/src%2Ftest%2Frun-pass%2Fcrust-take-value.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fcrust-take-value.rs?ref=0a503228f6b7a831418f0a247944e20c01c1b84e", "patch": "@@ -0,0 +1,7 @@\n+crust fn f() {\n+}\n+\n+fn main() {\n+    // Crust functions are *u8 types\n+    let _x: *u8 = f;\n+}\n\\ No newline at end of file"}]}