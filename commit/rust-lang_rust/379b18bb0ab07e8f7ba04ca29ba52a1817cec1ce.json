Fmod.rs?ref=379b18bb0ab07e8f7ba04ca29ba52a1817cec1ce", "patch": "@@ -1917,7 +1917,7 @@ impl<T, A: Allocator> VecDeque<T, A> {\n     #[stable(feature = \"append\", since = \"1.4.0\")]\n     pub fn append(&mut self, other: &mut Self) {\n         if T::IS_ZST {\n-            self.len += other.len;\n+            self.len = self.len.checked_add(other.len).expect(\"capacity overflow\");\n             other.len = 0;\n             other.head = 0;\n             return;"}]}