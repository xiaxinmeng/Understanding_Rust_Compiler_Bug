"@@ -0,0 +1,16 @@\n+// Verify that no column information is emitted for MSVC targets\n+//\n+// only-msvc\n+// compile-flags: -C debuginfo=2\n+\n+// CHECK-NOT: !DILexicalBlock({{.*}}column: {{.*}})\n+// CHECK-NOT: !DILocation({{.*}}column: {{.*}})\n+\n+pub fn add(a: u32, b: u32) -> u32 {\n+    a + b\n+}\n+\n+fn main() {\n+    let c = add(1, 2);\n+    println!(\"{}\", c);\n+}"}]}