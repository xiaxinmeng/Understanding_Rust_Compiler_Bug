== Some(&(b'\\\\' as u16)) {\n+        return Err(io::const_io_error!(\n+            io::ErrorKind::InvalidInput,\n+            \"Windows file names may not contain `\\\"` or end with `\\\\`\"\n+        ));\n+    }\n     cmd.extend_from_slice(script.strip_suffix(&[0]).unwrap_or(script));\n     cmd.push(b'\"' as u16);\n "}]}