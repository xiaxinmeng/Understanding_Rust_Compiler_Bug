se, Version 2.0 <LICENSE-APACHE or\n+// http://www.apache.org/licenses/LICENSE-2.0> or the MIT license\n+// <LICENSE-MIT or http://opensource.org/licenses/MIT>, at your\n+// option. This file may not be copied, modified, or distributed\n+// except according to those terms.\n+\n+extern crate toml;\n+\n+#[derive(RustcDecodable)]\n+pub struct Config {\n+    pub max_width: usize,\n+    pub ideal_width: usize,\n+    pub leeway: usize,\n+    pub tab_spaces: usize,\n+    pub newline_style: ::NewlineStyle,\n+    pub fn_brace_style: ::BraceStyle,\n+    pub fn_return_indent: ::ReturnIndent,\n+}\n+\n+impl Config {\n+    fn from_toml(toml: &str) -> Config {\n+        println!(\"About to parse: {}\", toml);\n+        let parsed = toml.parse().unwrap();\n+        toml::decode(parsed).unwrap()\n+    }\n+}\n+\n+pub fn set_config(toml: &str) {\n+    unsafe {\n+        ::CONFIG = Some(Config::from_toml(toml));\n+    }\n+}\n+\n+macro_rules! config {\n+    ($name: ident) => {\n+        unsafe { ::CONFIG.as_ref().unwrap().$name }\n+    };\n+}"}, {"sha": "386c39ad472be24460ad2790fac041f7b5e817ba", "filename": "src/lib.rs", "status": "modified", "additions": 0, "deletions": 1, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/6a5ef17eb12e1599ac6e14e267eaa6a246488cc2/src%2Flib.rs", "raw_url": "https://github.com/rust-lang/rust/raw/6a5ef17eb12e1599ac6e14e267eaa6a246488cc2/src%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flib.rs?ref=6a5ef17eb12e1599ac6e14e267eaa6a246488cc2", "patch": "@@ -267,7 +267,6 @@ impl<'a> CompilerCalls<'a> for RustFmtCalls {\n             changes.append_newlines();\n             fmt_lines(&mut changes);\n \n-            // FIXME(#5) Should be user specified whether to show or replace.\n             let result = changes.write_all_files(write_mode);\n \n             match result {"}]}