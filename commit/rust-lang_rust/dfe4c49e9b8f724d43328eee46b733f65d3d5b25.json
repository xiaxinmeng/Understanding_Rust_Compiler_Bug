e(feature = \"matches_macro\", since = \"1.42.0\")]\n #[cfg_attr(not(test), rustc_diagnostic_item = \"matches_macro\")]\n macro_rules! matches {\n-    ($expression:expr, $(|)? $( $pattern:pat_param )|+ $( if $guard: expr )? $(,)?) => {\n+    ($expression:expr, $pattern:pat $(if $guard:expr)? $(,)?) => {\n         match $expression {\n-            $( $pattern )|+ $( if $guard )? => true,\n+            $pattern $(if $guard)? => true,\n             _ => false\n         }\n     };"}]}