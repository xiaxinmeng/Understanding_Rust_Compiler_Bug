ruct Leak<'a> {\n+    dropped: &'a mut bool\n+}\n+\n+#[unsafe_destructor]\n+impl<'a> Drop for Leak<'a> {\n+    fn drop(&mut self) {\n+        *self.dropped = true;\n+    }\n+}\n+\n+fn main() {\n+    let mut dropped = false;\n+    {\n+        let leak = Leak { dropped: &mut dropped };\n+        for ((), leaked) in Some(((),leak)).move_iter() {}\n+    }\n+\n+    assert!(dropped);\n+}"}]}