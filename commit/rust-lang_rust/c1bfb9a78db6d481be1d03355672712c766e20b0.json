72712c766e20b0", "patch": "@@ -265,3 +265,26 @@ fn test_issue_82282() {\n         panic!();\n     }\n }\n+\n+#[test]\n+fn test_issue_82291() {\n+    use std::cell::Cell;\n+\n+    let mut v1 = [()];\n+    let v2 = [()];\n+\n+    let called = Cell::new(0);\n+\n+    let mut zip = v1\n+        .iter_mut()\n+        .map(|r| {\n+            called.set(called.get() + 1);\n+            r\n+        })\n+        .zip(&v2);\n+\n+    zip.next_back();\n+    assert_eq!(called.get(), 1);\n+    zip.next();\n+    assert_eq!(called.get(), 1);\n+}"}]}