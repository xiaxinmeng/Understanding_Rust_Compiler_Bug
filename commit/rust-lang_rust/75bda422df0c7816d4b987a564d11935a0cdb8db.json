t ( float::from_str(\"+3.14\") == 3.14 );\n+   assert ( float::from_str(\"-3.14\") == -3.14 );\n+   assert ( float::from_str(\"2.5E10\") == 25000000000. );\n+   assert ( float::from_str(\"2.5e10\") == 25000000000. );\n+   assert ( float::from_str(\"25000000000.E-10\") == 2.5 );\n+   assert ( float::from_str(\"\") == 0. );\n+   assert ( float::from_str(\"   \") == 0. );\n+   assert ( float::from_str(\".\") == 0. );\n+   assert ( float::from_str(\"5.\") == 5. );\n+   assert ( float::from_str(\".5\") == 0.5 );\n+   assert ( float::from_str(\"0.5\") == 0.5 );\n+\n+}"}, {"sha": "46292756a47e74a625286a07aa926078aac4efe1", "filename": "src/test/stdtest/stdtest.rc", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/75bda422df0c7816d4b987a564d11935a0cdb8db/src%2Ftest%2Fstdtest%2Fstdtest.rc", "raw_url": "https://github.com/rust-lang/rust/raw/75bda422df0c7816d4b987a564d11935a0cdb8db/src%2Ftest%2Fstdtest%2Fstdtest.rc", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fstdtest%2Fstdtest.rc?ref=75bda422df0c7816d4b987a564d11935a0cdb8db", "patch": "@@ -29,6 +29,7 @@ mod sys;\n mod task;\n mod test;\n mod uint;\n+mod float;\n \n // Local Variables:\n // mode: rust"}]}