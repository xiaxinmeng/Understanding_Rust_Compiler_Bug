 --no-install-recommends \\\n   xz-utils\n \n # FIXME: build the `ptx-linker` instead.\n-RUN curl -sL https://github.com/denzp/rust-ptx-linker/releases/download/v0.9.0-alpha.1/rust-ptx-linker.linux64.tar.gz | \\\n+RUN curl -sL https://github.com/denzp/rust-ptx-linker/releases/download/v0.9.0-alpha.2/rust-ptx-linker.linux64.tar.gz | \\\n   tar -xzvC /usr/bin\n \n RUN curl -sL https://nodejs.org/dist/v9.2.0/node-v9.2.0-linux-x64.tar.xz | \\"}, {"sha": "249715a7b6e26210741661766606297d568fbea5", "filename": "src/librustc_codegen_ssa/back/linker.rs", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/rust-lang/rust/blob/3f624456e6e284f918d4e189e766e357911b49e2/src%2Flibrustc_codegen_ssa%2Fback%2Flinker.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3f624456e6e284f918d4e189e766e357911b49e2/src%2Flibrustc_codegen_ssa%2Fback%2Flinker.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibrustc_codegen_ssa%2Fback%2Flinker.rs?ref=3f624456e6e284f918d4e189e766e357911b49e2", "patch": "@@ -1132,6 +1132,12 @@ impl<'a> Linker for PtxLinker<'a> {\n     }\n \n     fn finalize(&mut self) -> Command {\n+        // Provide the linker with fallback to internal `target-cpu`.\n+        self.cmd.arg(\"--fallback-arch\").arg(match self.sess.opts.cg.target_cpu {\n+            Some(ref s) => s,\n+            None => &self.sess.target.target.options.cpu\n+        });\n+\n         ::std::mem::replace(&mut self.cmd, Command::new(\"\"))\n     }\n "}, {"sha": "2c211b5c7850794d389c9c2c36d7bc7793caa68d", "filename": "src/test/run-make/nvptx-binary-crate/Makefile", "status": "modified", "additions": 5, "deletions": 2, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/3f624456e6e284f918d4e189e766e357911b49e2/src%2Ftest%2Frun-make%2Fnvptx-binary-crate%2FMakefile", "raw_url": "https://github.com/rust-lang/rust/raw/3f624456e6e284f918d4e189e766e357911b49e2/src%2Ftest%2Frun-make%2Fnvptx-binary-crate%2FMakefile", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-make%2Fnvptx-binary-crate%2FMakefile?ref=3f624456e6e284f918d4e189e766e357911b49e2", "patch": "@@ -2,8 +2,11 @@\n \n ifeq ($(TARGET),nvptx64-nvidia-cuda)\n all:\n-\t$(RUSTC) main.rs -Clink-arg=--arch=sm_60 --crate-type=\"bin\" -O --target $(TARGET)\n-\tFileCheck main.rs --input-file $(TMPDIR)/main.ptx\n+\t$(RUSTC) main.rs --crate-type=\"bin\" --target $(TARGET) -O -C link-arg=--arch=sm_60 -o $(TMPDIR)/main.link_arg.ptx\n+\t$(RUSTC) main.rs --crate-type=\"bin\" --target $(TARGET) -O -C target-cpu=sm_60 -o $(TMPDIR)/main.target_cpu.ptx\n+\n+\tFileCheck main.rs --input-file $(TMPDIR)/main.link_arg.ptx\n+\tFileCheck main.rs --input-file $(TMPDIR)/main.target_cpu.ptx\n else\n all:\n endif"}, {"sha": "63fd6b063dd8c01e47acd4d1b46b5ae3c08da3c9", "filename": "src/test/run-make/nvptx-dylib-crate/kernel.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/3f624456e6e284f918d4e189e766e357911b49e2/src%2Ftest%2Frun-make%2Fnvptx-dylib-crate%2Fkernel.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3f624456e6e284f918d4e189e766e357911b49e2/src%2Ftest%2Frun-make%2Fnvptx-dylib-crate%2Fkernel.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-make%2Fnvptx-dylib-crate%2Fkernel.rs?ref=3f624456e6e284f918d4e189e766e357911b49e2", "patch": "@@ -5,7 +5,7 @@\n extern crate dep;\n \n // Verify the default CUDA arch.\n-// CHECK: .target sm_20\n+// CHECK: .target sm_30\n // CHECK: .address_size 64\n \n // Make sure declarations are there."}]}