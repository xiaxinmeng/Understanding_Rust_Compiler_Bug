async_fn_in_traits)]\n+\n+trait Foo {\n+    fn get(&self) -> usize;\n+}\n+\n+impl Foo for usize {\n+    fn get(&self) -> usize {\n+        *self\n+    }\n+}\n+\n+fn invoke_dyn_star(i: dyn* Foo) -> usize {\n+    i.get()\n+}\n+\n+fn make_and_invoke_dyn_star(i: usize) -> usize {\n+    let dyn_i: dyn* Foo = i as dyn* Foo;\n+    invoke_dyn_star(dyn_i)\n+}\n+\n+fn main() {\n+    println!(\"{}\", make_and_invoke_dyn_star(42));\n+}"}]}