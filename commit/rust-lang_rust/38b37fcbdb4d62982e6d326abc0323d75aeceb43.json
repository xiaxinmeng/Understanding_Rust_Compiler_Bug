() {\n+      fn m1() -> str {\n+          ret \"foo.m1\";\n+      }\n+      fn m2() -> str {\n+          ret self.m1();\n+      }\n+      fn m3() -> str {\n+          let str s1 = self.m2();\n+          assert s1 == \"foo.m1\";\n+\n+          obj bar() {\n+              fn m1() -> str {\n+                  ret \"bar.m1\";\n+              }\n+              \n+              fn m2() -> str {\n+                  ret self.m1();\n+              }\n+          }\n+\n+          auto b = bar();\n+          let str s3 = b.m2();\n+\n+          let str s4 = self.m2();\n+          assert s4 == \"foo.m1\";\n+\n+          ret s3;\n+      }\n+  }\n+  \n+  auto a = foo();\n+\n+  let str s1 = a.m1();\n+  assert s1 == \"foo.m1\";\n+\n+  let str s2 = a.m2();\n+  assert s2 == \"foo.m1\";\n+\n+  let str s3 = a.m3();\n+  assert s3 == \"bar.m1\";\n+}\n+"}]}