  fn b(c: chan<chan<int>>) {\n+            let p = port();\n+            send(c, chan(p));\n+        }\n+        let p = port();\n+        spawn(chan(p), b);\n+        recv(p);\n+    }\n+    let i = 0;\n+    while i < 100 {\n+        doit();\n+        i += 1;\n+    }\n+}\n+\n+fn main() {\n+    let t = spawn_joinable((), a);\n+    join(t);\n+}"}]}