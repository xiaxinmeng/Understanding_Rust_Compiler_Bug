t/contents/src%2Ftest%2Fui%2Fdrop%2Fissue-90752.rs?ref=d846fe052294be6bd763bfb08479c9b93cda90fd", "patch": "@@ -0,0 +1,32 @@\n+// run-pass\n+\n+use std::cell::RefCell;\n+\n+struct S<'a>(i32, &'a RefCell<Vec<i32>>);\n+\n+impl<'a> Drop for S<'a> {\n+    fn drop(&mut self) {\n+        self.1.borrow_mut().push(self.0);\n+    }\n+}\n+\n+fn test(drops: &RefCell<Vec<i32>>) {\n+    let mut foo = None;\n+    match foo {\n+        None => (),\n+        _ => return,\n+    }\n+\n+    *(&mut foo) = Some((S(0, drops), S(1, drops))); // Both S(0) and S(1) should be dropped\n+\n+    match foo {\n+        Some((_x, _)) => {}\n+        _ => {}\n+    }\n+}\n+\n+fn main() {\n+    let drops = RefCell::new(Vec::new());\n+    test(&drops);\n+    assert_eq!(*drops.borrow(), &[0, 1]);\n+}"}]}