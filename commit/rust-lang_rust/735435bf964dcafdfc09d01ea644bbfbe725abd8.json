_ptr_eq[T](@T a, @T b) -> int;\n+}\n+\n+fn ptr_eq[T](@T a, @T b) -> bool { ret rustrt.rust_ptr_eq[T](a, b) != 0; }\n+"}, {"sha": "1056f37513801809dd9a09797917f65ede681ba3", "filename": "src/lib/std.rc", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Flib%2Fstd.rc", "raw_url": "https://github.com/rust-lang/rust/raw/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Flib%2Fstd.rc", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flib%2Fstd.rc?ref=735435bf964dcafdfc09d01ea644bbfbe725abd8", "patch": "@@ -69,6 +69,7 @@ mod sha1;\n mod ebml;\n mod UFind;\n mod ExtFmt;\n+mod Box;\n \n // Local Variables:\n // mode: rust;"}, {"sha": "30e2eb3ddeb00e272cde127b46866c004e4ff787", "filename": "src/rt/rust_builtin.cpp", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/rust-lang/rust/blob/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Frt%2Frust_builtin.cpp", "raw_url": "https://github.com/rust-lang/rust/raw/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Frt%2Frust_builtin.cpp", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Frt%2Frust_builtin.cpp?ref=735435bf964dcafdfc09d01ea644bbfbe725abd8", "patch": "@@ -468,6 +468,11 @@ rust_file_is_dir(rust_task *task, rust_str *path) {\n extern \"C\" CDECL FILE* rust_get_stdin() {return stdin;}\n extern \"C\" CDECL FILE* rust_get_stdout() {return stdout;}\n \n+extern \"C\" CDECL int\n+rust_ptr_eq(rust_task *task, type_desc *t, rust_box *a, rust_box *b) {\n+    return a == b;\n+}\n+\n //\n // Local Variables:\n // mode: C++"}, {"sha": "d724dd714cf28e5029f300a81b99863685fd7e1e", "filename": "src/rt/rustrt.def.in", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Frt%2Frustrt.def.in", "raw_url": "https://github.com/rust-lang/rust/raw/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Frt%2Frustrt.def.in", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Frt%2Frustrt.def.in?ref=735435bf964dcafdfc09d01ea644bbfbe725abd8", "patch": "@@ -19,6 +19,7 @@ rust_get_stdin\n rust_get_stdout\n rust_list_files\n rust_process_wait\n+rust_ptr_eq\n rust_run_program\n rust_start\n size_of"}, {"sha": "10f5e727110cff7ed67073dca5f3cde53a571065", "filename": "src/test/run-pass/lib-box.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Ftest%2Frun-pass%2Flib-box.rs", "raw_url": "https://github.com/rust-lang/rust/raw/735435bf964dcafdfc09d01ea644bbfbe725abd8/src%2Ftest%2Frun-pass%2Flib-box.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Flib-box.rs?ref=735435bf964dcafdfc09d01ea644bbfbe725abd8", "patch": "@@ -0,0 +1,12 @@\n+use std;\n+import std.Box;\n+\n+fn main() {\n+    auto x = @3;\n+    auto y = @3;\n+    check (Box.ptr_eq[int](x, x));\n+    check (Box.ptr_eq[int](y, y));\n+    check (!Box.ptr_eq[int](x, y));\n+    check (!Box.ptr_eq[int](y, x));\n+}\n+"}]}