vacy}", "received_events_url": "https://api.github.com/users/nikomatsakis/received_events", "type": "User", "site_admin": false}, "committer": {"login": "nikomatsakis", "id": 155238, "node_id": "MDQ6VXNlcjE1NTIzOA==", "avatar_url": "https://avatars.githubusercontent.com/u/155238?v=4", "gravatar_id": "", "url": "https://api.github.com/users/nikomatsakis", "html_url": "https://github.com/nikomatsakis", "followers_url": "https://api.github.com/users/nikomatsakis/followers", "following_url": "https://api.github.com/users/nikomatsakis/following{/other_user}", "gists_url": "https://api.github.com/users/nikomatsakis/gists{/gist_id}", "starred_url": "https://api.github.com/users/nikomatsakis/starred{/owner}{/repo}", "subscriptions_url": "https://api.github.com/users/nikomatsakis/subscriptions", "organizations_url": "https://api.github.com/users/nikomatsakis/orgs", "repos_url": "https://api.github.com/users/nikomatsakis/repos", "events_url": "https://api.github.com/users/nikomatsakis/events{/privacy}", "received_events_url": "https://api.github.com/users/nikomatsakis/received_events", "type": "User", "site_admin": false}, "parents": [{"sha": "4465c1ad022740eb9204f3111396be6b16351632", "url": "https://api.github.com/repos/rust-lang/rust/commits/4465c1ad022740eb9204f3111396be6b16351632", "html_url": "https://github.com/rust-lang/rust/commit/4465c1ad022740eb9204f3111396be6b16351632"}], "stats": {"total": 36, "additions": 36, "deletions": 0}, "files": [{"sha": "d6a63d5cfe49f5dc697ba0bd4f8fbb536ada7cd0", "filename": "src/test/run-pass/sendfn-generic-fn.rs", "status": "added", "additions": 36, "deletions": 0, "changes": 36, "blob_url": "https://github.com/rust-lang/rust/blob/34283c6a7863d5d69641f5c39422db70fe6c862a/src%2Ftest%2Frun-pass%2Fsendfn-generic-fn.rs", "raw_url": "https://github.com/rust-lang/rust/raw/34283c6a7863d5d69641f5c39422db70fe6c862a/src%2Ftest%2Frun-pass%2Fsendfn-generic-fn.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fsendfn-generic-fn.rs?ref=34283c6a7863d5d69641f5c39422db70fe6c862a", "patch": "@@ -0,0 +1,36 @@\n+use std;\n+\n+import std::comm;\n+import std::comm::chan;\n+import std::comm::send;\n+\n+fn main() { test05(); }\n+\n+type pair<A,B> = { a: A, b: B };\n+\n+fn make_generic_record<copy A,copy B>(a: A, b: B) -> pair<A,B> {\n+    ret {a: a, b: b};\n+}\n+\n+fn test05_start(&&f: sendfn(&&float, &&str) -> pair<float, str>) {\n+    let p = f(22.22f, \"Hi\");\n+    log p;\n+    assert p.a == 22.22f;\n+    assert p.b == \"Hi\";\n+\n+    let q = f(44.44f, \"Ho\");\n+    log q;\n+    assert q.a == 44.44f;\n+    assert q.b == \"Ho\";\n+}\n+\n+fn spawn<copy A, copy B>(f: fn(sendfn(A,B)->pair<A,B>)) {\n+    let arg = sendfn(a: A, b: B) -> pair<A,B> {\n+        ret make_generic_record(a, b);\n+    };\n+    task::spawn(arg, f);\n+}\n+\n+fn test05() {\n+    spawn::<float,str>(test05_start);\n+}"}]}