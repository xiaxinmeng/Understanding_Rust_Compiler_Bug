ebug_assert_msg() {\n+    debug_assert!(true, \"test\");\n+    debug_assert_eq!(true, true, \"test\");\n+    debug_assert_ne!(true, false, \"test\");\n+}\n+\n fn main() {\n     panic();\n     todo();"}]}