ach |x| { push(*x); }\n     }\n }\n+\n+/**\n+ * Helper function to transform an internal iterator into an owned vector.\n+ *\n+ * # Example:\n+ *\n+ * ~~~\n+ * let v = ~[1, 2, 3];\n+ * let v2 = do iter_to_vec |f| { v.each(|e| f(*e)) };\n+ * if v != v2 { fail!() }\n+ * ~~~\n+ */\n+#[inline(always)]\n+pub fn iter_to_vec<T>(pusher: &fn(it: &fn(T) -> bool)) -> ~[T] {\n+    let mut v = ~[];\n+    let pushf = |e| {v.push(e); true};\n+    pusher(pushf);\n+    v\n+}\n+\n+#[test]\n+fn test_iter_to_vec() {\n+    let v = ~[1, 2, 3];\n+    let v2 = do iter_to_vec |f| { v.each(|e| f(*e)) };\n+    if v != v2 { fail!() }\n+}"}]}