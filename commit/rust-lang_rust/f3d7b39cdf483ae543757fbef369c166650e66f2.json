ust/contents/src%2Ftest%2Fui%2Farray-slice-vec%2Fslice_is_sorted_by_borrow.rs?ref=f3d7b39cdf483ae543757fbef369c166650e66f2", "patch": "@@ -0,0 +1,20 @@\n+// check-pass\n+// regression test for https://github.com/rust-lang/rust/issues/53485#issuecomment-885393452\n+\n+#![feature(is_sorted)]\n+\n+struct A {\n+    name: String,\n+}\n+\n+fn main() {\n+    let a = &[\n+        A {\n+            name: \"1\".to_string(),\n+        },\n+        A {\n+            name: \"2\".to_string(),\n+        },\n+    ];\n+    assert!(a.is_sorted_by_key(|a| a.name.as_str()));\n+}"}]}