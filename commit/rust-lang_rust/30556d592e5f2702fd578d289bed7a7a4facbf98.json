.rs?ref=30556d592e5f2702fd578d289bed7a7a4facbf98", "patch": "@@ -3344,6 +3344,12 @@ impl<'a, 'gcx, 'tcx> FnCtxt<'a, 'gcx, 'tcx> {\n                                 }\n                             };\n                     }\n+                    ty::Array(ty, _) if ty.is_numeric() => {\n+                        let base = self.tcx.hir.node_to_pretty_string(base.id);\n+                        let msg = format!(\"attempting to use tuple indexing on an array; try\");\n+                        let suggestion = format!(\"{}[{}]\", base, field);\n+                        err.span_suggestion(field.span, &msg, suggestion);\n+                    },\n                     ty::RawPtr(..) => {\n                         let base = self.tcx.hir.node_to_pretty_string(base.id);\n                         let msg = format!(\"`{}` is a native pointer; try dereferencing it\", base);"}, {"sha": "c8b54c2d4beb4d0b410e1def0d129000c97116e4", "filename": "src/test/ui/issues/issue-53712.rs", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/rust-lang/rust/blob/30556d592e5f2702fd578d289bed7a7a4facbf98/src%2Ftest%2Fui%2Fissues%2Fissue-53712.rs", "raw_url": "https://github.com/rust-lang/rust/raw/30556d592e5f2702fd578d289bed7a7a4facbf98/src%2Ftest%2Fui%2Fissues%2Fissue-53712.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fissues%2Fissue-53712.rs?ref=30556d592e5f2702fd578d289bed7a7a4facbf98", "patch": "@@ -0,0 +1,9 @@\n+// issue #53712: make the error generated by using tuple indexing on an array more specific\n+\n+fn main() {\n+    let arr = [10, 20, 30, 40, 50];\n+    arr.0;\n+    //~^ ERROR no field `0` on type `[{integer}; 5]` [E0609]\n+    //~| HELP attempting to use tuple indexing on an array; try\n+    //~| SUGGESTION arr[0]\n+}"}, {"sha": "ef885a438c82795de544299bfce40cd425074b9c", "filename": "src/test/ui/issues/issue-53712.stderr", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/rust-lang/rust/blob/30556d592e5f2702fd578d289bed7a7a4facbf98/src%2Ftest%2Fui%2Fissues%2Fissue-53712.stderr", "raw_url": "https://github.com/rust-lang/rust/raw/30556d592e5f2702fd578d289bed7a7a4facbf98/src%2Ftest%2Fui%2Fissues%2Fissue-53712.stderr", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fissues%2Fissue-53712.stderr?ref=30556d592e5f2702fd578d289bed7a7a4facbf98", "patch": "@@ -0,0 +1,9 @@\n+error[E0609]: no field `0` on type `[{integer}; 5]`\n+  --> $DIR/issue-53712.rs:5:9\n+   |\n+LL |     arr.0;\n+   |         ^ help: attempting to use tuple indexing on an array; try: `arr[0]`\n+\n+error: aborting due to previous error\n+\n+For more information about this error, try `rustc --explain E0609`."}]}