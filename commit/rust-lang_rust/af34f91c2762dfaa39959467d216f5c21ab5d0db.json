o<[u8]>> {value: unsized::Foo<[u8]> {value: [...]}}\n+\n+\n+#![feature(omit_gdb_pretty_printer_section)]\n+#![omit_gdb_pretty_printer_section]\n+\n+struct Foo<T: ?Sized> {\n+    value: T\n+}\n+\n+fn main() {\n+    let foo: Foo<Foo<[u8; 4]>> = Foo {\n+        value: Foo {\n+            value: *b\"abc\\0\"\n+        }\n+    };\n+    let a: &Foo<[u8]> = &foo.value;\n+    let b: &Foo<Foo<[u8]>> = &foo;\n+\n+    zzz(); // #break\n+}\n+\n+fn zzz() { () }"}]}