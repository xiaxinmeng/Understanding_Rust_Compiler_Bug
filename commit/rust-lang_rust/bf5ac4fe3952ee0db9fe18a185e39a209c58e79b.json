4,4 +514,41 @@ fn main() {\n         \"###\n         );\n     }\n+\n+    #[test]\n+    fn hint_truncation() {\n+        let (analysis, file_id) = single_file(\n+            r#\"\n+struct Smol<T>(T);\n+\n+struct VeryLongOuterName<T>(T);\n+\n+fn main() {\n+    let a = Smol(0u32);\n+    let b = VeryLongOuterName(0usize);\n+    let c = Smol(Smol(0u32))\n+}\"#,\n+        );\n+\n+        assert_debug_snapshot!(analysis.inlay_hints(file_id, Some(8)).unwrap(), @r###\"\n+        [\n+            InlayHint {\n+                range: [74; 75),\n+                kind: TypeHint,\n+                label: \"Smol<u32>\",\n+            },\n+            InlayHint {\n+                range: [98; 99),\n+                kind: TypeHint,\n+                label: \"VeryLongOuterName<\u2026>\",\n+            },\n+            InlayHint {\n+                range: [137; 138),\n+                kind: TypeHint,\n+                label: \"Smol<Smol<\u2026>>\",\n+            },\n+        ]\n+        \"###\n+        );\n+    }\n }"}]}