@ pub fn span_lint_and_then<'a, T: LintContext, F>(cx: &'a T, lint: &'static Lint,\n /// Return the base type for references and raw pointers.\n pub fn walk_ptrs_ty(ty: ty::Ty) -> ty::Ty {\n     match ty.sty {\n-        ty::TyRef(_, ref tm) |\n-        ty::TyRawPtr(ref tm) => walk_ptrs_ty(tm.ty),\n+        ty::TyRef(_, ref tm) => walk_ptrs_ty(tm.ty),\n         _ => ty,\n     }\n }\n@@ -533,8 +532,7 @@ pub fn walk_ptrs_ty(ty: ty::Ty) -> ty::Ty {\n pub fn walk_ptrs_ty_depth(ty: ty::Ty) -> (ty::Ty, usize) {\n     fn inner(ty: ty::Ty, depth: usize) -> (ty::Ty, usize) {\n         match ty.sty {\n-            ty::TyRef(_, ref tm) |\n-            ty::TyRawPtr(ref tm) => inner(tm.ty, depth + 1),\n+            ty::TyRef(_, ref tm) => inner(tm.ty, depth + 1),\n             _ => (ty, depth),\n         }\n     }"}, {"sha": "9f611dd3fd917c54b0b2db7068dbe2f3ccdd9c28", "filename": "tests/compile-fail/float_cmp.rs", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/rust-lang/rust/blob/8c5e617c9a22de40cbde76e8aa2d0fd49d0303f5/tests%2Fcompile-fail%2Ffloat_cmp.rs", "raw_url": "https://github.com/rust-lang/rust/raw/8c5e617c9a22de40cbde76e8aa2d0fd49d0303f5/tests%2Fcompile-fail%2Ffloat_cmp.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/tests%2Fcompile-fail%2Ffloat_cmp.rs?ref=8c5e617c9a22de40cbde76e8aa2d0fd49d0303f5", "patch": "@@ -63,4 +63,10 @@ fn main() {\n     x > 0.0;\n     x <= 0.0;\n     x >= 0.0;\n+\n+    let xs : [f32; 1] = [0.0];\n+    let a: *const f32 = xs.as_ptr();\n+    let b: *const f32 = xs.as_ptr();\n+\n+    assert!(a == b); // no errors\n }"}]}