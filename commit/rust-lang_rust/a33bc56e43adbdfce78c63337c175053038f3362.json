l-stage1\n+//xfail-stage2\n+\n+// Reduced test case for issue #540.\n+fn main() {\n+    obj a() {\n+        fn foo() -> int {\n+            ret 2;\n+        }\n+    }\n+\n+    auto my_a = a();\n+    auto my_b = obj {\n+        fn baz() -> int {\n+            ret self.foo();\n+        }\n+        with my_a\n+    };\n+\n+    assert (my_b.baz() == 2);\n+}"}]}