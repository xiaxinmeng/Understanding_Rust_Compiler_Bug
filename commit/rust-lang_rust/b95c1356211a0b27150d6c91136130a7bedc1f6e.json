-lang/rust/contents/src%2Ftest%2Frun-pass%2Fissue-7563.rs?ref=b95c1356211a0b27150d6c91136130a7bedc1f6e", "patch": "@@ -0,0 +1,25 @@\n+trait IDummy {\n+    fn do_nothing(&self);\n+}\n+\n+struct A { a: int }\n+struct B<'self> { b: int, pa: &'self A }\n+\n+    impl IDummy for A {\n+        fn do_nothing(&self) {\n+            println(\"A::do_nothing() is called\");\n+        }\n+    }\n+\n+impl<'self> B<'self> {\n+    fn get_pa(&self) -> &'self IDummy { self.pa as &'self IDummy }\n+}\n+\n+pub fn main() {\n+    let sa = A { a: 100 };\n+    let sb = B { b: 200, pa: &sa };\n+\n+    debug!(\"sa is %?\", sa);\n+    debug!(\"sb is %?\", sb);\n+    debug!(\"sb.pa is %?\", sb.get_pa());\n+}"}]}