decl decl) {\n                 }\n                 case (_) {\n                     wrd1(s, \"auto\");\n+\n+                    // Print the type if necessary.\n+                    alt (s.mode) {\n+                        case (mo_untyped) { /* no-op */ }\n+                        case (mo_typed(?tcx, ?ntt, ?tc)) {\n+                            auto lty = ty::ann_to_type(ntt, loc.ann);\n+                            wrd1(s, ty::ty_to_str(tcx, lty));\n+                        }\n+                    }\n                 }\n             }\n             wrd(s.s, loc.ident);"}]}