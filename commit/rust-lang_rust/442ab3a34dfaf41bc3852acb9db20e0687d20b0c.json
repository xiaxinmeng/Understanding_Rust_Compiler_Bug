/crates%2Fra_lsp_server%2Fsrc%2Fmain.rs", "raw_url": "https://github.com/rust-lang/rust/raw/442ab3a34dfaf41bc3852acb9db20e0687d20b0c/crates%2Fra_lsp_server%2Fsrc%2Fmain.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/crates%2Fra_lsp_server%2Fsrc%2Fmain.rs?ref=442ab3a34dfaf41bc3852acb9db20e0687d20b0c", "patch": "@@ -6,7 +6,10 @@ use ra_prof;\n \n fn main() -> Result<()> {\n     setup_logging()?;\n-    run_server()?;\n+    match Args::parse()? {\n+        Args::Version => println!(\"rust-analyzer {}\", env!(\"REV\")),\n+        Args::Run => run_server()?,\n+    }\n     Ok(())\n }\n \n@@ -22,6 +25,19 @@ fn setup_logging() -> Result<()> {\n     Ok(())\n }\n \n+enum Args {\n+    Version,\n+    Run,\n+}\n+\n+impl Args {\n+    fn parse() -> Result<Args> {\n+        let res =\n+            if std::env::args().any(|it| it == \"--version\") { Args::Version } else { Args::Run };\n+        Ok(res)\n+    }\n+}\n+\n fn run_server() -> Result<()> {\n     log::info!(\"lifecycle: server started\");\n "}]}