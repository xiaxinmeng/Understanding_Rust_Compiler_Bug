hub.com/rust-lang/rust/raw/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_lints%2Fsrc%2Flib.rs?ref=34399d450844c355f2fd83d89c724062e86c2624", "patch": "@@ -14,7 +14,6 @@\n #![allow(indexing_slicing, shadow_reuse, unknown_lints, missing_docs_in_private_items)]\n #![allow(needless_lifetimes)]\n \n-#[macro_use]\n extern crate syntax;\n #[macro_use]\n extern crate rustc;"}, {"sha": "ab9819c53714b457beae7f8a34f483fdf318bdbe", "filename": "clippy_lints/src/matches.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Fmatches.rs", "raw_url": "https://github.com/rust-lang/rust/raw/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Fmatches.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_lints%2Fsrc%2Fmatches.rs?ref=34399d450844c355f2fd83d89c724062e86c2624", "patch": "@@ -361,7 +361,7 @@ fn all_ranges(cx: &LateContext, arms: &[Arm]) -> Vec<SpannedRange<ConstVal>> {\n                 }\n                 .filter_map(|pat| {\n                     if_let_chain! {[\n-                    let PatKind::Range(ref lhs, ref rhs) = pat.node,\n+                    let PatKind::Range(ref lhs, ref rhs, _) = pat.node,\n                     let Ok(lhs) = constcx.eval(lhs, ExprTypeChecked),\n                     let Ok(rhs) = constcx.eval(rhs, ExprTypeChecked)\n                 ], {"}, {"sha": "3bce1b3d7e576b1d9e68d32aa45c4335c0f41770", "filename": "clippy_lints/src/utils/hir.rs", "status": "modified", "additions": 2, "deletions": 2, "changes": 4, "blob_url": "https://github.com/rust-lang/rust/blob/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Futils%2Fhir.rs", "raw_url": "https://github.com/rust-lang/rust/raw/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Futils%2Fhir.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_lints%2Fsrc%2Futils%2Fhir.rs?ref=34399d450844c355f2fd83d89c724062e86c2624", "patch": "@@ -160,8 +160,8 @@ impl<'a, 'tcx: 'a> SpanlessEq<'a, 'tcx> {\n             (&PatKind::Tuple(ref l, ls), &PatKind::Tuple(ref r, rs)) => {\n                 ls == rs && over(l, r, |l, r| self.eq_pat(l, r))\n             },\n-            (&PatKind::Range(ref ls, ref le), &PatKind::Range(ref rs, ref re)) => {\n-                self.eq_expr(ls, rs) && self.eq_expr(le, re)\n+            (&PatKind::Range(ref ls, ref le, ref li), &PatKind::Range(ref rs, ref re, ref ri)) => {\n+                self.eq_expr(ls, rs) && self.eq_expr(le, re) && (*li == *ri)\n             },\n             (&PatKind::Ref(ref le, ref lm), &PatKind::Ref(ref re, ref rm)) => lm == rm && self.eq_pat(le, re),\n             (&PatKind::Slice(ref ls, ref li, ref le), &PatKind::Slice(ref rs, ref ri, ref re)) => {"}, {"sha": "e08a7d3bfdb7d54a35b78bbbc28d925d066a798d", "filename": "clippy_lints/src/utils/inspector.rs", "status": "modified", "additions": 5, "deletions": 1, "changes": 6, "blob_url": "https://github.com/rust-lang/rust/blob/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Futils%2Finspector.rs", "raw_url": "https://github.com/rust-lang/rust/raw/34399d450844c355f2fd83d89c724062e86c2624/clippy_lints%2Fsrc%2Futils%2Finspector.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/clippy_lints%2Fsrc%2Futils%2Finspector.rs?ref=34399d450844c355f2fd83d89c724062e86c2624", "patch": "@@ -475,10 +475,14 @@ fn print_pat(cx: &LateContext, pat: &hir::Pat, indent: usize) {\n             println!(\"{}Lit\", ind);\n             print_expr(cx, e, indent + 1);\n         },\n-        hir::PatKind::Range(ref l, ref r) => {\n+        hir::PatKind::Range(ref l, ref r, ref range_end) => {\n             println!(\"{}Range\", ind);\n             print_expr(cx, l, indent + 1);\n             print_expr(cx, r, indent + 1);\n+            match *range_end {\n+                hir::RangeEnd::Included => println!(\"{} end included\", ind),\n+                hir::RangeEnd::Excluded => println!(\"{} end excluded\", ind),\n+            }\n         },\n         hir::PatKind::Slice(ref first_pats, ref range, ref last_pats) => {\n             println!(\"{}Slice [a, b, ..i, y, z]\", ind);"}]}