ents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibcollections%2Fvec_deque.rs?ref=7ceafaee4f3d8ed2268b1659dd99a541d71689a3", "patch": "@@ -70,7 +70,12 @@ impl<T: Clone> Clone for VecDeque<T> {\n impl<T> Drop for VecDeque<T> {\n     #[unsafe_destructor_blind_to_params]\n     fn drop(&mut self) {\n-        self.clear();\n+        let (front, back) = self.as_mut_slices();\n+        unsafe {\n+            // use drop for [T]\n+            ptr::drop_in_place(front);\n+            ptr::drop_in_place(back);\n+        }\n         // RawVec handles deallocation\n     }\n }"}]}