ssue-54685.rs?ref=22029a2ea7ee0a6bc2dcde64a6799aaa5df5c6af", "patch": "@@ -0,0 +1,31 @@\n+// min-llvm-version: 12.0\n+// compile-flags: -C opt-level=3\n+// run-pass\n+\n+fn foo(_i: i32) -> i32 {\n+    1\n+}\n+fn bar(_i: i32) -> i32 {\n+    1\n+}\n+\n+fn main() {\n+    let x: fn(i32) -> i32 = foo;\n+    let y: fn(i32) -> i32 = bar;\n+\n+    let s1;\n+    if x == y {\n+        s1 = \"same\".to_string();\n+    } else {\n+        s1 = format!(\"{:?}, {:?}\", x, y);\n+    }\n+\n+    let s2;\n+    if x == y {\n+        s2 = \"same\".to_string();\n+    } else {\n+        s2 = format!(\"{:?}, {:?}\", x, y);\n+    }\n+\n+    assert_eq!(s1, s2);\n+}"}]}