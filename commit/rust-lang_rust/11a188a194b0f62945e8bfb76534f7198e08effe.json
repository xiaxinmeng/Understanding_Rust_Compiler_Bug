@\n+// check-pass\n+// Regression test of #70944, should compile fine.\n+\n+use std::ops::Index;\n+\n+pub struct KeyA;\n+pub struct KeyB;\n+pub struct KeyC;\n+\n+pub trait Foo: Index<KeyA> + Index<KeyB> + Index<KeyC> {}\n+pub trait FooBuilder {\n+    type Inner: Foo;\n+    fn inner(&self) -> &Self::Inner;\n+}\n+\n+pub fn do_stuff(foo: &impl FooBuilder) {\n+    let inner = foo.inner();\n+    &inner[KeyA];\n+    &inner[KeyB];\n+    &inner[KeyC];\n+}\n+\n+fn main() {}"}]}