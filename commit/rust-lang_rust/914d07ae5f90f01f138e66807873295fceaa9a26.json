erent owner.\n+    }\n+\n     fn visit_generic_param(&mut self, param: &'hir hir::GenericParam<'hir>) {\n         if let hir::GenericParamKind::Type {\n             synthetic: Some(hir::SyntheticTyParamKind::ImplTrait),"}, {"sha": "19f9e4e339e754c3697f8a9acd06e9ee971b5e13", "filename": "src/test/ui/foreign/foreign-pub-super.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/914d07ae5f90f01f138e66807873295fceaa9a26/src%2Ftest%2Fui%2Fforeign%2Fforeign-pub-super.rs", "raw_url": "https://github.com/rust-lang/rust/raw/914d07ae5f90f01f138e66807873295fceaa9a26/src%2Ftest%2Fui%2Fforeign%2Fforeign-pub-super.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fforeign%2Fforeign-pub-super.rs?ref=914d07ae5f90f01f138e66807873295fceaa9a26", "patch": "@@ -0,0 +1,12 @@\n+// Test for #79487\n+// check-pass\n+\n+#![allow(dead_code)]\n+\n+mod sha2 {\n+    extern \"C\" {\n+        pub(super) fn GFp_sha512_block_data_order();\n+    }\n+}\n+\n+fn main() {}"}]}