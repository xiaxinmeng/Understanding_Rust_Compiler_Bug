url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fvec.rs?ref=28a091fc7c7e23eee346d87fd34ea1546002ee2c", "patch": "@@ -2069,6 +2069,10 @@ fn merge_sort<T>(v: &mut [T], compare: |&T, &T| -> Ordering) {\n /// Extension methods for vectors such that their elements are\n /// mutable.\n pub trait MutableVector<'a, T> {\n+    /// Work with `self` as a mut slice.\n+    /// Primarily intended for getting a &mut [T] from a [T, ..N].\n+    fn as_mut_slice(self) -> &'a mut [T];\n+\n     /// Return a slice that points into another slice.\n     fn mut_slice(self, start: uint, end: uint) -> &'a mut [T];\n \n@@ -2302,6 +2306,8 @@ pub trait MutableVector<'a, T> {\n \n impl<'a,T> MutableVector<'a, T> for &'a mut [T] {\n     #[inline]\n+    fn as_mut_slice(self) -> &'a mut [T] { self }\n+\n     fn mut_slice(self, start: uint, end: uint) -> &'a mut [T] {\n         assert!(start <= end);\n         assert!(end <= self.len());"}]}