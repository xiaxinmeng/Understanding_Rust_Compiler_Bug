   */\n+    #[inline(always)]\n+    pub unsafe fn mut_buf_as_slice<T,U>(p: *mut T,\n+                                        len: uint,\n+                                        f: &fn(v: &mut [T]) -> U) -> U {\n+        let pair = (p, len * sys::nonzero_size_of::<T>());\n+        let v : *(&blk/mut [T]) =\n+            ::cast::reinterpret_cast(&addr_of(&pair));\n+        f(*v)\n+    }\n+\n     /**\n      * Unchecked vector indexing.\n      */"}]}