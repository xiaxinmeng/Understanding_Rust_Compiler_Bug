basic() {\n+  let bool res = { true };\n+  check (res);\n+}\n+\n+fn test_rec() {\n+  auto res = { rec(v1 = 10, v2 = 20) };\n+  check (res.v2 == 20);\n+}\n+\n+fn test_filled_with_stuff() {\n+  auto res = {\n+    auto a = 0;\n+    while (a < 10) {\n+      a += 1;\n+    }\n+    a\n+  };\n+  check (res == 10);\n+}\n+\n+fn main() {\n+  test_basic();\n+  test_rec();\n+  test_filled_with_stuff();\n+}"}]}