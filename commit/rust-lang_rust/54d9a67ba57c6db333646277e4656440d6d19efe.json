y to be set, but should be safe to unset.\n+// unset-rustc-env:PWD\n+\n+extern crate env;\n+\n+fn main() {\n+    assert_eq!(env!(\"COMPILETEST_FOO\"), \"foo\");\n+    assert_eq!(option_env!(\"COMPILETEST_BAR\"), None);\n+    assert_eq!(option_env!(\"PWD\"), None);\n+    env::test();\n+}"}]}