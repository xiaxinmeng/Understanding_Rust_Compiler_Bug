in() {\n \n     let x = 1;\n     foo!();\n+\n+    g();\n+}\n+\n+fn g() {\n+    let x = 0;\n+    macro_rules! m { ($x:ident) => {\n+        macro_rules! m2 { () => { ($x, x) } }\n+        let x = 1;\n+        macro_rules! m3 { () => { ($x, x) } }\n+    } }\n+\n+    let x = 2;\n+    m!(x);\n+\n+    let x = 3;\n+    assert_eq!(m2!(), (2, 0));\n+    assert_eq!(m3!(), (2, 1));\n+\n+    let x = 4;\n+    m!(x);\n+    assert_eq!(m2!(), (4, 0));\n+    assert_eq!(m3!(), (4, 1));\n }"}]}