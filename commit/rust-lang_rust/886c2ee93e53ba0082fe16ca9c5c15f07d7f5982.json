982", "patch": "@@ -0,0 +1,10 @@\n+use std;\n+import std::ivec;\n+\n+fn main() {\n+    let v = std::ivec::map2({|&i, &b| if b { -i } else { i }},\n+                            ~[1, 2, 3, 4, 5],\n+                            ~[true, false, false, true, true]);\n+    log_err v;\n+    assert v == ~[-1, 2, 3, -4, -5];\n+}"}]}