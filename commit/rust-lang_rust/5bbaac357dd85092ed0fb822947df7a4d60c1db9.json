\n     assert_eq!(*v0, 13);\n+    v.push(1);\n+    v.swap_remove(1);\n+    assert_eq!(v.len(), 2);\n+    v.swap_remove(1); // swap_remove the last element\n+    assert_eq!(*v0, 13);\n \n     // Appending\n     v.append(&mut vec![27, 19]);"}, {"sha": "c600a6b649f36fdfba5688f9f0bc8cccc540e7f4", "filename": "src/liballoc/vec.rs", "status": "modified", "additions": 4, "deletions": 3, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/5bbaac357dd85092ed0fb822947df7a4d60c1db9/src%2Fliballoc%2Fvec.rs", "raw_url": "https://github.com/rust-lang/rust/raw/5bbaac357dd85092ed0fb822947df7a4d60c1db9/src%2Fliballoc%2Fvec.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fliballoc%2Fvec.rs?ref=5bbaac357dd85092ed0fb822947df7a4d60c1db9", "patch": "@@ -963,12 +963,13 @@ impl<T> Vec<T> {\n     #[inline]\n     #[stable(feature = \"rust1\", since = \"1.0.0\")]\n     pub fn swap_remove(&mut self, index: usize) -> T {\n+        assert!(index < self.len);\n         unsafe {\n             // We replace self[index] with the last element. Note that if the\n-            // bounds check on hole succeeds there must be a last element (which\n+            // bounds check above succeeds there must be a last element (which\n             // can be self[index] itself).\n-            let hole: *mut T = &mut self[index];\n-            let last = ptr::read(self.get_unchecked(self.len - 1));\n+            let last = ptr::read(self.as_ptr().add(self.len - 1));\n+            let hole: *mut T = self.as_mut_ptr().add(index);\n             self.len -= 1;\n             ptr::replace(hole, last)\n         }"}]}