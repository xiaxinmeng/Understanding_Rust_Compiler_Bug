an::BytePos;\n use syntax::ast;\n use syntax::visit::FnKind;\n \n-use crate::utils::{match_path_ast, snippet_opt, span_lint_and_then};\n+use crate::utils::{in_macro, match_path_ast, snippet_opt, span_lint_and_then};\n \n declare_clippy_lint! {\n     /// **What it does:** Checks for return statements at the end of a block.\n@@ -205,6 +205,7 @@ impl Return {\n             if !in_external_macro(cx.sess(), initexpr.span);\n             if !in_external_macro(cx.sess(), retexpr.span);\n             if !in_external_macro(cx.sess(), local.span);\n+            if !in_macro(local.span);\n             then {\n                 span_lint_and_then(\n                     cx,"}, {"sha": "23645d48fe79913acf7f7c856ca5fa846ee2ee72", "filename": "tests/ui/let_return.rs", "status": "modified", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/rust-lang/rust/blob/2213989a0150b85c180466e31f67846e64590dc0/tests%2Fui%2Flet_return.rs", "raw_url": "https://github.com/rust-lang/rust/raw/2213989a0150b85c180466e31f67846e64590dc0/tests%2Fui%2Flet_return.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/tests%2Fui%2Flet_return.rs?ref=2213989a0150b85c180466e31f67846e64590dc0", "patch": "@@ -45,4 +45,26 @@ fn test_nowarn_5(x: i16) -> u16 {\n     x\n }\n \n+// False positive example\n+trait Decode {\n+    fn decode<D: std::io::Read>(d: D) -> Result<Self, ()>\n+    where\n+        Self: Sized;\n+}\n+\n+macro_rules! tuple_encode {\n+    ($($x:ident),*) => (\n+        impl<$($x: Decode),*> Decode for ($($x),*) {\n+            #[inline]\n+            #[allow(non_snake_case)]\n+            fn decode<D: std::io::Read>(mut d: D) -> Result<Self, ()> {\n+                // Shouldn't trigger lint\n+                Ok(($({let $x = Decode::decode(&mut d)?; $x }),*))\n+            }\n+        }\n+    );\n+}\n+\n+tuple_encode!(T0, T1, T2, T3, T4, T5, T6, T7);\n+\n fn main() {}"}]}