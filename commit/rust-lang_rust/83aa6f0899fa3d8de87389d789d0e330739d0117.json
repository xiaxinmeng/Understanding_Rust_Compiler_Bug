;\n \n-pub(super) fn mod_contents(p: &mut Parser) {\n+pub(super) fn mod_contents(p: &mut Parser, stop_on_r_curly: bool) {\n     attributes::inner_attributes(p);\n-    while !p.at(EOF) {\n+    while !p.at(EOF) && !(stop_on_r_curly && p.at(R_CURLY)) {\n         item(p);\n     }\n }\n@@ -152,7 +152,10 @@ fn mod_item(p: &mut Parser) {\n     p.bump();\n \n     if p.expect(IDENT) && !p.eat(SEMI) {\n-        p.curly_block(mod_contents);\n+        if p.expect(L_CURLY) {\n+            mod_contents(p, true);\n+            p.expect(R_CURLY);\n+        }\n     }\n }\n "}, {"sha": "b87f3ca8af5fa832c73b95a834669dfb62ae3c5b", "filename": "src/parser/event_parser/grammar/mod.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/83aa6f0899fa3d8de87389d789d0e330739d0117/src%2Fparser%2Fevent_parser%2Fgrammar%2Fmod.rs", "raw_url": "https://github.com/rust-lang/rust/raw/83aa6f0899fa3d8de87389d789d0e330739d0117/src%2Fparser%2Fevent_parser%2Fgrammar%2Fmod.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fparser%2Fevent_parser%2Fgrammar%2Fmod.rs?ref=83aa6f0899fa3d8de87389d789d0e330739d0117", "patch": "@@ -11,7 +11,7 @@ mod paths;\n pub(crate) fn file(p: &mut Parser) {\n     let file = p.start();\n     p.eat(SHEBANG);\n-    items::mod_contents(p);\n+    items::mod_contents(p, false);\n     file.complete(p, FILE);\n }\n "}]}