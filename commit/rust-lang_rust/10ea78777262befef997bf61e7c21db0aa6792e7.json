1,25 @@\n+// xfail-test\n+\n+// Tests that shapes respect linearize_ty_params().\n+\n+tag option<T> {\n+    none;\n+    some(T);\n+}\n+\n+type smallintmap<T> = @{mutable v: [mutable option<T>]};\n+\n+fn mk<@T>() -> smallintmap<T> {\n+    let v: [mutable option<T>] = [mutable];\n+    ret @{mutable v: v};\n+}\n+\n+fn f<@T,@U>() {\n+    let sim = mk::<U>();\n+    log_err sim;\n+}\n+\n+fn main() {\n+    f::<int,int>();\n+}\n+"}]}