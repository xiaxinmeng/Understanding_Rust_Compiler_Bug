ui%2Fptr_ops%2Fissue-80309-safe.rs?ref=8e99d92f4ecaf7c6d8deae4461d4781c4194bf24", "patch": "@@ -0,0 +1,16 @@\n+// run-pass\n+// compiler-flags: -O\n+\n+// Regression test for issue #80309\n+\n+pub fn zero(x: usize) -> usize {\n+    std::ptr::null::<i8>().wrapping_add(x) as usize - x\n+}\n+pub fn qux(x: &[i8]) -> i8 {\n+    x[zero(x.as_ptr() as usize)]\n+}\n+\n+fn main() {\n+    let z = vec![42, 43];\n+    println!(\"{}\", qux(&z));\n+}"}, {"sha": "191ae5f42d231d2a73a948807f4ec61044f8a673", "filename": "src/test/ui/ptr_ops/issue-80309.rs", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/rust-lang/rust/blob/8e99d92f4ecaf7c6d8deae4461d4781c4194bf24/src%2Ftest%2Fui%2Fptr_ops%2Fissue-80309.rs", "raw_url": "https://github.com/rust-lang/rust/raw/8e99d92f4ecaf7c6d8deae4461d4781c4194bf24/src%2Ftest%2Fui%2Fptr_ops%2Fissue-80309.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fptr_ops%2Fissue-80309.rs?ref=8e99d92f4ecaf7c6d8deae4461d4781c4194bf24", "patch": "@@ -0,0 +1,13 @@\n+// run-pass\n+// compiler-flags: -O\n+\n+// Regression test for issue #80309\n+\n+pub unsafe fn foo(x: *const i8) -> i8 {\n+    *x.wrapping_sub(x as _).wrapping_add(x as _)\n+}\n+\n+fn main() {\n+    let x = 42;\n+    println!(\"{}\", unsafe { foo(&x) });\n+}"}]}