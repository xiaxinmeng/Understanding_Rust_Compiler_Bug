y_test() {\n-    f64_exact_sanity_test(format_exact);\n+    // See comments in dragon.rs's exact_sanity_test for why this test is\n+    // ignored on MSVC\n+    if !cfg!(target_env = \"msvc\") {\n+        f64_exact_sanity_test(format_exact);\n+    }\n     f32_exact_sanity_test(format_exact);\n }\n "}]}