called = true;\n+}\n+\n+fn# g(f: fn#(int, &bool), &called: bool) {\n+    f(10, called);\n+}\n+\n+fn main() {\n+    let called = false;\n+    let h = f;\n+    g(h, called);\n+    assert called == true;\n+}\n\\ No newline at end of file"}]}