2Frun-pass%2Fsendfn-spawn-with-fn-arg.rs?ref=4465c1ad022740eb9204f3111396be6b16351632", "patch": "@@ -0,0 +1,20 @@\n+use std;\n+\n+import std::comm;\n+import std::comm::chan;\n+import std::comm::send;\n+\n+fn main() { test05(); }\n+\n+fn test05_start(&&f: sendfn(int)) {\n+    f(22);\n+}\n+\n+fn test05() {\n+    let three = ~3;\n+    let fn_to_send = sendfn(n: int) {\n+        log_err *three + n; // will copy x into the closure\n+        assert(*three == 3);\n+    };\n+    task::spawn(fn_to_send, test05_start);\n+}"}]}