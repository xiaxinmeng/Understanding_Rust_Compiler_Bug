2Ftest%2Fui%2For-patterns%2Fissue-70413-no-unreachable-pat-and-guard.rs?ref=ae7fa3042da8d432c6048b89d320c73e43615b97", "patch": "@@ -0,0 +1,22 @@\n+// check-pass\n+\n+#![deny(unreachable_patterns)]\n+\n+#![feature(or_patterns)]\n+fn main() {\n+    match (3,42) {\n+        (a,_) | (_,a) if a > 10 => {println!(\"{}\", a)}\n+        _ => ()\n+    }\n+\n+    match Some((3,42)) {\n+        Some((a, _)) | Some((_, a)) if a > 10 => {println!(\"{}\", a)}\n+        _ => ()\n+\n+    }\n+\n+    match Some((3,42)) {\n+        Some((a, _) | (_, a)) if a > 10 => {println!(\"{}\", a)}\n+        _ => ()\n+    }\n+}"}]}