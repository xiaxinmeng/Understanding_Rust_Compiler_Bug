_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fliballoc%2Ftests%2Flinked_list.rs?ref=1f373f4aeb279c6bf98976aee419db967c4f56f6", "patch": "@@ -565,6 +565,41 @@ fn drain_filter_drop_panic_leak() {\n     assert!(q.is_empty());\n }\n \n+#[test]\n+fn drain_filter_pred_panic_leak() {\n+    static mut DROPS: i32 = 0;\n+\n+    #[derive(Debug)]\n+    struct D(u32);\n+\n+    impl Drop for D {\n+        fn drop(&mut self) {\n+            unsafe {\n+                DROPS += 1;\n+            }\n+        }\n+    }\n+\n+    let mut q = LinkedList::new();\n+    q.push_back(D(3));\n+    q.push_back(D(4));\n+    q.push_back(D(5));\n+    q.push_back(D(6));\n+    q.push_back(D(7));\n+    q.push_front(D(2));\n+    q.push_front(D(1));\n+    q.push_front(D(0));\n+\n+    catch_unwind(AssertUnwindSafe(|| drop(q.drain_filter(|item| if item.0 >= 2 {\n+        panic!()\n+    } else {\n+        true\n+    })))).ok();\n+\n+    assert_eq!(unsafe { DROPS }, 2);  // 0 and 1\n+    assert_eq!(q.len(), 6);\n+}\n+\n #[test]\n fn test_drop() {\n     static mut DROPS: i32 = 0;"}]}