ype IntoIter = std::iter::Cloned<std::slice::Iter<'a, u32>>;\n+    fn into_iter(self) -> Self::IntoIter {\n+        (&self.0).into_iter().cloned()\n+    }\n+}\n+\n+fn main() {\n+}"}]}