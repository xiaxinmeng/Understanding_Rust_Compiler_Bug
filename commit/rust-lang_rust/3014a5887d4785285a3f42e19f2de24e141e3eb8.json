unresolved name\n+\n+// Tag variants are not exported with their tags. This allows for a\n+// simple sort of ADT.\n+\n+mod foo {\n+  export t;\n+\n+  tag t {\n+    t1;\n+  }\n+}\n+\n+fn main() {\n+  auto x = foo.t1;\n+}"}, {"sha": "b192b698ea81feb16a79e4f2a182f6ef90e269f0", "filename": "src/test/run-pass/export-abstract-tag.rs", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/rust-lang/rust/blob/3014a5887d4785285a3f42e19f2de24e141e3eb8/src%2Ftest%2Frun-pass%2Fexport-abstract-tag.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3014a5887d4785285a3f42e19f2de24e141e3eb8/src%2Ftest%2Frun-pass%2Fexport-abstract-tag.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fexport-abstract-tag.rs?ref=3014a5887d4785285a3f42e19f2de24e141e3eb8", "patch": "@@ -0,0 +1,19 @@\n+// We can export tags without exporting the variants to create a simple\n+// sort of ADT.\n+\n+mod foo {\n+  export t;\n+  export f;\n+\n+  tag t {\n+    t1;\n+  }\n+\n+  fn f() -> t {\n+    ret t1;\n+  }\n+}\n+\n+fn main() {\n+  let foo.t v = foo.f();\n+}"}, {"sha": "e99bc0410bbc5fd8a3148b91a46f8e3eb24b4c1f", "filename": "src/test/run-pass/export-tag-variant.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/rust-lang/rust/blob/3014a5887d4785285a3f42e19f2de24e141e3eb8/src%2Ftest%2Frun-pass%2Fexport-tag-variant.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3014a5887d4785285a3f42e19f2de24e141e3eb8/src%2Ftest%2Frun-pass%2Fexport-tag-variant.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fexport-tag-variant.rs?ref=3014a5887d4785285a3f42e19f2de24e141e3eb8", "patch": "@@ -0,0 +1,12 @@\n+// Export the tag variants, without the tag\n+\n+mod foo {\n+  export t1;\n+  tag t {\n+    t1;\n+  }\n+}\n+\n+fn main() {\n+  auto v = foo.t1;\n+}"}]}