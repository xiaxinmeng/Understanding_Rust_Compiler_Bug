macros() -> ~str {\n \n     macro_rules! error (\n         ($arg:expr) => (\n-            log(1u32, fmt!( \\\"%?\\\", $arg ))\n+            __log(1u32, fmt!( \\\"%?\\\", $arg ))\n         );\n         ($( $arg:expr ),+) => (\n-            log(1u32, fmt!( $($arg),+ ))\n+            __log(1u32, fmt!( $($arg),+ ))\n         )\n     )\n     macro_rules! warn (\n         ($arg:expr) => (\n-            log(2u32, fmt!( \\\"%?\\\", $arg ))\n+            __log(2u32, fmt!( \\\"%?\\\", $arg ))\n         );\n         ($( $arg:expr ),+) => (\n-            log(2u32, fmt!( $($arg),+ ))\n+            __log(2u32, fmt!( $($arg),+ ))\n         )\n     )\n     macro_rules! info (\n         ($arg:expr) => (\n-            log(3u32, fmt!( \\\"%?\\\", $arg ))\n+            __log(3u32, fmt!( \\\"%?\\\", $arg ))\n         );\n         ($( $arg:expr ),+) => (\n-            log(3u32, fmt!( $($arg),+ ))\n+            __log(3u32, fmt!( $($arg),+ ))\n         )\n     )\n     macro_rules! debug (\n         ($arg:expr) => (\n-            log(4u32, fmt!( \\\"%?\\\", $arg ))\n+            __log(4u32, fmt!( \\\"%?\\\", $arg ))\n         );\n         ($( $arg:expr ),+) => (\n-            log(4u32, fmt!( $($arg),+ ))\n+            __log(4u32, fmt!( $($arg),+ ))\n         )\n     )\n "}, {"sha": "7d3f5c126128d415cb2f32bec944b9c706900720", "filename": "src/libsyntax/parse/parser.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/9c7e16e48d15b3380cc680f6194fe3516867d2db/src%2Flibsyntax%2Fparse%2Fparser.rs", "raw_url": "https://github.com/rust-lang/rust/raw/9c7e16e48d15b3380cc680f6194fe3516867d2db/src%2Flibsyntax%2Fparse%2Fparser.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibsyntax%2Fparse%2Fparser.rs?ref=9c7e16e48d15b3380cc680f6194fe3516867d2db", "patch": "@@ -1184,7 +1184,7 @@ pub impl Parser {\n                 }\n             }\n             hi = self.span.hi;\n-        } else if self.eat_keyw