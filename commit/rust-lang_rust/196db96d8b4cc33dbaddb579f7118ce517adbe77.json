unstable(feature = \"dispatch_from_dyn\", issue = \"0\")]\n impl<T: ?Sized + Unsize<U>, U: ?Sized> DispatchFromDyn<Box<U>> for Box<T> {}\n \n+#[stable(feature = \"boxed_slice_from_iter\", since = \"1.32.0\")]\n+impl<A> FromIterator<A> for Box<[A]> {\n+    fn from_iter<T: IntoIterator<Item = A>>(iter: T) -> Self {\n+        iter.into_iter().collect::<Vec<_>>().into_boxed_slice()\n+    }\n+}\n+\n #[stable(feature = \"box_slice_clone\", since = \"1.3.0\")]\n impl<T: Clone> Clone for Box<[T]> {\n     fn clone(&self) -> Self {"}, {"sha": "f340ea01c5f07d0b6d8bc35865985d621ea4a50a", "filename": "src/liballoc/boxed_test.rs", "status": "modified", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/rust-lang/rust/blob/196db96d8b4cc33dbaddb579f7118ce517adbe77/src%2Fliballoc%2Fboxed_test.rs", "raw_url": "https://github.com/rust-lang/rust/raw/196db96d8b4cc33dbaddb579f7118ce517adbe77/src%2Fliballoc%2Fboxed_test.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fliballoc%2Fboxed_test.rs?ref=196db96d8b4cc33dbaddb579f7118ce517adbe77", "patch": "@@ -140,3 +140,11 @@ fn str_slice() {\n     let boxed: Box<str> = Box::from(s);\n     assert_eq!(&*boxed, s)\n }\n+\n+#[test]\n+fn boxed_slice_from_iter() {\n+    let iter = 0..100;\n+    let boxed: Box<[u32]> = iter.collect();\n+    assert_eq!(boxed.len(), 100);\n+    assert_eq!(boxed[7], 7);\n+}"}]}