44c778a632e9554216304f965875/src%2Ftest%2Frun-pass%2Fclass-exports.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fclass-exports.rs?ref=e0da0dd4466844c778a632e9554216304f965875", "patch": "@@ -0,0 +1,19 @@\n+/* Test that exporting a class also exports its\n+   public fields and methods */\n+\n+import kitty::*;\n+\n+mod kitty {\n+  export cat;\n+  class cat {\n+    let meows: uint;\n+    let name: str;\n+\n+    fn get_name() -> str {  self.name }\n+    new(in_name: str) { self.name = in_name; self.meows = 0u; }\n+  }\n+}\n+\n+fn main() {\n+  assert(cat(\"Spreckles\").get_name() == \"Spreckles\");\n+}\n\\ No newline at end of file"}]}