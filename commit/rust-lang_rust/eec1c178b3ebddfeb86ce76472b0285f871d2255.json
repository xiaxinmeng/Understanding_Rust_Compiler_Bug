6 +198,20 @@\n             \"llvm\": \"vsum2sws\",\n             \"ret\": \"s32\",\n             \"args\": [\"0\", \"0\"]\n+        },\n+        {\n+            \"intrinsic\": \"sum4{0.kind}bs\",\n+            \"width\": [128],\n+            \"llvm\": \"vsum4{0.kind}bs\",\n+            \"ret\": \"i32\",\n+            \"args\": [\"0NN\", \"0\"]\n+        },\n+        {\n+            \"intrinsic\": \"sum4shs\",\n+            \"width\": [128],\n+            \"llvm\": \"vsum4shs\",\n+            \"ret\": \"s32\",\n+            \"args\": [\"0N\", \"0\"]\n         }\n     ]\n }"}, {"sha": "4d71c42edda3f861764cf388d2396d37a412861e", "filename": "src/librustc_platform_intrinsics/powerpc.rs", "status": "modified", "additions": 15, "deletions": 0, "changes": 15, "blob_url": "https://github.com/rust-lang/rust/blob/eec1c178b3ebddfeb86ce76472b0285f871d2255/src%2Flibrustc_platform_intrinsics%2Fpowerpc.rs", "raw_url": "https://github.com/rust-lang/rust/raw/eec1c178b3ebddfeb86ce76472b0285f871d2255/src%2Flibrustc_platform_intrinsics%2Fpowerpc.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibrustc_platform_intrinsics%2Fpowerpc.rs?ref=eec1c178b3ebddfeb86ce76472b0285f871d2255", "patch": "@@ -377,6 +377,21 @@ pub fn find(name: &str) -> Option<Intrinsic> {\n             output: &::I32x4,\n             definition: Named(\"llvm.ppc.altivec.vsum2sws\")\n         },\n+        \"_vec_sum4sbs\" => Intrinsic {\n+            inputs: { static INPUTS: [&'static Type; 2] = [&::I8x16, &::I32x4]; &INPUTS },\n+            output: &::I32x4,\n+            definition: Named(\"llvm.ppc.altivec.vsum4sbs\")\n+        },\n+        \"_vec_sum4ubs\" => Intrinsic {\n+            inputs: { static INPUTS: [&'static Type; 2] = [&::U8x16, &::U32x4]; &INPUTS },\n+            output: &::U32x4,\n+            definition: Named(\"llvm.ppc.altivec.vsum4ubs\")\n+        },\n+        \"_vec_sum4shs\" => Intrinsic {\n+            inputs: { static INPUTS: [&'static Type; 2] = [&::I16x8, &::I32x4]; &INPUTS },\n+            output: &::I32x4,\n+            definition: Named(\"llvm.ppc.altivec.vsum4shs\")\n+        },\n         _ => return None,\n     })\n }"}]}