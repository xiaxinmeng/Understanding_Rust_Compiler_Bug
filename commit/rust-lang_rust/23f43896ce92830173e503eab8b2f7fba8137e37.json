-lang/rust/raw/23f43896ce92830173e503eab8b2f7fba8137e37/mk%2Fplatform.mk", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/mk%2Fplatform.mk?ref=23f43896ce92830173e503eab8b2f7fba8137e37", "patch": "@@ -242,6 +242,9 @@ $(foreach target,$(CFG_TARGET), \\\n # Windows MSVC in the compiler, but the gist of it is that we use `llvm-ar.exe`\n # instead of `lib.exe` for assembling archives, so we need to inject this custom\n # dependency here.\n+#\n+# FIXME(stage0): remove this and all other relevant support in the makefiles\n+#                after a snapshot is made\n define ADD_LLVM_AR_TO_MSVC_DEPS\n ifeq ($$(findstring msvc,$(1)),msvc)\n NATIVE_TOOL_DEPS_core_T_$(1) += llvm-ar.exe"}, {"sha": "63ce003c810ddc715e99ce9d7130b29548aa9824", "filename": "mk/target.mk", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/rust-lang/rust/blob/23f43896ce92830173e503eab8b2f7fba8137e37/mk%2Ftarget.mk", "raw_url": "https://github.com/rust-lang/rust/raw/23f43896ce92830173e503eab8b2f7fba8137e37/mk%2Ftarget.mk", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/mk%2Ftarget.mk?ref=23f43896ce92830173e503eab8b2f7fba8137e37", "patch": "@@ -205,6 +205,9 @@ $(foreach host,$(CFG_HOST), \\\n # $(3) - triple snapshot is built for\n # $(4) - crate\n # $(5) - tool\n+#\n+# FIXME(stage0): remove this and all other relevant support in the makefiles\n+#                after a snapshot is made\n define MOVE_TOOLS_TO_SNAPSHOT_HOST_DIR\n ifneq (,$(3))\n $$(TLIB$(1)_T_$(2)_H_$(2))/stamp.$(4): $$(HLIB$(1)_H_$(2))/rustlib/$(3)/bin/$(5)\n@@ -229,6 +232,9 @@ $(foreach target,$(CFG_TARGET), \\\n #   path instead of MinGW's /usr/bin/link.exe (entirely unrelated)\n #\n # The values for these variables are detected by the configure script.\n+#\n+# FIXME(stage0): remove this and all other relevant support in the makefiles\n+#                after a snapshot is made\n define SETUP_LIB_MSVC_ENV_VARS\n ifeq ($$(findstring msvc,$(2)),msvc)\n $$(TLIB$(1)_T_$(2)_H_$(3))/stamp.$(4): \\"}, {"sha": "2466c254e0517b2f790ee89bfd24059407ede66b", "filename": "src/librustc_back/target/windows_msvc_base.rs", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/rust-lang/rust/blob/23f43896ce92830173e503eab8b2f7fba8137e37/src%2Flibrustc_back%2Ftarget%2Fwindows_msvc_base.rs", "raw_url": "https://github.com/rust-lang/rust/raw/23f43896ce92830173e503eab8b2f7fba8137e37/src%2Flibrustc_back%2Ftarget%2Fwindows_msvc_base.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibrustc_back%2Ftarget%2Fwindows_msvc_base.rs?ref=23f43896ce92830173e503eab8b2f7fba8137e37", "patch": "@@ -59,6 +59,7 @@ pub fn opts() -> TargetOptions {\n             \"/NOLOGO\".to_string(),\n             \"/NXCOMPAT\".to_string(),\n         ],\n+        archive_format: \"gnu\".to_string(),\n \n         .. Default::default()\n     }"}]}