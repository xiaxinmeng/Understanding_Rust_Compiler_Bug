t#74933: Lifetime error when indexing with borrowed index\n+\n+use std::ops::{Index, IndexMut};\n+\n+struct S(V);\n+struct K<'a>(&'a ());\n+struct V;\n+\n+impl<'a> Index<&'a K<'a>> for S {\n+    type Output = V;\n+\n+    fn index(&self, _: &'a K<'a>) -> &V {\n+        &self.0\n+    }\n+}\n+\n+impl<'a> IndexMut<&'a K<'a>> for S {\n+    fn index_mut(&mut self, _: &'a K<'a>) -> &mut V {\n+        &mut self.0\n+    }\n+}\n+\n+impl V {\n+    fn foo(&mut self) {}\n+}\n+\n+fn test(s: &mut S, k: &K<'_>) {\n+    s[k] = V;\n+    s[k].foo();\n+}\n+\n+fn main() {\n+    let mut s = S(V);\n+    let k = K(&());\n+    test(&mut s, &k);\n+}"}]}