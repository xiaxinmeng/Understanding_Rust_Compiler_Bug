itions": 30, "deletions": 0, "changes": 30, "blob_url": "https://github.com/rust-lang/rust/blob/0c4ad1fda667a955df11aba294098153a1a1ba44/src%2Ftest%2Fui%2Fissues%2Fissue-69841.rs", "raw_url": "https://github.com/rust-lang/rust/raw/0c4ad1fda667a955df11aba294098153a1a1ba44/src%2Ftest%2Fui%2Fissues%2Fissue-69841.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fui%2Fissues%2Fissue-69841.rs?ref=0c4ad1fda667a955df11aba294098153a1a1ba44", "patch": "@@ -0,0 +1,30 @@\n+// This is a regression test for issue rust-lang/rust#69841, which exposed an\n+// LLVM bug which needed a fix to be backported.\n+\n+// run-pass\n+\n+fn main() {\n+    let buffer = [49u8, 10];\n+    let mut a : u64 = 0;\n+    'read: loop {\n+        for c in &buffer {\n+            match c {\n+                48..=57 => {\n+                    a*= 10;\n+                    a+= *c as u64 - 48;\n+                }\n+                10 => {\n+                    break 'read;\n+                }\n+                _ => {\n+                    unsafe { std::hint::unreachable_unchecked() };\n+                }\n+            }\n+        }\n+    }\n+    if a == 1 {\n+        println!(\"What did you expect?\");\n+    } else {\n+        panic!(\"this should be unreachable.\");\n+    }\n+}"}]}