   /// ```\n+    #[unstable(feature = \"mutex_unlock\", issue = \"81872\")]\n+    pub fn unlock(guard: MutexGuard<'_, T>) {\n+        drop(guard);\n+    }\n }\n \n impl<T: ?Sized> Mutex<T> {"}]}