\n }\n \n+fn filter<@T>(f: block(T) -> bool, v: [mutable? T]) -> [T] {\n+    let result = [];\n+    for elem: T in v {\n+        let elem2 = elem; // satisfies alias checker\n+        if f(elem2) {\n+            result += [elem2];\n+        }\n+    }\n+    ret result;\n+}\n+\n fn foldl<@T, @U>(p: block(U, T) -> U, z: U, v: [mutable? T]) -> U {\n     let sz = len(v);\n     if sz == 0u { ret z; }"}, {"sha": "b7571d5bf0e82853791e27397b95449f22ddddd5", "filename": "src/test/stdtest/vec.rs", "status": "modified", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/3a7a2943ddd1e7e91f625d6459c3e0636ca129cb/src%2Ftest%2Fstdtest%2Fvec.rs", "raw_url": "https://github.com/rust-lang/rust/raw/3a7a2943ddd1e7e91f625d6459c3e0636ca129cb/src%2Ftest%2Fstdtest%2Fvec.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fstdtest%2Fvec.rs?ref=3a7a2943ddd1e7e91f625d6459c3e0636ca129cb", "patch": "@@ -12,6 +12,8 @@ fn square_alias(n: uint) -> uint { ret n * n; }\n \n pure fn is_three(n: uint) -> bool { ret n == 3u; }\n \n+pure fn is_odd(n: uint) -> bool { ret n % 2u == 1u; }\n+\n fn square_if_odd(n: uint) -> option::t<uint> {\n     ret if n % 2u == 1u { some(n * n) } else { none };\n }\n@@ -269,7 +271,12 @@ fn test_filter_map() {\n     assert (filter_map(halve, all_odd1) == []);\n     assert (filter_map(halve, all_odd2) == []);\n     assert (filter_map(halve, mix) == mix_dest);\n+}\n \n+#[test]\n+fn test_filter() {\n+    assert filter(is_odd, [1u, 2u, 3u]) == [1u, 3u];\n+    assert filter(is_three, [1u, 2u, 4u, 8u, 16u]) == [];\n }\n \n #[test]"}]}