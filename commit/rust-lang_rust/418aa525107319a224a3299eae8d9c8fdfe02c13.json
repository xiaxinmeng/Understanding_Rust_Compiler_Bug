/ xfail-stage0\n+\n+fn fix_help[A,B](@fn (@fn (&A) -> B, &A) -> B f, &A x) -> B {\n+    ret f(@bind fix_help(f, _), x);\n+}\n+\n+fn fix[A,B](@fn (@fn (&A) -> B, &A) -> B f) -> (@fn(&A) -> B) {\n+    ret @bind fix_help(f, _);\n+}\n+\n+fn fact_(@fn (&int) -> int f, &int n) -> int {\n+    // fun fact 0 = 1\n+    ret if (n == 0) { 1 } else { n*f(n-1) };\n+}\n+\n+fn main() {\n+    auto fact = fix(@fact_);\n+    assert(fact(5) == 120);\n+    assert(fact(2) == 2);\n+}"}]}