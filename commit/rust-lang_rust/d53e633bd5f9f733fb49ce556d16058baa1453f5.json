ref=d53e633bd5f9f733fb49ce556d16058baa1453f5", "patch": "@@ -0,0 +1,13 @@\n+import swappable::{swappable, methods};\n+\n+fn main() {\n+    let d = swappable(3);\n+    assert d.get() == 3;\n+    d.set(4);\n+    assert d.get() == 4;\n+    d.swap { |i| i + 1 };\n+    assert d.get() == 5;\n+    assert d.with { |i| i + 1 } == 6;\n+    assert d.get() == 5;\n+    assert swappable::unwrap(d) == 5;\n+}\n\\ No newline at end of file"}]}