10f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Frustc%2Fmiddle%2Ftypeck.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Frustc%2Fmiddle%2Ftypeck.rs?ref=11610f9ca1a9da187c1b6f0d40ae2e369afd4fee", "patch": "@@ -934,8 +934,8 @@ mod collect {\n           }\n           ast_map::node_item(@{node: ast::item_class(_,its,_), _}, _) {\n               let (_,ms) = split_class_items(its);\n-              // Handling all methods here\n-              let ps = ast_util::ignore_privacy(ms);\n+              // Only public methods need to be stored\n+              let ps = ast_util::public_methods(ms);\n               store_methods::<@ast::method>(tcx, id, ps, {|m|\n                           ty_of_method(tcx, m_collect, m)});\n           }"}, {"sha": "a2995bf298667643e7e04b877bc8e3812e5cb77f", "filename": "src/test/auxiliary/cci_class_5.rs", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/rust-lang/rust/blob/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fauxiliary%2Fcci_class_5.rs", "raw_url": "https://github.com/rust-lang/rust/raw/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fauxiliary%2Fcci_class_5.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fauxiliary%2Fcci_class_5.rs?ref=11610f9ca1a9da187c1b6f0d40ae2e369afd4fee", "patch": "@@ -0,0 +1,14 @@\n+mod kitties {\n+\n+class cat {\n+  priv {\n+    let mutable meows : uint;\n+    fn nap() { uint::range(1u, 10000u) {|_i|}}\n+  }\n+\n+  let how_hungry : int;\n+\n+  new(in_x : uint, in_y : int) { meows = in_x; how_hungry = in_y; }\n+}\n+\n+}\n\\ No newline at end of file"}, {"sha": "b96adc19cbbac5433e403a81b8ecaa42ce584515", "filename": "src/test/compile-fail/private-class-field-cross-crate.rs", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-class-field-cross-crate.rs", "raw_url": "https://github.com/rust-lang/rust/raw/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-class-field-cross-crate.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fprivate-class-field-cross-crate.rs?ref=11610f9ca1a9da187c1b6f0d40ae2e369afd4fee", "patch": "@@ -0,0 +1,10 @@\n+// error-pattern:no public field or method with that name\n+// xfail-fast\n+// aux-build:cci_class.rs\n+use cci_class;\n+import cci_class::kitties::*;\n+\n+fn main() {\n+  let nyan : cat = cat(52u, 99);\n+  assert (nyan.meows == 52u);\n+}"}, {"sha": "279c61db26c182fa8a883b80ea40284fad36556b", "filename": "src/test/compile-fail/private-method-cross-crate.rs", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-method-cross-crate.rs", "raw_url": "https://github.com/rust-lang/rust/raw/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-method-cross-crate.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fprivate-method-cross-crate.rs?ref=11610f9ca1a9da187c1b6f0d40ae2e369afd4fee", "patch": "@@ -0,0 +1,10 @@\n+// error-pattern:attempted access of field nap on type\n+// xfail-fast\n+// aux-build:cci_class_5.rs\n+use cci_class_5;\n+import cci_class_5::kitties::*;\n+\n+fn main() {\n+  let nyan : cat = cat(52u, 99);\n+  nyan.nap();\n+}"}, {"sha": "7dc1f44d71bbf9b8fd33cd03b279093515ea91e3", "filename": "src/test/compile-fail/private-method.rs", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-method.rs", "raw_url": "https://github.com/rust-lang/rust/raw/11610f9ca1a9da187c1b6f0d40ae2e369afd4fee/src%2Ftest%2Fcompile-fail%2Fprivate-method.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Fcompile-fail%2Fprivate-method.rs?ref=11610f9ca1a9da187c1b6f0d40ae2e369afd4fee", "patch": "@@ -1,4 +1,4 @@\n-// error-pattern:Class doesn't have a public method named nap\n+// error-pattern:attempted access of field nap on type\n class cat {\n   priv {\n     let mutable meows : uint;"}]}