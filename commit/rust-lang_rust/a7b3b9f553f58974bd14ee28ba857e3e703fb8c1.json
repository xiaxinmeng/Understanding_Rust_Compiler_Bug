, "blob_url": "https://github.com/rust-lang/rust/blob/a7b3b9f553f58974bd14ee28ba857e3e703fb8c1/setup-toolchain.sh", "raw_url": "https://github.com/rust-lang/rust/raw/a7b3b9f553f58974bd14ee28ba857e3e703fb8c1/setup-toolchain.sh", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/setup-toolchain.sh?ref=a7b3b9f553f58974bd14ee28ba857e3e703fb8c1", "patch": "@@ -13,7 +13,7 @@ if [[ \"$INSTALLED\" == false || \"$RTIM_PATH\" == $CARGO_HOME/bin/rustup-toolchain-\n     cargo +nightly install rustup-toolchain-install-master\n else\n     VERSION=$(rustup-toolchain-install-master -V | grep -o \"[0-9.]*\")\n-    REMOTE=$(cargo search rustup-toolchain-install-master | grep -o \"[0-9.]*\")\n+    REMOTE=$(cargo +nightly search rustup-toolchain-install-master | grep -o \"[0-9.]*\")\n     echo \"info: skipping updating rustup-toolchain-install-master at $RTIM_PATH\"\n     echo \"      current version : $VERSION\"\n     echo \"      remote version  : $REMOTE\""}]}