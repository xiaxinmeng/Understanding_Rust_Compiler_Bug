tf.rs?ref=12a67af81df4f05d9bc9f6634f615a69098f1d04", "patch": "@@ -0,0 +1,16 @@\n+// From #1174:\n+\n+use std;\n+\n+#[link_name = \"\"]\n+native mod libc {\n+    fn printf(s: *u8, a: int); /* A tenuous definition. */\n+}\n+\n+fn main() {\n+    let b = std::str::bytes(\"%d\\n\");\n+    let b8 = unsafe { std::vec::unsafe::to_ptr(b) };\n+    libc::printf(b8, 4);\n+    let a = bind libc::printf(b8, 5);\n+    a(); /* core dump */\n+}"}]}