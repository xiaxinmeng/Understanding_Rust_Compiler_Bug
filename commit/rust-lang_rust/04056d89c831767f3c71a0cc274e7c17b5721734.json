ary.\n-    if (_unread == _buffer_sz) {\n+    if (_unread + unit_sz > _buffer_sz) {\n         size_t new_buffer_sz = _buffer_sz << 1;\n         I(dom, new_buffer_sz <= MAX_CIRCULAR_BUFFFER_SIZE);\n         void *new_buffer = dom->malloc(new_buffer_sz);"}, {"sha": "f3a4b9b5d660c531b956ac75c9416ed6af2b6152", "filename": "src/test/run-pass/chan-poweroftwo.rs", "status": "modified", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/rust-lang/rust/blob/04056d89c831767f3c71a0cc274e7c17b5721734/src%2Ftest%2Frun-pass%2Fchan-poweroftwo.rs", "raw_url": "https://github.com/rust-lang/rust/raw/04056d89c831767f3c71a0cc274e7c17b5721734/src%2Ftest%2Frun-pass%2Fchan-poweroftwo.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fchan-poweroftwo.rs?ref=04056d89c831767f3c71a0cc274e7c17b5721734", "patch": "@@ -25,8 +25,22 @@ impure fn test_init() {\n     mychan <| val;\n }\n \n+// Dump lots of items into the channel so it has to grow.\n+// Don't trigger any assertions.\n+impure fn test_grow() {\n+    let port[record] myport = port();\n+    auto mychan = chan(myport);\n+\n+    let record val = rec(val1=0i32, val2=0i32, val3=0i32);\n+\n+    for each (uint i in _uint.range(0u, 100u)) {\n+        mychan <| val;\n+    }\n+}\n+\n impure fn main() {\n     test_init();\n+    test_grow();\n }\n \n // Local Variables:"}]}