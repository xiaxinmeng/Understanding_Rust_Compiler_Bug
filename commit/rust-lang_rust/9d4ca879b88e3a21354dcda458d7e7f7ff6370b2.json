0 +1,17 @@\n+// Test that niche finding works with captured generator upvars.\n+\n+#![feature(generators)]\n+\n+use std::mem::size_of_val;\n+\n+fn take<T>(_: T) {}\n+\n+fn main() {\n+    let x = false;\n+    let gen1 = || {\n+        yield;\n+        take(x);\n+    };\n+\n+    assert_eq!(size_of_val(&gen1), size_of_val(&Some(gen1)));\n+}"}]}