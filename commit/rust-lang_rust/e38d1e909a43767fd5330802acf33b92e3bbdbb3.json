hub.com/repos/rust-lang/rust/contents/src%2Flibrustdoc%2Fhtml%2Fstatic%2Fcss%2Frustdoc.css?ref=e38d1e909a43767fd5330802acf33b92e3bbdbb3", "patch": "@@ -585,8 +585,8 @@ ul.block, .block li {\n \ttext-overflow: ellipsis;\n }\n /* Wrap non-pre code blocks (`text`) but not (```text```). */\n-.docblock > :not(pre) > code,\n-.docblock-short > code {\n+.docblock :not(pre) > code,\n+.docblock-short code {\n \twhite-space: pre-wrap;\n }\n "}, {"sha": "3ce921c2c91cb1a3de4aef659a576085852ca4c7", "filename": "src/test/rustdoc-gui/docblock-big-code-mobile.goml", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/rust-lang/rust/blob/e38d1e909a43767fd5330802acf33b92e3bbdbb3/src%2Ftest%2Frustdoc-gui%2Fdocblock-big-code-mobile.goml", "raw_url": "https://github.com/rust-lang/rust/raw/e38d1e909a43767fd5330802acf33b92e3bbdbb3/src%2Ftest%2Frustdoc-gui%2Fdocblock-big-code-mobile.goml", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc-gui%2Fdocblock-big-code-mobile.goml?ref=e38d1e909a43767fd5330802acf33b92e3bbdbb3", "patch": "@@ -7,3 +7,7 @@ show-text: true // We need to enable text draw to be able to have the \"real\" siz\n // Little explanations for this test: if the text wasn't displayed on two lines, it would take\n // around 20px (which is the font size).\n assert-property: (\".docblock p > code\", {\"offsetHeight\": \"44\"})\n+\n+// Same check, but where the long code block is also a link\n+goto: \"file://\" + |DOC_PATH| + \"/test_docs/long_code_block_link/index.html\"\n+assert-property: (\".docblock p > a > code\", {\"offsetHeight\": \"44\"})"}, {"sha": "f55b9ec3af515235d7a34617afcdf604af10daff", "filename": "src/test/rustdoc-gui/src/test_docs/lib.rs", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/rust-lang/rust/blob/e38d1e909a43767fd5330802acf33b92e3bbdbb3/src%2Ftest%2Frustdoc-gui%2Fsrc%2Ftest_docs%2Flib.rs", "raw_url": "https://github.com/rust-lang/rust/raw/e38d1e909a43767fd5330802acf33b92e3bbdbb3/src%2Ftest%2Frustdoc-gui%2Fsrc%2Ftest_docs%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc-gui%2Fsrc%2Ftest_docs%2Flib.rs?ref=e38d1e909a43767fd5330802acf33b92e3bbdbb3", "patch": "@@ -154,6 +154,11 @@ pub mod huge_amount_of_consts {\n /// Very long code text `hereIgoWithLongTextBecauseWhyNotAndWhyWouldntI`.\n pub mod long_code_block {}\n \n+/// Very long code text [`hereIgoWithLongTextBecauseWhyNotAndWhyWouldntI`][lnk].\n+///\n+/// [lnk]: crate::long_code_block_link\n+pub mod long_code_block_link {}\n+\n #[macro_export]\n macro_rules! repro {\n     () => {};"}]}