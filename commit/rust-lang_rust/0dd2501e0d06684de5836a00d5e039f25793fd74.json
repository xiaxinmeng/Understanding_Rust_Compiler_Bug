intArithmetic {\n                 }\n             }\n         } else {\n-            check_expm1(cx, expr);\n-            check_mul_add(cx, expr);\n-            check_custom_abs(cx, expr);\n-            check_log_division(cx, expr);\n+            if !is_no_std_crate(cx) {\n+                check_expm1(cx, expr);\n+                check_mul_add(cx, expr);\n+                check_custom_abs(cx, expr);\n+                check_log_division(cx, expr);\n+            }\n             check_radians(cx, expr);\n         }\n     }"}]}