ts/src%2Ftest%2Frun-pass%2Fborrowck-preserve-box.rs?ref=efa2252cfcabe6aec34861d06fa10d0c174b8556", "patch": "@@ -0,0 +1,22 @@\n+// compile-flags:--borrowck=err\n+// exec-env:RUST_POISON_ON_FREE=1\n+\n+fn borrow(x: &int, f: fn(x: &int)) {\n+    let before = *x;\n+    f(x);\n+    let after = *x;\n+    assert before == after;\n+}\n+\n+fn main() {\n+    let mut x = @3;\n+    borrow(x) {|b_x|\n+        assert *b_x == 3;\n+        assert ptr::addr_of(*x) == ptr::addr_of(*b_x);\n+        x = @22;\n+\n+        #debug[\"ptr::addr_of(*b_x) = %x\", ptr::addr_of(*b_x) as uint];\n+        assert *b_x == 3;\n+        assert ptr::addr_of(*x) != ptr::addr_of(*b_x);\n+    }\n+}\n\\ No newline at end of file"}]}