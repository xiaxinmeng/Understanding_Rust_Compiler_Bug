age0\n+\n+use std;\n+import std::str;\n+\n+fn main()\n+{\n+    // Make sure we properly handle repeated self-appends.\n+    let str a = \"A\";\n+    auto i = 20;\n+    auto expected_len = 1u;\n+    while (i > 0) {\n+        log_err str::byte_len(a);\n+        assert (str::byte_len(a) == expected_len);\n+        a += a;\n+        i -= 1;\n+        expected_len *= 2u;\n+    }\n+}"}, {"sha": "7cafdf9497a0f4a24d0e914f6d0806bfa04347d8", "filename": "src/test/run-pass/vec-self-append.rs", "status": "modified", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/rust-lang/rust/blob/12d9d07a84db3726b69255141899d5df4148211c/src%2Ftest%2Frun-pass%2Fvec-self-append.rs", "raw_url": "https://github.com/rust-lang/rust/raw/12d9d07a84db3726b69255141899d5df4148211c/src%2Ftest%2Frun-pass%2Fvec-self-append.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frun-pass%2Fvec-self-append.rs?ref=12d9d07a84db3726b69255141899d5df4148211c", "patch": "@@ -1,12 +1,19 @@\n // xfail-stage0\n \n+use std;\n+import std::vec;\n+\n fn main()\n {\n     // Make sure we properly handle repeated self-appends.\n     let vec[int] a = [0];\n     auto i = 20;\n+    auto expected_len = 1u;\n     while (i > 0) {\n+        log_err vec::len(a);\n+        assert (vec::len(a) == expected_len);\n         a += a;\n         i -= 1;\n+        expected_len *= 2u;\n     }\n }"}]}