rsion: 11.0\n+\n+#![crate_type = \"lib\"]\n+\n+// CHECK-LABEL: @trim_in_place\n+#[no_mangle]\n+pub fn trim_in_place(a: &mut &[u8]) {\n+    while a.first() == Some(&42) {\n+        // CHECK-NOT: slice_index_order_fail\n+        *a = &a[1..];\n+    }\n+}\n+\n+// CHECK-LABEL: @trim_in_place2\n+#[no_mangle]\n+pub fn trim_in_place2(a: &mut &[u8]) {\n+    while let Some(&42) = a.first() {\n+        // CHECK-NOT: slice_index_order_fail\n+        *a = &a[1..];\n+    }\n+}"}]}