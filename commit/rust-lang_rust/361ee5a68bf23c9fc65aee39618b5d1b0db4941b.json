(s.s, common.istr(val as int));\n             wrd(s.s, common.ty_mach_to_str(mach));\n         }\n+        case (ast.lit_mach_float(?mach,?val)) {\n+            // val is already a str\n+            wrd(s.s, val);\n+            wrd(s.s, common.ty_mach_to_str(mach));\n+        }\n         case (ast.lit_nil) {wrd(s.s, \"()\");}\n         case (ast.lit_bool(?val)) {\n             if (val) {wrd(s.s, \"true\");} else {wrd(s.s, \"false\");}"}]}