/1cfa01decfbec571ac2d985339d6ebe0dbdf4a47/src%2Flibstd%2Fbitv.rs", "raw_url": "https://github.com/rust-lang/rust/raw/1cfa01decfbec571ac2d985339d6ebe0dbdf4a47/src%2Flibstd%2Fbitv.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibstd%2Fbitv.rs?ref=1cfa01decfbec571ac2d985339d6ebe0dbdf4a47", "patch": "@@ -59,7 +59,7 @@ impl SmallBitv {\n \n     #[inline(always)]\n     fn difference(s: &SmallBitv, nbits: uint) -> bool {\n-        self.bits_op(s.bits, nbits, |u1, u2| u1 ^ u2)\n+        self.bits_op(s.bits, nbits, |u1, u2| u1 & !u2)\n     }\n \n     #[inline(always)]\n@@ -180,10 +180,7 @@ impl BigBitv {\n \n     #[inline(always)]\n     fn difference(b: &BigBitv, nbits: uint) -> bool {\n-        self.invert();\n-        let b = self.intersect(b, nbits);\n-        self.invert();\n-        b\n+        self.process(b, nbits, difference)\n     }\n \n     #[inline(always)]\n@@ -567,6 +564,8 @@ pure fn lor(w0: uint, w1: uint) -> uint { return w0 | w1; }\n \n pure fn land(w0: uint, w1: uint) -> uint { return w0 & w1; }\n \n+pure fn difference(w0: uint, w1: uint) -> uint { return w0 & !w1; }\n+\n pure fn right(_w0: uint, w1: uint) -> uint { return w1; }\n \n impl Bitv: ops::Index<uint,bool> {\n@@ -954,6 +953,34 @@ mod tests {\n         let bools = ~[false, false, true, false, false, true, true, false];\n         assert from_bytes([0b00100110]).to_bools() == bools;\n     }\n+\n+    #[test]\n+    fn test_small_difference() {\n+      let b1 = Bitv(3, false);\n+      let b2 = Bitv(3, false);\n+      b1.set(0, true);\n+      b1.set(1, true);\n+      b2.set(1, true);\n+      b2.set(2, true);\n+      assert b1.difference(&b2);\n+      assert b1[0];\n+      assert !b1[1];\n+      assert !b1[2];\n+    }\n+\n+    #[test]\n+    fn test_big_difference() {\n+      let b1 = Bitv(100, false);\n+      let b2 = Bitv(100, false);\n+      b1.set(0, true);\n+      b1.set(40, true);\n+      b2.set(40, true);\n+      b2.set(80, true);\n+      assert b1.difference(&b2);\n+      assert b1[0];\n+      assert !b1[40];\n+      assert !b1[80];\n+    }\n }\n \n //"}]}