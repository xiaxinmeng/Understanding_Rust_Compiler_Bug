{\n@@ -25,4 +27,12 @@ fn main() {\n         let mut x = 0u;\n         move |:| x += 1;\n     }\n+    {\n+        let mut x = 0u;\n+        move |&mut:| set(&mut x);\n+    }\n+    {\n+        let mut x = 0u;\n+        move |:| set(&mut x);\n+    }\n }"}]}