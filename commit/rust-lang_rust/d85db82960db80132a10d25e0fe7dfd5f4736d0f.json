-112,6 +112,14 @@ impl<T, E> Poll<Option<Result<T, E>>> {\n \n #[stable(feature = \"futures_api\", since = \"1.36.0\")]\n impl<T> From<T> for Poll<T> {\n+    /// Convert to a `Ready` variant.\n+    ///\n+    /// # Example\n+    ///\n+    /// ```\n+    /// # use core::task::Poll;\n+    /// assert_eq!(Poll::from(true), Poll::Ready(true));\n+    /// ```\n     fn from(t: T) -> Poll<T> {\n         Poll::Ready(t)\n     }"}]}