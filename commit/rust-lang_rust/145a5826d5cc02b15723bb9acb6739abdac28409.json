ble-isolation\n+\n+extern {\n+    pub fn chdir(dir: *const u8) -> i32;\n+}\n+\n+fn main() {\n+    let path = vec![0xc3u8, 0x28, 0];\n+    // test that `chdir` errors with invalid utf-8 path\n+    unsafe { chdir(path.as_ptr()) };  //~ ERROR is not a valid utf-8 string\n+}"}]}