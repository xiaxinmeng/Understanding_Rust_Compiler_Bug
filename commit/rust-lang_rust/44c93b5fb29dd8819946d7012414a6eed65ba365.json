stage0\n-// xfail-stage1\n-// xfail-stage2\n-// xfail-stage3\n-\n-// Test case for issue #763, provided by robarnold.\n-\n-use std;\n-import std::task;\n-\n-tag request {\n-  quit;\n-  close(int, chan[bool]);\n-}\n-\n-type ctx = chan[request];\n-\n-fn request_task(c : chan[ctx]) {\n-    let p = port();\n-    c <| chan(p);\n-    let req;\n-    while (true) {\n-        p |> req;\n-        alt (req) {\n-            quit. {\n-                ret;\n-            }\n-            close(what, status) {\n-                log \"closing now\";\n-                log what;\n-                status <| true;\n-            }\n-        }\n-    }\n-}\n-\n-fn new() -> ctx {\n-    let p = port();\n-    let t = spawn request_task(chan(p));\n-    let cx;\n-    p |> cx;\n-    ret cx;\n-}\n-\n-fn main() {\n-    let cx = new();\n-\n-    let p = port();\n-    cx <| close(4, chan(p));\n-    let result;\n-    p |> result;\n-    cx <| quit;\n-}"}]}