rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fbootstrap%2Fcompile.rs?ref=05045da9fdab511f39e88335b1bc7f92ea7973ba", "patch": "@@ -199,7 +199,7 @@ pub fn rustc<'a>(build: &'a Build, target: &str, compiler: &Compiler<'a>) {\n         cargo.env(\"CFG_DISABLE_UNSTABLE_FEATURES\", \"1\");\n     }\n     // Flag that rust llvm is in use\n-    if build.config.target_config.get(target).is_none() {\n+    if build.is_rust_llvm(target) {\n         cargo.env(\"LLVM_RUSTLLVM\", \"1\");\n     }\n     cargo.env(\"LLVM_CONFIG\", build.llvm_config(target));"}, {"sha": "5d61abe5e086ad23c54c2d2e42a70ece43d30ff7", "filename": "src/bootstrap/lib.rs", "status": "modified", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/rust-lang/rust/blob/05045da9fdab511f39e88335b1bc7f92ea7973ba/src%2Fbootstrap%2Flib.rs", "raw_url": "https://github.com/rust-lang/rust/raw/05045da9fdab511f39e88335b1bc7f92ea7973ba/src%2Fbootstrap%2Flib.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fbootstrap%2Flib.rs?ref=05045da9fdab511f39e88335b1bc7f92ea7973ba", "patch": "@@ -727,6 +727,16 @@ impl Build {\n         self.out.join(target).join(\"llvm\")\n     }\n \n+    /// Returns true if no custom `llvm-config` is set for the specified target.\n+    ///\n+    /// If no custom `llvm-config` was specified then Rust's llvm will be used.\n+    fn is_rust_llvm(&self, target: &str) -> bool {\n+        match self.config.target_config.get(target) {\n+            Some(ref c) => c.llvm_config.is_none(),\n+            None => true\n+        }\n+    }\n+\n     /// Returns the path to `llvm-config` for the specified target.\n     ///\n     /// If a custom `llvm-config` was specified for target then that's returned"}, {"sha": "786aad117be48547f4ca50fae84c4879fa992d4d", "filename": "src/llvm", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": null, "raw_url": null, "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Fllvm?ref=05045da9fdab511f39e88335b1bc7f92ea7973ba", "patch": "@@ -1 +1 @@\n-Subproject commit d295833b773313dbef26041f481fc91a2954fbb9\n+Subproject commit 786aad117be48547f4ca50fae84c4879fa992d4d"}, {"sha": "378810a8b89fc9f0caad49ae7101c62d4241e8a0", "filename": "src/rustllvm/llvm-auto-clean-trigger", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/rust-lang/rust/blob/05045da9fdab511f39e88335b1bc7f92ea7973ba/src%2Frustllvm%2Fllvm-auto-clean-trigger", "raw_url": "https://github.com/rust-lang/rust/raw/05045da9fdab511f39e88335b1bc7f92ea7973ba/src%2Frustllvm%2Fllvm-auto-clean-trigger", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Frustllvm%2Fllvm-auto-clean-trigger?ref=05045da9fdab511f39e88335b1bc7f92ea7973ba", "patch": "@@ -1,4 +1,4 @@\n # If this file is modified, then llvm will be forcibly cleaned and then rebuilt.\n # The actual contents of this file do not matter, but to trigger a change on the\n # build bots then the contents should be changed so git updates the mtime.\n-2016-07-25b\n+2016-08-07"}]}