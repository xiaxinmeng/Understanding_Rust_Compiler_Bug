ents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Flibrustdoc%2Fpasses%2Fhtml_tags.rs?ref=30cabfd215f374b855f6d6473b00a55c3511c3e2", "patch": "@@ -48,8 +48,11 @@ fn drop_tag(\n     if let Some(pos) = tags.iter().rev().position(|(t, _)| *t == tag_name) {\n         // Because this is from a `rev` iterator, the position is reversed as well!\n         let pos = tags.len() - 1 - pos;\n+        // If the tag is nested inside a \"<script>\", not warning should be emitted.\n+        let should_not_warn =\n+            tags.iter().take(pos + 1).any(|(at, _)| at == \"script\" || at == \"style\");\n         for (last_tag_name, last_tag_span) in tags.drain(pos + 1..) {\n-            if ALLOWED_UNCLOSED.iter().any(|&at| at == &last_tag_name) {\n+            if should_not_warn || ALLOWED_UNCLOSED.iter().any(|&at| at == &last_tag_name) {\n                 continue;\n             }\n             // `tags` is used as a queue, meaning that everything after `pos` is included inside it."}, {"sha": "0dc2002bd39d563d8a748c602162938d6774d32d", "filename": "src/test/rustdoc-ui/invalid-html-tags.rs", "status": "modified", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/rust-lang/rust/blob/30cabfd215f374b855f6d6473b00a55c3511c3e2/src%2Ftest%2Frustdoc-ui%2Finvalid-html-tags.rs", "raw_url": "https://github.com/rust-lang/rust/raw/30cabfd215f374b855f6d6473b00a55c3511c3e2/src%2Ftest%2Frustdoc-ui%2Finvalid-html-tags.rs", "contents_url": "https://api.github.com/repos/rust-lang/rust/contents/src%2Ftest%2Frustdoc-ui%2Finvalid-html-tags.rs?ref=30cabfd215f374b855f6d6473b00a55c3511c3e2", "patch": "@@ -39,3 +39,25 @@ pub fn b() {}\n ///   <h3>\n //~^ ERROR unclosed HTML tag `h3`\n pub fn c() {}\n+\n+// Unclosed tags shouldn't warn if they are nested inside a <script> elem.\n+/// <script>\n+///   <h3><div>\n+/// </script>\n+/// <script>\n+///   <div>\n+///     <p>\n+///   </div>\n+/// </script>\n+pub fn d() {}\n+\n+// Unclosed tags shouldn't warn if they are nested inside a <style> elem.\n+/// <style>\n+///   <h3><div>\n+/// </style>\n+/// <style>\n+///   <div>\n+///     <p>\n+///   </div>\n+/// </style>\n+pub fn e() {}"}]}