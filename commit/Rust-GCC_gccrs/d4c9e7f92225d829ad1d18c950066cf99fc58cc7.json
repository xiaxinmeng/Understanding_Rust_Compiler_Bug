 @@ expand_static_init (tree decl, tree init)\n   gcc_assert (TREE_STATIC (decl));\n \n   /* Some variables require no dynamic initialization.  */\n-  if (!init\n-      && TYPE_HAS_TRIVIAL_DESTRUCTOR (TREE_TYPE (decl)))\n+  if (TYPE_HAS_TRIVIAL_DESTRUCTOR (TREE_TYPE (decl)))\n     {\n       /* Make sure the destructor is callable.  */\n       cxx_maybe_build_cleanup (decl, tf_warning_or_error);\n-      return;\n+      if (!init)\n+\treturn;\n     }\n \n   if (DECL_THREAD_LOCAL_P (decl) && DECL_GNU_TLS_P (decl)"}, {"sha": "770bb9c305a631520191995fc2ed1710ae1f0263", "filename": "gcc/testsuite/g++.dg/cpp0x/deleted12.C", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d4c9e7f92225d829ad1d18c950066cf99fc58cc7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdeleted12.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d4c9e7f92225d829ad1d18c950066cf99fc58cc7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdeleted12.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdeleted12.C?ref=d4c9e7f92225d829ad1d18c950066cf99fc58cc7", "patch": "@@ -0,0 +1,10 @@\n+// PR c++/66542\n+// { dg-do compile { target c++11 } }\n+\n+struct A\n+{\n+  A() {}\n+  ~A() = delete;\t\t// { dg-message \"declared here\" }\n+};\n+\n+static A a;\t\t\t// { dg-error \"deleted\" }"}]}