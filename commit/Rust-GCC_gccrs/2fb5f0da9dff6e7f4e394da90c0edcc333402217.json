baz (void *, int);\n+\n+void\n+foo (struct S *x, int y, int z, void *f, int *p, struct T *e)\n+{\n+  while (x)\n+    {\n+      baz (f, &d > p);\n+      if (z & 1)\n+        bar (f > (void *) &f, z);\n+    }\n+  if (c)\n+    {\n+      asm (\"\" : \"+m\" (a) : \"i\" (0));\n+      y--;\n+    }\n+  if (e->b == e->c)\n+    c = y;\n+  y--;\n+}"}]}