+// PR c++/56428\n+// { dg-do compile { target c++11 } }\n+\n+struct A { };\n+\n+template<bool B>\n+  struct Builder\n+  {\n+    static A build() { return A(); }\n+  };\n+\n+template<A (*F)()>\n+  A f()\n+  {\n+    return Builder<F != nullptr>::build();\n+  }\n+\n+A g();\n+\n+int main()\n+{\n+  f< &g >();\n+  f< nullptr >();\n+  f< &f<nullptr> >();\n+}"}]}