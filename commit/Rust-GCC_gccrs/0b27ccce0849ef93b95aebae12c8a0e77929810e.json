gcc/ada/sinput.adb", "status": "modified", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0b27ccce0849ef93b95aebae12c8a0e77929810e/gcc%2Fada%2Fsinput.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0b27ccce0849ef93b95aebae12c8a0e77929810e/gcc%2Fada%2Fsinput.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fsinput.adb?ref=0b27ccce0849ef93b95aebae12c8a0e77929810e", "patch": "@@ -937,6 +937,8 @@ package body Sinput is\n \n    procedure Sloc_Range (N : Node_Id; Min, Max : out Source_Ptr) is\n \n+      Indx : constant Source_File_Index :=  Get_Source_File_Index (Sloc (N));\n+\n       function Process (N : Node_Id) return Traverse_Result;\n       --  Process function for traversing the node tree\n \n@@ -950,6 +952,14 @@ package body Sinput is\n          Orig : constant Node_Id := Original_Node (N);\n \n       begin\n+         --  Skip nodes that may have been added during expansion and\n+         --  that originate in other units, such as code for contracts\n+         --  in subprogram bodies.\n+\n+         if Get_Source_File_Index (Sloc (Orig)) /= Indx then\n+            return Skip;\n+         end if;\n+\n          if Sloc (Orig) < Min then\n             if Sloc (Orig) > No_Location then\n                Min := Sloc (Orig);"}]}