2B%2Fnew_op.cc?ref=80333d3d23015d7e6e3a4380ccea5a04d807b9db", "patch": "@@ -46,14 +46,13 @@ operator new (std::size_t sz) _GLIBCXX_THROW (std::bad_alloc)\n   /* malloc (0) is unpredictable; avoid it.  */\n   if (sz == 0)\n     sz = 1;\n-  p = (void *) malloc (sz);\n-  while (p == 0)\n+\n+  while (__builtin_expect ((p = malloc (sz)) == 0, false))\n     {\n       new_handler handler = std::get_new_handler ();\n       if (! handler)\n \t_GLIBCXX_THROW_OR_ABORT(bad_alloc());\n       handler ();\n-      p = (void *) malloc (sz);\n     }\n \n   return p;"}, {"sha": "968082f4096eaf44ef7619dbfc2af6e2f5f9b9dc", "filename": "libstdc++-v3/libsupc++/new_opnt.cc", "status": "modified", "additions": 2, "deletions": 4, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/80333d3d23015d7e6e3a4380ccea5a04d807b9db/libstdc%2B%2B-v3%2Flibsupc%2B%2B%2Fnew_opnt.cc", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/80333d3d23015d7e6e3a4380ccea5a04d807b9db/libstdc%2B%2B-v3%2Flibsupc%2B%2B%2Fnew_opnt.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Flibsupc%2B%2B%2Fnew_opnt.cc?ref=80333d3d23015d7e6e3a4380ccea5a04d807b9db", "patch": "@@ -39,8 +39,8 @@ operator new (std::size_t sz, const std::nothrow_t&) _GLIBCXX_USE_NOEXCEPT\n   /* malloc (0) is unpredictable; avoid it.  */\n   if (sz == 0)\n     sz = 1;\n-  p = (void *) malloc (sz);\n-  while (p == 0)\n+\n+  while (__builtin_expect ((p = malloc (sz)) == 0, false))\n     {\n       new_handler handler = std::get_new_handler ();\n       if (! handler)\n@@ -53,8 +53,6 @@ operator new (std::size_t sz, const std::nothrow_t&) _GLIBCXX_USE_NOEXCEPT\n \t{\n \t  return 0;\n \t}\n-\n-      p = (void *) malloc (sz);\n     }\n \n   return p;"}]}