5,12 @@ aarch64_rtx_costs (rtx x, int code, int outer ATTRIBUTE_UNUSED,\n       *cost += rtx_cost (op2, FMA, 2, speed);\n       return true;\n \n+    case FLOAT:\n+    case UNSIGNED_FLOAT:\n+      if (speed)\n+\t*cost += extra_cost->fp[mode == DFmode].fromint;\n+      return false;\n+\n     case FLOAT_EXTEND:\n       if (speed)\n \t*cost += extra_cost->fp[mode == DFmode].widen;"}]}