  if (spec && TREE_VALUE (spec) == tmpl)\n-\t    {\n-\t      tree inst_decl = (DECL_P (instance)\n-\t\t\t\t? instance : TYPE_NAME (instance));\n-\t      permerror (input_location,\n-\t\t\t \"partial specialization of %qD after instantiation \"\n-\t\t\t \"of %qD\", decl, inst_decl);\n-\t    }\n+\t  tree inst_decl = (DECL_P (instance)\n+\t\t\t    ? instance : TYPE_NAME (instance));\n+\t  if (!spec)\n+\t    /* OK */;\n+\t  else if (spec == error_mark_node)\n+\t    permerror (input_location,\n+\t\t       \"declaration of %qD ambiguates earlier template \"\n+\t\t       \"instantiation for %qD\", decl, inst_decl);\n+\t  else if (TREE_VALUE (spec) == tmpl)\n+\t    permerror (input_location,\n+\t\t       \"partial specialization of %qD after instantiation \"\n+\t\t       \"of %qD\", decl, inst_decl);\n \t}\n     }\n "}, {"sha": "c5f83bd9f1ce0ec8fe60f610a5be48af3be20773", "filename": "gcc/testsuite/g++.dg/template/partial-specialization3.C", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/6337bd75e256c0d0901d37178b26b273dcbb1078/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-specialization3.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/6337bd75e256c0d0901d37178b26b273dcbb1078/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-specialization3.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-specialization3.C?ref=6337bd75e256c0d0901d37178b26b273dcbb1078", "patch": "@@ -0,0 +1,7 @@\n+// DR 2179\n+\n+template <class T1, class T2> class A;\n+template <class T> struct A<T, void> { void f(); };\n+template <class T> void g(T) { A<char, void>().f(); }   // #1\n+template<typename T> struct A<char, T> {};\t\t// { dg-error \"\" }\n+A<char, void> f;   // #2"}]}