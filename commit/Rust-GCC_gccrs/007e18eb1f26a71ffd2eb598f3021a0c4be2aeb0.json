8731.c?ref=007e18eb1f26a71ffd2eb598f3021a0c4be2aeb0", "patch": "@@ -0,0 +1,41 @@\n+/* { dg-do run } */\n+\n+#include <stdio.h>\n+#include <stdlib.h>\n+\n+#define GENERAL 1\n+#define BRACKETS 2\n+#define QUOTES 3\n+\n+void __attribute__((noinline,noclone))\n+foo(char *qb, char* into)\n+{\n+  int state = QUOTES;\n+  int save_state = BRACKETS;\n+\n+  while (qb)\n+    {\n+      switch (state)\n+\t{\n+\tcase BRACKETS:\n+\t  exit(0);\n+\tcase GENERAL:\n+\t  abort ();\n+\tcase QUOTES:\n+\t  state = save_state;\n+\t  save_state = GENERAL;\n+\t  break;\n+\tdefault: ;\n+\t}\n+      printf(\"State %d btw GENERAL %d\\n\", state, GENERAL);\n+    }\n+  abort ();\n+}\n+\n+int main()\n+{\n+  char *b = \"123\";\n+  char out[4];\n+  foo(b, out);\n+  return 0;\n+}"}]}