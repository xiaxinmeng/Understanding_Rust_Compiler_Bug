@@ finish_options (struct gcc_options *opts, struct gcc_options *opts_set,\n       opts->x_flag_toplevel_reorder = 0;\n     }\n \n+  if (opts->x_flag_tm && opts->x_flag_non_call_exceptions)\n+    sorry (\"transactional memory is not supported with non-call exceptions\");\n+\n   /* -Wmissing-noreturn is alias for -Wsuggest-attribute=noreturn.  */\n   if (opts->x_warn_missing_noreturn)\n     opts->x_warn_suggest_attribute_noreturn = true;"}, {"sha": "9d2971bbbe04901e8246897a633275249fa36f40", "filename": "gcc/testsuite/g++.dg/tm/pr51212.C", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ee777b71644eda0bf9e3c6dc81e6518ed594ec8a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftm%2Fpr51212.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ee777b71644eda0bf9e3c6dc81e6518ed594ec8a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftm%2Fpr51212.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftm%2Fpr51212.C?ref=ee777b71644eda0bf9e3c6dc81e6518ed594ec8a", "patch": "@@ -0,0 +1,19 @@\n+// { dg-do compile }\n+// { dg-options \"-fgnu-tm -fnon-call-exceptions\" }\n+\n+struct S\n+{\n+  S ()\n+  {\n+  }\n+};\n+\n+__attribute__ ((transaction_callable))\n+void foo (int *p)\n+{\n+  S s;\n+  if (*p)\n+    ;\n+}\n+\n+// { dg-message \"sorry, unimplemented: transactional memory is not supported with non-call exceptions\" \"-fnon-call-exceptions and -fgnu-tm together\" { target *-*-* } 0 }"}]}