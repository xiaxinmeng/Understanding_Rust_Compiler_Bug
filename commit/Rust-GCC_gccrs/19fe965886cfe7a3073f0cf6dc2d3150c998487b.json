r\n+!\n+\n+module mod_fin_04\n+  implicit none\n+  type :: p_vec\n+  contains\n+     final :: delete\n+  end type p_vec\n+  type, extends(p_vec) :: bar\n+  contains\n+    final :: del2\n+  end type bar\n+contains\n+  subroutine delete(this)\n+    type(p_vec) :: this\n+  end subroutine delete\n+  subroutine del2(this)\n+    type(bar) :: this\n+  end subroutine del2\n+end module"}]}