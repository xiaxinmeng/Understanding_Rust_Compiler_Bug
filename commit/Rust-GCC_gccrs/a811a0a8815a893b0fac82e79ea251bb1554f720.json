ion.c?ref=a811a0a8815a893b0fac82e79ea251bb1554f720", "patch": "@@ -400,7 +400,9 @@ assign_stack_local_1 (machine_mode mode, poly_int64 size,\n     {\n       /* If the required alignment exceeds MAX_SUPPORTED_STACK_ALIGNMENT and\n \t it is not OK to reduce it.  Align the slot dynamically.  */\n-      if (mode == BLKmode && (kind & ASLK_REDUCE_ALIGN) == 0)\n+      if (mode == BLKmode\n+\t  && (kind & ASLK_REDUCE_ALIGN) == 0\n+\t  && currently_expanding_to_rtl)\n \tdynamic_align_addr = true;\n       else\n \t{"}, {"sha": "658f396c26f2a6a57ef62b1ccdd0e28b79dedbd1", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a811a0a8815a893b0fac82e79ea251bb1554f720/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a811a0a8815a893b0fac82e79ea251bb1554f720/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=a811a0a8815a893b0fac82e79ea251bb1554f720", "patch": "@@ -1,3 +1,8 @@\n+2019-01-09  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR rtl-optimization/88331\n+\t* gcc.target/i386/pr88331.c: New test.\n+\n 2019-01-08  Paolo Carlini  <paolo.carlini@oracle.com>\n \n \t* g++.dg/diagnostic/constexpr2.C: New."}, {"sha": "dbc63774bb74f39ab40d4c1f89847f17fcb8ff1e", "filename": "gcc/testsuite/gcc.target/i386/pr88331.c", "status": "added", "additions": 30, "deletions": 0, "changes": 30, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a811a0a8815a893b0fac82e79ea251bb1554f720/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr88331.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a811a0a8815a893b0fac82e79ea251bb1554f720/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr88331.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr88331.c?ref=a811a0a8815a893b0fac82e79ea251bb1554f720", "patch": "@@ -0,0 +1,30 @@\n+/* PR rtl-optimization/88331 */\n+/* { dg-do compile } */\n+/* { dg-options \"-O3 -march=core-avx2\" } */\n+\n+int b, d, e, g, i, j, l, m;\n+int *c, *h, *n, *o;\n+long f, k;\n+\n+void\n+foo (void)\n+{\n+  long p = i;\n+  int *a = o;\n+  while (p)\n+    {\n+      n = (int *) (__UINTPTR_TYPE__) a[0];\n+      for (; f; f += 4)\n+\tfor (; m <= d;)\n+\t  {\n+\t    for (; g <= e; ++g)\n+\t      l = (int) (__UINTPTR_TYPE__) (n + l);\n+\t    c[m] = (int) (__UINTPTR_TYPE__) n;\n+\t  }\n+    }\n+  int q = 0;\n+  k = 0;\n+  for (; k < j; k++)\n+    q += o[k] * h[k];\n+  b = q;\n+}"}]}