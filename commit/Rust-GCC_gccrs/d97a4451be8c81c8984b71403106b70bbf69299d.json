nts/gcc%2Ftestsuite%2FChangeLog?ref=d97a4451be8c81c8984b71403106b70bbf69299d", "patch": "@@ -1,3 +1,8 @@\n+2008-05-10  Francois-Xavier Coudert  <fxcoudert@gcc.gnu.org>\n+\n+\t* gfortran.dg/fseek.f90: Take care of Windows CRLF line\n+\tterminator.\n+\n 2008-05-10  Francois-Xavier Coudert  <fxcoudert@gcc.gnu.org>\n \n \t* gfortran.dg/fmt_t_5.f90: Take care of Windows CRLF line"}, {"sha": "2bf2e0dc8dd6e1d5f86794203fb380473527fe08", "filename": "gcc/testsuite/gfortran.dg/fseek.f90", "status": "modified", "additions": 10, "deletions": 2, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d97a4451be8c81c8984b71403106b70bbf69299d/gcc%2Ftestsuite%2Fgfortran.dg%2Ffseek.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d97a4451be8c81c8984b71403106b70bbf69299d/gcc%2Ftestsuite%2Fgfortran.dg%2Ffseek.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Ffseek.f90?ref=d97a4451be8c81c8984b71403106b70bbf69299d", "patch": "@@ -3,10 +3,18 @@\n PROGRAM test_fseek\n   INTEGER, PARAMETER :: SEEK_SET = 0, SEEK_CUR = 1, SEEK_END = 2, fd=10\n   INTEGER :: ierr = 0\n+  INTEGER :: newline_length\n \n-  ! expected position: 12, one leading blank + 10 + newline\n+  ! We first need to determine if a newline is one or two characters\n+  open (911,status=\"scratch\")\n+  write(911,\"()\")\n+  newline_length = ftell(911)\n+  close (911)\n+  if (newline_length < 1 .or. newline_length > 2) call abort()\n+\n+  ! expected position: one leading blank + 10 + newline\n   WRITE(fd, *) \"1234567890\"\n-  IF (FTELL(fd) /= 12) CALL abort()\n+  IF (FTELL(fd) /= 11 + newline_length) CALL abort()\n \n   ! move backward from current position\n   CALL FSEEK(fd, -12, SEEK_CUR, ierr)"}]}