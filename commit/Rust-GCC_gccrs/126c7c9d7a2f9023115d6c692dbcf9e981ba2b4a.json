w_url": "https://github.com/Rust-GCC/gccrs/raw/126c7c9d7a2f9023115d6c692dbcf9e981ba2b4a/gcc%2Frust%2Ftypecheck%2Frust-tyty.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Frust%2Ftypecheck%2Frust-tyty.h?ref=126c7c9d7a2f9023115d6c692dbcf9e981ba2b4a", "patch": "@@ -1057,6 +1057,8 @@ class TypeBoundPredicate : public SubstitutionRef\n \n   bool requires_generic_args () const;\n \n+  bool contains_associated_types () const;\n+\n private:\n   DefId reference;\n   Location locus;"}]}