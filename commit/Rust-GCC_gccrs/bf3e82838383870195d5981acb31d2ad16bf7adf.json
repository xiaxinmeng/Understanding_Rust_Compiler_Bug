      if (flags & TFF_DECL_SPECIFIERS)\n+        pp_cxx_declaration (cxx_pp, t);\n+      else\n+        pp_type_id (cxx_pp, t);\n+      break;\n+\n     default:\n       pp_unsupported_tree (cxx_pp, t);\n       /* Fall through to error.  */"}, {"sha": "5a1fcd97821563e9e675cf1f354e09152f59945c", "filename": "gcc/testsuite/g++.dg/other/nontype-1.C", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/bf3e82838383870195d5981acb31d2ad16bf7adf/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fother%2Fnontype-1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/bf3e82838383870195d5981acb31d2ad16bf7adf/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fother%2Fnontype-1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fother%2Fnontype-1.C?ref=bf3e82838383870195d5981acb31d2ad16bf7adf", "patch": "@@ -0,0 +1,7 @@\n+template <class Op>\n+bool asfun(Op f,\n+           Op::first_argument_type a, // { dg-error \"not a type\" }\n+           Op::second_argument_type b) // { dg-error \"not a type\" }\n+{                               // { dg-error \"no type\" }\n+   return Op(a, b);\n+}"}]}