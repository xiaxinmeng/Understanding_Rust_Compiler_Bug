/Rust-GCC/gccrs/raw/6f17d116b739645d9c331ffb4b741eebbfb69a5c/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftorture%2Fpr33589-2.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftorture%2Fpr33589-2.C?ref=6f17d116b739645d9c331ffb4b741eebbfb69a5c", "patch": "@@ -0,0 +1,13 @@\n+// { dg-do compile }\n+\n+void f(void*) throw();\n+\n+void somefunction()\n+{\n+try {\n+   void (*g)(void*) = (void (*)(void*))f;\n+   void (*g2)(int*) = (void (*)(int*))g;\n+    g2(0);\n+} catch (...)\n+{throw;}\n+}"}, {"sha": "fd22d327fe9c7380adff227e0e0625f84f880726", "filename": "gcc/tree-outof-ssa.c", "status": "modified", "additions": 6, "deletions": 1, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/6f17d116b739645d9c331ffb4b741eebbfb69a5c/gcc%2Ftree-outof-ssa.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/6f17d116b739645d9c331ffb4b741eebbfb69a5c/gcc%2Ftree-outof-ssa.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftree-outof-ssa.c?ref=6f17d116b739645d9c331ffb4b741eebbfb69a5c", "patch": "@@ -758,7 +758,12 @@ rewrite_trees (var_map map, tree *values)\n \t  if (remove)\n \t    bsi_remove (&si, true);\n \t  else\n-\t    bsi_next (&si);\n+\t    {\n+\t      if (changed)\n+\t\tif (maybe_clean_or_replace_eh_stmt (stmt, stmt))\n+\t\t  tree_purge_dead_eh_edges (bb);\n+\t      bsi_next (&si);\n+\t    }\n \t}\n \n       phi = phi_nodes (bb);"}]}