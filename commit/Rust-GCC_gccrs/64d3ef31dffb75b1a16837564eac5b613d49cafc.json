ttps://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr66680.f90?ref=64d3ef31dffb75b1a16837564eac5b613d49cafc", "patch": "@@ -0,0 +1,46 @@\n+! { dg-do run }\n+! PR 66680: ICE with openmp, a loop and a type bound procedure\n+! Contributed by Miha Polajnar <polajnar.miha@gmail.com>\n+!\n+module m1 \n+  implicit none\n+  integer :: n = 5\n+  type :: t1\n+  contains\n+    procedure :: s => s1\n+  end type t1\n+contains\n+  pure subroutine s1(self,p,esta)\n+    class(t1), intent(in) :: self\n+    integer, optional, intent(in) :: p\n+    integer, intent(out) :: esta\n+  end subroutine s1 \n+end module m1\n+module m2\n+  use m1, only: t1, n\n+  implicit none\n+  type(t1), allocatable :: test(:)\n+contains\n+  pure subroutine s2(test1,esta)\n+    type(t1), intent(in) :: test1\n+    integer, intent(out) :: esta\n+    integer :: p, i\n+    do p = 1, n\n+      i = p ! using i instead of p works\n+      call test1%s(p=p,esta=esta)\n+      if ( esta /= 0 ) return\n+    end do\n+  end subroutine s2\n+  subroutine s3()\n+    integer :: i, esta\n+    !$omp parallel do  &\n+    !$omp private(i)\n+    do i = 1, n\n+        call s2(test(i),esta)\n+    end do\n+    !$omp end parallel do\n+  end subroutine s3\n+end module m2\n+program main\n+  implicit none\n+end program main"}]}