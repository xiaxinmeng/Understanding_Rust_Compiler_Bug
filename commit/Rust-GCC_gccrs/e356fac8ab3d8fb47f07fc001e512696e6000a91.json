epos/Rust-GCC/gccrs/contents/gcc%2Fconfig%2Fi386%2Fi386.md?ref=e356fac8ab3d8fb47f07fc001e512696e6000a91", "patch": "@@ -5362,8 +5362,8 @@\n (define_split\n   [(set (match_operand:X87MODEF 0 \"register_operand\" \"\")\n \t(float:X87MODEF (match_operand:DI 1 \"register_operand\" \"\")))\n-   (clobber (match_operand:V4SI 3 \"register_operand\" \"\"))\n-   (clobber (match_operand:V4SI 4 \"register_operand\" \"\"))\n+   (clobber (match_scratch:V4SI 3 \"\"))\n+   (clobber (match_scratch:V4SI 4 \"\"))\n    (clobber (match_operand:DI 2 \"memory_operand\" \"\"))]\n   \"TARGET_80387 && TARGET_SSE2 && TARGET_INTER_UNIT_MOVES\n    && !TARGET_64BIT && !optimize_size\n@@ -5386,9 +5386,9 @@\n (define_split\n   [(set (match_operand:X87MODEF 0 \"register_operand\" \"\")\n \t(float:X87MODEF (match_operand:DI 1 \"memory_operand\" \"\")))\n-   (clobber (match_operand:V4SI 2 \"register_operand\" \"\"))\n-   (clobber (match_operand:V4SI 3 \"register_operand\" \"\"))\n-   (clobber (match_operand:DI 4 \"memory_operand\" \"\"))]\n+   (clobber (match_scratch:V4SI 3 \"\"))\n+   (clobber (match_scratch:V4SI 4 \"\"))\n+   (clobber (match_operand:DI 2 \"memory_operand\" \"\"))]\n   \"TARGET_80387 && TARGET_SSE2 && TARGET_INTER_UNIT_MOVES\n    && !TARGET_64BIT && !optimize_size\n    && reload_completed"}, {"sha": "1cb44d35ee144c34c8793c71141339b3c4389290", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e356fac8ab3d8fb47f07fc001e512696e6000a91/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e356fac8ab3d8fb47f07fc001e512696e6000a91/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=e356fac8ab3d8fb47f07fc001e512696e6000a91", "patch": "@@ -1,3 +1,8 @@\n+2008-04-28  Uros Bizjak  <ubizjak@gmail.com>\n+\n+\tPR target/36064\n+\t* gcc.target/i386/pr36064.c: New test.\n+\n 2008-04-28  Uros Bizjak  <ubizjak@gmail.com>\n \n \tPR testsuite/36056"}, {"sha": "7964f280cffb31a34813040f48534de7169e3869", "filename": "gcc/testsuite/gcc.target/i386/pr36064.c", "status": "added", "additions": 55, "deletions": 0, "changes": 55, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e356fac8ab3d8fb47f07fc001e512696e6000a91/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr36064.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e356fac8ab3d8fb47f07fc001e512696e6000a91/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr36064.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr36064.c?ref=e356fac8ab3d8fb47f07fc001e512696e6000a91", "patch": "@@ -0,0 +1,55 @@\n+/* { dg-do compile } */\n+/* { dg-require-effective-target ilp32 } */\n+/* { dg-options \"-O1 -march=core2\" } */\n+\n+typedef long long ogg_int64_t;\n+\n+typedef struct vorbis_info\n+{\n+  long rate;\n+} vorbis_info;\n+\n+typedef struct OggVorbis_File\n+{\n+  int seekable;\n+  int links;\n+  ogg_int64_t *pcmlengths;\n+  vorbis_info *vi;\n+  int ready_state;\n+} OggVorbis_File;\n+\n+extern double ov_time_total (OggVorbis_File * vf, int i);\n+extern int ov_pcm_seek_page (OggVorbis_File * vf, ogg_int64_t pos);\n+\n+int\n+ov_time_seek_page (OggVorbis_File * vf, double seconds)\n+{\n+  int link = -1;\n+  ogg_int64_t pcm_total = 0;\n+  double time_total = 0.;\n+\n+  if (vf->ready_state < 2)\n+    return (-131);\n+  if (!vf->seekable)\n+    return (-138);\n+  if (seconds < 0)\n+    return (-131);\n+\n+  for (link = 0; link < vf->links; link++)\n+    {\n+      double addsec = ov_time_total (vf, link);\n+      if (seconds < time_total + addsec)\n+\tbreak;\n+      time_total += addsec;\n+      pcm_total += vf->pcmlengths[link * 2 + 1];\n+    }\n+\n+  if (link == vf->links)\n+    return (-131);\n+\n+  {\n+    ogg_int64_t target =\n+      pcm_total + (seconds - time_total) * vf->vi[link].rate;\n+    return (ov_pcm_seek_page (vf, target));\n+  }\n+}"}]}