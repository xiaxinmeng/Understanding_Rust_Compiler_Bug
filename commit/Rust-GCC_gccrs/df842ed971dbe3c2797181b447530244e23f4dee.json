 PR44371 STOP parsing rejects valid code.\n+  real, dimension(5,5,5) :: i\n+  character(1) c, y\n+  y = 'y'\n+  read(y,*) c\n+  if (c=='x') stop size(i); if (c=='X') stop\n+\n+  if (c=='y') stop size(i) if (c=='Y') stop ! { dg-error \"Syntax error in STOP\" }\n+  end"}]}