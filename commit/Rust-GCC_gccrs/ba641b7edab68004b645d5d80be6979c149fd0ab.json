__SIZEOF_INT__ < __SIZEOF_FLOAT__) \\\n+  && (__SIZEOF_LONG__ == __SIZEOF_FLOAT__)\n+#define int long\n+#endif\n+\n int l;\n void bar (void);\n \n@@ -10,7 +15,11 @@ foo (int *x, float y)\n {\n   float b;\n   union { float f; int i; } u = { .f = y };\n+#if  (__SIZEOF_INT__ < __SIZEOF_FLOAT__)\n+  u.i += 127L << 23;\n+#else\n   u.i += 127 << 23;\n+#endif\n   u.f = ((-1.0f / 3) * u.f + 2) * u.f - 2.0f / 3;\n   b = 0.5 * (u.f + l);\n   if (b >= *x)"}]}