765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Fdeep_old.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fdeep_old.adb?ref=aa94ab941251a643ab8e4a765b721acebecadd44", "patch": "@@ -1,3 +1,5 @@\n+--  { dg-options \"-gnatws\" }\n+\n procedure Deep_Old (X : Integer) is\n begin\n    begin"}, {"sha": "91e02e79383b9ae7a6768b4542551cc8a4771c84", "filename": "gcc/testsuite/gnat.dg/fixce.adb", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffixce.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffixce.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ffixce.adb?ref=aa94ab941251a643ab8e4a765b721acebecadd44", "patch": "@@ -0,0 +1,13 @@\n+--  { dg-do run }\n+\n+procedure fixce is\n+   type D is delta 128.0 / (2 ** 15) range 0.0 .. 256.0;\n+   type R is range 0 .. 200;\n+   dd : D;\n+   RA : constant array (1 .. 3) of R := (127, 128, 200);\n+begin\n+   dd := D (RA (2));\n+   for i in RA'range loop\n+      dd := D (RA (i));\n+   end loop;\n+end fixce;"}, {"sha": "a57e87227e08a45e6f08f18ff4f0f38f970bbd51", "filename": "gcc/testsuite/gnat.dg/frunaligned.adb", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned.adb?ref=aa94ab941251a643ab8e4a765b721acebecadd44", "patch": "@@ -0,0 +1,8 @@\n+--  { dg-do compile }\n+--  { dg-options \"-gnatws\" }\n+with FRUnaligned1; use FRUnaligned1;\n+function FRUnaligned return r is\n+   ss : s;\n+begin\n+   return ss.y;\n+end;"}, {"sha": "4a8db361b484a094c751ffdbd5ae240ed8ed6049", "filename": "gcc/testsuite/gnat.dg/frunaligned1.ads", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned1.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned1.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ffrunaligned1.ads?ref=aa94ab941251a643ab8e4a765b721acebecadd44", "patch": "@@ -0,0 +1,12 @@\n+package FRUnaligned1 is\n+   type r is array (1 .. 72) of Boolean;\n+   pragma Pack (r);\n+   type s is record\n+      x : Boolean;\n+      y : r;\n+   end record;\n+   for s use record\n+      x at 0 range 0 .. 0;\n+      y at 0 range 1 .. 72;\n+   end record;\n+end FRUnaligned1;"}, {"sha": "a5e4d42177f267c92f0af20df8432d8e064fc9c1", "filename": "gcc/testsuite/gnat.dg/old_errors.adb", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Fold_errors.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/aa94ab941251a643ab8e4a765b721acebecadd44/gcc%2Ftestsuite%2Fgnat.dg%2Fold_errors.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fold_errors.adb?ref=aa94ab941251a643ab8e4a765b721acebecadd44", "patch": "@@ -37,7 +37,7 @@ package body Old_Errors is\n       begin\n          Y := Z'Old; -- { dg-error \"cannot refer to local variable\" }\n       end;\n-      Y := I'Old;   -- OK\n+      Y := I'Old;   -- { dg-warning \"Old applied to constant has no effect\" }\n       Y := O'Old;   -- OK\n       Y := IO'Old;  -- OK\n       Y := G;       -- OK, error has been signalled at G declaration"}]}