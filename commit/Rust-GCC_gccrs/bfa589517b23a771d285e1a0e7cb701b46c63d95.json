STRUCTOR_P (cand->fn))\n+    convtype = non_reference (TREE_TYPE (TREE_TYPE (cand->fn)));\n+  else if (cand->second_conv->kind == ck_rvalue)\n+    /* DR 5: [in the first step of copy-initialization]...if the function\n+       is a constructor, the call initializes a temporary of the\n+       cv-unqualified version of the destination type. */\n+    convtype = cv_unqualified (totype);\n+  else\n+    convtype = totype;\n   /* Build the user conversion sequence.  */\n   conv = build_conv\n     (ck_user,\n-     (DECL_CONSTRUCTOR_P (cand->fn)\n-      ? totype : non_reference (TREE_TYPE (TREE_TYPE (cand->fn)))),\n+     convtype,\n      build_identity_conv (TREE_TYPE (expr), expr));\n   conv->cand = cand;\n   if (cand->viable == -1)"}, {"sha": "655fae21e66e876767275e45346492b0893fd195", "filename": "gcc/testsuite/g++.dg/init/copy7.C", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/bfa589517b23a771d285e1a0e7cb701b46c63d95/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finit%2Fcopy7.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/bfa589517b23a771d285e1a0e7cb701b46c63d95/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finit%2Fcopy7.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finit%2Fcopy7.C?ref=bfa589517b23a771d285e1a0e7cb701b46c63d95", "patch": "@@ -0,0 +1,9 @@\n+// CWG 5\n+\n+struct C { };\n+C c;\n+struct A {\n+  A(const A&);\n+  A(const C&);\n+};\n+const volatile A a = c;    // Okay"}]}