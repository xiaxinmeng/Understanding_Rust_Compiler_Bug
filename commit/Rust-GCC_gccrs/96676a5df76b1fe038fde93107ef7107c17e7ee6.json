%2Fpostreload.c?ref=96676a5df76b1fe038fde93107ef7107c17e7ee6", "patch": "@@ -1415,7 +1415,8 @@ reload_combine_note_store (rtx dst, const_rtx set, void *data ATTRIBUTE_UNUSED)\n     {\n       dst = XEXP (dst, 0);\n       if (GET_CODE (dst) == PRE_INC || GET_CODE (dst) == POST_INC\n-\t  || GET_CODE (dst) == PRE_DEC || GET_CODE (dst) == POST_DEC)\n+\t  || GET_CODE (dst) == PRE_DEC || GET_CODE (dst) == POST_DEC\n+\t  || GET_CODE (dst) == PRE_MODIFY || GET_CODE (dst) == POST_MODIFY)\n \t{\n \t  regno = REGNO (XEXP (dst, 0));\n \t  mode = GET_MODE (XEXP (dst, 0));"}, {"sha": "3a66f1c15d3991d8012ffac42e6a32eabc813317", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/96676a5df76b1fe038fde93107ef7107c17e7ee6/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/96676a5df76b1fe038fde93107ef7107c17e7ee6/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=96676a5df76b1fe038fde93107ef7107c17e7ee6", "patch": "@@ -1,3 +1,8 @@\n+2010-12-21  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR rtl-optimization/47008\n+\t* gfortran.dg/pr47008.f03: New test.\n+\n 2010-12-21  Nicola Pero  <nicola.pero@meta-innovation.com>\n \n \tPR libobjc/45953"}, {"sha": "a3e1e1dae0b5544240bc5a492c4dae74f665c06b", "filename": "gcc/testsuite/gfortran.dg/pr47008.f03", "status": "added", "additions": 24, "deletions": 0, "changes": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/96676a5df76b1fe038fde93107ef7107c17e7ee6/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr47008.f03", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/96676a5df76b1fe038fde93107ef7107c17e7ee6/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr47008.f03", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr47008.f03?ref=96676a5df76b1fe038fde93107ef7107c17e7ee6", "patch": "@@ -0,0 +1,24 @@\n+! PR rtl-optimization/47008\n+! { dg-do run }\n+! { dg-options \"-Os -fno-asynchronous-unwind-tables -fschedule-insns -fsched-pressure -fno-inline\" { target i?86-*-* x86_64-*-* } }\n+\n+program main\n+  type :: t\n+    integer :: i\n+    character(24) :: c\n+    type (t), pointer :: p\n+  end type t\n+  type(t), pointer :: r, p\n+  allocate (p)\n+  p = t (123455, \"\", p)\n+  r => entry (\"\", 123456, 1, \"\", 99, \"\", p)\n+  if (p%i /= 123455) call abort\n+contains\n+  function entry (x, i, j, c, k, d, p) result (q)\n+    integer :: i, j, k\n+    character (*) :: x, c, d\n+    type (t), pointer :: p, q\n+    allocate (q)\n+    q = t (i, c, p)\n+  end function\n+end program main"}]}