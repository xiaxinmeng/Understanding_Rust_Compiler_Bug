e_argument (tree type, tree expr)\n     }\n   else \n     {\n-      error (\"object `%E' cannot be used as template argument\", expr);\n+      if (TYPE_P (expr))\n+        error (\"type '%T' cannot be used as a value for a non-type \"\n+               \"template-parameter\", expr);\n+      else if (DECL_P (expr))\n+        error (\"invalid use of '%D' as a non-type template-argument\", expr);\n+      else\n+        error (\"invalid use of '%E' as a non-type template-argument\", expr);\n+\n       return NULL_TREE;\n     }\n "}, {"sha": "fbe5f3f456bf7e633861c62588e4cbdbd4ff131e", "filename": "gcc/testsuite/g++.dg/template/non-type-template-argument-1.C", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c61dce3a814a535c2d8c0ce1b8d7dd7e476116f8/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fnon-type-template-argument-1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c61dce3a814a535c2d8c0ce1b8d7dd7e476116f8/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fnon-type-template-argument-1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fnon-type-template-argument-1.C?ref=c61dce3a814a535c2d8c0ce1b8d7dd7e476116f8", "patch": "@@ -0,0 +1,12 @@\n+struct A { static const bool b=false; };\n+\n+struct B { typedef A X; };\n+\n+template <bool> struct C {};\n+\n+template <typename T> struct D\n+{\n+   C<T::X> c;                   // { dg-error \"invalid use\" }\n+};\n+\n+D<B> d;                         // { dg-error \"\" }"}]}