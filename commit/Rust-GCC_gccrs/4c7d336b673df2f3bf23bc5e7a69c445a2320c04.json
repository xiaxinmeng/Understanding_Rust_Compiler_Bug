target%2Friscv%2Fpr107786-2.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Friscv%2Fpr107786-2.c?ref=4c7d336b673df2f3bf23bc5e7a69c445a2320c04", "patch": "@@ -0,0 +1,17 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-march=rv64gc -mabi=lp64\" } */\n+/* { dg-skip-if \"\" { *-*-* } { \"-O0\" \"-Og\" \"-O1\" } } */\n+\n+int c;\n+\n+int main() {\n+  for (;;) {\n+    short h = c * 100;\n+    if (h & 0x7ff0)\n+      break;\n+  }\n+}\n+\n+/* { dg-final { scan-assembler-times \"andi\\t\" 1 } } */\n+/* { dg-final { scan-assembler-times \"srli\\t\" 1 } } */\n+"}, {"sha": "5246ec7d33828e405ebffb3dc9cd8559fbd6c460", "filename": "gcc/testsuite/gcc.target/riscv/pr107786.c", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4c7d336b673df2f3bf23bc5e7a69c445a2320c04/gcc%2Ftestsuite%2Fgcc.target%2Friscv%2Fpr107786.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4c7d336b673df2f3bf23bc5e7a69c445a2320c04/gcc%2Ftestsuite%2Fgcc.target%2Friscv%2Fpr107786.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Friscv%2Fpr107786.c?ref=4c7d336b673df2f3bf23bc5e7a69c445a2320c04", "patch": "@@ -0,0 +1,17 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-march=rv64gc -mabi=lp64\" } */\n+/* { dg-skip-if \"\" { *-*-* } { \"-O0\" \"-Og\" \"-O1\" } } */\n+\n+int c;\n+\n+int main() {\n+  for (;;) {\n+    char h = c * 100;\n+    if (h)\n+      break;\n+  }\n+}\n+\n+/* { dg-final { scan-assembler-times \"andi\\t\" 1 } } */\n+/* { dg-final { scan-assembler-not \"srli\\t\" } } */\n+"}]}