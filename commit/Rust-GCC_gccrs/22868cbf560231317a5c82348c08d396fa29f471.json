epos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fgcc-interface%2Fdecl.c?ref=22868cbf560231317a5c82348c08d396fa29f471", "patch": "@@ -548,6 +548,7 @@ gnat_to_gnu_entity (Entity_Id gnat_entity, tree gnu_expr, int definition)\n \tbool const_flag\n \t  = ((kind == E_Constant || kind == E_Variable)\n \t     && Is_True_Constant (gnat_entity)\n+\t     && !Treat_As_Volatile (gnat_entity)\n \t     && (((Nkind (Declaration_Node (gnat_entity))\n \t\t   == N_Object_Declaration)\n \t\t  && Present (Expression (Declaration_Node (gnat_entity))))"}, {"sha": "e9b7422141cf17050562143fbe71002fc19cff5b", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/22868cbf560231317a5c82348c08d396fa29f471/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/22868cbf560231317a5c82348c08d396fa29f471/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=22868cbf560231317a5c82348c08d396fa29f471", "patch": "@@ -1,3 +1,7 @@\n+2008-09-22  Olivier Hainque  <hainque@adacore.com>\n+\n+\t* gnat.dg/volatile3.adb: New test.\n+\n 2008-09-22  Ira Rosen  <irar@il.ibm.com>\n \n \tPR tree-optimization/37482"}, {"sha": "bd0e96a27c6e06f1f1a3f3ce98f100ac46cf7ef2", "filename": "gcc/testsuite/gnat.dg/volatile3.adb", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/22868cbf560231317a5c82348c08d396fa29f471/gcc%2Ftestsuite%2Fgnat.dg%2Fvolatile3.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/22868cbf560231317a5c82348c08d396fa29f471/gcc%2Ftestsuite%2Fgnat.dg%2Fvolatile3.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fvolatile3.adb?ref=22868cbf560231317a5c82348c08d396fa29f471", "patch": "@@ -0,0 +1,16 @@\n+-- { dg-do compile }\n+-- { dg-options \"-O2\" }\n+\n+procedure volatile3 is\n+\n+   v1 : Integer := 0;\n+   v2 : Integer := 0;\n+   pragma Volatile (v1);\n+   pragma Volatile (v2);\n+begin\n+   if v1 /= v2 then\n+      raise Program_Error;\n+   end if;\n+end;\n+\n+-- { dg-final { scan-assembler \"__gnat_rcheck\" } }"}]}