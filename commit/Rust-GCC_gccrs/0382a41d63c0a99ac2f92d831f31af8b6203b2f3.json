/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-86953.C?ref=0382a41d63c0a99ac2f92d831f31af8b6203b2f3", "patch": "@@ -0,0 +1,19 @@\n+// PR c++/86953\n+// { dg-do run { target c++11 } }\n+// { dg-options \"-O2\" }\n+\n+struct B {\n+  double x;\n+  bool y, z;\n+  constexpr bool operator== (const B& o) const noexcept\n+  {\n+    return x == o.x && y == o.y && z == o.z;\n+  }\n+  constexpr bool operator!= (const B& o) const noexcept { return !(*this == o); }\n+};\n+\n+int\n+main ()\n+{\n+  bool b = B{} == B{};\n+}"}]}