n \n   /* 14.3.2/5: The null pointer{,-to-member} conversion is applied\n      to a non-type argument of \"nullptr\".  */\n-  if (expr == nullptr_node && TYPE_PTR_OR_PTRMEM_P (type))\n+  if (NULLPTR_TYPE_P (expr_type) && TYPE_PTR_OR_PTRMEM_P (type))\n     expr = fold_simple (convert (type, expr));\n \n   /* In C++11, integral or enumeration non-type template arguments can be"}, {"sha": "82d75ed28163a3bc7690c743b22fe219aba0d35c", "filename": "gcc/testsuite/g++.dg/cpp0x/nullptr38.C", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c5d6c6d94e74dd2f72e323ff01ad2506115ffe05/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr38.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c5d6c6d94e74dd2f72e323ff01ad2506115ffe05/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr38.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr38.C?ref=c5d6c6d94e74dd2f72e323ff01ad2506115ffe05", "patch": "@@ -0,0 +1,9 @@\n+// { dg-do compile { target c++11 } }\n+\n+using nullptr_t = decltype(nullptr);\n+\n+constexpr nullptr_t n = nullptr;\n+\n+template <void *> struct A { };\n+\n+A<n> a;"}]}