om/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-non-local.C?ref=774fb6c4eb205eaf9d3b6667e7de9c90cc1784ad", "patch": "@@ -0,0 +1,10 @@\n+// PR c++/86499\n+// { dg-do compile { target c++11 } }\n+\n+auto l1 = [=]{}; // { dg-error \"non-local lambda expression cannot have a capture-default\" }\n+auto l2 = [&]{}; // { dg-error \"non-local lambda expression cannot have a capture-default\" }\n+\n+namespace {\n+  auto l3 = [=]{}; // { dg-error \"non-local lambda expression cannot have a capture-default\" }\n+  auto l4 = [&]{}; // { dg-error \"non-local lambda expression cannot have a capture-default\" }\n+}"}, {"sha": "e8e94771ef0615ed2b9bfe8a2919564ca0516544", "filename": "gcc/testsuite/g++.dg/cpp0x/lambda/lambda-this10.C", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/774fb6c4eb205eaf9d3b6667e7de9c90cc1784ad/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this10.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/774fb6c4eb205eaf9d3b6667e7de9c90cc1784ad/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this10.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this10.C?ref=774fb6c4eb205eaf9d3b6667e7de9c90cc1784ad", "patch": "@@ -1,4 +1,4 @@\n // PR c++/54383\n // { dg-do compile { target c++11 } }\n \n-auto foo = [&](int a) { return a > this->b; }; // { dg-error \"this\" }\n+auto foo = [&](int a) { return a > this->b; }; // { dg-error \"non-local|this\" }"}]}