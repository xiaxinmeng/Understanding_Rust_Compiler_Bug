      tree decl = create_temporary_var (type);\n       DECL_INITIAL (decl) = compound_literal;\n       TREE_STATIC (decl) = 1;\n+      cp_apply_type_quals_to_decl (cp_type_quals (type), decl);\n       decl = pushdecl_top_level (decl);\n       DECL_NAME (decl) = make_anon_name ();\n       SET_DECL_ASSEMBLER_NAME (decl, DECL_NAME (decl));"}, {"sha": "891e33d5e5a78efe6155713bdf92036f14f4ba26", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1acb8caa791e6bddc4ba4ab531aedad64803323a/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1acb8caa791e6bddc4ba4ab531aedad64803323a/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=1acb8caa791e6bddc4ba4ab531aedad64803323a", "patch": "@@ -1,3 +1,8 @@\n+2010-02-14  Jason Merrill  <jason@redhat.com>\n+\n+\tPR c++/41997\n+\t* g++.dg/cpp0x/initlist-opt.C: New.\n+\n 2010-02-13  Paul Thomas  <pault@gcc.gnu.org>\n \n \tPR fortran/41113"}, {"sha": "f515ed89d986d7e4ac1add0f42a93b3d97fe9449", "filename": "gcc/testsuite/g++.dg/cpp0x/initlist-opt.C", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1acb8caa791e6bddc4ba4ab531aedad64803323a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist-opt.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1acb8caa791e6bddc4ba4ab531aedad64803323a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist-opt.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist-opt.C?ref=1acb8caa791e6bddc4ba4ab531aedad64803323a", "patch": "@@ -0,0 +1,19 @@\n+// PR c++/41997\n+// { dg-options \"-std=c++0x -O2 -fdump-tree-optimized\" }\n+// { dg-final { scan-tree-dump-not \"_0\" \"optimized\" } }\n+// { dg-final { cleanup-tree-dump \"optimized\" } }\n+\n+#include <initializer_list>\n+\n+int max_val(std::initializer_list<int> il)\n+{\n+        int i = *(il.begin());\n+        int j = *(il.begin() + 1);\n+        return (i > j ? i : j);\n+}\n+\n+int main(void)\n+{\n+        return max_val({1,2});\n+}\n+"}]}