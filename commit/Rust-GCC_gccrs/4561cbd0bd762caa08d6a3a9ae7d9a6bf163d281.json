%2Ftestsuite%2FChangeLog?ref=4561cbd0bd762caa08d6a3a9ae7d9a6bf163d281", "patch": "@@ -1,3 +1,8 @@\n+2009-08-11  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/41022\n+\t* gfortran.dg/proc_ptr_comp_14.f90: New.\n+\n 2009-08-11  Adam Nemet  <anemet@caviumnetworks.com>\n \n \t* gcc.target/mips/ext-5.c: New test."}, {"sha": "811223ee2d116b7bfdaaf70f377b36dc8bfcbc74", "filename": "gcc/testsuite/gfortran.dg/proc_ptr_comp_14.f90", "status": "added", "additions": 40, "deletions": 0, "changes": 40, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4561cbd0bd762caa08d6a3a9ae7d9a6bf163d281/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_14.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4561cbd0bd762caa08d6a3a9ae7d9a6bf163d281/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_14.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_14.f90?ref=4561cbd0bd762caa08d6a3a9ae7d9a6bf163d281", "patch": "@@ -0,0 +1,40 @@\n+! { dg-do run }\n+!\n+! PR 41022: [F03] procedure pointer components as actual arguments\n+!\n+! Contributed by Juergen Reuter <reuter@physik.uni-freiburg.de>\n+\n+program foo\n+\n+   type :: container_t\n+      procedure(proc), nopass, pointer :: proc => null ()\n+   end type container_t\n+\n+   type(container_t), target :: obj1\n+   type(container_t) :: obj2\n+\n+   obj1%proc => proc\n+   call transfer_proc_ptr (obj2, obj1)\n+\n+   if (obj2%proc()/=7) call abort()\n+\n+contains\n+\n+   subroutine transfer_proc_ptr (obj2, obj1)\n+     type(container_t), intent(out) :: obj2\n+     type(container_t), intent(in), target :: obj1\n+     call assign_proc_ptr (obj2%proc, obj1)\n+   end subroutine transfer_proc_ptr\n+\n+   subroutine assign_proc_ptr (ptr, obj1)\n+     procedure(proc), pointer :: ptr\n+     type(container_t), intent(in), target :: obj1\n+     ptr => obj1%proc\n+   end subroutine assign_proc_ptr\n+\n+   integer function proc ()\n+      proc = 7\n+   end function\n+\n+end program foo\n+"}]}