p (tree def, sese region)\n   scev = scalar_evolution_in_region (region, loop, def);\n \n   return !chrec_contains_undetermined (scev)\n-    && TREE_CODE (scev) != SSA_NAME\n+    && (TREE_CODE (scev) != SSA_NAME\n+\t|| !defined_in_sese_p (scev, region))\n     && (tree_does_not_contain_chrecs (scev)\n \t|| evolution_function_is_affine_p (scev));\n }"}, {"sha": "22b83bf675875fddde960fe190bff46a9860183e", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28", "patch": "@@ -1,3 +1,8 @@\n+2010-12-22  Sebastian Pop  <sebastian.pop@amd.com>\n+\n+\tPR tree-optimization/47019\n+\t* gfortran.dg/graphite/pr47019.f: New.\n+\n 2010-12-22  Jason Merrill  <jason@redhat.com>\n \n \t* g++.dg/init/volatile2.C: New."}, {"sha": "69067e9c500e86cc563845d921f6ed5763e590f6", "filename": "gcc/testsuite/gfortran.dg/graphite/pr47019.f", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28/gcc%2Ftestsuite%2Fgfortran.dg%2Fgraphite%2Fpr47019.f", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28/gcc%2Ftestsuite%2Fgfortran.dg%2Fgraphite%2Fpr47019.f", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgraphite%2Fpr47019.f?ref=9be8ba7e828bed6ebcbfdae4d2ca60333b8f6f28", "patch": "@@ -0,0 +1,12 @@\n+! { dg-options \"-O -ftree-pre -fgraphite-identity -fno-tree-copy-prop\" }\n+\n+      subroutine foo (ldmx,ldmy,v)\n+      integer :: ldmx, ldmy, v, l, m\n+      dimension v(5,ldmx,ldmy)\n+      do m = 5, 1, -1\n+        do l = m+1, 5\n+          v(m,3,2) = v(1,3,2)\n+        end do\n+        v(m,3,2) = m\n+      end do\n+      end"}]}