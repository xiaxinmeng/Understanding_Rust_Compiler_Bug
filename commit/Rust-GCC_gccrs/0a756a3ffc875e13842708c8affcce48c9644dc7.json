h": "@@ -11285,8 +11285,8 @@ warn_for_sign_compare (location_t location,\n               if ((mask & constant) != mask)\n \t\t{\n \t\t  if (constant == 0)\n-\t\t    warning (OPT_Wsign_compare,\n-\t\t\t     \"promoted ~unsigned is always non-zero\");\n+\t\t    warning_at (location, OPT_Wsign_compare,\n+\t\t\t\t\"promoted ~unsigned is always non-zero\");\n \t\t  else\n \t\t    warning_at (location, OPT_Wsign_compare,\n \t\t\t\t\"comparison of promoted ~unsigned with constant\");"}, {"sha": "ef4a72e68b2ffa06f5d43f470541ce7c70b9ae18", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0a756a3ffc875e13842708c8affcce48c9644dc7/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0a756a3ffc875e13842708c8affcce48c9644dc7/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=0a756a3ffc875e13842708c8affcce48c9644dc7", "patch": "@@ -1,3 +1,8 @@\n+2014-02-06  Marek Polacek  <polacek@redhat.com>\n+\n+\tPR c/60087\n+\t* gcc.dg/pr60087.c: New test.\n+\n 2014-02-06  Alan Modra  <amodra@gmail.com>\n \n \t* gcc.target/powerpc/pr60032.c: New."}, {"sha": "9cdd5897354e1a95ca6630d44355cb756a68f43d", "filename": "gcc/testsuite/gcc.dg/pr60087.c", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0a756a3ffc875e13842708c8affcce48c9644dc7/gcc%2Ftestsuite%2Fgcc.dg%2Fpr60087.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0a756a3ffc875e13842708c8affcce48c9644dc7/gcc%2Ftestsuite%2Fgcc.dg%2Fpr60087.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fpr60087.c?ref=0a756a3ffc875e13842708c8affcce48c9644dc7", "patch": "@@ -0,0 +1,14 @@\n+/* PR c/60087 */\n+/* { dg-do compile } */\n+/* { dg-options \"-Wsign-compare\" } */\n+\n+void\n+foo (unsigned int ui, int i)\n+{\n+  const unsigned char uc = 0;\n+  _Bool b;\n+  b = 0 != ~uc; /* { dg-warning \"9:promoted ~unsigned is always non-zero\" } */\n+  b = 2 != ~uc; /* { dg-warning \"9:comparison of promoted ~unsigned with constant\" } */\n+  b = uc == ~uc; /* { dg-warning \"10:comparison of promoted ~unsigned with unsigned\" } */\n+  b = i == ui; /* { dg-warning \"9:comparison between signed and unsigned integer expressions\" } */\n+}"}]}