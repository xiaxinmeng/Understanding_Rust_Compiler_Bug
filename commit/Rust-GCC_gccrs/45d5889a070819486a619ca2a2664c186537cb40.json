Ffortran%2Ftrans-stmt.c?ref=45d5889a070819486a619ca2a2664c186537cb40", "patch": "@@ -784,12 +784,12 @@ gfc_trans_sync (gfc_code *code, gfc_exec_op type)\n       else\n \t{\n \t  tree cond2;\n-\t  cond = fold_build2_loc (input_location, GE_EXPR, boolean_type_node,\n+\t  cond = fold_build2_loc (input_location, GT_EXPR, boolean_type_node,\n \t\t\t\t  images, gfort_gvar_caf_num_images);\n \t  cond2 = fold_build2_loc (input_location, LT_EXPR, boolean_type_node,\n \t\t\t\t   images,\n \t\t\t\t   build_int_cst (TREE_TYPE (images), 1));\n-\t  cond = fold_build2_loc (input_location, TRUTH_AND_EXPR,\n+\t  cond = fold_build2_loc (input_location, TRUTH_OR_EXPR,\n \t\t\t\t  boolean_type_node, cond, cond2);\n \t}\n       gfc_trans_runtime_check (true, false, cond, &se.pre,"}, {"sha": "d270ee0936ec77a3afefbd62896ec1ee04550f08", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/45d5889a070819486a619ca2a2664c186537cb40/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/45d5889a070819486a619ca2a2664c186537cb40/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=45d5889a070819486a619ca2a2664c186537cb40", "patch": "@@ -1,3 +1,8 @@\n+2012-11-28  Tobias Burnus  <burnus@net-b.de>\n+\n+\tPR fortran/52161\n+\t* coarray/sync_3.f90: Extend test.\n+\n 2012-11-29  Jakub Jelinek  <jakub@redhat.com>\n \n \tPR rtl-optimization/55512"}, {"sha": "205a787767807ca74a2e32e785f59e5971271182", "filename": "gcc/testsuite/gfortran.dg/coarray/sync_3.f90", "status": "modified", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/45d5889a070819486a619ca2a2664c186537cb40/gcc%2Ftestsuite%2Fgfortran.dg%2Fcoarray%2Fsync_3.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/45d5889a070819486a619ca2a2664c186537cb40/gcc%2Ftestsuite%2Fgfortran.dg%2Fcoarray%2Fsync_3.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fcoarray%2Fsync_3.f90?ref=45d5889a070819486a619ca2a2664c186537cb40", "patch": "@@ -1,5 +1,6 @@\n ! { dg-do run }\n ! { dg-options \"-fcheck=all\" }\n+! { dg-shouldfail \"Invalid image number -1 in SYNC IMAGES\" }\n !\n ! As sync_1, but with bounds checking enabled.\n ! PR fortran/52161\n@@ -65,4 +66,10 @@\n sync images (*,errmsg=str,stat=n)\n if (n /= 0) call abort()\n \n+n = -1\n+sync images ( num_images() )\n+sync images (n) ! Invalid: \"-1\"\n+\n end\n+\n+! { dg-output \"Fortran runtime error: Invalid image number -1 in SYNC IMAGES\" }"}]}