-GCC/gccrs/contents/gcc%2Fconfig%2Fia64%2Fia64.c?ref=dca1376799471fb6005a43f3a8836f45db5b78c9", "patch": "@@ -2039,8 +2039,13 @@ ia64_expand_atomic_op (enum rtx_code code, rtx mem, rtx val,\n   enum insn_code icode;\n \n   /* Special case for using fetchadd.  */\n-  if ((mode == SImode || mode == DImode) && fetchadd_operand (val, mode))\n+  if ((mode == SImode || mode == DImode)\n+      && (code == PLUS || code == MINUS)\n+      && fetchadd_operand (val, mode))\n     {\n+      if (code == MINUS)\n+\tval = GEN_INT (-INTVAL (val));\n+\n       if (!old_dst)\n         old_dst = gen_reg_rtx (mode);\n "}]}