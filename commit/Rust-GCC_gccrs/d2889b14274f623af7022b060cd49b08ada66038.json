e_specifier (cp_parser* parser,\n     {\n       /* Indicate whether this class was declared as a `class' or as a\n \t `struct'.  */\n-      if (TREE_CODE (type) == RECORD_TYPE)\n+      if (CLASS_TYPE_P (type))\n \tCLASSTYPE_DECLARED_CLASS (type) = (tag_type == class_type);\n       cp_parser_check_class_key (tag_type, type);\n     }"}, {"sha": "d0ac27d893c066b401e78bd6d0696ef8ff2b99f7", "filename": "gcc/testsuite/g++.dg/cpp0x/alias-decl-pmf1.C", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d2889b14274f623af7022b060cd49b08ada66038/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Falias-decl-pmf1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d2889b14274f623af7022b060cd49b08ada66038/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Falias-decl-pmf1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Falias-decl-pmf1.C?ref=d2889b14274f623af7022b060cd49b08ada66038", "patch": "@@ -0,0 +1,16 @@\n+// PR c++/67339\n+// { dg-do compile { target c++11 } }\n+\n+template < typename T>\n+struct A\n+{\n+    void foo();\n+    template < typename S, typename W >\n+        using N = void (T::*)(S, W) const ;\n+};\n+\n+template < typename T>\n+void A<T>::foo()\n+{\n+    typename A<T>::template N<int, int> fun = &T::out;\n+}"}]}