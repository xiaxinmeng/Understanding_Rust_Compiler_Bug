@@ -20602,16 +20602,20 @@ gen_decl_die (tree decl, tree origin, dw_die_ref context_die)\n       else if (debug_info_level > DINFO_LEVEL_TERSE)\n \t{\n \t  /* Before we describe the FUNCTION_DECL itself, make sure that we\n-\t     have described its return type.  */\n+\t     have its containing type.  */\n+\t  if (!origin)\n+\t    origin = decl_class_context (decl);\n+\t  if (origin != NULL_TREE)\n+\t    gen_type_die (origin, context_die);\n+\n+\t  /* And its return type.  */\n \t  gen_type_die (TREE_TYPE (TREE_TYPE (decl)), context_die);\n \n \t  /* And its virtual context.  */\n \t  if (DECL_VINDEX (decl) != NULL_TREE)\n \t    gen_type_die (DECL_CONTEXT (decl), context_die);\n \n-\t  /* And its containing type.  */\n-\t  if (!origin)\n-\t    origin = decl_class_context (decl);\n+\t  /* Make sure we have a member DIE for decl.  */\n \t  if (origin != NULL_TREE)\n \t    gen_type_die_for_member (origin, decl, context_die);\n "}, {"sha": "d9ebdd0751ea92fe6086f14a9db102b99cd4a915", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4a6e5cc885d6592194ff7e0c884d0bb802251c73/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4a6e5cc885d6592194ff7e0c884d0bb802251c73/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=4a6e5cc885d6592194ff7e0c884d0bb802251c73", "patch": "@@ -1,3 +1,8 @@\n+2010-09-14  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR debug/45660\n+\t* g++.dg/debug/pr45660.C: New test.\n+\n 2010-09-14  Ira Rosen  <irar@il.ibm.com>\n \n \tPR tree-optimization/45470"}, {"sha": "4f03fd5e7e3197c5a7b2d5047992c817186c7c57", "filename": "gcc/testsuite/g++.dg/debug/pr45660.C", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4a6e5cc885d6592194ff7e0c884d0bb802251c73/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fdebug%2Fpr45660.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4a6e5cc885d6592194ff7e0c884d0bb802251c73/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fdebug%2Fpr45660.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fdebug%2Fpr45660.C?ref=4a6e5cc885d6592194ff7e0c884d0bb802251c73", "patch": "@@ -0,0 +1,16 @@\n+// PR debug/45660\n+// { dg-do compile }\n+// { dg-options \"-g -fno-inline\" }\n+\n+void\n+test ()\n+{\n+  struct S\n+  {\n+    typedef void (**T) (void);\n+    static T i (void) { return 0; }\n+  };\n+  S s;\n+  if (s.i ())\n+    *s.i () = 0;\n+}"}]}