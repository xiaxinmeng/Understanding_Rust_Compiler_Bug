target sse_runtime } } */\n+/* { dg-options \"-O2 -mno-sse2 -msse\" } */\n+\n+typedef unsigned char v16qi __attribute__ ((vector_size (16)));\n+typedef unsigned int v4si __attribute__ ((vector_size (16)));\n+\n+char __attribute__ ((noinline, noclone))\n+test (v4si vec)\n+{\n+  vec[1] = 0x5fb856;\n+  return ((v16qi) vec)[0];\n+}\n+\n+int\n+main ()\n+{\n+  char z = test ((v4si) { -1, -1, -1, -1 });\n+\n+  if (z != -1)\n+    __builtin_abort ();\n+  return 0;\n+}"}]}