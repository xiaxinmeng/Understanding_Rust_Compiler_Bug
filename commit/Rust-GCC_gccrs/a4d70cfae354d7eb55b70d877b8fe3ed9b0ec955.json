C/gccrs/contents/gcc%2Fexpr.c?ref=a4d70cfae354d7eb55b70d877b8fe3ed9b0ec955", "patch": "@@ -3726,12 +3726,21 @@ compress_float_constant (rtx x, rtx y)\n \t into a new pseudo.  This constant may be used in different modes,\n \t and if not, combine will put things back together for us.  */\n       trunc_y = force_reg (srcmode, trunc_y);\n-      emit_unop_insn (ic, x, trunc_y, UNKNOWN);\n+\n+      /* If x is a hard register, perform the extension into a pseudo,\n+\t so that e.g. stack realignment code is aware of it.  */\n+      rtx target = x;\n+      if (REG_P (x) && HARD_REGISTER_P (x))\n+\ttarget = gen_reg_rtx (dstmode);\n+\n+      emit_unop_insn (ic, target, trunc_y, UNKNOWN);\n       last_insn = get_last_insn ();\n \n-      if (REG_P (x))\n+      if (REG_P (target))\n \tset_unique_reg_note (last_insn, REG_EQUAL, y);\n \n+      if (target != x)\n+\treturn emit_move_insn (x, target);\n       return last_insn;\n     }\n "}, {"sha": "881aa3df2a0c34e913c5677a151957161ccd9f7d", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a4d70cfae354d7eb55b70d877b8fe3ed9b0ec955/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a4d70cfae354d7eb55b70d877b8fe3ed9b0ec955/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=a4d70cfae354d7eb55b70d877b8fe3ed9b0ec955", "patch": "@@ -1,3 +1,8 @@\n+2014-02-13  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR target/43546\n+\t* gcc.target/i386/pr43546.c: New test.\n+\n 2014-02-13  Dominik Vogt  <vogt@linux.vnet.ibm.com>\n \n \t* gcc.target/s390/hotpatch-compile-8.c: New test."}, {"sha": "53cb3a07fdf06b556ee70212e05e941bb0385e48", "filename": "gcc/testsuite/gcc.target/i386/pr43546.c", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a4d70cfae354d7eb55b70d877b8fe3ed