Rust-GCC/gccrs/contents/gcc%2Ffold-const.c?ref=b77856544be1bccf962f5864fb6280b612667905", "patch": "@@ -8351,11 +8351,16 @@ fold_unary (enum tree_code code, tree type, tree op0)\n \t    mult_type = type;\n \t  else\n \t    mult_type = unsigned_type_for (type);\n-\t  \n-\t  tem = fold_build2 (MULT_EXPR, mult_type,\n-\t\t\t     fold_convert (mult_type, TREE_OPERAND (op0, 0)),\n-\t\t\t     fold_convert (mult_type, TREE_OPERAND (op0, 1)));\n-\t  return fold_convert (type, tem);\n+\n+\t  if (TYPE_PRECISION (mult_type) < TYPE_PRECISION (TREE_TYPE (op0)))\n+\t    {\n+\t      tem = fold_build2 (MULT_EXPR, mult_type,\n+\t\t\t\t fold_convert (mult_type,\n+\t\t\t\t\t       TREE_OPERAND (op0, 0)),\n+\t\t\t\t fold_convert (mult_type,\n+\t\t\t\t\t       TREE_OPERAND (op0, 1)));\n+\t      return fold_convert (type, tem);\n+\t    }\n \t}\n \n       tem = fold_convert_const (code, type, op0);"}, {"sha": "bf19792649c04a987d636d3c7fe05b29d245c665", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b77856544be1bccf962f5864fb6280b612667905/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b77856544be1bccf962f5864fb6280b612667905/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=b77856544be1bccf962f5864fb6280b612667905", "patch": "@@ -1,3 +1,8 @@\n+2008-12-06  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR middle-end/38422\n+\t* gcc.c-torture/execute/pr38422.c: New test.\n+\n 2008-12-06  Janus Weil  <janus@gcc.gnu.org>\n \n \tPR fortran/38415"}, {"sha": "e36879d7fe6d6141d2b54b9220d8d69910caa0c7", "filename": "gcc/testsuite/gcc.c-torture/execute/pr38422.c", "status": "added", "additions": 24, "deletions": 0, "changes": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b77856544be1bccf962f5864fb6280b612667905/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38422.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b77856544be1bccf962f5864fb6280b612667905/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38422.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38422.c?ref=b77856544be1bccf962f5864fb6280b612667905", "patch": "@@ -0,0 +1,24 @@\n+/* PR middle-end/38422 */\n+\n+extern void abort (void);\n+\n+struct S\n+{\n+  int s : (sizeof (int) * __CHAR_BIT__ - 2);\n+} s;\n+\n+void\n+foo (void)\n+{\n+  s.s *= 2;\n+}\n+\n+int\n+main ()\n+{\n+  s.s = 24;\n+  foo ();\n+  if (s.s != 48)\n+    abort ();\n+  return 0;\n+}"}]}