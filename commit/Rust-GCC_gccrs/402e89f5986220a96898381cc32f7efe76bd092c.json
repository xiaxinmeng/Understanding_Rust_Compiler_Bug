mpile }\n+-- { dg-options \"-g\" }\n+\n+procedure Debug5 is\n+\n+   type Record_Type (L1, L2 : Natural) is record\n+      S1 : String (1 .. L1);\n+      case L2 is\n+         when 0      => null;\n+         when others => S2 : String (L1 .. L2);\n+      end case;\n+   end record;\n+\n+   procedure Discard (R : Record_Type) is\n+   begin\n+      null;\n+   end Discard;\n+\n+   R : constant Record_Type := (0, 0, others => <>);\n+begin\n+   Discard (R);\n+end Debug5;"}]}