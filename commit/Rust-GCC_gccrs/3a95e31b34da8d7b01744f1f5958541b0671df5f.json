can-assembler {movl[ \\t][Ll]_xxx\\$non_lazy_ptr-L1\\$pb\\(%eax\\),[ \\t]%eax} { target { ia32 && *-*-darwin* } } } } */\n+/* Darwin m32 PIC requires the picbase adjustment.  */\n+/* { dg-final { scan-assembler {movl[ \\t]_xxx-L1\\$pb\\(%eax\\),[ \\t]%eax} { target { ia32 && *-*-darwin* } } } } */\n "}]}