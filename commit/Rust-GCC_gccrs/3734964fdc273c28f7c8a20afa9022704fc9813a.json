     if (!VOID_TYPE_P (TREE_TYPE (result)))\n+\tcomplete_type_or_else (TREE_TYPE (result), NULL_TREE);\n       bool aggr = aggregate_value_p (result, fco);\n #ifdef PCC_STATIC_STRUCT_RETURN\n       cfun->returns_pcc_struct = aggr;"}, {"sha": "d35f9edf2ae0f9c3f9811ea713a7bd1704dfa8eb", "filename": "gcc/testsuite/g++.dg/cpp0x/pr57958.C", "status": "added", "additions": 39, "deletions": 0, "changes": 39, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/3734964fdc273c28f7c8a20afa9022704fc9813a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fpr57958.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/3734964fdc273c28f7c8a20afa9022704fc9813a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fpr57958.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fpr57958.C?ref=3734964fdc273c28f7c8a20afa9022704fc9813a", "patch": "@@ -0,0 +1,39 @@\n+// { dg-do run { target c++11 } }\n+\n+#define assert(E) if(!(E))__builtin_abort();\n+\n+int n = 0;\n+\n+template <class T>\n+class Foo {\n+ public:\n+  Foo() { \n+   n--; \n+  }\n+  Foo(const Foo&) { \n+   n--; \n+  }\n+  ~Foo() { \n+   n++; \n+  }\n+};\n+\n+struct Data {};\n+\n+void a()\n+{\n+\tData b;\n+}\n+\n+int main(int argc, char *argv[]) {\n+  auto fn = [] (const Foo<Data>& x) {\n+    return (x);\n+  };\n+\n+  {\n+    Foo<Data> a;\n+    fn(a);\n+  }\n+\n+  assert(n == 0);\n+}"}]}