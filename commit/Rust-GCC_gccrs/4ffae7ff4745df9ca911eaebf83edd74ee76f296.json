detect_local_cpu (int argc, const char **argv)\n \tdefault:\n \t  if (arch)\n \t    {\n-\t      if (has_ssse3)\n-\t\t/* If it is an unknown CPU with SSSE3, assume Core 2.  */\n-\t\tcpu = \"core2\";\n+\t      /* This is unknown family 0x6 CPU.  */\n+\t      if (has_avx)\n+\t\t/* Assume Sandy Bridge.  */\n+\t\tcpu = \"corei7-avx\";\n+\t      else if (has_sse4_2)\n+\t\t/* Assume Core i7.  */\n+\t\tcpu = \"corei7\";\n+\t      else if (has_ssse3)\n+\t\t{\n+\t\t  if (has_movbe)\n+\t\t    /* Assume Atom.  */\n+\t\t    cpu = \"atom\";\n+\t\t  else\n+\t\t    /* Assume Core 2.  */\n+\t\t    cpu = \"core2\";\n+\t\t}\n \t      else if (has_sse3)\n \t\t/* It is Core Duo.  */\n \t\tcpu = \"pentium-m\";"}]}