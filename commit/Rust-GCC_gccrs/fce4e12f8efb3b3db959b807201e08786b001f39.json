ha": "f27d3f8c87ee5daa56cfff7b1fe28ff5a50ecb86", "filename": "libstdc++-v3/src/Makefile.am", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/fce4e12f8efb3b3db959b807201e08786b001f39/libstdc%2B%2B-v3%2Fsrc%2FMakefile.am", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/fce4e12f8efb3b3db959b807201e08786b001f39/libstdc%2B%2B-v3%2Fsrc%2FMakefile.am", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Fsrc%2FMakefile.am?ref=fce4e12f8efb3b3db959b807201e08786b001f39", "patch": "@@ -369,7 +369,7 @@ endif\n # Build a debug variant.\n # Take care to fix all possibly-relative paths.\n debugdir = ${glibcxx_builddir}/src/debug\n-stamp-debug:\n+stamp-debug: Makefile $(foreach dir,$(SUBDIRS),$(dir)/Makefile)\n \tif test ! -d ${debugdir} || test ! -f ${debugdir}/Makefile ; then \\\n \t  mkdir -p ${debugdir}; \\\n \t  for d in $(SUBDIRS); do mkdir -p  ${debugdir}/$$d; done; \\"}, {"sha": "05b25ed5e9db211c186d6d20d74af93916c874a7", "filename": "libstdc++-v3/src/Makefile.in", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/fce4e12f8efb3b3db959b807201e08786b001f39/libstdc%2B%2B-v3%2Fsrc%2FMakefile.in", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/fce4e12f8efb3b3db959b807201e08786b001f39/libstdc%2B%2B-v3%2Fsrc%2FMakefile.in", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Fsrc%2FMakefile.in?ref=fce4e12f8efb3b3db959b807201e08786b001f39", "patch": "@@ -1090,7 +1090,7 @@ libstdc++convenience.la: $(toolexeclib_LTLIBRARIES)\n \t  cp .libs/libstdc++convenience.a .libs/libstdc++.a; \\\n \tfi; \\\n \techo `date` > stamp-libstdc++convenience;\n-stamp-debug:\n+stamp-debug: Makefile $(foreach dir,$(SUBDIRS),$(dir)/Makefile)\n \tif test ! -d ${debugdir} || test ! -f ${debugdir}/Makefile ; then \\\n \t  mkdir -p ${debugdir}; \\\n \t  for d in $(SUBDIRS); do mkdir -p  ${debugdir}/$$d; done; \\"}]}