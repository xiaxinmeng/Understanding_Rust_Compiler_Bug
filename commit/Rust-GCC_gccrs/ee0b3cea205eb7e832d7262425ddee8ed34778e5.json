ust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=ee0b3cea205eb7e832d7262425ddee8ed34778e5", "patch": "@@ -1,3 +1,8 @@\n+2019-01-09  Thomas Koenig  <tkoenig@gcc.gnu.org>\n+\n+\tPR fortran/68426\n+\t* gfortran.dg/spread_simplify_1.f90: New test.\n+\n 2019-01-09  Uro\u0161 Bizjak  <ubizjak@gmail.com>\n \n \t* lib/target-supports.exp"}, {"sha": "afc254ac39aeeb72e252989a321729e383af6508", "filename": "gcc/testsuite/gfortran.dg/spread_simplify_1.f90", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ee0b3cea205eb7e832d7262425ddee8ed34778e5/gcc%2Ftestsuite%2Fgfortran.dg%2Fspread_simplify_1.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ee0b3cea205eb7e832d7262425ddee8ed34778e5/gcc%2Ftestsuite%2Fgfortran.dg%2Fspread_simplify_1.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fspread_simplify_1.f90?ref=ee0b3cea205eb7e832d7262425ddee8ed34778e5", "patch": "@@ -0,0 +1,16 @@\n+! { dg-do  run  }\n+! PR 68426 - simplification used to fail.\n+  module m\n+    implicit none\n+    type t\n+      integer :: i\n+    end type t\n+    type(t), dimension(2), parameter :: a1  = (/ t(1), t(2) /)\n+    type(t), dimension(1), parameter :: c = spread ( a1(1), 1, 1 )\n+  end module m\n+\n+\n+program main\n+  use m\n+  if (c(1)%i /= 1) stop 1\n+end program main"}]}