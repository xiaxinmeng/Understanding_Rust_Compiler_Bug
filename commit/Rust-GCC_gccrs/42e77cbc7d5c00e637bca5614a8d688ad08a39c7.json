nts/gcc%2Ftestsuite%2FChangeLog?ref=42e77cbc7d5c00e637bca5614a8d688ad08a39c7", "patch": "@@ -1,3 +1,7 @@\n+2016-06-13  Eric Botcazou  <ebotcazou@adacore.com>\n+\n+\t* gnat.dg/renaming10.ad[sb]: New test.\n+\n 2016-06-13  Paul Thomas  <pault@gcc.gnu.org>\n \n \tPR fortran/70673"}, {"sha": "07d4312b060e71d7f52e889683ec2b05fc4f7e9b", "filename": "gcc/testsuite/gnat.dg/renaming10.adb", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/42e77cbc7d5c00e637bca5614a8d688ad08a39c7/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/42e77cbc7d5c00e637bca5614a8d688ad08a39c7/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.adb?ref=42e77cbc7d5c00e637bca5614a8d688ad08a39c7", "patch": "@@ -0,0 +1,12 @@\n+-- { dg-do compile }\n+\n+package body Renaming10 is\n+\n+   function F (Input : Rec) return Natural is\n+      Position : Natural renames Input.Position;\n+      Index : Natural renames Natural'Succ(Position);\n+   begin\n+      return Index;\n+   end;\n+\n+end Renaming10;"}, {"sha": "aeb9fc1a201fceceea97bc8bddab5dd63e76bc19", "filename": "gcc/testsuite/gnat.dg/renaming10.ads", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/42e77cbc7d5c00e637bca5614a8d688ad08a39c7/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/42e77cbc7d5c00e637bca5614a8d688ad08a39c7/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Frenaming10.ads?ref=42e77cbc7d5c00e637bca5614a8d688ad08a39c7", "patch": "@@ -0,0 +1,9 @@\n+package Renaming10 is\n+\n+   type Rec is record\n+      Position : Natural;\n+   end record;\n+\n+   function F (Input : Rec) return Natural;\n+\n+end Renaming10;"}]}