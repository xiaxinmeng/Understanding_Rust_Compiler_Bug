print.cc", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f7e26913187ce0ed35e340c4fd14104bbcd1932e/gcc%2Fc-family%2Fc-pretty-print.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fc-family%2Fc-pretty-print.cc?ref=f7e26913187ce0ed35e340c4fd14104bbcd1932e", "patch": "@@ -32,6 +32,7 @@ along with GCC; see the file COPYING3.  If not see\n #include \"selftest.h\"\n #include \"langhooks.h\"\n #include \"options.h\"\n+#include \"internal-fn.h\"\n \n /* The pretty-printer code is primarily designed to closely follow\n    (GNU) C and C++ grammars.  That is to be contrasted with spaghetti\n@@ -1601,7 +1602,10 @@ c_pretty_printer::postfix_expression (tree e)\n       {\n \tcall_expr_arg_iterator iter;\n \ttree arg;\n-\tpostfix_expression (CALL_EXPR_FN (e));\n+\tif (CALL_EXPR_FN (e) != NULL_TREE)\n+\t  postfix_expression (CALL_EXPR_FN (e));\n+\telse\n+\t  pp_string (this, internal_fn_name (CALL_EXPR_IFN (e)));\n \tpp_c_left_paren (this);\n \tFOR_EACH_CALL_EXPR_ARG (arg, iter, e)\n \t  {"}, {"sha": "7fa3d84119753801afd800e267e2d96e77ca4add", "filename": "gcc/testsuite/c-c++-common/pr104505.c", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f7e26913187ce0ed35e340c4fd14104bbcd1932e/gcc%2Ftestsuite%2Fc-c%2B%2B-common%2Fpr104505.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f7e26913187ce0ed35e340c4fd14104bbcd1932e/gcc%2Ftestsuite%2Fc-c%2B%2B-common%2Fpr104505.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fc-c%2B%2B-common%2Fpr104505.c?ref=f7e26913187ce0ed35e340c4fd14104bbcd1932e", "patch": "@@ -0,0 +1,12 @@\n+/* { dg-do compile } */\n+\n+typedef char __attribute__((__vector_size__ (8))) U;\n+typedef short __attribute__((__vector_size__ (16))) V;\n+\n+U u;\n+\n+void\n+foo (V v)\n+{\n+  u = __builtin_shufflevector (u, u, __builtin_convertvector (v, U)); /* { dg-error \"invalid element index\" } */\n+}"}]}