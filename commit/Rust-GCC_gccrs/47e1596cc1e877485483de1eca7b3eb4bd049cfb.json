te%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=47e1596cc1e877485483de1eca7b3eb4bd049cfb", "patch": "@@ -1,3 +1,8 @@\n+2018-12-08  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR fortran/88304\n+\t* gfortran.fortran-torture/compile/pr88304-2.f90: New test.\n+\n 2018-12-07  Paolo Carlini  <paolo.carlini@oracle.com>\n \n \t* g++.dg/other/bitfield7.C: New."}, {"sha": "15bd451074d973ec6f85778dcf08475223baa061", "filename": "gcc/testsuite/gfortran.fortran-torture/compile/pr88304-2.f90", "status": "added", "additions": 28, "deletions": 0, "changes": 28, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/47e1596cc1e877485483de1eca7b3eb4bd049cfb/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fcompile%2Fpr88304-2.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/47e1596cc1e877485483de1eca7b3eb4bd049cfb/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fcompile%2Fpr88304-2.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fcompile%2Fpr88304-2.f90?ref=47e1596cc1e877485483de1eca7b3eb4bd049cfb", "patch": "@@ -0,0 +1,28 @@\n+! PR fortran/88304\n+\n+module pr88304\n+  implicit none\n+  integer :: p\n+contains\n+  function foo (x, y, z, w)\n+    integer, intent(in) :: x, y\n+    character(*), optional, intent(out) :: z\n+    integer, optional, intent(out) :: w\n+    integer :: foo\n+    foo = 1\n+  end function foo\n+  subroutine bar ()\n+    integer :: s\n+    s = baz (1)\n+  contains\n+    function baz (u)\n+      integer, intent(in) :: u\n+      integer :: baz\n+      integer :: q\n+      integer :: r (10)\n+      r = 0\n+      baz = 1\n+      q = foo (p, r(u), w = baz)\n+    end function baz\n+  end subroutine bar\n+end module pr88304"}, {"sha": "0ad469ada4982a4b88c3a770f9f29b743b78b52d", "filename": "gcc/tree-nested.c", "status": "modified", "additions": 2, "deletions": 1, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/47e1596cc1e877485483de1eca7b3eb4bd049cfb/gcc%2Ftree-nested.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/47e1596cc1e877485483de1eca7b3eb4bd049cfb/gcc%2Ftree-nested.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftree-nested.c?ref=47e1596cc1e877485483de1eca7b3eb4bd049cfb", "patch": "@@ -2324,7 +2324,8 @@ convert_local_reference_stmt (gimple_stmt_iterator *gsi, bool *handled_ops_p,\n       if (gimple_clobber_p (stmt))\n \t{\n \t  tree lhs = gimple_assign_lhs (stmt);\n-\t  if (!use_pointer_in_frame (lhs)\n+\t  if (DECL_P (lhs)\n+\t      && !use_pointer_in_frame (lhs)\n \t      && lookup_field_for_decl (info, lhs, NO_INSERT))\n \t    {\n \t      gsi_replace (gsi, gimple_build_nop (), true);"}]}