n+  implicit none\n+  integer :: i\n+  character(*), parameter :: s = 'abcdef'\n+  character(*), parameter :: t = transfer ([(s(i:i), i=1,len(s))], s)\n+  if (len (t) /= len (s) .or. t /= s) stop 1\n+end"}]}