ntents/gcc%2Fcp%2Fcall.c?ref=6bebae75035889a4844eb4d32a695bebf412bcd7", "patch": "@@ -5345,6 +5345,7 @@ build_conditional_expr_1 (location_t loc, tree arg1, tree arg2, tree arg3,\n   /* If the ARG2 and ARG3 are the same and don't have side-effects,\n      warn here, because the COND_EXPR will be turned into ARG2.  */\n   if (warn_duplicated_branches\n+      && (complain & tf_warning)\n       && (arg2 == arg3 || operand_equal_p (arg2, arg3, 0)))\n     warning_at (EXPR_LOCATION (result), OPT_Wduplicated_branches,\n \t\t\"this condition has identical branches\");"}, {"sha": "2deccd822c26e6c9ed811dedca59e8f16aecbd21", "filename": "gcc/cp/pt.c", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Fcp%2Fpt.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Fcp%2Fpt.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Fpt.c?ref=6bebae75035889a4844eb4d32a695bebf412bcd7", "patch": "@@ -17846,6 +17846,7 @@ tsubst_copy_and_build (tree t,\n \t    exp2 = RECUR (TREE_OPERAND (t, 2));\n \t  }\n \n+\twarning_sentinel s(warn_duplicated_branches);\n \tRETURN (build_x_conditional_expr (EXPR_LOCATION (t),\n \t\t\t\t\t cond, exp1, exp2, complain));\n       }"}, {"sha": "3936188e2f4945704396e05785f7143c0bb8fdee", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=6bebae75035889a4844eb4d32a695bebf412bcd7", "patch": "@@ -1,3 +1,8 @@\n+2018-01-10  Marek Polacek  <polacek@redhat.com>\n+\n+\tPR c++/82541\n+\t* g++.dg/warn/Wduplicated-branches4.C: New test.\n+\n 2018-01-10  Jakub Jelinek  <jakub@redhat.com>\n \n \tPR c++/81327"}, {"sha": "7963c9e8ab5091c23d6268b19609666a1416865e", "filename": "gcc/testsuite/g++.dg/warn/Wduplicated-branches4.C", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWduplicated-branches4.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/6bebae75035889a4844eb4d32a695bebf412bcd7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWduplicated-branches4.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWduplicated-branches4.C?ref=6bebae75035889a4844eb4d32a695bebf412bcd7", "patch": "@@ -0,0 +1,16 @@\n+// PR c++/82541\n+// { dg-do compile }\n+// { dg-options \"-Wduplicated-branches\" }\n+\n+template<int N>\n+struct AR\n+{\n+    char a1[N > 0 ? N : 1]; // { dg-bogus \"this condition has identical branches\" }\n+    char a2[N > 0 ? 1 : 1]; // { dg-warning \"this condition has identical branches\" }\n+};\n+\n+int\n+main ()\n+{\n+    AR<1> w;\n+}"}]}