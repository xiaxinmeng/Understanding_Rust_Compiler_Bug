g option files...\" >&2\n     field = 0\n     while (getline < file) {\n \tif (/^[ \\t]*(;|$)/ || !/^[^ \\t]/) {\n+\t    if (field > 2)\n+\t\tprintf(\"_(\\\"%s\\\")\\n\", line)\n \t    field = 0\n \t} else {\n \t    if ((field == 1) && /MissingArgError/) {\n@@ -275,12 +277,15 @@ echo \"scanning option files...\" >&2\n \t    if (field == 2) {\n \t\tline = $0\n \t\tprintf(\"#line %d \\\"%s\\\"\\n\", lineno, file)\n-\t\tprintf(\"_(\\\"%s\\\")\\n\", line)\n+\t    } else if (field > 2) {\n+\t\tline = line \" \" $0\n \t    }\n \t    field++;\n \t}\n \tlineno++;\n     }\n+    if (field > 2)\n+\tprintf(\"_(\\\"%s\\\")\\n\", line)\n   }') >> $emsg\n \n # Run the xgettext commands, with temporary added as a file to scan."}, {"sha": "93871c92b4352977906eb5f042fff1aa348f98c3", "filename": "gcc/po/gcc.pot", "status": "modified", "additions": 16335, "deletions": 14975, "changes": 31310, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/38381536cff46e4964f92b102dff30bb84353142/gcc%2Fpo%2Fgcc.pot", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/38381536cff46e4964f92b102dff30bb84353142/gcc%2Fpo%2Fgcc.pot", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fpo%2Fgcc.pot?ref=38381536cff46e4964f92b102dff30bb84353142"}]}