ps://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWstrict-aliasing-bogus-escape.C?ref=ab05897240fb27993741138f137dd096d003058e", "patch": "@@ -0,0 +1,30 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-O2 -Wstrict-aliasing\" } */\n+\n+#include <string>\n+#include <list>\n+\n+class A;\n+\n+class B {\n+public:\n+    void foo(A&);\n+    std::string s;\n+};\n+\n+class A {\n+public:\n+    A& qaz() {\n+\tl.push_back( new A() );\n+\treturn *l.back();\n+    }\n+    std::list<A*> l;\n+};\n+\n+void bar()\n+{\n+  A a;\n+  B b;\n+  b.foo(a.qaz());\n+}\n+"}]}