ake_typename_type (strip_typedefs (TYPE_CONTEXT (t),\n \t\t\t\t\t\t     remove_attributes),\n \t\t\t\t     fullname, typename_type, tf_none);\n+\t/* Handle 'typedef typename A::N N;'  */\n+\tif (typedef_variant_p (result))\n+\t  result = TYPE_MAIN_VARIANT (DECL_ORIGINAL_TYPE (TYPE_NAME (result)));\n       }\n       break;\n     case DECLTYPE_TYPE:"}, {"sha": "e5e59b1efd379f2a68be969b73ba595cdf92ab2f", "filename": "gcc/testsuite/g++.dg/template/typename21.C", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b54eff8bb1131e7bbd1e03633460be120f11ec99/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Ftypename21.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b54eff8bb1131e7bbd1e03633460be120f11ec99/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Ftypename21.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Ftypename21.C?ref=b54eff8bb1131e7bbd1e03633460be120f11ec99", "patch": "@@ -0,0 +1,11 @@\n+// PR c++/70067\n+// { dg-do compile { target c++98 } }\n+\n+template <class> struct A;\n+template <class T> struct B { struct N { }; };\n+template <class T> struct D: B<T> {\n+  typedef typename D::N N;\n+  A<N> *a;\n+};\n+\n+D<int> d;"}]}