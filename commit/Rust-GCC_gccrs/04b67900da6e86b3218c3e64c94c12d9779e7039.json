asm?ref=04b67900da6e86b3218c3e64c94c12d9779e7039", "patch": "@@ -92,6 +92,10 @@ _start:\n \t! access those data anyway.  Instead, go straight to main:\n \tmov\t%l0, %o0\t! argc\n \tmov\t%l1, %o1\t! argv\n+#ifdef GCRT1\n+\tsetn(___Argv, %o4, %o3)\n+\tstn\t%o1, [%o3]      ! *___Argv\n+#endif\n \t! Skip argc words past argv, to env:\n \tsll\t%l0, CPTRSHIFT, %o2\n \tadd\t%o2, CPTRSIZE, %o2"}]}