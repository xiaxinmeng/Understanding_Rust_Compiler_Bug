ble_field (DECL_CHAIN (field));\n       tree max = build_simple_component_ref (object, field);\n-      type = build_array_type (TREE_TYPE (TREE_TYPE (ptr)),\n-\t\t\t       build_index_type (max));\n+      type = build_cplus_array_type (TREE_TYPE (TREE_TYPE (ptr)),\n+\t\t\t\t     build_index_type (max));\n       type = build_reference_type (type);\n       REFERENCE_VLA_OK (type) = true;\n       object = convert (type, ptr);"}, {"sha": "556722ca62d70df7929fa0dbef5421f52034e3e9", "filename": "gcc/testsuite/g++.dg/cpp1y/lambda-generic-vla1.C", "status": "added", "additions": 24, "deletions": 0, "changes": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/8bebb9532b4724afacf514dfe1f0d127cb7f6f07/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Flambda-generic-vla1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/8bebb9532b4724afacf514dfe1f0d127cb7f6f07/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Flambda-generic-vla1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Flambda-generic-vla1.C?ref=8bebb9532b4724afacf514dfe1f0d127cb7f6f07", "patch": "@@ -0,0 +1,24 @@\n+// PR c++/59271\n+// { dg-options -std=c++1y }\n+\n+extern \"C\" int printf (const char *, ...);\n+\n+void f(int n)\n+{\n+  int  a[n];\n+\n+  for (auto& i : a)\n+    {\n+      i = &i - a;\n+    }\n+\n+  [&a] (auto m)\n+    {\n+      for (auto i : a)\n+\t{\n+\t  printf (\"%d\", i);\n+\t}\n+\n+      return m;\n+    };\n+}"}]}