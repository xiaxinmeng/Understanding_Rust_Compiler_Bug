_url": "https://github.com/Rust-GCC/gccrs/raw/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Fd%2Fd-convert.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fd%2Fd-convert.cc?ref=29ff25e70671ce2046aab5cf89bf519ec6cf2bed", "patch": "@@ -430,10 +430,10 @@ convert_expr (tree exp, Type *etype, Type *totype)\n \t      /* d_convert will make a no-op cast.  */\n \t      break;\n \t    }\n-\t  else if (cdfrom->isCPPclass ())\n+\t  else if (cdfrom->isCPPclass () || cdto->isCPPclass ())\n \t    {\n \t      /* Downcasting in C++ is a no-op.  */\n-\t      if (cdto->isCPPclass ())\n+\t      if (cdfrom->isCPPclass () && cdto->isCPPclass ())\n \t\tbreak;\n \n \t      /* Casting from a C++ interface to a class/non-C++ interface"}, {"sha": "74d7de807752d539d7a5c6803e4fdb1df75af97b", "filename": "gcc/testsuite/gdc.dg/Waddress.d", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWaddress.d", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWaddress.d", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgdc.dg%2FWaddress.d?ref=29ff25e70671ce2046aab5cf89bf519ec6cf2bed", "patch": "@@ -0,0 +1,12 @@\n+// { dg-do compile }\n+// { dg-options \"-Waddress\" }\n+\n+void* ptr;\n+\n+int test()\n+{\n+    if (&ptr) // { dg-warning \"the address of 'ptr' will always evaluate as 'true'\" }\n+        return 1;\n+\n+    return 0;\n+}"}, {"sha": "759b338acdc85a46f9f96707f9b0e0030dce3f28", "filename": "gcc/testsuite/gdc.dg/Wcastresult1.d", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult1.d", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult1.d", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult1.d?ref=29ff25e70671ce2046aab5cf89bf519ec6cf2bed", "patch": "@@ -0,0 +1,18 @@\n+// { dg-do compile }\n+// { dg-options \"-Wcast-result\" }\n+\n+extern(C++) class CPPClass\n+{\n+    int a;\n+}\n+\n+extern(D) class DClass\n+{\n+    int a;\n+}\n+\n+void test()\n+{\n+    auto cpptod = cast(DClass)new CPPClass; // { dg-warning \"cast to 'Wcastresult1.DClass' will produce null result\" }\n+    auto dtocpp = cast(CPPClass)new DClass; // { dg-warning \"cast to 'Wcastresult1.CPPClass' will produce null result\" }\n+}"}, {"sha": "56d2dd20e82bb89931bcc387dac1bdb4caf0b8d3", "filename": "gcc/testsuite/gdc.dg/Wcastresult2.d", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult2.d", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/29ff25e70671ce2046aab5cf89bf519ec6cf2bed/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult2.d", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgdc.dg%2FWcastresult2.d?ref=29ff25e70671ce2046aab5cf89bf519ec6cf2bed", "patch": "@@ -0,0 +1,12 @@\n+// { dg-do compile }\n+// { dg-options \"-Wcast-result\" }\n+\n+void test()\n+{\n+    auto imvalue = 1.23i;\n+    auto revalue = 1.23;\n+\n+    auto imtore = cast(double)imvalue; // { dg-warning \"cast from 'idouble' to 'double' will produce zero result\" }\n+    auto retoim = cast(idouble)revalue; // { dg-warning \"cast from 'double' to 'idouble' will produce zero result\" }\n+    return;\n+}"}]}