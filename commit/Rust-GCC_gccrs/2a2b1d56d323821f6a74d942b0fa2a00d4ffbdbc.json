\n+\tvoid f() {}\n+\tvoid g(T* t) {\n+\t\tA<typeof(t)> a;\n+\t\ta.f();\n+\t}\n+};\n+\n+int main()\n+{\n+\tA<int> a;\n+\tint b;\n+\ta.g(&b);\n+}"}]}