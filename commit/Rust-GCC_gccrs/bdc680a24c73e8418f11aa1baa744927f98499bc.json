nts/gcc%2Fcccp.c?ref=bdc680a24c73e8418f11aa1baa744927f98499bc", "patch": "@@ -5693,6 +5693,10 @@ do_line (buf, limit, op, keyword)\n      we must store a line number now that is one less.  */\n   new_lineno = atoi (bp) - 1;\n \n+  /* NEW_LINENO is one less than the actual line number here.  */\n+  if (pedantic && new_lineno < 0)\n+    pedwarn (\"line number out of range in `#line' command\");\n+\n   /* skip over the line number.  */\n   while (isdigit (*bp))\n     bp++;\n@@ -5726,6 +5730,8 @@ do_line (buf, limit, op, keyword)\n     bp++;\n     SKIP_WHITE_SPACE (bp);\n     if (*bp) {\n+      if (pedantic)\n+\tpedwarn (\"garbage at end of `#line' command\");\n       if (*bp == '1')\n \tfile_change = enter_file;\n       else if (*bp == '2')"}]}