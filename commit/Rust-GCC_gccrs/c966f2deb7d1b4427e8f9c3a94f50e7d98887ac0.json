d (fn);\n \n-  if (DECL_VINDEX (fn) && (flags & LOOKUP_NONVIRTUAL) == 0)\n+  if (DECL_VINDEX (fn) && (flags & LOOKUP_NONVIRTUAL) == 0\n+      /* Don't mess with virtual lookup in fold_non_dependent_expr; virtual\n+\t functions can't be constexpr.  */\n+      && !in_template_function ())\n     {\n       tree t;\n       tree binfo = lookup_base (TREE_TYPE (TREE_TYPE (argarray[0])),"}, {"sha": "a2c7420b402e7ec6c648668e88ebbd16ab42058c", "filename": "gcc/testsuite/g++.dg/template/virtual4.C", "status": "added", "additions": 30, "deletions": 0, "changes": 30, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c966f2deb7d1b4427e8f9c3a94f50e7d98887ac0/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fvirtual4.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c966f2deb7d1b4427e8f9c3a94f50e7d98887ac0/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fvirtual4.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fvirtual4.C?ref=c966f2deb7d1b4427e8f9c3a94f50e7d98887ac0", "patch": "@@ -0,0 +1,30 @@\n+// PR c++/56243\n+\n+struct A\n+{\n+  virtual int String ();\n+};\n+\n+struct F: A { };\n+\n+struct G\n+{\n+  F value;\n+};\n+\n+struct D\n+{\n+  template <int>\n+  void Verify()\n+  {\n+    G x;\n+    F& name = x.value;\n+    name.String();\n+  }\n+};\n+\n+int main()\n+{\n+  D d;\n+  d.Verify<42>();\n+}"}]}