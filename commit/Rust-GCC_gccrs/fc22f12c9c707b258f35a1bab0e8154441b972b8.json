n+// { dg-output \"A\\n\" }\n+macro_rules! cfg {\n+    () => {{}};\n+}\n+\n+extern \"C\" {\n+    fn printf(fmt: *const i8, ...);\n+}\n+\n+fn print(s: &str) {\n+    printf(\"%s\\n\" as *const str as *const i8, s as *const str as *const i8);\n+}\n+\n+\n+fn main() -> i32 {\n+    let cfg = cfg!(A);\n+    if cfg {\n+        print(\"A\");\n+    }\n+    let cfg = cfg!(B);\n+    if cfg {\n+        print(\"B\");\n+    }\n+\n+    0\n+}"}]}