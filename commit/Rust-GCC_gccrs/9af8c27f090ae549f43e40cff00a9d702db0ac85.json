0ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fgen_il-gen-gen_nodes.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9af8c27f090ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fgen_il-gen-gen_nodes.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fgen_il-gen-gen_nodes.adb?ref=9af8c27f090ae549f43e40cff00a9d702db0ac85", "patch": "@@ -509,6 +509,7 @@ begin -- Gen_IL.Gen.Gen_Nodes\n \n    Cc (N_Delta_Aggregate, N_Subexpr,\n        (Sy (Expression, Node_Id, Default_Empty),\n+        Sy (Is_Homogeneous_Aggregate, Flag),\n         Sy (Component_Associations, List_Id, Default_No_List)));\n \n    Cc (N_Extension_Aggregate, N_Subexpr,"}, {"sha": "bfefd144f21efbb637a5d4e9a6248d08f0be6f0c", "filename": "gcc/ada/par-ch4.adb", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9af8c27f090ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fpar-ch4.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9af8c27f090ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fpar-ch4.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fpar-ch4.adb?ref=9af8c27f090ae549f43e40cff00a9d702db0ac85", "patch": "@@ -1682,6 +1682,7 @@ package body Ch4 is\n \n       case Start_Token is\n          when Tok_Left_Bracket =>\n+\n             Set_Component_Associations (Aggregate_Node, Assoc_List);\n             Set_Is_Homogeneous_Aggregate (Aggregate_Node);\n             T_Right_Bracket;"}, {"sha": "e349128e6a293383f3af30f8dbba8fe63b9462f8", "filename": "gcc/ada/sem_aggr.adb", "status": "modified", "additions": 10, "deletions": 1, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9af8c27f090ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fsem_aggr.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9af8c27f090ae549f43e40cff00a9d702db0ac85/gcc%2Fada%2Fsem_aggr.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fsem_aggr.adb?ref=9af8c27f090ae549f43e40cff00a9d702db0ac85", "patch": "@@ -3291,6 +3291,15 @@ package body Sem_Aggr is\n       if Is_Array_Type (Typ) then\n          Resolve_Delta_Array_Aggregate (N, Typ);\n       else\n+\n+         --  Delta aggregates for record types must use parentheses,\n+         --  not square brackets.\n+\n+         if Is_Homogeneous_Aggregate (N) then\n+            Error_Msg_N\n+              (\"delta aggregates for record types must use (), not '[']\", N);\n+         end if;\n+\n          Resolve_Delta_Record_Aggregate (N, Typ);\n       end if;\n \n@@ -4916,7 +4925,7 @@ package body Sem_Aggr is\n       if Nkind (N) = N_Aggregate\n         and then Is_Homogeneous_Aggregate (N)\n       then\n-         Error_Msg_N (\"record aggregate must use () and not '[']\", N);\n+         Error_Msg_N (\"record aggregate must use (), not '[']\", N);\n          return;\n       end if;\n "}]}