3f73ef636d26727b6de", "filename": "libstdc++-v3/testsuite/lib/libstdc++.exp", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b2bc1bb6cabf30f332938d648fe6432a3a9d27b6/libstdc%2B%2B-v3%2Ftestsuite%2Flib%2Flibstdc%2B%2B.exp", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b2bc1bb6cabf30f332938d648fe6432a3a9d27b6/libstdc%2B%2B-v3%2Ftestsuite%2Flib%2Flibstdc%2B%2B.exp", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Ftestsuite%2Flib%2Flibstdc%2B%2B.exp?ref=b2bc1bb6cabf30f332938d648fe6432a3a9d27b6", "patch": "@@ -1129,7 +1129,7 @@ proc check_effective_target_gthreads { } {\n proc check_v3_target_gthreads_timed { } {\n     return [check_v3_target_prop_cached et_gthreads_timed {\n \tif [check_v3_target_gthreads] {\n-\t    set cond \"defined _GTHREAD_USE_MUTEX_TIMEDLOCK\"\n+\t    set cond \"_GTHREAD_USE_MUTEX_TIMEDLOCK\"\n \t    return [v3_check_preprocessor_condition gthreads_timed $cond]\n \t} else {\n \t    return 0"}]}