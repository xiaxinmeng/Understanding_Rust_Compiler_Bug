estsuite%2Fgcc.dg%2Fguality%2Fasm-1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fasm-1.c?ref=a677307de8fab7c7a558fb523148698a16cc5727", "patch": "@@ -1,14 +1,16 @@\n /* { dg-do run } */\n /* { dg-options \"-g\" } */\n \n+#include \"nop.h\"\n+\n struct A { int x; unsigned short y; char z[64]; };\n \n void __attribute__((noinline))\n foo (struct A *p, char *q)\n {\n   int f = &p->z[p->y] - q;\n-  asm volatile (\"nop\");\n-  asm volatile (\"nop\" : : \"g\" (f));\t\t/* { dg-final { gdb-test 12 \"f\" \"14\" } } */\n+  asm volatile (NOP);\n+  asm volatile (NOP : : \"g\" (f));\t\t/* { dg-final { gdb-test 14 \"f\" \"14\" } } */\n   asm volatile (\"\" : : \"g\" (p), \"g\" (q));\n }\n "}, {"sha": "a0c19a344141c26a0a297fe1e8642614c3254adc", "filename": "gcc/testsuite/gcc.dg/guality/nop.h", "status": "added", "additions": 7, "deletions": 0, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a677307de8fab7c7a558fb523148698a16cc5727/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fnop.h", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a677307de8fab7c7a558fb523148698a16cc5727/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fnop.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fnop.h?ref=a677307de8fab7c7a558fb523148698a16cc5727", "patch": "@@ -0,0 +1,7 @@\n+#if defined (__ia64__) || defined (__s390__) || defined (__s390x__)\n+#define NOP \"nop 0\"\n+#elif defined (__MMIX__)\n+#define NOP \"swym 0\"\n+#else\n+#define NOP \"nop\"\n+#endif"}, {"sha": "7820727e97844a5a670d244f95b9c2899d62a9ee", "filename": "gcc/testsuite/gcc.dg/guality/pr43329-1.c", "status": "modified", "additions": 4, "deletions": 2, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a677307de8fab7c7a558fb523148698a16cc5727/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fpr43329-1.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a677307de8fab7c7a558fb523148698a16cc5727/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fpr43329-1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fguality%2Fpr43329-1.c?ref=a677307de8fab7c7a558fb523148698a16cc5727", "patch": "@@ -2,12 +2,14 @@\n /* { dg-do run } */\n /* { dg-options \"-g\" } */\n \n+#include \"nop.h\"\n+\n static inline void\n foo (int argx)\n {\n   int varx = argx;\n-  __asm__ volatile (\"nop\");\t\t\t/* { dg-final { gdb-test 10 \"argx\" \"25\" } } */\n-  __asm__ volatile (\"nop\" : : \"g\" (varx));\t/* { dg-final { gdb-test 10 \"varx\" \"25\" } } */\n+  __asm__ volatile (NOP);\t\t\t/* { dg-final { gdb-test 12 \"argx\" \"25\" } } */\n+  __asm__ volatile (NOP : : \"g\" (varx));\t/* { dg-final { gdb-test 12 \"varx\" \"25\" } } */\n }\n \n int i;"}]}