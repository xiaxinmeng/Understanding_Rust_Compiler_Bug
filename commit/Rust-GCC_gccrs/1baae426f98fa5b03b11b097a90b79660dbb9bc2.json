rs: Makefile\n \n # Install the headers needed to build a plugin.\n install-plugin: installdirs lang.install-plugin s-header-vars\n-# We keep the directory structure for files in config and .def files. All\n-# other files are flattened to a single directory.\n+# We keep the directory structure for files in config or c-family and .def\n+# files. All other files are flattened to a single directory.\n \t$(mkinstalldirs) $(DESTDIR)$(plugin_includedir)\n \theaders=`echo $(PLUGIN_HEADERS) | tr ' ' '\\012' | sort -u`; \\\n \tsrcdirstrip=`echo \"$(srcdir)\" | sed 's/[].[^$$\\\\*|]/\\\\\\\\&/g'`; \\\n@@ -4615,7 +4615,7 @@ install-plugin: installdirs lang.install-plugin s-header-vars\n \t  else continue; \\\n \t  fi; \\\n \t  case $$path in \\\n-\t  \"$(srcdir)\"/config/* | \"$(srcdir)\"/*.def ) \\\n+\t  \"$(srcdir)\"/config/* | \"$(srcdir)\"/c-family/* | \"$(srcdir)\"/*.def ) \\\n \t    base=`echo \"$$path\" | sed -e \"s|$$srcdirstrip/||\"`;; \\\n \t  *) base=`basename $$path` ;; \\\n \t  esac; \\"}, {"sha": "7d8529911d0f984341d2272e96bce0ddb525287f", "filename": "gcc/c-family/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1baae426f98fa5b03b11b097a90b79660dbb9bc2/gcc%2Fc-family%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1baae426f98fa5b03b11b097a90b79660dbb9bc2/gcc%2Fc-family%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fc-family%2FChangeLog?ref=1baae426f98fa5b03b11b097a90b79660dbb9bc2", "patch": "@@ -1,3 +1,7 @@\n+2011-07-25  Romain Geissler  <romain.geissler@gmail.com>\n+\n+\t* c-pretty-print.h: Search c-common.h in c-family.\n+\t\n 2011-07-22  Jason Merrill  <jason@redhat.com>\n \n \tPR c++/49793"}, {"sha": "8d399ddf96e8c37a902c590f5bfd9568438a09ea", "filename": "gcc/c-family/c-pretty-print.h", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1baae426f98fa5b03b11b097a90b79660dbb9bc2/gcc%2Fc-family%2Fc-pretty-print.h", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1baae426f98fa5b03b11b097a90b79660dbb9bc2/gcc%2Fc-family%2Fc-pretty-print.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fc-family%2Fc-pretty-print.h?ref=1baae426f98fa5b03b11b097a90b79660dbb9bc2", "patch": "@@ -23,7 +23,7 @@ along with GCC; see the file COPYING3.  If not see\n #define GCC_C_PRETTY_PRINTER\n \n #include \"tree.h\"\n-#include \"c-common.h\"\n+#include \"c-family/c-common.h\"\n #include \"pretty-print.h\"\n \n "}]}