s/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=7d58b9e77aab0f4c8cd8f6eb4dc618b240325099", "patch": "@@ -1,3 +1,8 @@\n+2010-09-16  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/45674\n+\t* gfortran.dg/class_dummy_2.f03: New.\n+\n 2010-09-16  Richard Guenther  <rguenther@suse.de>\n \n \tPR tree-optimization/45623"}, {"sha": "c1735822bb456c7a83f1c1b9a0fd487fd0c16c47", "filename": "gcc/testsuite/gfortran.dg/class_dummy_2.f03", "status": "added", "additions": 33, "deletions": 0, "changes": 33, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7d58b9e77aab0f4c8cd8f6eb4dc618b240325099/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_dummy_2.f03", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7d58b9e77aab0f4c8cd8f6eb4dc618b240325099/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_dummy_2.f03", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_dummy_2.f03?ref=7d58b9e77aab0f4c8cd8f6eb4dc618b240325099", "patch": "@@ -0,0 +1,33 @@\n+! { dg-do run }\n+!\n+! PR 45674: [OOP] Undefined references for extended types\n+!\n+! Contributed by Dietmar Ebner <dietmar.ebner@gmail.com>\n+\n+module fails_mod\n+  implicit none \n+  type :: a_t\n+     integer :: a\n+  end type\n+  type, extends(a_t) :: b_t\n+     integer :: b\n+  end type\n+contains\n+  subroutine foo(a)\n+    class(a_t) :: a\n+  end subroutine foo\n+end module fails_mod\n+\n+module fails_test\n+  implicit none\n+contains\n+  subroutine bar\n+    use fails_mod\n+    type(b_t) :: b\n+    call foo(b)\n+  end subroutine bar\n+end module fails_test\n+\n+end\n+\n+! { dg-final { cleanup-modules \"fails_mod fails_test\" } }"}]}