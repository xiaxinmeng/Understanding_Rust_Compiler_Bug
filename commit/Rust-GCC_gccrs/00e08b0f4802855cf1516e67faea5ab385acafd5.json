DR 1847: If a particular P contains no template-parameters that\n+\t participate in template argument deduction, that P is not used to\n+\t determine the ordering.  */\n+      if (!uses_deducible_template_parms (arg1)\n+\t  && !uses_deducible_template_parms (arg2))\n+\tgoto next;\n+\n       if (TREE_CODE (arg1) == REFERENCE_TYPE)\n \t{\n \t  ref1 = TYPE_REF_IS_RVALUE (arg1) + 1;\n@@ -21303,6 +21310,8 @@ more_specialized_fn (tree pat1, tree pat2, int len)\n \t   These must be unordered.  */\n \tbreak;\n \n+    next:\n+\n       if (TREE_CODE (arg1) == TYPE_PACK_EXPANSION\n           || TREE_CODE (arg2) == TYPE_PACK_EXPANSION)\n         /* We have already processed all of the arguments in our"}, {"sha": "0832ea535caa6a0dac3a4c423700fe59e71f9f17", "filename": "gcc/testsuite/g++.dg/template/partial-order1.C", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/00e08b0f4802855cf1516e67faea5ab385acafd5/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-order1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/00e08b0f4802855cf1516e67faea5ab385acafd5/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-order1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fpartial-order1.C?ref=00e08b0f4802855cf1516e67faea5ab385acafd5", "patch": "@@ -0,0 +1,18 @@\n+// { dg-do compile { target c++11 } }\n+\n+using size_t = decltype(sizeof(0));\n+template <class T> struct A\n+{\n+  using size_type = size_t;\n+};\n+\n+template <class T>\n+void f(size_t, T);\n+\n+template <class T>\n+void f(typename A<T>::size_type, T);\n+\n+int main()\n+{\n+  f(1,2);\t\t\t// { dg-error \"ambiguous\" }\n+}"}]}