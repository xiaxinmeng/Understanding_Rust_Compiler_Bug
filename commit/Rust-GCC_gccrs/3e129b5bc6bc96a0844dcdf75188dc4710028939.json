ew_bb = bb->prev_bb;\n+\t      /* Don't move any labels into ENTRY block.  */\n+\t      if (new_bb == ENTRY_BLOCK_PTR_FOR_FN (cfun))\n+\t\t{\n+\t\t  new_bb = single_succ (new_bb);\n+\t\t  gcc_assert (new_bb != bb);\n+\t\t}\n \t      new_gsi = gsi_start_bb (new_bb);\n \t      gsi_remove (&i, false);\n \t      gsi_insert_before (&new_gsi, stmt, GSI_NEW_STMT);"}]}