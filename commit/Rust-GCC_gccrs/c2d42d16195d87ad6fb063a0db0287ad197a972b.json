%2Ftrans-expr.c?ref=c2d42d16195d87ad6fb063a0db0287ad197a972b", "patch": "@@ -6009,6 +6009,19 @@ gfc_conv_procedure_call (gfc_se * se, gfc_symbol * sym,\n \t    {\n \t      var = gfc_create_var (type, \"pstr\");\n \n+\t      /* Emit a DECL_EXPR for the VLA type.  */\n+\t      tmp = TREE_TYPE (type);\n+\t      if (TYPE_SIZE (tmp)\n+\t\t  && TREE_CODE (TYPE_SIZE (tmp)) != INTEGER_CST)\n+\t\t{\n+\t\t  tmp = build_decl (input_location, TYPE_DECL, NULL_TREE, tmp);\n+\t\t  DECL_ARTIFICIAL (tmp) = 1;\n+\t\t  DECL_IGNORED_P (tmp) = 1;\n+\t\t  tmp = fold_build1_loc (input_location, DECL_EXPR,\n+\t\t\t\t\t TREE_TYPE (tmp), tmp);\n+\t\t  gfc_add_expr_to_block (&se->pre, tmp);\n+\t\t}\n+\n \t      if ((!comp && sym->attr.allocatable)\n \t\t  || (comp && comp->attr.allocatable))\n \t\t{"}, {"sha": "c0b849324d21eebf41ff4e0b0bf97c001ea29cf3", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c2d42d16195d87ad6fb063a0db0287ad197a972b/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c2d42d16195d87ad6fb063a0db0287ad197a972b/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=c2d42d16195d87ad6fb063a0db0287ad197a972b", "patch": "@@ -1,3 +1,8 @@\n+2016-12-16  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR fortran/78757\n+\t* gfortran.dg/char_result_16.f90: New test.\n+\n 2016-12-16  Kyrylo Tkachov  <kyrylo.tkachov@arm.com>\n \n \t* gcc.target/aarch64/ubfiz_lsl_1.c: New test."}, {"sha": "adb86d5090cb6c3302b37af32b6e4efcc4139e97", "filename": "gcc/testsuite/gfortran.dg/char_result_16.f90", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c2d42d16195d87ad6fb063a0db0287ad197a972b/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_16.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c2d42d16195d87ad6fb063a0db0287ad197a972b/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_16.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_16.f90?ref=c2d42d16195d87ad6fb063a0db0287ad197a972b", "patch": "@@ -0,0 +1,16 @@\n+! PR fortran/78757\n+! { dg-do compile }\n+! { dg-options \"-O1\" }\n+\n+program pr78757\n+  implicit none\n+  character (len = 30), target :: x\n+  character (len = 30), pointer :: s\n+  s => foo (30_8)\n+contains\n+  function foo (i)\n+    integer (8) :: i\n+    character (len = i), pointer :: foo\n+    foo => x\n+  end function foo\n+end program pr78757"}]}