,18 @@\n+/* PR c/35738 */\n+/* { dg-do compile } */\n+/* { dg-options \"-fopenmp\" } */\n+\n+void foo (void);\n+\n+void\n+bar (void *p)\n+{\n+  int i = 0;\n+  char q[10];\n+#pragma omp atomic\n+  i += q;\t\t/* { dg-warning \"makes integer from pointer without a cast\" } */\n+#pragma omp atomic\n+  i += foo;\t\t/* { dg-warning \"makes integer from pointer without a cast\" } */\n+#pragma omp atomic\n+  i += p;\t\t/* { dg-warning \"makes integer from pointer without a cast\" } */\n+}"}]}