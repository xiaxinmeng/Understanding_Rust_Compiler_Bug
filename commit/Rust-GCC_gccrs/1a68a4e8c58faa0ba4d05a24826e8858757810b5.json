n       gcc_assert (binfo && binfo != error_mark_node);\n \n+      /* Warn about deprecated virtual functions now, since we're about\n+\t to throw away the decl.  */\n+      if (TREE_DEPRECATED (fn))\n+\twarn_deprecated_use (fn);\n+\n       argarray[0] = build_base_path (PLUS_EXPR, argarray[0], binfo, 1);\n       if (TREE_SIDE_EFFECTS (argarray[0]))\n \targarray[0] = save_expr (argarray[0]);"}, {"sha": "e2f1a2c33cafcc5c8bd95ee6be8a35c114736831", "filename": "gcc/testsuite/g++.dg/warn/deprecated-4.C", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1a68a4e8c58faa0ba4d05a24826e8858757810b5/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2Fdeprecated-4.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1a68a4e8c58faa0ba4d05a24826e8858757810b5/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2Fdeprecated-4.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2Fdeprecated-4.C?ref=1a68a4e8c58faa0ba4d05a24826e8858757810b5", "patch": "@@ -0,0 +1,9 @@\n+// PR c++/15269\n+\n+struct B { \n+    virtual int foo() __attribute__((deprecated)); \n+}; \n+ \n+int main(void) { \n+  ((B*)0)->foo(); \t\t// { dg-warning \"deprecated\" }\n+} "}]}