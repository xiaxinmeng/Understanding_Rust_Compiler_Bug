c252376afb41e1fd14fe684beac5f1", "patch": "@@ -2973,6 +2973,9 @@ ix86_option_override_internal (bool main_args_p)\n   SUBSUBTARGET_OVERRIDE_OPTIONS;\n #endif\n \n+  if (TARGET_X32)\n+    ix86_isa_flags |= OPTION_MASK_ISA_64BIT;\n+\n   /* -fPIC is the default for x86_64.  */\n   if (TARGET_MACHO && TARGET_64BIT)\n     flag_pic = 2;\n@@ -3064,6 +3067,9 @@ ix86_option_override_internal (bool main_args_p)\n \t  if (!TARGET_64BIT)\n \t    error (\"code model %qs not supported in the %s bit mode\",\n \t\t   \"medium\", \"32\");\n+\t  else if (TARGET_X32)\n+\t    error (\"code model %qs not supported in x32 mode\",\n+\t\t   \"medium\");\n \t  break;\n \n \tcase CM_LARGE:\n@@ -3073,6 +3079,9 @@ ix86_option_override_internal (bool main_args_p)\n \t  if (!TARGET_64BIT)\n \t    error (\"code model %qs not supported in the %s bit mode\",\n \t\t   \"large\", \"32\");\n+\t  else if (TARGET_X32)\n+\t    error (\"code model %qs not supported in x32 mode\",\n+\t\t   \"medium\");\n \t  break;\n \n \tcase CM_32:"}]}