instructions (rtx_insn *f, unsigned int nregs)\n   FOR_EACH_BB_FN (this_basic_block, cfun)\n     {\n       rtx_insn *last_combined_insn = NULL;\n+\n+      /* Ignore instruction combination in basic blocks that are going to\n+\t be removed as unreachable anyway.  See PR82386.  */\n+      if (EDGE_COUNT (this_basic_block->preds) == 0)\n+\tcontinue;\n+\n       optimize_this_for_speed_p = optimize_bb_for_speed_p (this_basic_block);\n       last_call_luid = 0;\n       mem_last_set = -1;"}, {"sha": "74023abc015ae1c35e6394b680b09d365c8f9e39", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/bb81a576f259256b38d46dd41d3bd5f5a36cfd6f/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/bb81a576f259256b38d46dd41d3bd5f5a36cfd6f/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=bb81a576f259256b38d46dd41d3bd5f5a36cfd6f", "patch": "@@ -1,3 +1,8 @@\n+2017-10-03  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR target/82386\n+\t* gcc.dg/pr82386.c: New test.\n+\n 2017-10-02  Paolo Carlini  <paolo.carlini@oracle.com>\n \n \tPR c++/70570"}, {"sha": "8901f2b4cc7b17bbefbb2154f4e342f417311fba", "filename": "gcc/testsuite/gcc.dg/pr82386.c", "status": "added", "additions": 38, "deletions": 0, "changes": 38, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/bb81a576f259256b38d46dd41d3bd5f5a36cfd6f/gcc%2Ftestsuite%2Fgcc.dg%2Fpr82386.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/bb81a576f259256b38d46dd41d3bd5f5a36cfd6f/gcc%2Ftestsuite%2Fgcc.dg%2Fpr82386.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fpr82386.c?ref=bb81a576f259256b38d46dd41d3bd5f5a36cfd6f", "patch": "@@ -0,0 +1,38 @@\n+/* PR target/82386 */\n+/* { dg-do compile } */\n+/* { dg-options \"-O2 -w\" } */\n+/* { dg-additional-options \"-misel\" { target powerpc*-*-* } } */\n+\n+long long int fs;\n+int vm;\n+\n+void\n+sd (void)\n+{\n+  fs = 1;\n+  vm = 2;\n+  goto zf;\n+\n+  if (0)\n+    {\n+      int y6 = 0;\n+      int *uu = &y6;\n+      short int he;\n+      int of = 0;\n+\n+ zf:\n+      for (;;)\n+      {\n+          he = of;\n+          if (he || (fs |= vm))\n+            {\n+              *uu = fs;\n+              fs += vm;\n+            }\n+          if (y6 == vm)\n+            fs |= he;\n+          he = y6 || fs;\n+          fs /= 0;\n+        }\n+    }\n+}"}]}