_url": "https://github.com/Rust-GCC/gccrs/blob/c3d2600cfb476e576fd27f3f29f49e968e86774d/gcc%2Ftree-ssa-propagate.cc", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c3d2600cfb476e576fd27f3f29f49e968e86774d/gcc%2Ftree-ssa-propagate.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftree-ssa-propagate.cc?ref=c3d2600cfb476e576fd27f3f29f49e968e86774d", "patch": "@@ -1272,7 +1272,21 @@ clean_up_loop_closed_phi (function *fun)\n \t      rhs = gimple_phi_arg_def (phi, 0);\n \t      lhs = gimple_phi_result (phi);\n \n-\t      if (rhs && may_propagate_copy (lhs, rhs))\n+\t      if (virtual_operand_p (rhs))\n+\t\t{\n+\t\t  imm_use_iterator iter;\n+\t\t  use_operand_p use_p;\n+\t\t  gimple *stmt;\n+\n+\t\t  FOR_EACH_IMM_USE_STMT (stmt, iter, lhs)\n+\t\t    FOR_EACH_IMM_USE_ON_STMT (use_p, iter)\n+\t\t      SET_USE (use_p, rhs);\n+\n+\t\t  if (SSA_NAME_OCCURS_IN_ABNORMAL_PHI (lhs))\n+\t\t    SSA_NAME_OCCURS_IN_ABNORMAL_PHI (rhs) = 1;\n+\t\t  remove_phi_node (&gsi, true);\n+\t\t}\n+\t      else if (may_propagate_copy (lhs, rhs))\n \t\t{\n \t\t  /* Dump details.  */\n \t\t  if (dump_file && (dump_flags & TDF_DETAILS))"}]}