ch": "@@ -25,7 +25,9 @@\n #define _MM_MALLOC_H_INCLUDED\n \n #include <stdlib.h>\n+#if __STDC_HOSTED__\n #include <errno.h>\n+#endif\n \n static __inline__ void * \n _mm_malloc (size_t __size, size_t __align)\n@@ -36,7 +38,9 @@ _mm_malloc (size_t __size, size_t __align)\n   /* Error if align is not a power of two.  */\n   if (__align & (__align - 1))\n     {\n+#if __STDC_HOSTED__\n       errno = EINVAL;\n+#endif\n       return ((void *) 0);\n     }\n "}]}