_expr_real_1 (tree exp, rtx target, enum machine_mode tmode,\n \t\tbftype = TREE_TYPE (base);\n \t\tif (TYPE_MODE (TREE_TYPE (exp)) != BLKmode)\n \t\t  bftype = TREE_TYPE (exp);\n+\t\telse\n+\t\t  {\n+\t\t    temp = assign_stack_temp (DECL_MODE (base),\n+\t\t\t\t\t      GET_MODE_SIZE (DECL_MODE (base)),\n+\t\t\t\t\t      0);\n+\t\t    store_expr (base, temp, 0, false);\n+\t\t    temp = adjust_address (temp, BLKmode, offset);\n+\t\t    set_mem_size (temp, int_size_in_bytes (TREE_TYPE (exp)));\n+\t\t    return temp;\n+\t\t  }\n \t\treturn expand_expr (build3 (BIT_FIELD_REF, bftype,\n \t\t\t\t\t    base,\n \t\t\t\t\t    TYPE_SIZE (TREE_TYPE (exp)),"}, {"sha": "32baf0e09a7958fca45830b147f000b912859233", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/892123df86260d2bddfc9bbb9f2d60915a5b0b8d/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/892123df86260d2bddfc9bbb9f2d60915a5b0b8d/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=892123df86260d2bddfc9bbb9f2d60915a5b0b8d", "patch": "@@ -1,3 +1,8 @@\n+2012-01-26  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR middle-end/51895\n+\t* g++.dg/opt/pr51895.C: New test.\n+\n 2012-01-26  Michael Matz  <matz@suse.de>\n \n \tPR tree-optimization/48794"}, {"sha": "84ac5e9962f95fe54c98b26ea42adea110e9cc4c", "filename": "gcc/testsuite/g++.dg/opt/pr51895.C", "status": "added", "additions": 25, "deletions": 0, "changes": 25, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/892123df86260d2bddfc9bbb9f2d60915a5b0b8d/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fopt%2Fpr51895.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/892123df86260d2bddfc9bbb9f2d60915a5b0b8d/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fopt%2Fpr51895.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fopt%2Fpr51895.C?ref=892123df86260d2bddfc9bbb9f2d60915a5b0b8d", "patch": "@@ -0,0 +1,25 @@\n+// PR middle-end/51895\n+// { dg-do compile }\n+// { dg-options \"-O2\" }\n+\n+struct S\n+{\n+  long a;\n+  char b;\n+  S () : a (0), b (0) {}\n+  bool baz ();\n+};\n+\n+__attribute__((noinline)) static bool\n+bar (S x, S y)\n+{\n+  y = x;\n+  return y.baz ();\n+}\n+\n+bool\n+foo (S x)\n+{\n+  S y;\n+  return bar (x, y);\n+}"}]}