@@\n+// PR c++/86926\n+// { dg-do compile { target c++17 } }\n+\n+int\n+main()\n+{\n+    constexpr auto f = [](auto self, auto n) {\n+        if(n < 2)\n+\t  return n;\n+        return self(self, n - 1) + self(self, n - 2);\n+    };\n+\n+    constexpr auto fibonacci = [=](auto n) { return f(f, n); };\n+\n+    static_assert(fibonacci(7) == 13);\n+}"}]}