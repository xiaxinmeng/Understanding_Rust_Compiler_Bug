Fsse_nolib.adb?ref=3ffcc54f1d69618192bc3481e5b399c8fbb6aaec", "patch": "@@ -1,5 +1,6 @@\n --  { dg-do run { target i?86-*-* x86_64-*-* } }\n --  { dg-options \"-O1 -msse\" }\n+--  { dg-require-effective-target sse_runtime } \n \n with Ada.Unchecked_Conversion;\n "}, {"sha": "79fb3e6064dec181b1b5004d097aa32033e702cc", "filename": "gcc/testsuite/lib/gnat.exp", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/3ffcc54f1d69618192bc3481e5b399c8fbb6aaec/gcc%2Ftestsuite%2Flib%2Fgnat.exp", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/3ffcc54f1d69618192bc3481e5b399c8fbb6aaec/gcc%2Ftestsuite%2Flib%2Fgnat.exp", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Flib%2Fgnat.exp?ref=3ffcc54f1d69618192bc3481e5b399c8fbb6aaec", "patch": "@@ -27,6 +27,7 @@\n load_lib libgloss.exp\n load_lib prune.exp\n load_lib gcc-defs.exp\n+load_lib gcc.exp\n load_lib timeout.exp\n \n #\n@@ -132,6 +133,11 @@ proc gnat_target_compile { source dest type options } {\n     global gnat_libgcc_s_path\n     global gnat_target_current\n \n+    # dg-require-effective-target tests must be compiled as C.\n+    if [ string match \"*.c\" $source ] then {\n+\treturn [gcc_target_compile $source $dest $type $options]\n+    }\n+\n     # If we detect a change of target, we need to recompute both\n     # GNAT_UNDER_TEST and the appropriate RTS.\n     if { $gnat_target_current!=\"[current_target_name]\" } {"}]}