/Rust-GCC/gccrs/blob/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ffortran%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ffortran%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2FChangeLog?ref=cf73cdac51a20dacfc1b569b0bd6fcef1d71f852", "patch": "@@ -1,3 +1,9 @@\n+2004-08-31  Tobias Schlueter  <tobias.schlueter@physik.uni-muenchen.de>\n+\n+\tPR fortran/17244\n+\t* trans-types.c (gfc_return_by_reference): Remove TODO error,\n+\tadd comment pointing out possible issue WRT compatibility with g77.\n+\n 2004-08-31  Tobias Schlueter  <tobias.schlueter@physik.uni-muenchen.de>\n \n \t* trans-decl.c, trans-expr.c, trans-io.c, trans-types.c: Replace"}, {"sha": "26866b4c016e298fe0a07a6ef4533aad7c0407c0", "filename": "gcc/fortran/trans-types.c", "status": "modified", "additions": 1, "deletions": 3, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ffortran%2Ftrans-types.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ffortran%2Ftrans-types.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Ftrans-types.c?ref=cf73cdac51a20dacfc1b569b0bd6fcef1d71f852", "patch": "@@ -1451,9 +1451,7 @@ gfc_return_by_reference (gfc_symbol * sym)\n   if (sym->ts.type == BT_CHARACTER)\n     return 1;\n \n-  if (sym->ts.type == BT_DERIVED)\n-    gfc_todo_error (\"Returning derived types\");\n-  /* Possibly return derived types by reference.  */\n+  /* Possibly return complex numbers by reference for g77 compatibility.  */\n   return 0;\n }\n \f"}, {"sha": "86a392c8f20dc1b54300a73540d00d1f6c02dbd6", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=cf73cdac51a20dacfc1b569b0bd6fcef1d71f852", "patch": "@@ -1,3 +1,8 @@\n+2004-08-31  Tobias Schlueter  <tobias.schlueter@physik.uni-muenchen.de>\n+\n+\tPR fortran/17244\n+\t* gfortran.dg/func_derived_1.f90: New test.\n+\n 2004-08-31  Paul Brook  <paul@codesourcery.com>\n \n \t* gfortran.dg/eof_1.f90: New test."}, {"sha": "5160e5ae7f238596f8bc8d32d9e6bb1e75164cb4", "filename": "gcc/testsuite/gfortran.dg/func_derived_1.f90", "status": "added", "additions": 38, "deletions": 0, "changes": 38, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ftestsuite%2Fgfortran.dg%2Ffunc_derived_1.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cf73cdac51a20dacfc1b569b0bd6fcef1d71f852/gcc%2Ftestsuite%2Fgfortran.dg%2Ffunc_derived_1.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Ffunc_derived_1.f90?ref=cf73cdac51a20dacfc1b569b0bd6fcef1d71f852", "patch": "@@ -0,0 +1,38 @@\n+! { dg-do run }\n+! PR 17244\n+! verifies that functions returning derived type work\n+module m\n+  type t\n+     integer i\n+     real x\n+     character*5 c\n+     integer arr(5,5)\n+  end type t\n+end module m\n+\n+use m\n+type(t) :: r\n+integer arr(5,5), vect(25), vect2(25)\n+do i=1,25\n+   vect = 0\n+   vect(i) = i\n+   arr = reshape (vect, shape(arr))\n+   r = f(i,real(i),\"HALLO\",arr)\n+\n+   if (r%i .ne. i) call abort()\n+   if (r%x .ne. real(i)) call abort()\n+   if (r%c .ne. \"HALLO\") call abort()\n+   vect2 = reshape (r%arr, shape(vect2))\n+   if (any(vect2.ne.vect)) call abort()\n+end do\n+contains\n+\n+function f(i,x,c,arr)\n+  type(t) :: f\n+  character*5 c\n+  integer arr(5,5)\n+  \n+  f = t(i,x,c,arr)\n+end function f\n+\n+end"}]}