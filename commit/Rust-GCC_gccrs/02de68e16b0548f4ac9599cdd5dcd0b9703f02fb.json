funroll-loops\" } */\n+\n+extern void abort ();\n+\n+void\n+foo (int *a, int n)\n+{\n+  int *lasta = a + n;\n+  for (; a != lasta; a++)\n+    {\n+      *a *= 2;\n+      a[1] = a[-1] + a[-2];\n+    }\n+}\n+\n+int a[16];\n+int ref[16] = { 0, 1, 4, 2, 10, 12, 24, 44,\n+                72, 136, 232, 416, 736, 1296, 2304, 2032 };\n+\n+int\n+main ()\n+{\n+  int i;\n+  for (i = 0; i < 16; i++)\n+    a[i] = i;\n+  foo (a + 2, 16 - 3);\n+  for (i = 0; i < 16; i++)\n+    if (ref[i] != a[i])\n+      abort ();\n+  return 0;\n+}"}]}