/570d5bbc7b7c5f7eb0eb03660a93687a6698ae7e/gcc%2Ftestsuite%2Fgnat.dg%2Faddr13.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Faddr13.adb?ref=570d5bbc7b7c5f7eb0eb03660a93687a6698ae7e", "patch": "@@ -0,0 +1,9 @@\n+--  { dg-do compile }\n+\n+package body Addr13 is\n+   procedure Overlay is\n+      Over : Integer with Address => Gen_Obj'Address;\n+   begin\n+      Over := 123;\n+   end Overlay;\n+end Addr13;"}, {"sha": "f1f1646b9df2fdefb0aeeb58bcd5198db75be359", "filename": "gcc/testsuite/gnat.dg/addr13.ads", "status": "added", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/570d5bbc7b7c5f7eb0eb03660a93687a6698ae7e/gcc%2Ftestsuite%2Fgnat.dg%2Faddr13.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/570d5bbc7b7c5f7eb0eb03660a93687a6698ae7e/gcc%2Ftestsuite%2Fgnat.dg%2Faddr13.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Faddr13.ads?ref=570d5bbc7b7c5f7eb0eb03660a93687a6698ae7e", "patch": "@@ -0,0 +1,5 @@\n+generic\n+   Gen_Obj : in out Integer;\n+package Addr13 is\n+   procedure Overlay;\n+end Addr13;"}]}