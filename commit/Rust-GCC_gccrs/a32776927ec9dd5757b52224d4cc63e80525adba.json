%2Fvect%2Fpr48329.f90?ref=a32776927ec9dd5757b52224d4cc63e80525adba", "patch": "@@ -0,0 +1,29 @@\n+! { dg-do compile }\n+! { dg-require-effective-target vect_float }\n+! { dg-require-effective-target vect_intfloat_cvt }\n+! { dg-additional-options \"-ffast-math\" }\n+\n+program calcpi\n+\n+    implicit none\n+    real(kind=4):: h,x,sum,pi\n+    integer:: n,i\n+    real(kind=4):: f\n+\n+   f(x) = 4.0/(1.0+x**2)\n+\n+   n = 2100000000\n+\n+   h= 1.0 / n\n+   sum = 0.0\n+  DO i=1, n\n+     x = h * (i-0.5)\n+     sum = sum + f(x)\n+  END DO\n+  pi = h * sum\n+  write(*,*) 'Pi=',pi\n+\n+end program calcpi\n+\n+! { dg-final { scan-tree-dump-times \"vectorized 1 loops\" 1 \"vect\" } }\n+! { dg-final { cleanup-tree-dump \"vect\" } }"}]}