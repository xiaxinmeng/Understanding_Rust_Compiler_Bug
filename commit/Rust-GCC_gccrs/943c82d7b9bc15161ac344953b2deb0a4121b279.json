, "raw_url": "https://github.com/Rust-GCC/gccrs/raw/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Fada%2Fexp_ch4.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fexp_ch4.adb?ref=943c82d7b9bc15161ac344953b2deb0a4121b279", "patch": "@@ -4421,10 +4421,13 @@ package body Exp_Ch4 is\n \n    begin\n       --  Warn on the presence of an allocator of an anonymous access type when\n-      --  enabled.\n+      --  enabled except when its an object declaration at library level.\n \n       if Warn_On_Anonymous_Allocators\n         and then Ekind (PtrT) = E_Anonymous_Access_Type\n+        and then not (Is_Library_Level_Entity (PtrT)\n+                       and then Nkind (Associated_Node_For_Itype (PtrT)) =\n+                                  N_Object_Declaration)\n       then\n          Error_Msg_N (\"?use of an anonymous access type allocator\", N);\n       end if;"}, {"sha": "8ec5ec02440d30812480d704a9ec1e213acfc9d3", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=943c82d7b9bc15161ac344953b2deb0a4121b279", "patch": "@@ -1,3 +1,7 @@\n+2019-08-13  Justin Squirek  <squirek@adacore.com>\n+\n+\t* gnat.dg/anon3.adb, gnat.dg/anon3.ads: New testcase.\n+\n 2019-08-13  Eric Botcazou  <ebotcazou@adacore.com>\n \n \t* gnat.dg/generic_inst8.adb, gnat.dg/generic_inst8.ads,"}, {"sha": "862863359817bbfcf3655f4a3361935fad5bb078", "filename": "gcc/testsuite/gnat.dg/anon3.adb", "status": "added", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.adb?ref=943c82d7b9bc15161ac344953b2deb0a4121b279", "patch": "@@ -0,0 +1,6 @@\n+--  { dg-do compile }\n+--  { dg-options \"-gnatwa\" }\n+\n+package body Anon3 is\n+   procedure Dummy is null;\n+end Anon3;"}, {"sha": "39978c20188db2b6e423b3fde983f080315eb27a", "filename": "gcc/testsuite/gnat.dg/anon3.ads", "status": "added", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/943c82d7b9bc15161ac344953b2deb0a4121b279/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fanon3.ads?ref=943c82d7b9bc15161ac344953b2deb0a4121b279", "patch": "@@ -0,0 +1,4 @@\n+package Anon3 is\n+   X : access Integer := new Integer;\n+   procedure Dummy;\n+end Anon3;"}]}