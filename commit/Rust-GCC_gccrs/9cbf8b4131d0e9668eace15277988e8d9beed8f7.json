n+  /* Generate COMMON blocks.  */\n+  gfc_trans_common (ns);\n+\n   /* Create decls for all the module variables.  */\n   gfc_traverse_ns (ns, gfc_create_module_variable);\n }"}, {"sha": "99083c291adf8781b40dc400951ceee8a63a4d17", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9cbf8b4131d0e9668eace15277988e8d9beed8f7/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9cbf8b4131d0e9668eace15277988e8d9beed8f7/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=9cbf8b4131d0e9668eace15277988e8d9beed8f7", "patch": "@@ -1,3 +1,8 @@\n+2004-07-10  Tobias Schlueter  <tobias.schlueter@physik.uni-muenchen.de>\n+\t\n+\t* gfortran.fortran-torture/execute/common_2.f90: Add check for\n+\taccess to common var from module.\n+\n 2004-07-10  Tobias Schlueter  <tobias.schlueter@physik.uni-muenchen.de>\n \n \tPR fortran/16336"}, {"sha": "8bcdbb87a4fa232993e5a026d300b6bf00c0bf48", "filename": "gcc/testsuite/gfortran.fortran-torture/execute/common_2.f90", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9cbf8b4131d0e9668eace15277988e8d9beed8f7/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fexecute%2Fcommon_2.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9cbf8b4131d0e9668eace15277988e8d9beed8f7/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fexecute%2Fcommon_2.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.fortran-torture%2Fexecute%2Fcommon_2.f90?ref=9cbf8b4131d0e9668eace15277988e8d9beed8f7", "patch": "@@ -2,6 +2,10 @@\n MODULE bar\n INTEGER :: I\n COMMON /X/I\n+contains \n+subroutine set_i()\n+i = 5\n+end subroutine set_i\n END MODULE bar\n \n USE bar\n@@ -11,4 +15,6 @@ END MODULE bar\n i = 2\n if (j.ne.i) call abort()\n if (j.ne.2) call abo