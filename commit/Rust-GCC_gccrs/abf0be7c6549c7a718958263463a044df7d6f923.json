      cat << __EOF__ >$LIB/$file\n #ifndef _CURSES_H_WRAPPER\n #ifdef __cplusplus\n # define bool __curses_bool_t\n #endif\n-#include_next <curses.h>\n+#include_next <$file>\n #ifdef __cplusplus\n # undef bool\n #endif\n@@ -396,7 +396,7 @@ __EOF__\n       # Define _CURSES_H_WRAPPER at the end of the wrapper, not the start,\n       # so that if #include_next gets another instance of the wrapper,\n       # this will follow the #include_next chain until we arrive at\n-      # the real <curses.h>.\n+      # the real system include file.\n       chmod a+r $LIB/$file\n     fi\n   fi"}, {"sha": "5a1ca9f9ee1410d4bed1129d7469904d73f39049", "filename": "gcc/fixinc/fixinc.sco", "status": "modified", "additions": 2, "deletions": 2, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/abf0be7c6549c7a718958263463a044df7d6f923/gcc%2Ffixinc%2Ffixinc.sco", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/abf0be7c6549c7a718958263463a044df7d6f923/gcc%2Ffixinc%2Ffixinc.sco", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffixinc%2Ffixinc.sco?ref=abf0be7c6549c7a718958263463a044df7d6f923", "patch": "@@ -381,12 +381,12 @@ do\n     then\n       echo Fixed $file\n       rm -f $LIB/$file\n-      cat <<'__EOF__' >$LIB/$file\n+      cat <<__EOF__ >$LIB/$file\n #ifndef _CURSES_H_WRAPPER\n #ifdef __cplusplus\n # define bool __curses_bool_t\n #endif\n-#include_next <curses.h>\n+#include_next <$file>\n #ifdef __cplusplus\n # undef bool\n #endif"}]}