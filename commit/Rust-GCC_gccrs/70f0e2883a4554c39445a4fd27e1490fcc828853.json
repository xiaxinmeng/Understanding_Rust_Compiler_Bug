) != NULL_TREE)\n     POP_TIMEVAR_AND_RETURN (TV_NAME_LOOKUP, NULL_TREE);\n \n-  /* Recursively add all namespaces used.  */\n-  for (iter = DECL_NAMESPACE_USING (used); iter; iter = TREE_CHAIN (iter))\n-    push_using_directive (TREE_PURPOSE (iter));\n-\n   ancestor = namespace_ancestor (current_decl_namespace (), used);\n   ud = current_binding_level->using_directives;\n   ud = tree_cons (used, ancestor, ud);\n   current_binding_level->using_directives = ud;\n+\n+  /* Recursively add all namespaces used.  */\n+  for (iter = DECL_NAMESPACE_USING (used); iter; iter = TREE_CHAIN (iter))\n+    push_using_directive (TREE_PURPOSE (iter));\n+\n   POP_TIMEVAR_AND_RETURN (TV_NAME_LOOKUP, ud);\n }\n "}, {"sha": "f3642759717dc1dc98f538264f905988b4923cc5", "filename": "gcc/testsuite/g++.dg/lookup/using3.C", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/70f0e2883a4554c39445a4fd27e1490fcc828853/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Flookup%2Fusing3.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/70f0e2883a4554c39445a4fd27e1490fcc828853/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Flookup%2Fusing3.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Flookup%2Fusing3.C?ref=70f0e2883a4554c39445a4fd27e1490fcc828853", "patch": "@@ -0,0 +1,12 @@\n+// PR c++/9798\n+\n+namespace std { }\n+namespace STL { using namespace std; }\n+namespace std {\n+  using namespace STL;\n+}\n+namespace STL {\n+  struct A {\n+    void B() { using namespace std; }\n+  };\n+}"}]}