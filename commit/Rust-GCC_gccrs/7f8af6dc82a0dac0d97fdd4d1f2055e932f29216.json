ust-GCC/gccrs/raw/7f8af6dc82a0dac0d97fdd4d1f2055e932f29216/libstdc%2B%2B-v3%2Ftestsuite%2Fexperimental%2Fnet%2Finternet%2Fresolver%2Fops%2Flookup.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Ftestsuite%2Fexperimental%2Fnet%2Finternet%2Fresolver%2Fops%2Flookup.cc?ref=7f8af6dc82a0dac0d97fdd4d1f2055e932f29216", "patch": "@@ -97,15 +97,15 @@ test03()\n   std::error_code ec;\n   io_context ctx;\n   ip::tcp::resolver resolv(ctx);\n-  auto addrs = resolv.resolve(\"test.invalid\", \"http\", ec);\n+  auto addrs = resolv.resolve(\"test.invalid.\", \"http\", ec);\n   VERIFY( ec );\n   VERIFY( addrs.size() == 0 );\n   VERIFY( addrs.begin() == addrs.end() );\n   VERIFY( addrs.empty() );\n #if __cpp_exceptions\n   bool caught = false;\n   try {\n-    resolv.resolve(\"test.invalid\", \"http\");\n+    resolv.resolve(\"test.invalid.\", \"http\");\n   } catch (const std::system_error& e) {\n     caught = true;\n     VERIFY( e.code() == ec );"}]}