ch": "@@ -0,0 +1,32 @@\n+// PR c++/37806\n+\n+extern \"C\" int printf (const char *, ...);\n+\n+template <typename T>\n+struct S1\n+{\n+  typedef void (function_type)(int) const;\n+};\n+\n+\n+struct S2: public S1<int>\n+{\n+  virtual function_type f = 0;\n+};\n+\n+struct S3: public S2\n+{\n+  void \n+  f (int i) const\n+  {\n+    printf (\"Hello world: %d\\n\", i);\n+  }\n+};\n+\n+\n+int\n+main()\n+{\n+  S3 s;\n+  s.f(5);\n+}"}]}