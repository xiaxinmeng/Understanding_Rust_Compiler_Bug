\n+struct foo\n+{\n+ int *f;\n+ int i;\n+};\n+\n+int baz;\n+\n+void __attribute__ ((noinline))\n+bar (struct foo x)\n+{\n+ *(x.f) = x.i;\n+}\n+\n+int\n+main ()\n+{\n+  struct foo x = { &baz, 0xdeadbeef };\n+\n+  bar (x);\n+\n+  if (baz != 0xdeadbeef)\n+    abort ();\n+\n+  return 0;\n+}"}]}