tch": "@@ -9565,7 +9565,8 @@ maybe_resolve_dummy (tree object)\n \n   if (type != current_class_type\n       && current_class_type\n-      && LAMBDA_TYPE_P (current_class_type))\n+      && LAMBDA_TYPE_P (current_class_type)\n+      && DERIVED_FROM_P (type, current_nonlambda_class_type ()))\n     {\n       /* In a lambda, need to go through 'this' capture.  */\n       tree lam = CLASSTYPE_LAMBDA_EXPR (current_class_type);"}, {"sha": "736d5f50e55cd3fbdcdc3558eb34340c7f9a4db4", "filename": "gcc/testsuite/g++.dg/cpp0x/lambda/lambda-this16.C", "status": "added", "additions": 28, "deletions": 0, "changes": 28, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f02296ddb9d4a183b90600659341baf91436f7d9/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this16.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f02296ddb9d4a183b90600659341baf91436f7d9/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this16.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-this16.C?ref=f02296ddb9d4a183b90600659341baf91436f7d9", "patch": "@@ -0,0 +1,28 @@\n+// PR c++/56699\n+// { dg-require-effective-target c++11 }\n+\n+struct A\n+{\n+    int a;\n+};\n+\n+struct T\n+{\n+    int x;\n+\n+    T() : x([]{\n+        sizeof(::A::a);\n+        return 0;\n+    }())\n+    {}\n+};\n+\n+struct B\n+{\n+    int a;\n+};\n+\n+void f()\n+{\n+    []{sizeof(B::a);};\n+}"}]}