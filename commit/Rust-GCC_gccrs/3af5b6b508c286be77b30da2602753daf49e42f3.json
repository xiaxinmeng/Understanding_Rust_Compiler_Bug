LINK) mshlink\n+LIBS    = $(ARLIB) $(ARLINK) $(SHLIB) $(SHLINK) mshlink\n SHFLAGS = -nostdlib -Wl,-Bshareable,-Bforcearchive\n SHDEPS  = -lm\n DEPLIBS = ../$(SHLIB)"}]}