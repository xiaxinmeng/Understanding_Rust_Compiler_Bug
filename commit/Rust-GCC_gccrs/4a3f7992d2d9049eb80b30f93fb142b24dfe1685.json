+/89851\n+\n+void\n+test01()\n+{\n+  using V = std::variant<int, int>;\n+  V v1{std::in_place_index<0>, 0};\n+  V v2{std::in_place_index<1>, 0};\n+  VERIFY(   v1 != v2  );\n+  VERIFY( !(v1 == v2) );\n+}\n+\n+int\n+main()\n+{\n+  test01();\n+}"}]}