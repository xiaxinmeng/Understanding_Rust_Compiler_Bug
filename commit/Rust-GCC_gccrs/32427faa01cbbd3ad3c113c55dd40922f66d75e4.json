aw/32427faa01cbbd3ad3c113c55dd40922f66d75e4/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38048-1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38048-1.c?ref=32427faa01cbbd3ad3c113c55dd40922f66d75e4", "patch": "@@ -0,0 +1,22 @@\n+extern void abort(void);\n+\n+int foo ()\n+{\n+  int mat[2][1];\n+  int (*a)[1] = mat;\n+  int det = 0;\n+  int i;\n+  mat[0][0] = 1;\n+  mat[1][0] = 2;\n+  for (i = 0; i < 2; ++i)\n+    det += a[i][0];\n+  return det;\n+}\n+\n+int main()\n+{\n+  if (foo () != 3)\n+    abort ();\n+  return 0;\n+}\n+"}, {"sha": "f4fe40cac8768b1ca8e97fa23d50191c1af1de8d", "filename": "gcc/testsuite/gcc.c-torture/execute/pr38048-2.c", "status": "added", "additions": 28, "deletions": 0, "changes": 28, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/32427faa01cbbd3ad3c113c55dd40922f66d75e4/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38048-2.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/32427faa01cbbd3ad3c113c55dd40922f66d75e4/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38048-2.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.c-torture%2Fexecute%2Fpr38048-2.c?ref=32427faa01cbbd3ad3c113c55dd40922f66d75e4", "patch": "@@ -0,0 +1,28 @@\n+extern void abort (void);\n+\n+static int inv_J(int a[][2])\n+{\n+  int i, j;\n+  int det = 0.0;\n+   for (j=0; j<2; ++j)\n+     det += a[j][0] + a[j][1];\n+  return det;\n+}\n+\n+int foo()\n+{\n+  int mat[2][2];\n+  mat[0][0] = 1;\n+  mat[0][1] = 2;\n+  mat[1][0] = 4;\n+  mat[1][1] = 8;\n+  return inv_J(mat);\n+}\n+\n+int main()\n+{\n+  if (foo () != 15)\n+    abort ();\n+  return 0;\n+}\n+"}, {"sha": "85c2b5072ae4e6cbaff02f7f074fff782786a6c8", "filename": "gcc/testsuite/gcc.dg/torture/pr37868.c", "status": "added", "additions": 28, "deletions": 0, "changes": 28, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/32427faa01cbbd3ad3c113c55dd40922f66d75e4/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr37868.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/32427faa01cbbd3ad3c113c55dd40922f66d75e4/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr37868.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr37868.c?ref=32427faa01cbbd3ad3c113c55dd40922f66d75e4", "patch": "@@ -0,0 +1,28 @@\n+/* { dg-do run } */\n+/* { dg-options \"-fno-strict-aliasing\" } */\n+\n+extern void abort (void);\n+\n+struct X {\n+  unsigned char pad : 4;\n+  unsigned int a : 32;\n+  unsigned int b : 24;\n+  unsigned int c : 6;\n+} __attribute__((packed));\n+\n+int main (void)\n+{\n+  struct X x;\n+  unsigned int bad_bits;\n+\n+  x.pad = -1;\n+  x.a = -1;\n+  x.b = -1;\n+  x.c = -1;\n+\n+  bad_bits = ((unsigned int)-1) ^ *(1+(unsigned int *) &x);\n+  if (bad_bits != 0)\n+    abort ();\n+  return 0;\n+}\n+"}]}