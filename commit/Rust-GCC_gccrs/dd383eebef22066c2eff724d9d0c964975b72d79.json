k is\n+\n+     entry Pick_Up when not Busy is\n+     begin\n+        Busy := True;\n+     end Pick_Up;\n+\n+     procedure Put_Down is\n+     begin\n+        Busy := False;\n+     end Put_Down;\n+   end Chopstick;\n+end sync1;"}, {"sha": "81c2f4aabebbe6c9f7ca30058ff8fc616f0ed5da", "filename": "gcc/testsuite/gnat.dg/sync1.ads", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/dd383eebef22066c2eff724d9d0c964975b72d79/gcc%2Ftestsuite%2Fgnat.dg%2Fsync1.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/dd383eebef22066c2eff724d9d0c964975b72d79/gcc%2Ftestsuite%2Fgnat.dg%2Fsync1.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fsync1.ads?ref=dd383eebef22066c2eff724d9d0c964975b72d79", "patch": "@@ -0,0 +1,12 @@\n+package sync1 is\n+   type Chopstick_Type is synchronized interface;\n+   \n+   type Chopstick is new Chopstick_Type with private;\n+private\n+   protected type Chopstick is new Chopstick_Type with\n+      entry Pick_Up;\n+      procedure Put_Down;\n+   private\n+      Busy : Boolean := False;\n+   end Chopstick;\n+end sync1;"}]}