\t  vargs.quick_push (arg);\n+\t  {\n+\t    tree tmp = arg;\n+\t    if (!is_gimple_val (arg))\n+\t      {\n+\t\ttmp = create_tmp_reg (TYPE_MAIN_VARIANT\n+\t\t\t\t      (TREE_TYPE (arg)), \"arg\");\n+\t\tgimple stmt = gimple_build_assign (tmp, arg);\n+\t\tgsi_insert_after (&bsi, stmt, GSI_NEW_STMT);\n+\t      }\n+\t    vargs.quick_push (tmp);\n+\t  }\n       call = gimple_build_call_vec (build_fold_addr_expr_loc (0, alias), vargs);\n       node->callees->call_stmt = call;\n       gimple_call_set_from_thunk (call, true);"}, {"sha": "42cdf8700811ebb966ec90f90602b0381119ab67", "filename": "gcc/testsuite/g++.dg/inherit/covariant21.C", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/fc044323f492ebb6913398604104f293df99dbe3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finherit%2Fcovariant21.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/fc044323f492ebb6913398604104f293df99dbe3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finherit%2Fcovariant21.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Finherit%2Fcovariant21.C?ref=fc044323f492ebb6913398604104f293df99dbe3", "patch": "@@ -0,0 +1,17 @@\n+// PR c++/59645\n+\n+struct A { virtual ~A(); };\n+struct B { virtual ~B(); };\n+struct C : A, B {};\n+\n+struct X\n+{\n+  virtual B* foo(volatile int);\n+};\n+\n+struct Y : X\n+{\n+  virtual C* foo(volatile int);\n+};\n+\n+C* Y::foo(volatile int) { return 0; }"}]}