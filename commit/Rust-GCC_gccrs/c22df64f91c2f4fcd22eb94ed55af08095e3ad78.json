t_field_seq (void)\n     {\n       ty = type (&opts, true);\n \n+      /* Ignore access-control keywords (\"public:\" etc).  */\n+      while (!ty && token () == IGNORABLE_CXX_KEYWORD)\n+\t{\n+\t  const char *keyword = advance ();\n+\t  if (strcmp (keyword, \"public:\") != 0\n+\t      && strcmp (keyword, \"private:\") != 0\n+\t      && strcmp (keyword, \"protected:\") != 0)\n+\t    break;\n+\t  ty = type (&opts, true);\n+\t}\n+\n       if (!ty || token () == ':')\n \t{\n \t  consume_until_eos ();"}]}