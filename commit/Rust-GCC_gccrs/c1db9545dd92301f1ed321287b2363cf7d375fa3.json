st-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=c1db9545dd92301f1ed321287b2363cf7d375fa3", "patch": "@@ -1,3 +1,8 @@\n+2008-06-08  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/36459\n+\t* gfortran.dg/proc_decl_16.f90: New.\n+\t\n 2008-06-08  Tobias Burnus  <burnus@net-b.de>\n \n \tPR fortran/35830"}, {"sha": "3251e52f512adace55fc6ee1617c96ea5ca1aa5a", "filename": "gcc/testsuite/gfortran.dg/proc_decl_16.f90", "status": "added", "additions": 21, "deletions": 0, "changes": 21, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c1db9545dd92301f1ed321287b2363cf7d375fa3/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_decl_16.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c1db9545dd92301f1ed321287b2363cf7d375fa3/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_decl_16.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_decl_16.f90?ref=c1db9545dd92301f1ed321287b2363cf7d375fa3", "patch": "@@ -0,0 +1,21 @@\n+! { dg-do compile }\n+! PR fortran/36459\n+!\n+abstract interface\n+  function dim()\n+    integer :: dim\n+  end function dim\n+end interface\n+procedure(dim) :: f\n+\n+interface\n+  integer function tan()\n+  end function\n+end interface\n+procedure(tan) :: g\n+\n+print *, f()\n+\n+print *, tan()\n+\n+end"}]}