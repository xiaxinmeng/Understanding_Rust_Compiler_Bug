s.  */\n \t      if (rhstype == unknown_type_node)\n-\t\tinstantiate_type (type, rhs, tf_warning_or_error);\n+\t\t{\n+\t\t  tree r = instantiate_type (type, rhs, tf_warning_or_error);\n+\t\t  /* -fpermissive might allow this.  */\n+\t\t  if (!seen_error ())\n+\t\t    return r;\n+\t\t}\n \t      else if (fndecl)\n \t\terror (\"cannot convert %qT to %qT for argument %qP to %qD\",\n \t\t       rhstype, type, parmnum, fndecl);"}, {"sha": "37b26895e4b0414096048e0cf43ed833a5da0448", "filename": "gcc/testsuite/g++.dg/parse/ptrmem7.C", "status": "added", "additions": 16, "deletions": 0, "changes": 16, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/559a77e112285e37f34e4ef2c1eec89771138c3b/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Fptrmem7.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/559a77e112285e37f34e4ef2c1eec89771138c3b/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Fptrmem7.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Fptrmem7.C?ref=559a77e112285e37f34e4ef2c1eec89771138c3b", "patch": "@@ -0,0 +1,16 @@\n+// PR c++/80043\n+// { dg-options -fpermissive }\n+\n+struct A\n+{\n+  template<int> void foo()\n+  {\n+    void (A::* fp)();\n+    fp = A::foo<0>;\t\t// { dg-warning \"assuming pointer to member\" }\n+  }\n+};\n+\n+void bar()\n+{\n+  A().foo<0>();\n+}"}]}