S\n+if ![info exists DEFAULT_DFLAGS] then {\n+    set DEFAULT_DFLAGS \"\"\n }\n \n # Initialize `dg'.\n dg-init\n \n-# Initialize use of torture lists.\n-torture-init\n-set-torture-options $TORTURE_OPTIONS\n-\n # Main loop.\n gdc-dg-runtest [lsort \\\n-       [glob -nocomplain $srcdir/$subdir/*.d ] ] \"\" \"\"\n-\n-# Finalize use of torture lists.\n-torture-finish\n+       [glob -nocomplain $srcdir/$subdir/*.d ] ] \"\" $DEFAULT_DFLAGS\n \n # All done.\n dg-finish"}, {"sha": "79693e8d32960a9e088b6caa055b40a52776e2a1", "filename": "gcc/testsuite/gdc.dg/torture/gdc115.d", "status": "renamed", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/72ddef620b2dec03bb81d5784d15f00dc08c9a3e/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc115.d", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/72ddef620b2dec03bb81d5784d15f00dc08c9a3e/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc115.d", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc115.d?ref=72ddef620b2dec03bb81d5784d15f00dc08c9a3e", "patch": "@@ -1,5 +1,5 @@\n // https://bugzilla.gdcproject.org/show_bug.cgi?id=115\n-// { dg-do run { target hw } }\n+// { dg-do run }\n // { dg-skip-if \"needs gcc/config.d\" { ! d_runtime } }\n \n void main()", "previous_filename": "gcc/testsuite/gdc.dg/gdc115.d"}, {"sha": "63502ffceaeb6f95afc0e6fcd562482da8690dc4", "filename": "gcc/testsuite/gdc.dg/torture/gdc131.d", "status": "renamed", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/72ddef620b2dec03bb81d5784d15f00dc08c9a3e/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc131.d", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/72ddef620b2dec03bb81d5784d15f00dc08c9a3e/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc131.d", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgdc.dg%2Ftorture%2Fgdc131.d?ref=72ddef620b2dec03bb81d5784d15f00dc08c9a3e", "patch": "@@ -1,5 +1,5 @@\n // 