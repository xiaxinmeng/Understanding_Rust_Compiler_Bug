@@ -549,6 +549,7 @@ simple_empty_class_p (tree type, tree op)\n   return\n     ((TREE_CODE (op) == COMPOUND_EXPR\n       && simple_empty_class_p (type, TREE_OPERAND (op, 1)))\n+     || TREE_CODE (op) == EMPTY_CLASS_EXPR\n      || is_gimple_lvalue (op)\n      || INDIRECT_REF_P (op)\n      || (TREE_CODE (op) == CONSTRUCTOR"}, {"sha": "b223ee24d44df745aa4930c6fffef75ac95c7c6d", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/30d309aaf14343dc0e9f00c9cf37075afdfba905/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/30d309aaf14343dc0e9f00c9cf37075afdfba905/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=30d309aaf14343dc0e9f00c9cf37075afdfba905", "patch": "@@ -1,3 +1,8 @@\n+2017-03-03  Richard Biener  <rguenther@suse.de>\n+\n+\tPR c++/79825\n+\t* g++.dg/warn/Wuninitialized-8.C: New testcase.\n+\n 2017-03-03  Jakub Jelinek  <jakub@redhat.com>\n \n \t* gcc.dg/pr57134.c: Use empty inline asm string literal."}, {"sha": "cb9cfee6cabc3a8e1a3551a31838a726e1086a2a", "filename": "gcc/testsuite/g++.dg/warn/Wuninitialized-8.C", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/30d309aaf14343dc0e9f00c9cf37075afdfba905/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWuninitialized-8.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/30d309aaf14343dc0e9f00c9cf37075afdfba905/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWuninitialized-8.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fwarn%2FWuninitialized-8.C?ref=30d309aaf14343dc0e9f00c9cf37075afdfba905", "patch": "@@ -0,0 +1,18 @@\n+// { dg-do compile }\n+// { dg-options \"-Wuninitialized\" }\n+\n+struct A;\n+struct B\n+{\n+  B(A);\n+};\n+struct C\n+{\n+  template <typename PassT> void m_fn1(PassT p1) { new B(p1); }  // { dg-bogus \"uninitialized\" }\n+};\n+struct A {};\n+void fn1()\n+{\n+  C a;\n+  a.m_fn1(A());\n+}"}]}