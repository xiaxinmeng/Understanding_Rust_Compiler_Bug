86.c?ref=c95f9494fe67feaccfbb33f7d066f21427ce8ed6", "patch": "@@ -9307,7 +9307,13 @@ ix86_compute_frame_layout (struct ix86_frame *frame)\n   offset += frame->va_arg_size;\n \n   /* Align start of frame for local function.  */\n-  offset = (offset + stack_alignment_needed - 1) & -stack_alignment_needed;\n+  if (stack_realign_fp\n+      || offset != frame->sse_reg_save_offset\n+      || size != 0\n+      || !current_function_is_leaf\n+      || cfun->calls_alloca\n+      || ix86_current_function_calls_tls_descriptor)\n+    offset = (offset + stack_alignment_needed - 1) & -stack_alignment_needed;\n \n   /* Frame pointer points here.  */\n   frame->frame_pointer_offset = offset;"}, {"sha": "1c15f633825bed6656dc66f81b38d59e89c79c82", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c95f9494fe67feaccfbb33f7d066f21427ce8ed6/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c95f9494fe67feaccfbb33f7d066f21427ce8ed6/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=c95f9494fe67feaccfbb33f7d066f21427ce8ed6", "patch": "@@ -1,3 +1,8 @@\n+2011-02-15  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR middle-end/47581\n+\t* gcc.target/i386/pr47581.c: New test.\n+\n 2011-02-14  Janus Weil  <janus@gcc.gnu.org>\n \n \tPR fortran/47730"}, {"sha": "dfc02a14478297174d6f700b2aaee5ac8427097b", "filename": "gcc/testsuite/gcc.target/i386/pr47581.c", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c95f9494fe67feaccfbb33f7d066f21427ce8ed6/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr47581.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c95f9494fe67feaccfbb33f7d066f21427ce8ed6/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr47581.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2