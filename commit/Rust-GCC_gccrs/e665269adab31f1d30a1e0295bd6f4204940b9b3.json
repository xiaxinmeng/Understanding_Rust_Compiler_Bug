n+    {\n+      gimple use_stmt = USE_STMT (use_p);\n+      if (!is_gimple_debug (use_stmt)\n+\t  && loop != loop_containing_stmt (use_stmt))\n+\treturn true;\n+    }\n \n   return false;\n }"}]}