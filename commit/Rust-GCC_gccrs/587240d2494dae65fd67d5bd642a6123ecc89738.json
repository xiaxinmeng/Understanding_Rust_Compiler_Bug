18 @@\n+/* { dg-do compile } */\n+/* { dg-additional-options \"-Wno-psabi -w\" } */\n+\n+typedef unsigned V __attribute__ ((vector_size (8)));\n+V\n+foo (unsigned x, V v)\n+{\n+  do {\n+      v %= x;\n+      x = 1;\n+  } while (v[1]);\n+  return v;\n+}\n+void fn2 ()\n+{\n+  V x = foo (5, (V) { 0, 1 });\n+  if (x[0] || x[1] || x[2] || x[3]);\n+}"}]}