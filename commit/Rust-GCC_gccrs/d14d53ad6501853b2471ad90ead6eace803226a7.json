 (parser,\n \t\t\t\t\t    /*allow_non_constant_p=*/false,\n \t\t\t\t\t    /*non_constant_p=*/NULL);\n-  argument = fold_non_dependent_expr (argument);\n   if (!maybe_type_id)\n     return argument;\n   if (!cp_parser_next_token_ends_template_argument_p (parser))"}, {"sha": "7adcae83abd4db0d8c4304f208e11c04631823af", "filename": "gcc/testsuite/g++.dg/cpp0x/constexpr-template4.C", "status": "added", "additions": 26, "deletions": 0, "changes": 26, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d14d53ad6501853b2471ad90ead6eace803226a7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-template4.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d14d53ad6501853b2471ad90ead6eace803226a7/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-template4.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-template4.C?ref=d14d53ad6501853b2471ad90ead6eace803226a7", "patch": "@@ -0,0 +1,26 @@\n+// PR c++/55931\n+// { dg-do compile { target c++11 } }\n+\n+#include <type_traits>\n+\n+template<typename Type>\n+class Test\n+{\n+    public:\n+        constexpr Test(const Type val) : _value(val) {}\n+        constexpr Type get() const {return _value;}\n+        static void test()\n+        {\n+            static constexpr Test<int> x(42);\n+            std::integral_constant<int, x.get()> i; // This is not working\n+        }\n+    protected:\n+        Type _value;\n+};\n+\n+int main()\n+{\n+    static constexpr Test<int> x(42);\n+    std::integral_constant<int, x.get()> i; // This is working\n+    Test<double>::test();\n+}"}]}