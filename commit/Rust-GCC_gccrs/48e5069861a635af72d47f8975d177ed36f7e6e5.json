39417.C?ref=48e5069861a635af72d47f8975d177ed36f7e6e5", "patch": "@@ -0,0 +1,56 @@\n+// { dg-do run }\n+\n+#include <vector>\n+\n+std::vector <int>\n+sequence(int l, int n)\n+{\n+  std::vector <int> ret;\n+  for(int i=n;i<=100;i++)\n+    {\n+      if(i%2==0)\n+\t{\n+\t  if(l%i==i/2)\n+\t    {\n+\t      int init =l/i-i/2+1;\n+\t      if(init>=0)\n+\t\t{\n+\t\t  for(int j=0;j<i;j++)\n+\t\t    {\n+\t\t      ret.push_back(init);\n+\t\t      init ++;\n+\t\t    }\n+\t\t  break;\n+\t\t}\n+\t    }\n+\t}\n+      else\n+\t{\n+\t  if(l%i==0)\n+\t    {\n+\t      int init =l/i-i/2;\n+\t      if(init>=0)\n+\t\t{\n+\t\t  for(int j=0;j<i;j++)\n+\t\t    {\n+\t\t      ret.push_back(init);\n+\t\t      init ++;\n+\t\t    }\n+\t\t  break;\n+\t\t}\n+\t    }\n+\t}\n+    }\n+  return ret;\n+}\n+extern \"C\" void abort (void);\n+int main()\n+{\n+  std::vector<int> res = sequence(18, 2);\n+  if (res.size () != 3\n+      || res[0] != 5\n+      || res[1] != 6\n+      || res[2] != 7)\n+    abort ();\n+  return 0;\n+}"}]}