CC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=9b63dcab1e9c9243604c8762ff9145951e485b9e", "patch": "@@ -1,3 +1,8 @@\n+2012-04-15  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/51082\n+\t* gfortran.dg/proc_ptr_comp_34.f90: New test case.\n+\n 2012-04-14  Tobias Burnus  <burnus@net-b.de>\n \n \tPR fortran/52916"}, {"sha": "031f74418ca33e289767a6084b212159930486b3", "filename": "gcc/testsuite/gfortran.dg/proc_ptr_comp_34.f90", "status": "added", "additions": 30, "deletions": 0, "changes": 30, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/9b63dcab1e9c9243604c8762ff9145951e485b9e/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_34.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/9b63dcab1e9c9243604c8762ff9145951e485b9e/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_34.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_comp_34.f90?ref=9b63dcab1e9c9243604c8762ff9145951e485b9e", "patch": "@@ -0,0 +1,30 @@\n+! { dg-do run }\n+!\n+! PR 51082: [F03] Wrong result for a pointer to a proc-pointer component\n+!\n+! Contributed by Tobias Burnus <burnus@gcc.gnu.org>\n+\n+program ala\n+  implicit none\n+\n+  type process_list\n+    procedure(ala1), pointer, nopass :: process\n+  end type\n+\n+  type(process_list), target  :: p_list\n+  type(process_list), pointer :: p\n+\n+  p_list%process => ala1\n+  p => p_list\n+\n+   write(*,*) p_list%process(1.0)\n+   write(*,*) p%process(1.0)       !!!! failed\n+\n+contains\n+\n+  real function ala1(x)\n+    real, intent(in) :: x\n+    ala1 = x\n+  end function\n+\n+end program"}]}