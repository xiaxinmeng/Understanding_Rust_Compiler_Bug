w/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ffortran%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2FChangeLog?ref=4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174", "patch": "@@ -1,3 +1,9 @@\n+2016-12-03  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/43207\n+\t* primary.c (gfc_match_varspec): Reject nonpolymorphic references to\n+\tabstract types.\n+\n 2016-12-03  Janus Weil  <janus@gcc.gnu.org>\n \n \tPR fortran/42188"}, {"sha": "f13b0f02f15f896a8b47cccbd2a800539de4e28f", "filename": "gcc/fortran/primary.c", "status": "modified", "additions": 9, "deletions": 1, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ffortran%2Fprimary.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ffortran%2Fprimary.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Fprimary.c?ref=4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174", "patch": "@@ -2222,7 +2222,15 @@ gfc_match_varspec (gfc_expr *primary, int equiv_flag, bool sub_flag,\n \t}\n     }\n \n-  /* F2008, C727.  */\n+  /* F08:C611.  */\n+  if (primary->ts.type == BT_DERIVED && primary->ref\n+      && primary->ts.u.derived && primary->ts.u.derived->attr.abstract)\n+    {\n+      gfc_error (\"Nonpolymorphic reference to abstract type at %C\");\n+      return MATCH_ERROR;\n+    }\n+\n+  /* F08:C727.  */\n   if (primary->expr_type == EXPR_PPC && gfc_is_coindexed (primary))\n     {\n       gfc_error (\"Coindexed procedure-pointer component at %C\");"}, {"sha": "84fc6fa83bc65ddae44c1b2076fc0ec4f4ed742a", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174", "patch": "@@ -1,3 +1,8 @@\n+2016-12-03  Janus Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/43207\n+\t* gfortran.dg/abstract_type_9.f90: New test case.\n+\n 2016-12-03  Janus Weil  <janus@gcc.gnu.org>\n \n \tPR fortran/42188"}, {"sha": "77d48ba61f5a73440225f3c1cf420aa9d0abdcdf", "filename": "gcc/testsuite/gfortran.dg/abstract_type_9.f90", "status": "added", "additions": 34, "deletions": 0, "changes": 34, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ftestsuite%2Fgfortran.dg%2Fabstract_type_9.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174/gcc%2Ftestsuite%2Fgfortran.dg%2Fabstract_type_9.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fabstract_type_9.f90?ref=4ceda20498ec82d0fd24a2ab1e56c6c1e38b0174", "patch": "@@ -0,0 +1,34 @@\n+! { dg-do compile }\n+!\n+! PR 43207: [OOP] invalid (pointer) assignment to and from abstract non-polymorphic expressions\n+!\n+! Contributed by Tobias Burnus <burnus@gcc.gnu.org>\n+\n+  implicit none\n+  type, abstract :: parent\n+    integer :: i\n+  end type\n+  type, extends(parent) :: child\n+    class(parent), pointer :: comp\n+  end type\n+\n+  type(child), target :: c1\n+  class(child), allocatable :: c2\n+  class(parent), pointer :: cp\n+\n+  c1%parent = c1%parent  ! { dg-error \"Nonpolymorphic reference to abstract type\" }\n+  c2%parent = c1%parent  ! { dg-error \"Nonpolymorphic reference to abstract type\" }\n+\n+  cp => c1%comp\n+  cp => c1%parent        ! { dg-error \"Nonpolymorphic reference to abstract type\" }\n+\n+  call sub(c1%comp)\n+  call sub(c1%parent)    ! { dg-error \"Nonpolymorphic reference to abstract type\" }\n+\n+contains\n+\n+  subroutine sub(arg)\n+    class(parent) :: arg\n+  end subroutine\n+\n+end"}]}