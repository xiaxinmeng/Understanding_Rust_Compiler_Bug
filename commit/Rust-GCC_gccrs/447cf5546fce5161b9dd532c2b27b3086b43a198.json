\n \t  && variably_modified_type_p (TREE_TYPE (type), NULL_TREE))\n \tinform (input_location, \"because the array element type %qT has \"\n \t\t\"variable size\", TREE_TYPE (type));\n+      type = error_mark_node;\n     }\n   else if (by_reference_p)\n     {"}, {"sha": "ea5c4d8eedb0ee10955cb164efc27e676629ca09", "filename": "gcc/testsuite/g++.dg/cpp1y/vla9.C", "status": "added", "additions": 31, "deletions": 0, "changes": 31, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/447cf5546fce5161b9dd532c2b27b3086b43a198/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Fvla9.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/447cf5546fce5161b9dd532c2b27b3086b43a198/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Fvla9.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Fvla9.C?ref=447cf5546fce5161b9dd532c2b27b3086b43a198", "patch": "@@ -0,0 +1,31 @@\n+// PR c++/57408\n+// { dg-options \"-std=c++1y -pedantic-errors\" }\n+\n+template<typename Callable>\n+  struct Impl\n+  {\n+    Callable func;\n+    Impl(Callable f) : func(f) { }\n+    virtual void run() { func(); }\n+  };\n+\n+template<typename Callable>\n+void call(Callable f)\n+  {\n+    Impl<Callable>(f).run();\n+  }\n+\n+extern \"C\" int printf(const char*, ...);\n+\n+int main(){\n+    int y = 2;\n+    float fa[2][y];\t    // { dg-error \"array of array of runtime bound\" }\n+    fa[0][0]=0.8;\n+    fa[0][1]=1.8;\n+    auto fx=[&](){\n+        for(int c=0; c<2; c++){\n+            printf(\"use me\", fa[0][c]);\t// { dg-error \"capture of variable-size type\" }\n+        }\n+    };\n+    call(fx);\n+}"}]}