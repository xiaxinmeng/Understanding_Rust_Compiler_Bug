UE (pack)\n               && (pargs = ARGUMENT_PACK_EXPLICIT_ARGS (TREE_VALUE (pack)))\n-              && (i < TREE_VEC_LENGTH (pargs)))\n+              && (i - start < TREE_VEC_LENGTH (pargs)))\n             {\n               any_explicit = true;\n-              arg = TREE_VEC_ELT (pargs, i);\n+              arg = TREE_VEC_ELT (pargs, i - start);\n             }\n           TMPL_ARG (targs, level, idx) = arg;\n         }"}, {"sha": "4a80745293a7716f7c54234da9645f2c8f79cf29", "filename": "gcc/testsuite/g++.dg/cpp0x/variadic-explicit2.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4185fb7350876f0012455c3dd159f80494081686/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic-explicit2.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4185fb7350876f0012455c3dd159f80494081686/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic-explicit2.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic-explicit2.C?ref=4185fb7350876f0012455c3dd159f80494081686", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/56774\n+// { dg-require-effective-target c++11 }\n+\n+template <class ... Args>\n+struct mytype {};\n+\n+template <class T, class ... Args>\n+void something( mytype<T, Args...> )\n+{ }\n+\n+int main()\n+{\n+  something<int, char, bool>( mytype<int, char, bool>() );\n+}"}]}