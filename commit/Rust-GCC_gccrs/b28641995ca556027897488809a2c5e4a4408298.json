/* { dg-options \"-Os -ftree-loop-linear\" } */\n+\n+#define N 16\n+\n+int\n+main1 (int n, int *a)\n+{\n+  int i, j;\n+\n+  for (i = 0; i < n; i++)\n+    for (j = 0; j < n; j++)\n+      a[j] = i + n;\n+\n+  for (j = 0; j < n; j++)\n+    if (a[j] != i + n - 1)\n+      __builtin_abort ();\n+\n+  return 0;\n+}\n+\n+int\n+main ()\n+{\n+  int a[N];\n+  main1 (N, a);\n+  return 0;\n+}"}]}