ontents/gcc%2Ftree-chrec.h?ref=105e8c0682d220f6abc7320ff1ae05e20136080a", "patch": "@@ -117,7 +117,7 @@ no_evolution_in_loop_p (tree chrec, unsigned loop_num, bool *res)\n \n   STRIP_NOPS (chrec);\n   scev = hide_evolution_in_other_loops_than_loop (chrec, loop_num);\n-  *res = !tree_is_chrec (scev);\n+  *res = !tree_contains_chrecs (scev, NULL);\n   return true;\n }\n "}]}