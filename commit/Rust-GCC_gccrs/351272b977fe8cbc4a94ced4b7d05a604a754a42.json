nts{/privacy}", "received_events_url": "https://api.github.com/users/hainque/received_events", "type": "User", "site_admin": false}, "committer": null, "parents": [{"sha": "fc87142b855651a57828a070f617b6067d045aaa", "url": "https://api.github.com/repos/Rust-GCC/gccrs/commits/fc87142b855651a57828a070f617b6067d045aaa", "html_url": "https://github.com/Rust-GCC/gccrs/commit/fc87142b855651a57828a070f617b6067d045aaa"}], "stats": {"total": 42, "additions": 42, "deletions": 0}, "files": [{"sha": "929e76f5347e57b8bed59e682d3731f1743941f5", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/351272b977fe8cbc4a94ced4b7d05a604a754a42/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/351272b977fe8cbc4a94ced4b7d05a604a754a42/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=351272b977fe8cbc4a94ced4b7d05a604a754a42", "patch": "@@ -1,3 +1,7 @@\n+2006-09-14  Olivier Hainque  <hainque@adacore.com>\n+\n+\t* gnat.dg/in_out_parameter.adb: New test.\n+\n 2006-09-13  Andrew Pinski  <pinskia@physics.uc.edu>\n \n \tPR debug/28980"}, {"sha": "c936ec1e7e6f2e9d516d39fa03796bcf07a06a9d", "filename": "gcc/testsuite/gnat.dg/in_out_parameter.adb", "status": "added", "additions": 38, "deletions": 0, "changes": 38, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/351272b977fe8cbc4a94ced4b7d05a604a754a42/gcc%2Ftestsuite%2Fgnat.dg%2Fin_out_parameter.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/351272b977fe8cbc4a94ced4b7d05a604a754a42/gcc%2Ftestsuite%2Fgnat.dg%2Fin_out_parameter.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fin_out_parameter.adb?ref=351272b977fe8cbc4a94ced4b7d05a604a754a42", "patch": "@@ -0,0 +1,38 @@\n+-- { dg-do run }\n+\n+with Ada.Streams.Stream_IO;\n+\n+procedure In_Out_Parameter is\n+\n+   use Ada.Streams;  use Stream_IO;\n+\n+   File : Stream_IO.File_Type;\n+\n+   type Bitmap is array (Natural range <>) of Boolean;\n+   for Bitmap'Component_Size use 1;\n+\n+   type Message   is record\n+      B : Bitmap (0 .. 14);\n+   end record;\n+   for Message use record\n+      B  at 0 range 2 .. 16;\n+   end record;\n+\n+   TX, RX : Message;\n+\n+begin\n+\n+   TX.B  := (others => False);\n+   Stream_IO.Create (File => File, Mode => Out_File, Name => \"data\");\n+   Message'Output (Stream (File), TX);\n+   Stream_IO.Close (File);\n+   --\n+   Stream_IO.Open (File => File, Mode => In_File, Name => \"data\");\n+   RX := Message'Input (Stream (File));\n+   Stream_IO.Close (File);\n+\n+   if RX /= TX then\n+      raise Program_Error;\n+   end if;\n+\n+end In_Out_Parameter;"}]}