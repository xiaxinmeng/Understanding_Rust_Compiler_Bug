e args, tsubst_flags_t complain,\n   if (closure)\n     {\n       tree tparm = build_this_parm (r, closure, type_memfn_quals (type));\n+      DECL_NAME (tparm) = closure_identifier;\n       DECL_CHAIN (tparm) = parms;\n       parms = tparm;\n     }"}]}