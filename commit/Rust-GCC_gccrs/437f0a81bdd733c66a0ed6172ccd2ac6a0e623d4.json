nsigned long d;\n+\n+__attribute__((noinline, noclone)) void\n+foo (int *a)\n+{\n+  for (g = 0; g < 32; g++)\n+    if (f)\n+      {\n+        e = d;\n+        __builtin_memcpy (&b, &e, sizeof (float));\n+        b = *a;\n+      }\n+}\n+\n+int\n+main ()\n+{\n+  int h = 5;\n+  f = 1;\n+  asm volatile (\"\" : : : \"memory\");\n+  foo (&h);\n+  asm volatile (\"\" : : : \"memory\");\n+  foo (&b);\n+  asm volatile (\"\" : : : \"memory\");\n+  return 0;\n+}"}, {"sha": "0806df02bfe4fdd4336d602cb4e59f8f18bc234d", "filename": "gcc/testsuite/gcc.target/powerpc/pr79354.c", "status": "added", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/437f0a81bdd733c66a0ed6172ccd2ac6a0e623d4/gcc%2Ftestsuite%2Fgcc.target%2Fpowerpc%2Fpr79354.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/437f0a81bdd733c66a0ed6172ccd2ac6a0e623d4/gcc%2Ftestsuite%2Fgcc.target%2Fpowerpc%2Fpr79354.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Fpowerpc%2Fpr79354.c?ref=437f0a81bdd733c66a0ed6172ccd2ac6a0e623d4", "patch": "@@ -0,0 +1,22 @@\n+/* PR target/79354 */\n+/* { dg-do compile } */\n+/* { dg-skip-if \"do not override -mcpu\" { powerpc*-*-* } { \"-mcpu=*\" } { \"-mcpu=power8\" } } */\n+/* { dg-require-effective-target powerpc_p8vector_ok } */\n+/* { dg-options \"-mcpu=power8 -O2\" } */\n+/* { dg-final { scan-assembler-not {\\mstxssp\\M} } } */\n+\n+int b, f, g;\n+float e;\n+unsigned long d;\n+\n+void\n+foo (int *a)\n+{\n+  for (g = 0; g < 32; g++)\n+    if (f)\n+      {\n+        e = d;\n+        __builtin_memcpy (&b, &e, sizeof (float));\n+        b = *a;\n+      }\n+}"}]}