bool operator() (S &x)\n+   {\n+     T a;\n+     return (a.*m) < x;\t// { dg-error \"invalid use of non-static member\" }\n+   }\n+};\n+\n+void foo (S &x)\n+{\n+  U m;\n+  m.m = &T::t;\n+  m (x);\n+}"}]}