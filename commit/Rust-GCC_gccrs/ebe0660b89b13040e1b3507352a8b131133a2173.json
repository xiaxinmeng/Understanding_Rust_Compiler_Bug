ps://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Fada%2Fdecl.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fdecl.c?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -2996,7 +2996,8 @@ gnat_to_gnu_entity (Entity_Id gnat_entity, tree gnu_expr, int definition)\n \t     : (IN (Ekind (gnat_desig_equiv), Incomplete_Or_Private_Kind)\n \t\t? Full_View (gnat_desig_equiv) : Empty));\n \tEntity_Id gnat_desig_full_direct\n-\t  = ((Present (gnat_desig_full_direct_first)\n+\t  = ((is_from_limited_with\n+\t      && Present (gnat_desig_full_direct_first)\n \t      && IN (Ekind (gnat_desig_full_direct_first), Private_Kind))\n \t     ? Full_View (gnat_desig_full_direct_first)\n \t     : gnat_desig_full_direct_first);"}, {"sha": "1b0fbbd00195d3b61ceb5e55267ae15460e8a4fa", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -1,3 +1,8 @@\n+2007-12-07  Olivier Hainque  <hainque@adacore.com>\n+\n+\t* gnat.dg/tamdt*.ad?: Support for ...\n+\t* gnat.dg/test_tamdt.adb: New test.\n+\n 2007-12-07  Olivier Hainque  <hainque@adacore.com>\n \n \t* gnat.dg/unc_memops.ads: Comment out the alloc/free/realloc"}, {"sha": "81af6ade2838643ccf2d3fdb41907f5030e3bcea", "filename": "gcc/testsuite/gnat.dg/tamdt.adb", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.adb?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -0,0 +1,19 @@\n+\n+with Tamdt_Aux;\n+\n+package body TAMDT is\n+   type TAMT1 is new Tamdt_Aux.Priv (X => 1);\n+   type TAMT2 is new Tamdt_Aux.Priv;\n+\n+   procedure Check is\n+      Ptr1 : TAMT1_Access := new TAMT1;\n+      Ptr2 : TAMT2_Access := new TAMT2 (X => 2);\n+   begin\n+      if Ptr1.all.X /= 1 then\n+         raise Program_Error;\n+      end if;\n+      if Ptr2.all.X /= 2 then\n+         raise Program_Error;\n+      end if;\n+   end;\n+end;"}, {"sha": "09d9388ee1d5920ed091d3f18294dd5a489330ed", "filename": "gcc/testsuite/gnat.dg/tamdt.ads", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt.ads?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -0,0 +1,10 @@\n+\n+package TAMDT is\n+   procedure Check;\n+private\n+   type TAMT1;\n+   type TAMT1_Access is access TAMT1;\n+\n+   type TAMT2;\n+   type TAMT2_Access is access TAMT2;\n+end;"}, {"sha": "d5cca103fb427fbb2352aa1226de6bca6dc68b1d", "filename": "gcc/testsuite/gnat.dg/tamdt_aux.ads", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt_aux.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt_aux.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ftamdt_aux.ads?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -0,0 +1,9 @@\n+\n+package Tamdt_Aux is\n+   type Priv (X : Integer) is private;\n+private\n+   type Priv (X : Integer) is null record;\n+end;\n+\n+\n+"}, {"sha": "d0658ecc4a2d5c7b586e910a7fc4e470369877f6", "filename": "gcc/testsuite/gnat.dg/test_tamdt.adb", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftest_tamdt.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ebe0660b89b13040e1b3507352a8b131133a2173/gcc%2Ftestsuite%2Fgnat.dg%2Ftest_tamdt.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Ftest_tamdt.adb?ref=ebe0660b89b13040e1b3507352a8b131133a2173", "patch": "@@ -0,0 +1,8 @@\n+-- { dg-do run }\n+\n+with Tamdt;\n+\n+procedure Test_Tamdt is\n+begin\n+   Tamdt.Check;\n+end;"}]}