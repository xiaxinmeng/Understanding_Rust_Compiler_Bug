+\t      && DECL_ANON_UNION_VAR_P (decl))\n+\t    {\n+\t      error (\"cannot capture member %qD of anonymous union\", decl);\n+\t      return error_mark_node;\n+\t    }\n \t  if (context == containing_function)\n \t    {\n \t      decl = add_default_capture (lambda_stack,"}, {"sha": "482193e975d60151541c41bfe82a649b0dbb5434", "filename": "gcc/testsuite/g++.dg/cpp0x/lambda/lambda-anon1.C", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/08afbd3bcaa0c6ca8ec54fddaf80dac5b97a659c/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-anon1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/08afbd3bcaa0c6ca8ec54fddaf80dac5b97a659c/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-anon1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-anon1.C?ref=08afbd3bcaa0c6ca8ec54fddaf80dac5b97a659c", "patch": "@@ -0,0 +1,18 @@\n+// DR 1612\n+// { dg-require-effective-target c++11 }\n+\n+int main() {\n+  static int result;\n+  struct A { int x; };\n+  struct B { int y; };\n+  union {\n+    A a; B b;\n+  };\n+  a.x = 1;\n+  [=]() mutable {\n+    a.x = 2;\t\t\t// { dg-error \"anonymous union\" }\n+    result = b.y;\t\t// { dg-error \"anonymous union\" }\n+  }();\n+  if (result == 1) return 0;\n+  throw 0;\n+}"}]}