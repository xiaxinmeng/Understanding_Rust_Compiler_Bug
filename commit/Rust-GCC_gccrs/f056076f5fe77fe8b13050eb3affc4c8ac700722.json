st-GCC/gccrs/raw/f056076f5fe77fe8b13050eb3affc4c8ac700722/gcc%2Fada%2Fexp_aggr.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fexp_aggr.adb?ref=f056076f5fe77fe8b13050eb3affc4c8ac700722", "patch": "@@ -2689,8 +2689,10 @@ package body Exp_Aggr is\n                Discr_Constr :=\n                  First_Elmt (Stored_Constraint (Full_View (Base_Typ)));\n \n+            --  Otherwise, no discriminant to process\n+\n             else\n-               Discr_Constr := First_Elmt (Stored_Constraint (Typ));\n+               Discr_Constr := No_Elmt;\n             end if;\n \n             while Present (Discr) and then Present (Discr_Constr) loop"}, {"sha": "dadeb4fecc07fbaa0b04a78c5d4a0cf221368476", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f056076f5fe77fe8b13050eb3affc4c8ac700722/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f056076f5fe77fe8b13050eb3affc4c8ac700722/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=f056076f5fe77fe8b13050eb3affc4c8ac700722", "patch": "@@ -1,3 +1,7 @@\n+2019-08-14  Bob Duff  <duff@adacore.com>\n+\n+\t* gnat.dg/discr57.adb: New testcase.\n+\n 2019-08-14  Eric Botcazou  <ebotcazou@adacore.com>\n \n \t* gnat.dg/generic_inst11.adb, gnat.dg/generic_inst11_pkg.adb,"}, {"sha": "cb5cecca92cdd8a75786972209889fcafecead21", "filename": "gcc/testsuite/gnat.dg/discr57.adb", "status": "added", "additions": 17, "deletions": 0, "changes": 17, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f056076f5fe77fe8b13050eb3affc4c8ac700722/gcc%2Ftestsuite%2Fgnat.dg%2Fdiscr57.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f056076f5fe77fe8b13050eb3affc4c8ac700722/gcc%2Ftestsuite%2Fgnat.dg%2Fdiscr57.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fdiscr57.adb?ref=f056076f5fe77fe8b13050eb3affc4c8ac700722", "patch": "@@ -0,0 +1,17 @@\n+--  { dg-do compile }\n+\n+procedure Discr57 is\n+\n+   type T1(Scalar : Boolean) is abstract tagged null record;\n+\n+   subtype S1 is T1 (Scalar => False);\n+\n+   type T2(Lower_Bound : Natural) is new\n+     S1 with null record;\n+\n+   Obj : constant T2 :=\n+       (Lower_Bound => 123);\n+\n+begin\n+   null;\n+end Discr57;"}]}