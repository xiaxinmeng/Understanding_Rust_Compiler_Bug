ithub.com/Rust-GCC/gccrs/raw/639b3d61ab124ab14e0935893abe437ed7f6ad52/gcc%2Ftestsuite%2Frust%2Fcompile%2Fissue-1129-2.rs", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Frust%2Fcompile%2Fissue-1129-2.rs?ref=639b3d61ab124ab14e0935893abe437ed7f6ad52", "patch": "@@ -0,0 +1,22 @@\n+// { dg-additional-options \"-w\" }\n+pub trait Hasher {\n+    fn finish(&self) -> u64;\n+    fn write(&mut self, bytes: &[u8]);\n+    fn write_u8(&mut self, i: u8) {\n+        self.write(&[i])\n+    }\n+}\n+\n+struct SipHasher;\n+\n+impl Hasher for SipHasher {\n+    #[inline]\n+    fn write(&mut self, msg: &[u8]) {\n+        loop {}\n+    }\n+\n+    #[inline]\n+    fn finish(&self) -> u64 {\n+        0\n+    }\n+}"}]}