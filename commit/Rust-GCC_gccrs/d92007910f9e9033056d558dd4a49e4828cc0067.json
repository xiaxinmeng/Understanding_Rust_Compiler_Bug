9550e8da5e485804d775c75489a703050", "filename": "gcc/testsuite/g++.dg/ext/gnu-inline-global-reject.C", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fext%2Fgnu-inline-global-reject.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fext%2Fgnu-inline-global-reject.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fext%2Fgnu-inline-global-reject.C?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -4,6 +4,7 @@\n */\n \n /* { dg-do compile } */\n+/* { dg-options \" -ansi -Wno-long-long\" } */\n \n #include \"gnu-inline-common.h\"\n "}, {"sha": "7cd7daa3ad20d79c69592491c5cb894e91b12e31", "filename": "gcc/testsuite/gcc.dg/cpp/20000519-1.c", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F20000519-1.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F20000519-1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F20000519-1.c?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -1,6 +1,7 @@\n /* Regression test for preprocessor crash.\n    Reported by Mathias Froehlich <frohlich@na.uni-tuebingen.de>.  */\n /* { dg-do preprocess } */\n+/* { dg-options \"-ansi\" } */\n #define foo\n \n #define __CAT__(a,b,c,d) a##b##c##d"}, {"sha": "3591f9c08aeaa001381e46feaee795ad8e1a5c21", "filename": "gcc/testsuite/gcc.dg/cpp/avoidpaste1.c", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste1.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste1.c?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -1,6 +1,7 @@\n /* Copyright (C) 2001, 2003 Free Software Foundation, Inc.  */\n \n /* { dg-do preprocess } */\n+/* { dg-options \"-ansi\" } */\n \n /* This tests that we avoid accidental pasting only before and after\n    macros and arguments, and not when the tokens are already pasted"}, {"sha": "dc5bbc7f270502a70e0cbeb68490d5d8f4bf4d05", "filename": "gcc/testsuite/gcc.dg/cpp/avoidpaste2.c", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste2.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste2.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Favoidpaste2.c?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -1,6 +1,7 @@\n /* Copyright (C) 2001, 2003 Free Software Foundation, Inc.  */\n \n /* { dg-do preprocess } */\n+/* { dg-options \"-ansi\" } */\n \n /* This tests that we avoid accidental pasting, as well as gratuitous\n    space insertion, in various nasty places _inside_ a macro's"}, {"sha": "e90f8e6cb69c1e817187caef3957c51c6751a8aa", "filename": "gcc/testsuite/gcc.dg/cpp/c90-empty-macro-args.c", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc90-empty-macro-args.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc90-empty-macro-args.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc90-empty-macro-args.c?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -0,0 +1,19 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-std=c89 -pedantic\" } */\n+\n+#define f(a,b)   f2(a,,b) \n+#define f2(a,b,c) a; b; c;\n+#define f3(a)    a\n+\n+#define g()   p()\n+\n+void p(void) {}\n+\n+\n+void foo(void)\n+{\n+    f(p(),p()); /* { dg-warning \"macro f2 argument 2: empty macro arguments are undefined\" } */\n+    f2(p(),,p()); /* { dg-warning \"macro f2 argument 2: empty macro arguments are undefined\" } */\n+    f3(); /* { dg-warning \"macro f3 argument 1: empty macro arguments are undefined\" } */\n+    g();\n+}"}, {"sha": "e1e0c61ec2973ed684cd6df3de63568c93c07bcd", "filename": "gcc/testsuite/gcc.dg/cpp/c99-empty-macro-args.c", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc99-empty-macro-args.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d92007910f9e9033056d558dd4a49e4828cc0067/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc99-empty-macro-args.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2Fc99-empty-macro-args.c?ref=d92007910f9e9033056d558dd4a49e4828cc0067", "patch": "@@ -0,0 +1,19 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-std=c99 -pedantic\" } */\n+\n+#define f(a,b)   f2(a,,b) \n+#define f2(a,b,c) a; b; c;\n+#define f3(a)    a\n+\n+#define g()   p()\n+\n+void p(void) {}\n+\n+\n+void foo(void)\n+{\n+    f(p(),p());\n+    f2(p(),,p());\n+    f3();\n+    g();\n+}"}]}