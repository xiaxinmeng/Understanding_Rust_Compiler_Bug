t ch;\n+  dst = src = (src ? src : *last);\n+\n+  if (*src == 0)\n+    return 0;\n+\n+  while (src[0])\n+    {\n+      if (!src[1])\n+\t{\n+\t  bad ();\n+\t  break;\n+\t}\n+      *dst = *src;\n+      dst += 1;\n+      src += 2;\n+    }\n+  *last = src;\n+  *dst = 0;\n+  return *last;\n+}"}]}