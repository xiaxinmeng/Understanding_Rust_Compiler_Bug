  update_call_from_tree (&gsi, value);\n   FOR_EACH_IMM_USE_STMT (use_stmt, iter, lhs)\n-    FOR_EACH_IMM_USE_ON_STMT (use_p, iter)\n-      SET_USE (use_p, value);\n+    {\n+      FOR_EACH_IMM_USE_ON_STMT (use_p, iter)\n+\tSET_USE (use_p, value);\n+      update_stmt (use_stmt);\n+    }\n }\n \n /* Function vectorize_loops."}]}