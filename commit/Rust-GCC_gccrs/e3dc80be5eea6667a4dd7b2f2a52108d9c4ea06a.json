      for (; i--; )\n \tif (TREE_VEC_ELT (tempargs, i))\n-\t  TREE_VEC_ELT (targs, i) = TREE_VEC_ELT (tempargs, i);\n+\t  {\n+\t    tree old = TREE_VEC_ELT (targs, i);\n+\t    tree new_ = TREE_VEC_ELT (tempargs, i);\n+\t    if (new_ && old && ARGUMENT_PACK_P (old)\n+\t\t&& ARGUMENT_PACK_EXPLICIT_ARGS (old))\n+\t      /* Don't forget explicit template arguments in a pack.  */\n+\t      ARGUMENT_PACK_EXPLICIT_ARGS (new_)\n+\t\t= ARGUMENT_PACK_EXPLICIT_ARGS (old);\n+\t    TREE_VEC_ELT (targs, i) = new_;\n+\t  }\n     }\n   if (good)\n     return true;"}, {"sha": "2b14d3005f01e4a316eba1ea9e36b214c97950d2", "filename": "gcc/testsuite/g++.dg/cpp0x/variadic159.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e3dc80be5eea6667a4dd7b2f2a52108d9c4ea06a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic159.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e3dc80be5eea6667a4dd7b2f2a52108d9c4ea06a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic159.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fvariadic159.C?ref=e3dc80be5eea6667a4dd7b2f2a52108d9c4ea06a", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/61507\n+// { dg-do compile { target c++11 } }\n+\n+struct A {\n+  void foo(const int &);\n+  void foo(float);\n+};\n+\n+template <typename... Args>\n+void bar(void (A::*memfun)(Args...), Args... args);\n+\n+void go(const int& i) {\n+  bar<const int &>(&A::foo, i);\n+}"}]}