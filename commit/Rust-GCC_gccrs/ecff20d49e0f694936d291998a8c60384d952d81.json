ak DECL_ABSTRACT_ORIGIN.  */\n+  fndecl = inl_f->decl;\n+\n   nargs = list_length (DECL_ARGUMENTS (fndecl));\n \n   if (cfun->preferred_stack_boundary < inl_f->preferred_stack_boundary)"}]}