le }\n+// { dg-options \"\" }\n+\n+struct A {};\n+struct B : virtual A {};\n+int vi;\n+void *vp;\n+\n+void\n+f1 (int i)\n+{\n+  goto *i;\n+}\n+\n+void\n+f2 (B b)\n+{\n+  goto *b;\t// { dg-error \"cannot convert\" }\n+}\n+\n+template <typename T>\n+void\n+f3 (T i)\n+{\n+  goto *i;\n+}\n+\n+void\n+f3a ()\n+{\n+  f3 (vi);\n+}\n+\n+template <typename T>\n+void\n+f4 (T i)\n+{\n+  goto *i;\n+}\n+\n+void\n+f4a ()\n+{\n+  f4 (vp);\n+}"}]}