s_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=edb7412e0ec4bfbc8e054d50b52f047ba70fd66d", "patch": "@@ -1,3 +1,8 @@\n+2012-02-10  Eric Botcazou  <ebotcazou@adacore.com>\n+\n+\t* gnat.dg/specs/aggr4.ads: New test.\n+\t* gnat.dg/specs/aggr4_pkg.ads: New helper.\n+\n 2012-02-10  H.J. Lu  <hongjiu.lu@intel.com>\n \n \tPR target/52146"}, {"sha": "4cdf652ff2cb43df359786b1b53e95971462862d", "filename": "gcc/testsuite/gnat.dg/specs/aggr4.ads", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/edb7412e0ec4bfbc8e054d50b52f047ba70fd66d/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/edb7412e0ec4bfbc8e054d50b52f047ba70fd66d/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4.ads?ref=edb7412e0ec4bfbc8e054d50b52f047ba70fd66d", "patch": "@@ -0,0 +1,9 @@\n+-- { dg-do compile }\n+\n+with Aggr4_Pkg; use Aggr4_Pkg;\n+\n+package Aggr4 is\n+\n+   C : constant Rec3 := (Data => (D => One, Value => Zero));\n+\n+end Aggr4;"}, {"sha": "c0f3c8c828adf87c5a72984eb4c90d1b72db4ddf", "filename": "gcc/testsuite/gnat.dg/specs/aggr4_pkg.ads", "status": "added", "additions": 26, "deletions": 0, "changes": 26, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/edb7412e0ec4bfbc8e054d50b52f047ba70fd66d/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4_pkg.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/edb7412e0ec4bfbc8e054d50b52f047ba70fd66d/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4_pkg.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fspecs%2Faggr4_pkg.ads?ref=edb7412e0ec4bfbc8e054d50b52f047ba70fd66d", "patch": "@@ -0,0 +1,26 @@\n+-- { dg-excess-errors \"cannot generate code\" }\n+\n+package Aggr4_Pkg is\n+\n+   function F return Integer;\n+\n+   type Rec1 is tagged record\n+      I : Integer;\n+   end record;\n+\n+   Zero : constant Rec1 := (I => F);\n+\n+   type Enum is (One, Two);\n+\n+   type Rec2 (D : Enum := One) is record\n+      case D is\n+         when One => Value : Rec1;\n+         when others => null;\n+      end case;\n+   end record;\n+\n+   type Rec3 is record\n+      Data : Rec2;\n+   end record;\n+\n+end Aggr4_Pkg;"}]}