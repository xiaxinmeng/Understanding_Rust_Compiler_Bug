arm_neon.h\"\n+#include <stdlib.h>\n+#include <string.h>\n+\n+int8x8_t x = { 1, 2, 3, 4, 5, 6, 7, 8 };\n+int8x8_t y = { 1, 2, 3, 16, 5, 6, 7, 8 };\n+\n+int main (void)\n+{\n+  x = vset_lane_s8 (16, x, 3);\n+  if (memcmp (&x, &y, sizeof (x)) != 0)\n+    abort();\n+  return 0;\n+}"}]}