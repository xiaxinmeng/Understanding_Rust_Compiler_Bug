r_save_nsdmi (cp_parser* parser)\n   cp_token *last;\n   tree node;\n \n-  cp_parser_cache_group (parser, CPP_CLOSE_PAREN, /*depth=*/0);\n+  /* Save tokens until the next comma or semicolon.  */\n+  cp_parser_cache_group (parser, CPP_COMMA, /*depth=*/0);\n \n   last = parser->lexer->next_token;\n \n@@ -21719,6 +21720,12 @@ cp_parser_cache_group (cp_parser *parser,\n \t   kind of syntax error.  */\n \treturn true;\n \n+      /* If we're caching something finished by a comma (or semicolon),\n+\t such as an NSDMI, don't consume the comma.  */\n+      if (end == CPP_COMMA\n+\t  && (token->type == CPP_SEMICOLON || token->type == CPP_COMMA))\n+\treturn false;\n+\n       /* Consume the token.  */\n       cp_lexer_consume_token (parser->lexer);\n       /* See if it starts a new group.  */"}, {"sha": "ed5d30e7feceede7ff475f1b58ed6344d20c1ef0", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7204877f43b9e70fbb357d26c43c3d26c5fbbc41/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7204877f43b9e70fbb357d26c43c3d26c5fbbc41/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=7204877f43b9e70fbb357d26c43c3d26c5fbbc41", "patch": "@@ -1,5 +1,8 @@\n 2011-10-14  Jason Merrill  <jason@redhat.com>\n \n+\tPR c++/50563\n+\t* g++.dg/cpp0x/nsdmi-list1.C: New.\n+\n \tPR c++/50507\n \t* g++.dg/cpp0x/nsdmi-const1.C: New.\n "}, {"sha": "526f29ae2850ba518788b232101bd718636573f9", "filename": "gcc/testsuite/g++.dg/cpp0x/nsdmi-list1.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7204877f43b9e70fbb357d26c43c3d26c5fbbc41/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnsdmi-list1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7204877f43b9e70fbb357d26c43c3d26c5fbbc41/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnsdmi-list1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnsdmi-list1.C?ref=7204877f43b9e70fbb357d26c43c3d26c5fbbc41", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/50563\n+// { dg-options -std=c++0x }\n+\n+struct S1 {\n+  int a{10}, b{20};     // OK\n+};\n+\n+struct S2 {\n+  int a, b = 20;        // OK\n+};\n+\n+struct S3 {\n+  int a = 10, b = 20;\n+};"}]}