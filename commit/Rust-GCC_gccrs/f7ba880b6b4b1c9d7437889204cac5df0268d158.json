 "patch": "@@ -1338,6 +1338,13 @@ gfc_match_omp_clauses (gfc_omp_clauses **cp, uint64_t mask,\n \t    continue;\n \t  break;\n \tcase 'v':\n+\t  /* VECTOR_LENGTH must be matched before VECTOR, because the latter\n+\t     doesn't unconditionally match '('.  */\n+\t  if ((mask & OMP_CLAUSE_VECTOR_LENGTH)\n+\t      && c->vector_length_expr == NULL\n+\t      && (gfc_match (\"vector_length ( %e )\", &c->vector_length_expr)\n+\t\t  == MATCH_YES))\n+\t    continue;\n \t  if ((mask & OMP_CLAUSE_VECTOR)\n \t      && !c->vector\n \t      && gfc_match (\"vector\") == MATCH_YES)\n@@ -1353,11 +1360,6 @@ gfc_match_omp_clauses (gfc_omp_clauses **cp, uint64_t mask,\n \t\tneeds_space = true;\n \t      continue;\n \t    }\n-\t  if ((mask & OMP_CLAUSE_VECTOR_LENGTH)\n-\t      && c->vector_length_expr == NULL\n-\t      && (gfc_match (\"vector_length ( %e )\", &c->vector_length_expr)\n-\t\t  == MATCH_YES))\n-\t    continue;\n \t  break;\n \tcase 'w':\n \t  if ((mask & OMP_CLAUSE_WAIT)"}, {"sha": "2a6ca3f2bc05ba60aea2022c225e7fc47d37f092", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f7ba880b6b4b1c9d7437889204cac5df0268d158/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f7ba880b6b4b1c9d7437889204cac5df0268d158/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=f7ba880b6b4b1c9d7437889204cac5df0268d158", "patch": "@@ -1,3 +1,7 @@\n+2016-07-15  Cesar Philippidis  <cesar@codesourcery.com>\n+\n+\t* gfortran.dg/goacc/vector_length.f90: New test.\n+\n 2016-07-15  Richard Biener  <rguenther@suse.de>\n \n \tPR tree-optimization/71881"}, {"sha": "ddab9cfd8c1127d8fbdac15e6460b29e904e11b7", "filename": "gcc/testsuite/gfortran.dg/goacc/vector_length.f90", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f7ba880b6b4b1c9d7437889204cac5df0268d158/gcc%2Ftestsuite%2Fgfortran.dg%2Fgoacc%2Fvector_length.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f7ba880b6b4b1c9d7437889204cac5df0268d158/gcc%2Ftestsuite%2Fgfortran.dg%2Fgoacc%2Fvector_length.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgoacc%2Fvector_length.f90?ref=f7ba880b6b4b1c9d7437889204cac5df0268d158", "patch": "@@ -0,0 +1,11 @@\n+program t\n+  implicit none\n+  integer, parameter :: n = 100\n+  integer a(n), i\n+\n+  !$acc parallel loop num_gangs(100) num_workers(1) vector_length(32)\n+  do i = 1, n\n+     a(i) = i\n+  enddo\n+  !$acc end parallel loop\n+end program t"}]}