.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fsem_aggr.adb?ref=92d4508a7db153423b0572047fa6947af569d4b6", "patch": "@@ -2159,7 +2159,9 @@ package body Sem_Aggr is\n             Imm_Type := Etype (Base_Type (Imm_Type));\n          end loop;\n \n-         if Etype (Imm_Type) /= Base_Type (A_Type) then\n+         if Etype (Imm_Type) /= Base_Type (A_Type)\n+           or else Base_Type (Typ) = Base_Type (A_Type)\n+         then\n             Error_Msg_NE (\"expect ancestor type of &\", A, Typ);\n             return False;\n          else"}, {"sha": "6932bedb8f47100e6e5350a0a26d05971084aa91", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=92d4508a7db153423b0572047fa6947af569d4b6", "patch": "@@ -1,3 +1,8 @@\n+2008-04-13  Samuel Tardieu  <sam@rfc1149.net> \n+\n+\tPR ada/17985\n+\t* gnat.dg/ancestor_type.ads, gnat.dg/ancestor_type.adb: New test.\n+\n 2008-04-12  Andrew Pinski  <pinskia@gmail.com>\n \n \t* gcc.target/powerpc/darwin-save-world-1.c: New test."}, {"sha": "b5e9e2c5c1d8c827486c490a4ab7cf59cc9ac70e", "filename": "gcc/testsuite/gnat.dg/ancestor_type.adb", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.adb?ref=92d4508a7db153423b0572047fa6947af569d4b6", "patch": "@@ -0,0 +1,13 @@\n+-- { dg-do compile }\n+\n+package body Ancestor_Type is\n+\n+   package body B is\n+      function make return T is\n+      begin\n+         return (T with n => 0);  -- { dg-error \"expect ancestor\" }\n+      end make;\n+\n+   end B;\n+\n+end Ancestor_Type;"}, {"sha": "2ed1f19c2c82044598f712d1905babc11e4b62ca", "filename": "gcc/testsuite/gnat.dg/ancestor_type.ads", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/92d4508a7db153423b0572047fa6947af569d4b6/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fancestor_type.ads?ref=92d4508a7db153423b0572047fa6947af569d4b6", "patch": "@@ -0,0 +1,13 @@\n+package Ancestor_Type is\n+\n+   type T is tagged private;\n+\n+   package B is\n+      function make return T;\n+   end B;\n+\n+private\n+   type T is tagged record\n+      n: Natural;\n+   end record;\n+end Ancestor_Type;"}]}