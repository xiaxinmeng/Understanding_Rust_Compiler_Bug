GCC/gccrs/contents/gcc%2Fcp%2Fpt.c?ref=eb5c3f052a57f3b14c24848d6a195d96f0a6bbef", "patch": "@@ -974,7 +974,10 @@ retrieve_specialization (tree tmpl, tree args, hashval_t hash)\n       for (fns = VEC_index (tree, methods, idx); fns; fns = OVL_NEXT (fns))\n \t{\n \t  tree fn = OVL_CURRENT (fns);\n-\t  if (DECL_TEMPLATE_INFO (fn) && DECL_TI_TEMPLATE (fn) == tmpl)\n+\t  if (DECL_TEMPLATE_INFO (fn) && DECL_TI_TEMPLATE (fn) == tmpl\n+\t      /* using-declarations can add base methods to the method vec,\n+\t\t and we don't want those here.  */\n+\t      && DECL_CONTEXT (fn) == class_specialization)\n \t    return fn;\n \t}\n       return NULL_TREE;"}, {"sha": "69279cd4883917c6882e662678981c993f15b9ac", "filename": "gcc/cp/ptree.c", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Fcp%2Fptree.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Fcp%2Fptree.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Fptree.c?ref=eb5c3f052a57f3b14c24848d6a195d96f0a6bbef", "patch": "@@ -91,6 +91,10 @@ cxx_print_type (FILE *file, tree node, int indent)\n \tprint_node (file, \"throws\", TYPE_RAISES_EXCEPTIONS (node), indent + 4);\n       return;\n \n+    case RECORD_TYPE:\n+    case UNION_TYPE:\n+      break;\n+\n     default:\n       return;\n     }"}, {"sha": "da27711cf66fa7b868089d0788c8a4b5101bf298", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=eb5c3f052a57f3b14c24848d6a195d96f0a6bbef", "patch": "@@ -1,3 +1,7 @@\n+2009-07-04  Jason Merrill  <jason@redhat.com>\n+\n+\t* g++.dg/template/using15.C: New.\n+\n 2009-07-04  Jakub Jelinek  <jakub@redhat.com>\n \n \t* gfortran.dg/maxloc_1.f90: New test."}, {"sha": "b158ac09cf67126ca4441c84bb43c4b14707ae97", "filename": "gcc/testsuite/g++.dg/template/using15.C", "status": "added", "additions": 25, "deletions": 0, "changes": 25, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fusing15.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/eb5c3f052a57f3b14c24848d6a195d96f0a6bbef/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fusing15.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fusing15.C?ref=eb5c3f052a57f3b14c24848d6a195d96f0a6bbef", "patch": "@@ -0,0 +1,25 @@\n+// Reduced from the testcase for c++/29433\n+\n+template <class T>\n+struct A: T\n+{\n+  void f(typename T::type);\n+  using T::f;\n+  void g() { f(1); }\n+};\n+\n+template <class T>\n+struct B: T\n+{ typedef int type; };\n+\n+struct C\n+{\n+  typedef double type;\n+  void f();\n+};\n+\n+int main()\n+{\n+  A<B<A<C> > > a;\n+  a.g();\n+}"}]}