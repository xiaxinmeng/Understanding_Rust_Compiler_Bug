rVec outer_attrs);\n   std::unique_ptr<AST::MacroInvocation>"}, {"sha": "c2658721bdfc2e83e69984c19eb8a8ac18bb7f15", "filename": "gcc/testsuite/rust/execute/torture/macros25.rs", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5651331236d39ce685e1de3396463fd88c3a83d2/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros25.rs", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5651331236d39ce685e1de3396463fd88c3a83d2/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros25.rs", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros25.rs?ref=5651331236d39ce685e1de3396463fd88c3a83d2", "patch": "@@ -0,0 +1,13 @@\n+macro_rules! t {\n+    ($t:tt) => {\n+        $t\n+    };\n+}\n+\n+fn frob() -> i32 {\n+    t!(15) + t!((14))\n+}\n+\n+fn main() -> i32 {\n+    frob() - 29\n+}"}, {"sha": "30f0beef0d969879a45c915123831422b3d92b61", "filename": "gcc/testsuite/rust/execute/torture/macros26.rs", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5651331236d39ce685e1de3396463fd88c3a83d2/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros26.rs", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5651331236d39ce685e1de3396463fd88c3a83d2/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros26.rs", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Frust%2Fexecute%2Ftorture%2Fmacros26.rs?ref=5651331236d39ce685e1de3396463fd88c3a83d2", "patch": "@@ -0,0 +1,12 @@\n+macro_rules! count_tt {\n+    ($t:tt) => { 1 };\n+    ($t:tt $($ts:tt)*) => { 1 + count_tt!($($ts)*) };\n+}\n+\n+fn main() -> i32 {\n+    let count = count_tt!(1 2 let a = 15) + count_tt!(1 2 (let a = 15));\n+    //                    ^ ^ ^^^ ^ ^ ^^              ^ ^ ^^^^^^^^^^^^\n+    //                    6 token-trees               3 token-trees\n+\n+    count - 9\n+}"}]}