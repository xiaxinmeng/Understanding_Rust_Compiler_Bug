 { 1, 3 });\n+}\n+\n+int main()\n+{\n+  v2df x = (v2df) { 0.0, 2.0 };\n+  foo (&x, &x);\n+  if (x[0] != 0.0 || x[1] != 2.0)\n+    __builtin_abort ();\n+  return 0;\n+}"}]}