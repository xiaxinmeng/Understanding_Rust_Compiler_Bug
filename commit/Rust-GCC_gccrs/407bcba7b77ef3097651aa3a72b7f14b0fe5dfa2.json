eturn (x << 2) | 4;\n+}\n+\n+__attribute__((noinline, noclone)) int\n+bar (int x)\n+{\n+  return (x << 2) | 3;\n+}\n+\n+int\n+main ()\n+{\n+  if (foo (43) != 172 || foo (1) != 4 || foo (2) != 12)\n+    abort ();\n+  if (bar (43) != 175 || bar (1) != 7 || bar (2) != 11)\n+    abort ();\n+  return 0;\n+}"}]}