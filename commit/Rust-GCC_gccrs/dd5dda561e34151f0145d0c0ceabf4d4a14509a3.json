n (const constexpr_ctx *ctx, tree t,\n \t       && (null_member_pointer_value_p (lhs)\n \t\t   || null_member_pointer_value_p (rhs)))\n \tr = constant_boolean_node (!is_code_eq, type);\n+      else if (TREE_CODE (lhs) == PTRMEM_CST)\n+\tlhs = cplus_expand_constant (lhs);\n+      else if (TREE_CODE (rhs) == PTRMEM_CST)\n+\trhs = cplus_expand_constant (rhs);\n     }\n \n   if (r == NULL_TREE)"}, {"sha": "ed18ab1a63040565954eb5a46b980f74eec2e709", "filename": "gcc/testsuite/g++.dg/cpp0x/constexpr-ptrmem6.C", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/dd5dda561e34151f0145d0c0ceabf4d4a14509a3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-ptrmem6.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/dd5dda561e34151f0145d0c0ceabf4d4a14509a3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-ptrmem6.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-ptrmem6.C?ref=dd5dda561e34151f0145d0c0ceabf4d4a14509a3", "patch": "@@ -0,0 +1,13 @@\n+// PR c++/71896\n+// { dg-do compile { target c++11 } }\n+\n+struct Foo {\n+  int x;\n+};\n+\n+constexpr bool compare(int Foo::*t) { return t == &Foo::x; }\n+\n+constexpr bool b = compare(&Foo::x);\n+\n+#define SA(X) static_assert ((X),#X)\n+SA(b);"}]}