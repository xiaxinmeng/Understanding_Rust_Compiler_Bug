tch": "@@ -6,13 +6,17 @@\n #include <math.h>\n #include <float.h>\n \n+#include \"builtins-config.h\"\n+\n int main (void)\n {\n   int result = 0;\n   double local_DBL_MAX = DBL_MAX; \n   double local_DBL_MIN = DBL_MIN;\n+#ifdef HAVE_C99_RUNTIME\n   if (isinf (local_DBL_MAX))\n     result |= 1;\n+#endif\n   if (local_DBL_MIN <= 0.0)\n     result |= 1;\n   return result;"}]}