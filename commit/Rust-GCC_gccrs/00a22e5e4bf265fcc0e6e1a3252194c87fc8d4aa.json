.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa", "patch": "@@ -1,3 +1,8 @@\n+2012-10-22  Eric Botcazou  <ebotcazou@adacore.com>\n+\n+\t* gnat.dg/modular4.adb: New test.\n+\t* gnat.dg/modular4_pkg.ads: New helper.\n+\n 2012-10-21  Eric Botcazou  <ebotcazou@adacore.com>\n \n \t* gnat.dg/specs/atomic1.ads: XFAIL on MIPS."}, {"sha": "e8e8f2956e19546446ffae314a331ee181046eb6", "filename": "gcc/testsuite/gnat.dg/modular4.adb", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4.adb?ref=00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa", "patch": "@@ -0,0 +1,11 @@\n+-- { dg-do compile }\n+-- { dg-options \"-O\" }\n+\n+with Modular4_Pkg; use Modular4_Pkg;\n+\n+procedure Modular4 is\n+begin\n+  for I in Zero .. F mod 8 loop\n+    raise Program_Error;\n+  end loop;\n+end;"}, {"sha": "b38026201ed2c188e5cc573f2f165a6707b6537c", "filename": "gcc/testsuite/gnat.dg/modular4_pkg.ads", "status": "added", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4_pkg.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4_pkg.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fmodular4_pkg.ads?ref=00a22e5e4bf265fcc0e6e1a3252194c87fc8d4aa", "patch": "@@ -0,0 +1,9 @@\n+package Modular4_Pkg is\n+\n+   type Word is mod 2**48;\n+\n+   Zero : constant Word := 0;\n+\n+   function F return Word;\n+\n+end Modular4_Pkg;"}]}