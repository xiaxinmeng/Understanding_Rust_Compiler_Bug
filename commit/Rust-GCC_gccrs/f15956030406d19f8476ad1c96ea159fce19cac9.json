,15 @@ for i in ${target_configdirs_all} ; do\n   fi\n done\n \n+# Exclude target-zlib if target-libjava isn't built.\n+case ${target_configdirs} in\n+*target-libjava*)\n+  ;;\n+*)\n+  target_configdirs=\"`echo ${target_configdirs} | sed -e 's/target-zlib//'`\"\n+  ;;\n+esac\n+\n # libiberty-linker-plugin is special: it doesn't have its own source directory,\n # so we have to add it after the preceding checks.\n if test x\"$extra_linker_plugin_flags$extra_linker_plugin_configure_flags\" != x"}, {"sha": "2ff9be008cd0ab94e77db0afa559d46bb477c220", "filename": "configure.ac", "status": "modified", "additions": 9, "deletions": 0, "changes": 9, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/f15956030406d19f8476ad1c96ea159fce19cac9/configure.ac", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/f15956030406d19f8476ad1c96ea159fce19cac9/configure.ac", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/configure.ac?ref=f15956030406d19f8476ad1c96ea159fce19cac9", "patch": "@@ -2265,6 +2265,15 @@ for i in ${target_configdirs_all} ; do\n   fi\n done\n \n+# Exclude target-zlib if target-libjava isn't built.\n+case ${target_configdirs} in\n+*target-libjava*)\n+  ;;\n+*)\n+  target_configdirs=\"`echo ${target_configdirs} | sed -e 's/target-zlib//'`\"\n+  ;;\n+esac\n+\n # libiberty-linker-plugin is special: it doesn't have its own source directory,\n # so we have to add it after the preceding checks.\n if test x\"$extra_linker_plugin_flags$extra_linker_plugin_configure_flags\" != x"}]}