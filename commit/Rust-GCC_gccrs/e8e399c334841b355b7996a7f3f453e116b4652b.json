\n+\n+template <int N>\n+struct S\n+{\n+  static const int k = 6;\n+};\n+\n+template <int N>\n+const int S<N>::k;\n+\n+template <int N>\n+void\n+foo ()\n+{\n+  const int i = S<N>::k;\n+  unsigned char a[i];\n+  bar (a);\n+}\n+\n+void\n+baz ()\n+{\n+  foo<0> ();\n+}"}]}