(const ip_addr_t * addr, char *buf, int buflen)\n+{\n+  char inv[3];\n+  char *rp;\n+  u8_t *ap;\n+  u8_t n;\n+  u8_t i;\n+  int len = 0;\n+  for (n = 0; n < 4; n++)\n+    {\n+      while (*ap);\n+      while (i--)\n+\t{\n+\t  if (len++ >= buflen)\n+\t    return ((void *) 0);\n+\t  *rp++ = inv[i];\n+\t} ap++;\n+    }\n+}"}]}