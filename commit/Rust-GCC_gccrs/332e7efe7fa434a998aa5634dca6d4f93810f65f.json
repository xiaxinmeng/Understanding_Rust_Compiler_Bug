db77472109f6351c0fe5ee72f9788c559f2cd9", "filename": "gcc/testsuite/gfortran.dg/achar_1.f90", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/332e7efe7fa434a998aa5634dca6d4f93810f65f/gcc%2Ftestsuite%2Fgfortran.dg%2Fachar_1.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/332e7efe7fa434a998aa5634dca6d4f93810f65f/gcc%2Ftestsuite%2Fgfortran.dg%2Fachar_1.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fachar_1.f90?ref=332e7efe7fa434a998aa5634dca6d4f93810f65f", "patch": "@@ -0,0 +1,8 @@\n+! { dg-do run }\n+! achar() should work with all supported integer kinds.\n+program  bug6\n+  integer(1) :: i = 65\n+  character a\n+  a = achar(i)\n+  if (a /= 'A') call abort\n+end program  bug6"}]}