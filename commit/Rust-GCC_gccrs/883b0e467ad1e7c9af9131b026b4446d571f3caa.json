 {\n \t} elsif($right eq '/dev/null') {\n \t\t$filename = $left;\n \t} else {\n-\t\tprint STDERR \"Error: failed to parse diff for $left and $right\\n\";\n-\t\texit 1;\n+\t\tmy @ldirs = split /[\\/\\\\]/, $left;\n+\t\tmy @rdirs = split /[\\/\\\\]/, $right;\n+\n+\t\t$filename = '';\n+\t\twhile ((my $l = pop @ldirs) && (my $r = pop @rdirs)) {\n+\t\t\tlast if ($l ne $r);\n+\t\t\t$filename = \"$l/$filename\";\n+\t\t}\n+\t\t$filename =~ s/\\/$//;\n+\n+\t\tif (!$filename) {\n+\t\t\tprint STDERR \"Error: failed to parse diff for $left and $right\\n\";\n+\t\t\texit 1;\n+\t\t}\n \t}\n \t$left = $right = undef;\n \t($clname, $relname) = get_clname ($filename);"}]}