s_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1y%2Flambda-generic-variadic21.C?ref=08ca2d744d50d0d5a341530031e6bbb82831109f", "patch": "@@ -0,0 +1,26 @@\n+// PR c++/91104\n+// { dg-do run { target c++14 } }\n+\n+void\n+test (void (*f)(int, int, int))\n+{\n+  f(1, 2, 3);\n+}\n+\n+void\n+check (int a, int b, int c)\n+{\n+  if (a != 1 || b != 2 || c != 3)\n+    __builtin_abort ();\n+}\n+\n+int\n+main ()\n+{\n+  test ([](auto... args) {\n+    check (args...);\n+  });\n+  test ([](int a, int b, int c) {\n+    check (a, b, c);\n+  });\n+}"}]}