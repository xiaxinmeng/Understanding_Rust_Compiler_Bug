"@@ -0,0 +1,26 @@\n+! { dg-do run }\n+! { dg-options \"-O2 -ftree-loop-linear\" }\n+\n+! Program to test the scalarizer\n+program testarray\n+   implicit none\n+   integer, dimension (6, 5) :: a, b\n+   integer n\n+\n+   a = 0\n+   do n = 1, 5\n+      a(4, n) = n\n+   end do\n+\n+   b(:, 5:1:-1) = a\n+   a(1:5, 2) = a(4, :) + 1\n+\n+   ! The following expression should cause loop reordering\n+   a(:, 2:4) = a(:, 1:3)\n+\n+   do n = 1, 5\n+      if (a(n, 3) .ne. (n + 1)) call abort\n+      if (b(4, n) .ne. (6 - n)) call abort\n+   end do\n+end program\n+"}]}