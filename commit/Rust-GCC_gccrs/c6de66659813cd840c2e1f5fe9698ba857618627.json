 void\n+foo (int *x, int y (void))\n+{\n+  while (1)\n+    {\n+      int a = 0;\n+      if (*x)\n+\t{\n+\t  baz (x, &a);\n+\t  while (a && !y ())\n+\t    ;\n+\t  break;\n+\t}\n+      abrt (\"\", 1);\n+    }\n+}\n+\n+void\n+test (int x)\n+{\n+  foo (&x, bar);\n+  foo (&x, bar);\n+}"}]}