%2Fdownload_prerequisites?ref=796190e538c96c28b5803f6d3dab1add87950c0e", "patch": "@@ -47,9 +47,12 @@ force=0\n OS=$(uname)\n \n case $OS in\n-  \"Darwin\"|\"FreeBSD\"|\"DragonFly\")\n+  \"Darwin\"|\"FreeBSD\"|\"DragonFly\"|\"AIX\")\n     chksum='shasum -a 512 --check'\n   ;;\n+  \"OpenBSD\")\n+    chksum='sha512 -c'\n+  ;;\n   *)\n     chksum='sha512sum -c'\n   ;;\n@@ -242,8 +245,19 @@ for ar in $(echo_archives)\n do\n     package=\"${ar%.tar*}\"\n     if [ ${force} -gt 0 ]; then rm -rf \"${directory}/${package}\"; fi\n+    case $ar in\n+    *.gz)\n+\tuncompress='gzip -d'\n+\t;;\n+    *.bz2)\n+\tuncompress='bzip2 -d'\n+\t;;\n+    *)\n+\tuncompress='cat'\n+\t;;\n+    esac\n     [ -e \"${directory}/${package}\" ]                                          \\\n-        || ( cd \"${directory}\" && tar -xf \"${ar}\" )                           \\\n+        || ( cd \"${directory}\" && $uncompress <\"${ar}\" | tar -xf - )          \\\n         || die \"Cannot extract package from ${ar}\"\n     unset package\n done"}]}