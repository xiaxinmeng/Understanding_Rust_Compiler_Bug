\n \t   && vector_types_convertible_p (from, to, false))\n     return build_conv (ck_std, to, conv);\n-  /* A derived-to-base conversion sequence is a user-defined conversion\n-     because it involves a constructor call, even though it has the rank of\n-     a standard conversion, so we don't consider it if we aren't allowing\n-     user-defined conversions.  But if we're binding directly to a\n-     reference, it's only a pointer conversion.  */\n-  else if ((!(flags & LOOKUP_NO_CONVERSION)\n-\t    || (flags & LOOKUP_NO_TEMP_BIND))\n-\t   && IS_AGGR_TYPE (to) && IS_AGGR_TYPE (from)\n+  else if (IS_AGGR_TYPE (to) && IS_AGGR_TYPE (from)\n \t   && is_properly_derived_from (from, to))\n     {\n       if (conv->kind == ck_rvalue)"}, {"sha": "63c66d4a099883886878dd25d5407bb37a33619f", "filename": "gcc/testsuite/g++.dg/overload/arg5.C", "status": "added", "additions": 24, "deletions": 0, "changes": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/4c12c8ea94136784569197991369ed98329719ae/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Farg5.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/4c12c8ea94136784569197991369ed98329719ae/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Farg5.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Farg5.C?ref=4c12c8ea94136784569197991369ed98329719ae", "patch": "@@ -0,0 +1,24 @@\n+// PR c++/34111\n+\n+class QChar\n+{\n+};\n+struct QString\n+{\n+  QString(QChar);\n+};\n+struct QPainter\n+{\n+  void drawText (int x, int y, const QString &);\n+};\n+\n+  class KHEChar:public QChar\n+  {\n+  public:KHEChar (QChar C);\n+  };\n+\n+void\n+drawByte (QPainter * P, char, KHEChar B)\n+{\n+  P->drawText (0, 0, B);\n+}"}]}