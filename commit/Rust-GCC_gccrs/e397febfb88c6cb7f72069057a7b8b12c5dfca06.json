/78796 */\n+/* { dg-do run } */\n+/* { dg-options \"-O2\" } */\n+/* { dg-additional-options \"-mcmodel=large\" { target aarch64-*-* } } */\n+/* { dg-require-effective-target tls } */\n+\n+struct S { int a, b, c, d, e; };\n+struct S t;\n+__thread struct S s;\n+\n+__attribute__((used, noinline, noclone)) void\n+foo (int *x, int *y)\n+{\n+  asm volatile (\"\" : : \"g\" (x), \"g\" (y) : \"memory\");\n+  if (*x != 1 || *y != 2)\n+    __builtin_abort ();\n+}\n+\n+__attribute__((used, noinline, noclone)) void\n+bar (void)\n+{\n+  foo (&t.c, &s.c);\n+}\n+\n+int\n+main ()\n+{\n+  t.c = 1;\n+  s.c = 2;\n+  bar ();\n+  return 0;\n+}"}]}