os/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Ftrans-array.c?ref=18b0679f097a3656acf82ebbb7a703bd4c0bb174", "patch": "@@ -5155,6 +5155,9 @@ gfc_conv_array_parameter (gfc_se * se, gfc_expr * expr, gfc_ss * ss, int g77,\n \t\t      && expr->ref->u.ar.type == AR_FULL);\n   sym = full_array_var ? expr->symtree->n.sym : NULL;\n \n+  /* The symbol should have an array specification.  */\n+  gcc_assert (!sym || sym->as);\n+\n   if (expr->expr_type == EXPR_ARRAY && expr->ts.type == BT_CHARACTER)\n     {\n       get_array_ctor_strlen (&se->pre, expr->value.constructor, &tmp);"}, {"sha": "02f91122da7ca81ba69f55a37f299e837185d11e", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/18b0679f097a3656acf82ebbb7a703bd4c0bb174/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/18b0679f097a3656acf82ebbb7a703bd4c0bb174/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=18b0679f097a3656acf82ebbb7a703bd4c0bb174", "patch": "@@ -1,3 +1,8 @@\n+2008-09-09  Daniel Kraft  <d@domob.eu>\n+\n+\tPR fortran/37411\n+\t* gfortran.dg/array_function_4.f90: New test.\n+\n 2008-09-08  Daniel Jacobowitz  <dan@codesourcery.com>\n \t    Mark Mitchell  <mark@codesourcery.com>\n "}, {"sha": "c7e7d6e57c48925a475d123fa846000bf60b5cc2", "filename": "gcc/testsuite/gfortran.dg/array_function_4.f90", "status": "added", "additions": 29, "deletions": 0, "changes": 29, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/18b0679f097a3656acf82ebbb7a703bd4c0bb174/gcc%2Ftestsuite%2Fgfortran.dg%2Farray_function_4.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/18b0679f097a3656acf82ebbb7a703bd4c0bb174/gcc%2Ftestsuite%2Fgfortran.dg%2Farray_function_4.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Farray_function_4.f90?ref=18b0679f097a3656acf82ebbb7a703bd4c0bb174", "patch": "@@ -0,0 +1,29 @@\n+! { dg-do compile }\n+\n+! PR fortran/37411\n+! This used to cause an ICE because of a missing array spec after interface\n+! mapping.\n+\n+! Contributed by Kristjan Jonasson <jonasson@hi.is>\n+\n+MODULE B1\n+CONTAINS\n+  subroutine sub()\n+    integer :: x(1)\n+    character(3) :: st\n+    st = fun(x)\n+  end subroutine sub\n+\n+  function fun(x) result(st)\n+    integer, intent(in) :: x(1)\n+    character(lenf(x)) :: st\n+    st = 'abc'\n+  end function fun\n+\n+  pure integer function lenf(x)\n+    integer, intent(in) :: x(1)\n+    lenf = x(1)\n+  end function lenf\n+END MODULE B1\n+\n+! { dg-final { cleanup-modules \"B1\" } }"}]}