re%2Fpr105598.c?ref=6230911cb91d428631e741d7d981fab578102084", "patch": "@@ -0,0 +1,32 @@\n+/* { dg-do run } */\n+\n+typedef struct { unsigned int num; } info_t;\n+typedef struct { unsigned int flag, type; } block_t;\n+info_t info;\n+block_t blocks[] = { {2,0}, {3,0}, {1,0}, {1,0} };\n+\n+static block_t *\n+f (info_t *i, block_t *b)\n+{\n+  while (1) {\n+    unsigned int is_last = b->flag & 0x01;\n+    i->num++;\n+    if (b->flag & 0x02) {\n+      if (b->type != 0x1) b->type = b->type;\n+      b = f (i, b+1);\n+    }\n+    if (is_last)\n+      break;\n+    b++;\n+  }\n+  return b;\n+}\n+\n+int\n+main ()\n+{\n+  f(&info, &blocks[0]);\n+  if (info.num != 4)\n+    __builtin_abort ();\n+  return 0;\n+}"}]}