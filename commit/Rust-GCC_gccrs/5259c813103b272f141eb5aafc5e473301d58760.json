ic int errors = 0;\n+static int thrs = 4;\n+\n+int\n+main ()\n+{\n+  omp_set_dynamic (0);\n+\n+  #pragma omp parallel num_threads (thrs)\n+  {\n+    static int shrd = 0;\n+\n+    #pragma omp atomic\n+      shrd += 1;\n+\n+    #pragma omp barrier\n+\n+    if (shrd != thrs)\n+      #pragma omp atomic\n+\terrors += 1;\n+  }\n+\n+  if (errors)\n+    abort ();\n+}"}, {"sha": "76c7ac1f3a54c0833da1489964d0aecaef686506", "filename": "libgomp/testsuite/libgomp.c/pr34513.c", "status": "added", "additions": 33, "deletions": 0, "changes": 33, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5259c813103b272f141eb5aafc5e473301d58760/libgomp%2Ftestsuite%2Flibgomp.c%2Fpr34513.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5259c813103b272f141eb5aafc5e473301d58760/libgomp%2Ftestsuite%2Flibgomp.c%2Fpr34513.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libgomp%2Ftestsuite%2Flibgomp.c%2Fpr34513.c?ref=5259c813103b272f141eb5aafc5e473301d58760", "patch": "@@ -0,0 +1,33 @@\n+/* PR c++/34513 */\n+/* { dg-do run } */\n+\n+#include <omp.h>\n+\n+extern void abort ();\n+\n+static int errors = 0;\n+static int thrs = 4;\n+\n+int\n+main ()\n+{\n+  omp_set_dynamic (0);\n+\n+  #pragma omp parallel num_threads (thrs)\n+  {\n+    static int shrd = 0;\n+\n+    #pragma omp atomic\n+      shrd += 1;\n+\n+    #pragma omp barrier\n+\n+    if (shrd != thrs)\n+      #pragma omp atomic\n+\terrors += 1;\n+  }\n+\n+  if (errors)\n+    abort ();\n+  return 0;\n+}"}]}