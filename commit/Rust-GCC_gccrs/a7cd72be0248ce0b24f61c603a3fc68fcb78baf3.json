;\n+  bool dep;\n+\n+  /* Don't bother mangling uninstantiated templates.  */\n+  ++processing_template_decl;\n+  if (TREE_CODE (decl) == TYPE_DECL)\n+    dep = dependent_type_p (TREE_TYPE (decl));\n+  else\n+    dep = (DECL_LANG_SPECIFIC (decl) && DECL_TEMPLATE_INFO (decl)\n+\t   && any_dependent_template_arguments_p (DECL_TI_ARGS (decl)));\n+  --processing_template_decl;\n+  if (dep)\n+    return;\n+\n+  id = get_mangled_id (decl);\n   SET_DECL_ASSEMBLER_NAME (decl, id);\n \n   if (G.need_abi_warning"}, {"sha": "9fa8e43365f242bf092f8b54f539fe4da0fff688", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a7cd72be0248ce0b24f61c603a3fc68fcb78baf3/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a7cd72be0248ce0b24f61c603a3fc68fcb78baf3/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=a7cd72be0248ce0b24f61c603a3fc68fcb78baf3", "patch": "@@ -1,3 +1,8 @@\n+2012-01-16  Jason Merrill  <jason@redhat.com>\n+\n+\tPR c++/51827\n+\t* g++.dg/pch/mangle1.{C,Hs}: New.\n+\n 2012-01-16  Mikael Morin  <mikael@gcc.gnu.org>\n \t    Tobias Burnus  <burnus@net-b.de>\n "}, {"sha": "504fa2dcb77ec8f7686e01ce795e61d49d8d3c7b", "filename": "gcc/testsuite/g++.dg/pch/mangle1.C", "status": "added", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a7cd72be0248ce0b24f61c603a3fc68fcb78baf3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fpch%2Fmangle1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a7cd72be0248ce0b24f61c603a3fc68fcb78baf3/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fpch%2Fmangle1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fpch%2Fmangle1.C?ref=a7cd72be0248ce0b24f61c603a3fc68fcb78baf3", "patch": "@@ -0,0 +1,3 @@\n+// { dg-options -std=c++11 }\n+\n+#include \"mangle1.Hs\""}, {"sha": 