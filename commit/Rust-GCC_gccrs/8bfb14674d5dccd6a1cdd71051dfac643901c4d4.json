buffer (pfile, (const uchar *) buf, count,\n \t\t   /* from_stage3 */ true, 1);\n+  /* Disgusting hack.  */\n+  if (dir_no == T_PRAGMA)\n+    pfile->buffer->inc = pfile->buffer->prev->inc;\n   start_directive (pfile);\n   /* We don't want a leading # to be interpreted as a directive.  */\n   pfile->buffer->saved_flags = 0;\n@@ -467,6 +470,8 @@ run_directive (pfile, dir_no, buf, count)\n     prepare_directive_trad (pfile);\n   (void) (*pfile->directive->handler) (pfile);\n   end_directive (pfile, 1);\n+  if (dir_no == T_PRAGMA)\n+    pfile->buffer->inc = NULL;\n   _cpp_pop_buffer (pfile);\n }\n "}, {"sha": "e9fd9277f84277bc607dad763684cfa938624fec", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/8bfb14674d5dccd6a1cdd71051dfac643901c4d4/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/8bfb14674d5dccd6a1cdd71051dfac643901c4d4/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=8bfb14674d5dccd6a1cdd71051dfac643901c4d4", "patch": "@@ -1,3 +1,7 @@\n+2002-08-14  Neil Booth  <neil@daikokuya.co.uk>\n+\n+\t* gcc.dg/cpp/_Pragma3.c: New test.\n+\n 2002-08-13  Mark Mitchell  <mark@codesourcery.com>\n \n \t* g++.dg/template/inherit3: New test."}, {"sha": "7d1b42aab63cd3dc015f8b46ba7ff80155c29bf9", "filename": "gcc/testsuite/gcc.dg/cpp/_Pragma3.c", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/8bfb14674d5dccd6a1cdd71051dfac643901c4d4/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F_Pragma3.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/8bfb14674d5dccd6a1cdd71051dfac643901c4d4/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F_Pragma3.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fcpp%2F_Pragma3.c?ref=8bfb14674d5dccd6a1cdd71051dfac643901c4d4", "patch": "@@ -0,0 +1,11 @@\n+/* Copyright (C) 2002 Free Software Foundation, Inc.  */\n+\n+/* { dg-do preprocess } */\n+\n+/* Pragma buffers have a NULL \"inc\" member, which we would dereference\n+   when getting a file's date and time.\n+\n+   Based on PR 7526.  14 Aug 2002.  */\n+\n+#define GCC_PRAGMA(x) _Pragma (#x)\n+GCC_PRAGMA(GCC dependency \"mi1c.h\")"}]}