atch": "@@ -8590,9 +8590,10 @@ convert_for_assignment (tree type, tree rhs,\n \t      if (rhstype == unknown_type_node)\n \t\t{\n \t\t  tree r = instantiate_type (type, rhs, tf_warning_or_error);\n-\t\t  /* -fpermissive might allow this.  */\n+\t\t  /* -fpermissive might allow this; recurse.  */\n \t\t  if (!seen_error ())\n-\t\t    return r;\n+\t\t    return convert_for_assignment (type, r, errtype, fndecl,\n+\t\t\t\t\t\t   parmnum, complain, flags);\n \t\t}\n \t      else if (fndecl)\n \t\terror (\"cannot convert %qH to %qI for argument %qP to %qD\","}, {"sha": "5c66b72cb8f1df6df913a3ccc4497229eac24f9a", "filename": "gcc/testsuite/g++.dg/template/ptrmem31.C", "status": "added", "additions": 23, "deletions": 0, "changes": 23, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b126bff44d96201be452bf3d1b220c515bc0e26a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fptrmem31.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b126bff44d96201be452bf3d1b220c515bc0e26a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fptrmem31.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fptrmem31.C?ref=b126bff44d96201be452bf3d1b220c515bc0e26a", "patch": "@@ -0,0 +1,23 @@\n+// PR c++/80639\n+// { dg-do compile { target c++14 } }\n+\n+template < typename > struct A;\n+\n+struct B\n+{ \n+  template < int > void m ();\n+  template < int > struct K { static void n (); };\n+  void p () { K < 0 >::n (); }\n+};\n+\n+template <> struct A < B >\n+{ \n+  using T = void (A::*)();\n+  template < int u > static constexpr T h = &B::m < u >; // { dg-error \"cannot convert\" }\n+};\n+\n+template < int v > void B::K < v >::n ()\n+{ \n+  using S = A < B >;\n+  S::h < 0 >;\n+}"}]}