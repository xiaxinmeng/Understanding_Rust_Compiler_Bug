 PR c++/85827\n+// { dg-do compile { target c++17 } }\n+// { dg-options \"-Wunused-variable -Wunused-parameter\" }\n+\n+template <int N> int f()\n+{\n+    constexpr bool _1 = N == 1;\n+    constexpr bool _2 = N == 2;\n+    constexpr bool _3 = N == 3;\n+    \n+    if constexpr (_1) {\n+        return 5;\n+    } else if constexpr (_2) {\n+        return 1;\n+    } else if constexpr (_3) {\n+        return 7;\n+    }\n+}\n+\n+int a() {\n+    return f<1>();\n+}\n+int b() {\n+    return f<2>();\n+}\n+int c() {\n+    return f<3>();\n+}"}]}