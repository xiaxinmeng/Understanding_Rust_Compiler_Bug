": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c8e07c7951421e718bcafbe5924e75c9aa133af9/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr96102b.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c8e07c7951421e718bcafbe5924e75c9aa133af9/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr96102b.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr96102b.f90?ref=c8e07c7951421e718bcafbe5924e75c9aa133af9", "patch": "@@ -0,0 +1,24 @@\n+! { dg-do compile }\n+!\n+! PR fortran/108544 - host association\n+! Variation of testcase pr96102.f90 using subroutines instead of functions\n+\n+module m\n+  type mytype\n+    integer :: i\n+  end type\n+  type(mytype) :: d = mytype (42) ! { dg-error \"is host associated\" }\n+  integer      :: n = 2           ! { dg-error \"is host associated\" }\n+contains\n+  subroutine s\n+    if ( n   /= 0 ) stop 1  ! { dg-error \"internal procedure of the same name\" }\n+    if ( d%i /= 0 ) stop 2  ! { dg-error \"internal procedure of the same name\" }\n+  contains\n+    subroutine n()\n+    end\n+    subroutine d()\n+    end\n+  end\n+end\n+\n+! { dg-prune-output \"Operands of comparison operator\" }"}]}