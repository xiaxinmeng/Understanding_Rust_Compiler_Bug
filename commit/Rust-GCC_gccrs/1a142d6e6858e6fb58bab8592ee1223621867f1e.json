n \n   /* No need to check access within an enum.  */\n-  if (TREE_CODE (qualifying_class) == ENUMERAL_TYPE)\n+  if (TREE_CODE (qualifying_class) == ENUMERAL_TYPE\n+      && TREE_CODE (expr) != IDENTIFIER_NODE)\n     return expr;\n \n   /* Within the scope of a class, turn references to non-static"}, {"sha": "3bd66395787ac753b5c5b92d88ac6c0ac779e200", "filename": "gcc/testsuite/g++.dg/cpp0x/scoped_enum8.C", "status": "added", "additions": 25, "deletions": 0, "changes": 25, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1a142d6e6858e6fb58bab8592ee1223621867f1e/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fscoped_enum8.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1a142d6e6858e6fb58bab8592ee1223621867f1e/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fscoped_enum8.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fscoped_enum8.C?ref=1a142d6e6858e6fb58bab8592ee1223621867f1e", "patch": "@@ -0,0 +1,25 @@\n+// PR c++/85587\n+// { dg-do compile { target c++11 } }\n+\n+template <int N>\n+struct S\n+{\n+  enum class T\n+  {\n+    E, F\n+  };\n+  void foo ();\n+};\n+\n+template <int N>\n+void S<N>::foo ()\n+{\n+  decltype (T::F) t;\n+}\n+\n+void\n+bar ()\n+{\n+  S<0> s;\n+  s.foo ();\n+}"}]}