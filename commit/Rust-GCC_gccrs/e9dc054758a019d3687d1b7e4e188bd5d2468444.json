bd5d2468444", "patch": "@@ -0,0 +1,14 @@\n+-- { dg-do run }\n+\n+with Aggr21_Pkg; use Aggr21_Pkg;\n+\n+procedure Aggr21 is\n+  V : Rec;\n+begin\n+  V.A := 12;\n+  V.S (1 .. 10) := \"Hello init\";\n+  V.N := 123;\n+  Init (V);\n+  --  Probably not reliable, but the compiler is supposed not to modify V.S\n+  pragma Assert (V.s (1 .. 5) = \"Hello\");\n+end;"}, {"sha": "5cdd089cf1e6c1cd5a6cc3f4528247afe0ef6047", "filename": "gcc/testsuite/gnat.dg/aggr21_pkg.adb", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e9dc054758a019d3687d1b7e4e188bd5d2468444/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e9dc054758a019d3687d1b7e4e188bd5d2468444/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.adb?ref=e9dc054758a019d3687d1b7e4e188bd5d2468444", "patch": "@@ -0,0 +1,8 @@\n+package body Aggr21_Pkg is\n+\n+   procedure Init (R : out Rec) is\n+   begin\n+      R := (A => 5, S => <>, N => 7);\n+   end;\n+\n+end Aggr21_Pkg;"}, {"sha": "3a14963b13047f0688c44ff6cdf1161ad74e0359", "filename": "gcc/testsuite/gnat.dg/aggr21_pkg.ads", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e9dc054758a019d3687d1b7e4e188bd5d2468444/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e9dc054758a019d3687d1b7e4e188bd5d2468444/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Faggr21_pkg.ads?ref=e9dc054758a019d3687d1b7e4e188bd5d2468444", "patch": "@@ -0,0 +1,11 @@\n+package Aggr21_Pkg is\n+\n+   type Rec is record\n+      A : Integer;\n+      S : String (1 .. 120);\n+      N : Natural;\n+   end record;\n+\n+   procedure Init (R : out Rec);\n+\n+end Aggr21_Pkg;"}]}