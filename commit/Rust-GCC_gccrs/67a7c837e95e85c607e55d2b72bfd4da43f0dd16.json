n+\n+\tPR fortran/49074\n+\t* gfortran.dg/typebound_assignment_3.f03: New.\n+\n 2011-06-16  Steve Ellcey  <sje@cup.hp.com>\n \n \tPR testsuite/48727"}, {"sha": "ce84a3957d00da03bbbba443a8b1a19ac8266926", "filename": "gcc/testsuite/gfortran.dg/typebound_assignment_3.f03", "status": "added", "additions": 30, "deletions": 0, "changes": 30, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/67a7c837e95e85c607e55d2b72bfd4da43f0dd16/gcc%2Ftestsuite%2Fgfortran.dg%2Ftypebound_assignment_3.f03", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/67a7c837e95e85c607e55d2b72bfd4da43f0dd16/gcc%2Ftestsuite%2Fgfortran.dg%2Ftypebound_assignment_3.f03", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Ftypebound_assignment_3.f03?ref=67a7c837e95e85c607e55d2b72bfd4da43f0dd16", "patch": "@@ -0,0 +1,30 @@\n+! { dg-do compile }\n+!\n+! PR 49074: [OOP] Defined assignment w/ CLASS arrays: Incomplete error message\n+!\n+! Contribute by Jerry DeLisle <jvdelisle@gcc.gnu.org>\n+\n+module foo\n+\n+  type bar\n+  contains\n+    generic :: assignment (=) => assgn\n+    procedure :: assgn\n+  end type\n+\n+contains\n+\n+  elemental subroutine assgn (a, b)\n+    class (bar), intent (inout) :: a\n+    class (bar), intent (in) :: b\n+  end subroutine\n+\n+end module\n+\n+\n+  use foo\n+  type (bar) :: foobar(2)\n+  foobar = bar()           ! { dg-error \"currently not implemented\" }\n+end\n+\n+! { dg-final { cleanup-modules \"foo\" } }"}]}