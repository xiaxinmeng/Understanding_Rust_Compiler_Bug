/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Flookup%2Ftwo-stage5.C?ref=5b8ce8f102ac8717eada20fb7f1f9fd2be091258", "patch": "@@ -0,0 +1,19 @@\n+// { dg-do run }\n+extern \"C\" void abort ();\n+\n+namespace N {\n+template <class T> T foo (T) { return T (); }\n+template <class T> T bar (T t) { return foo (t); }\n+}\n+\n+struct S { S (int i = 0): i_ (i) { } int i_; };\n+\n+namespace N {\n+/* template <> */ S foo (S) { return S (1); }\n+}\n+\n+int main ()\n+{\n+  if (1 == N::bar (S()).i_)\n+     abort ();\n+} "}]}