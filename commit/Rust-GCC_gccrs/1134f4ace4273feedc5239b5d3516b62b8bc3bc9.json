+#include <typeinfo>\n+\n+struct A\n+{\n+  virtual ~A() { }\n+};\n+\n+class B : public A { };\n+\n+int main ()\n+{\n+  B b;\n+  A &aref = b;\n+\n+  return typeid (aref) != typeid (b);\n+}"}]}