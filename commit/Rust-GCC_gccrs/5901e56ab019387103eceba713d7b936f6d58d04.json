rs/contents/gcc%2Flra-spills.c?ref=5901e56ab019387103eceba713d7b936f6d58d04", "patch": "@@ -528,11 +528,12 @@ spill_pseudos (void)\n \t  if (removed_pseudo_p)\n \t    {\n \t      lra_assert (DEBUG_INSN_P (insn));\n-\t      lra_set_insn_deleted (insn);\n+\t      lra_invalidate_insn_data (insn);\n+\t      INSN_VAR_LOCATION_LOC (insn) = gen_rtx_UNKNOWN_VAR_LOC ();\n \t      if (lra_dump_file != NULL)\n \t\tfprintf (lra_dump_file,\n-\t\t\t \"Debug insn #%u is deleted as containing removed pseudo\\n\",\n-\t\t\t INSN_UID (insn));\n+\t\t\t \"Debug insn #%u is reset because it referenced \"\n+\t\t\t \"removed pseudo\\n\", INSN_UID (insn));\n \t    }\n \t  bitmap_and_compl_into (df_get_live_in (bb), &spilled_pseudos);\n \t  bitmap_and_compl_into (df_get_live_out (bb), &spilled_pseudos);"}, {"sha": "e49f83c101f4304d196a04836d10c6ece6338e23", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5901e56ab019387103eceba713d7b936f6d58d04/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5901e56ab019387103eceba713d7b936f6d58d04/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=5901e56ab019387103eceba713d7b936f6d58d04", "patch": "@@ -1,3 +1,8 @@\n+2016-04-12  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR rtl-optimization/70596\n+\t* gcc.target/i386/pr70596.c: New test.\n+\n 2016-04-12  Nathan Sidwell  <nathan@acm.org>\n \n \tPR c++/70501"}, {"sha": "0c73e838be1897c8f629fbb400522e78bbc6ee2f", "filename": "gcc/testsuite/gcc.target/i386/pr70596.c", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5901e56ab019387103eceba713d7b936f6d58d04/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr70596.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5901e56ab019387103eceba713d7b936f6d58d04/gcc%2Ftestsuite%2Fgcc.target%