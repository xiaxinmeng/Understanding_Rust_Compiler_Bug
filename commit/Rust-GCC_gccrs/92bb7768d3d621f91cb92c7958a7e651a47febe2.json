7e651a47febe2/gcc%2Ffortran%2Ftrans-expr.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Ftrans-expr.c?ref=92bb7768d3d621f91cb92c7958a7e651a47febe2", "patch": "@@ -2492,12 +2492,14 @@ gfc_conv_derived_to_class (gfc_se *parmse, gfc_expr *e,\n   ss = gfc_walk_expr (e);\n   if (ss == gfc_ss_terminator)\n     {\n+      parmse->ss = NULL;\n       gfc_conv_expr_reference (parmse, e);\n       tmp = fold_convert (TREE_TYPE (ctree), parmse->expr);\n       gfc_add_modify (&parmse->pre, ctree, tmp);\n     }\n   else\n     {\n+      parmse->ss = ss;\n       gfc_conv_expr (parmse, e);\n       gfc_add_modify (&parmse->pre, ctree, parmse->expr);\n     }"}, {"sha": "aeda5b9399da66a3ff21e3e5b56008f4eef43985", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/92bb7768d3d621f91cb92c7958a7e651a47febe2/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/92bb7768d3d621f91cb92c7958a7e651a47febe2/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=92bb7768d3d621f91cb92c7958a7e651a47febe2", "patch": "@@ -1,3 +1,9 @@\n+2010-06-11  Paul Thomas  <pault@gcc.gnu.org>\n+\n+\tPR fortran/42051\n+\tPR fortran/43896\n+\t* gfortran.dg/class_23.f03: New test.\n+\n 2010-06-11  Jan Hubicka  <jh@suse.cz>\n \n \t* gcc.dg/ipa/pure-const-2.c: New testcase."}, {"sha": "e1e3517629485ec5a1dd15b5c17cae7424f61776", "filename": "gcc/testsuite/gfortran.dg/class_23.f03", "status": "added", "additions": 24, "deletions": 0, "changes": 24, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/92bb7768d3d621f91cb92c7958a7e651a47febe2/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_23.f03", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/92bb7768d3d621f91cb92c7958a7e651a47febe2/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_23.f03", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fclass_23.f03?ref=92bb7768d3d621f91cb92c7958a7e651a47febe2", "patch": "@@ -0,0 +1,24 @@\n+! { dg-do compile }\n+!\n+! PR 42051: [OOP] ICE on array-valued function with CLASS formal argument\n+!\n+! Original test case by Damian Rouson <damian@rouson.net>\n+! Modified by Janus Weil <janus@gcc.gnu.org>\n+\n+  type grid\n+  end type \n+\n+contains\n+\n+  function return_x(this) result(this_x)\n+    class(grid) :: this\n+    real  ,dimension(1) :: this_x\n+  end function\n+\n+  subroutine output()\n+    type(grid) :: mesh\n+    real ,dimension(1) :: x\n+    x = return_x(mesh)\n+  end subroutine\n+\n+end"}]}