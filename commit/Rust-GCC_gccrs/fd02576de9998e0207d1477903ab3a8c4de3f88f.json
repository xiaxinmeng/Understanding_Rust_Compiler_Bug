rl": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Fpr66174.c?ref=fd02576de9998e0207d1477903ab3a8c4de3f88f", "patch": "@@ -0,0 +1,30 @@\n+/* { dg-do compile } */\n+/* { dg-options \"-O -ftree-vectorize -mavx512f\" } */\n+\n+extern void abort (void);\n+\n+typedef struct {\n+   unsigned int a;\n+   unsigned int b;\n+} ii;\n+\n+void foo (unsigned short *arr, ii *iarr)\n+{\n+  ii *iptr = iarr;\n+  unsigned short res[128];\n+  ii ires[128];\n+  int i;\n+  for (i = 0; i < 128; i++)\n+    {\n+      ires[i].a = iptr->b - iptr->a;\n+      ires[i].b = iptr->b + iptr->a;\n+      iptr++;\n+    }\n+  for (i = 0; i < 128; i++)\n+    {\n+      if (res[i] != arr[i]\n+          || ires[i].a != iarr[i].b - iarr[i].a\n+          || ires[i].b != iarr[i].b + iarr[i].a)\n+        abort ();\n+    }\n+}"}]}