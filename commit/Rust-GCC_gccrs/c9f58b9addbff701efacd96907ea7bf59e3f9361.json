_static_dynamic=yes\n   else\n     case \"$target\" in\n+      # IRIX 6 ld supports -Bstatic/-Bdynamic.\n+      mips-sgi-irix6*)\n+        gcc_cv_ld_static_dynamic=yes\n+        ;;\n       # Solaris 2 ld always supports -Bstatic/-Bdynamic.\n       *-*-solaris2*)\n         gcc_cv_ld_static_dynamic=yes"}, {"sha": "60d4f3173fef20d35d320a4d16017fd3150635b4", "filename": "gcc/configure.ac", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c9f58b9addbff701efacd96907ea7bf59e3f9361/gcc%2Fconfigure.ac", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c9f58b9addbff701efacd96907ea7bf59e3f9361/gcc%2Fconfigure.ac", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fconfigure.ac?ref=c9f58b9addbff701efacd96907ea7bf59e3f9361", "patch": "@@ -3123,6 +3123,10 @@ elif test x$gcc_cv_ld != x; then\n       gcc_cv_ld_static_dynamic=yes\n   else\n     case \"$target\" in\n+      # IRIX 6 ld supports -Bstatic/-Bdynamic.\n+      mips-sgi-irix6*)\n+        gcc_cv_ld_static_dynamic=yes\n+        ;;\n       # Solaris 2 ld always supports -Bstatic/-Bdynamic.\n       *-*-solaris2*)\n         gcc_cv_ld_static_dynamic=yes"}]}