ny_dependent_bases_p (tree type)\n {\n-  if (!type || !CLASS_TYPE_P (type) || !processing_template_decl)\n+  if (!type || !CLASS_TYPE_P (type) || !uses_template_parms (type))\n     return false;\n \n   /* If we haven't set TYPE_BINFO yet, we don't know anything about the bases."}, {"sha": "e38b968e7749389cc8b80d274a82232e3a14fac0", "filename": "gcc/testsuite/g++.dg/template/dependent-base3.C", "status": "added", "additions": 26, "deletions": 0, "changes": 26, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/e597f6822c64f3886bdfa7f6a371470cdb642903/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fdependent-base3.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/e597f6822c64f3886bdfa7f6a371470cdb642903/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fdependent-base3.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Fdependent-base3.C?ref=e597f6822c64f3886bdfa7f6a371470cdb642903", "patch": "@@ -0,0 +1,26 @@\n+// PR c++/85060\n+// { dg-do compile { target c++14 } }\n+\n+struct CA {\n+  constexpr int foo() const { return 42; }\n+};\n+\n+template <class T>\n+struct CB : CA { };\n+\n+template <class T>\n+struct CC : CB<T> {\n+  constexpr int bar() const {\n+    const int m = CA::foo();\n+    return m;\n+  }\n+\n+  constexpr int baz() const {\n+    const T m = CA::foo();\n+    return m;\n+  }\n+};\n+\n+constexpr CC<double> c;\n+\n+static_assert( c.bar() == 42, \"\" );"}]}