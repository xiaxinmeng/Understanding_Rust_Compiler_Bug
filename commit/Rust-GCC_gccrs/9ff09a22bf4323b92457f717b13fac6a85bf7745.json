/Rust-GCC/gccrs/contents/gcc%2Ftree-ssa-loop-ivcanon.c?ref=9ff09a22bf4323b92457f717b13fac6a85bf7745", "patch": "@@ -1085,8 +1085,9 @@ propagate_constants_for_unrolling (basic_block bb)\n       tree lhs;\n \n       if (is_gimple_assign (stmt)\n+\t  && gimple_assign_rhs_code (stmt) == INTEGER_CST\n \t  && (lhs = gimple_assign_lhs (stmt), TREE_CODE (lhs) == SSA_NAME)\n-\t  && gimple_assign_rhs_code (stmt) == INTEGER_CST)\n+\t  && !SSA_NAME_OCCURS_IN_ABNORMAL_PHI (lhs))\n \t{\n \t  propagate_into_all_uses (lhs, gimple_assign_rhs1 (stmt));\n \t  gsi_remove (&gsi, true);"}]}