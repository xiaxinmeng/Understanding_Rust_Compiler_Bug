ntype-class12.C?ref=52af30719fde1430bd4c4a686fe4375be1a7b0cf", "patch": "@@ -0,0 +1,23 @@\n+// PR c++/88744\n+// { dg-do compile { target c++2a } }\n+\n+#define SA(X) static_assert((X),#X)\n+\n+struct S {\n+  int a;\n+  int b;\n+  constexpr S(int a_, int b_) : a{a_}, b{b_} { }\n+};\n+\n+template<S s = {1, 2}>\n+struct X {\n+  static constexpr int i = s.a;\n+  static constexpr int j = s.b;\n+};\n+X x; // ok, X<{1, 2}>\n+X<{3, 4}> x2;\n+\n+SA (x.i == 1);\n+SA (x.j == 2);\n+SA (x2.i == 3);\n+SA (x2.j == 4);"}]}