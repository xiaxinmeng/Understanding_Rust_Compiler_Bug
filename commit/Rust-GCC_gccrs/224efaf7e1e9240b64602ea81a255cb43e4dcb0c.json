a255cb43e4dcb0c/gcc%2Ftestsuite%2Fgfortran.dg%2Fselect_rank_4.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/224efaf7e1e9240b64602ea81a255cb43e4dcb0c/gcc%2Ftestsuite%2Fgfortran.dg%2Fselect_rank_4.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fselect_rank_4.f90?ref=224efaf7e1e9240b64602ea81a255cb43e4dcb0c", "patch": "@@ -0,0 +1,26 @@\n+! { dg-do compile }\n+!\n+! PR fortran/93522\n+!\n+! Contributed by Shubham Narlawar\n+\n+program rank_new\n+    implicit none\n+    integer :: some_var_assumed\n+    integer, DIMENSION(3,2,1) :: array\n+    PRINT *, RANK(array)\n+   call CALL_ME(array)\n+   contains\n+!No error expected\n+  subroutine CALL_ME23(x)\n+                implicit none\n+                integer:: x(..), a=10,b=20\n+                integer, dimension(10) :: arr = (/1,2,3,4,5/)  ! { dg-error \"Different shape for array assignment at .1. on dimension 1 .10 and 5.\" }\n+                select rank(arr(1:3))  ! { dg-error \"Syntax error in argument list\" }\n+                RANK(1)  ! { dg-error \"Unexpected RANK statement\" }\n+                        print *, \"1\"\n+                 rank(2)  ! { dg-error \"Unexpected RANK statement\" }\n+                        print *, \"2\"\n+                end select  ! { dg-error \"Expecting END SUBROUTINE statement\" }\n+        end subroutine\n+end program"}]}