 (define_insn \"*vec_dupv4si\"\n-  [(set (match_operand:V4SI 0 \"register_operand\"     \"=x,x,x\")\n+  [(set (match_operand:V4SI 0 \"register_operand\"     \"=v,v,x\")\n \t(vec_duplicate:V4SI\n-\t  (match_operand:SI 1 \"nonimmediate_operand\" \" x,m,0\")))]\n+\t  (match_operand:SI 1 \"nonimmediate_operand\" \"Yv,m,0\")))]\n   \"TARGET_SSE\"\n   \"@\n    %vpshufd\\t{$0, %1, %0|%0, %1, 0}\n@@ -17019,13 +17019,13 @@\n    (set_attr \"type\" \"sselog1,ssemov,sselog1\")\n    (set_attr \"length_immediate\" \"1,0,1\")\n    (set_attr \"prefix_extra\" \"0,1,*\")\n-   (set_attr \"prefix\" \"maybe_vex,vex,orig\")\n+   (set_attr \"prefix\" \"maybe_vex,maybe_evex,orig\")\n    (set_attr \"mode\" \"TI,V4SF,V4SF\")])\n \n (define_insn \"*vec_dupv2di\"\n-  [(set (match_operand:V2DI 0 \"register_operand\"     \"=x,x,x,x\")\n+  [(set (match_operand:V2DI 0 \"register_operand\"     \"=x,v,v,x\")\n \t(vec_duplicate:V2DI\n-\t  (match_operand:DI 1 \"nonimmediate_operand\" \" 0,x,m,0\")))]\n+\t  (match_operand:DI 1 \"nonimmediate_operand\" \" 0,Yv,m,0\")))]\n   \"TARGET_SSE\"\n   \"@\n    punpcklqdq\\t%0, %0\n@@ -17034,7 +17034,7 @@\n    movlhps\\t%0, %0\"\n   [(set_attr \"isa\" \"sse2_noavx,avx,sse3,noavx\")\n    (set_attr \"type\" \"sselog1,sselog1,sselog1,ssemov\")\n-   (set_attr \"prefix\" \"orig,vex,maybe_vex,orig\")\n+   (set_attr \"prefix\" \"orig,maybe_evex,maybe_vex,orig\")\n    (set_attr \"mode\" \"TI,TI,DF,V4SF\")])\n \n (define_insn \"avx2_vbroadcasti128_<mode>\""}, {"sha": "05f4e3a7dd63e231e2cebdef11d1ac4a83784e7e", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/94ad56c7c264af0af07492b60716fab08559ef57/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/94ad56c7c264af0af07492b60716fab08559ef57/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=94ad56c7c264af0af07492b60716fab08559ef57", "patch": "@@ -1,3 +1,7 @@\n+2016-05-22  Jakub Jelinek  <jakub@redhat.com>\n+\n+\t* gcc.target/i386/avx512vl-vbroadcast-1.c: New test.\n+\n 2016-05-22  Kugan Vivekanandarajah  <kuganv@linaro.org>\n \n \tPR middle-end/40921"}, {"sha": "8a74675f4dc4b9b156e554b56c1253393255dccd", "filename": "gcc/testsuite/gcc.target/i386/avx512vl-vbroadcast-1.c", "status": "added", "additions": 41, "deletions": 0, "changes": 41, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/94ad56c7c264af0af07492b60716fab08559ef57/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Favx512vl-vbroadcast-1.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/94ad56c7c264af0af07492b60716fab08559ef57/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Favx512vl-vbroadcast-1.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.target%2Fi386%2Favx512vl-vbroadcast-1.c?ref=94ad56c7c264af0af07492b60716fab08559ef57", "patch": "@@ -0,0 +1,41 @@\n+/* { dg-do compile { target { ! ia32 } } } */\n+/* { dg-options \"-O2 -mavx512vl\" } */\n+\n+#include <x86intrin.h>\n+\n+void\n+f1 (__m128d x)\n+{\n+  register __m128d a __asm (\"xmm16\");\n+  register __m256d b __asm (\"xmm17\");\n+  a = x;\n+  asm volatile (\"\" : \"+v\" (a));\n+  b = _mm256_broadcastsd_pd (a);\n+  asm volatile (\"\" : \"+v\" (b));\n+}\n+\n+/* { dg-final { scan-assembler \"vbroadcastsd\\[^\\n\\r]*(xmm16\\[^\\n\\r]*ymm17|ymm17\\[^\\n\\r]*xmm16)\" } } */\n+\n+void\n+f2 (float const *x)\n+{\n+  register __m128 a __asm (\"xmm16\");\n+  a = _mm_broadcast_ss (x);\n+  asm volatile (\"\" : \"+v\" (a));\n+}\n+\n+/* { dg-final { scan-assembler \"vbroadcastss\\[^\\n\\r]*(\\\\)\\[^\\n\\r]*xmm16|xmm16\\[^\\n\\r]*PTR)\" } } */\n+\n+void\n+f3 (float x)\n+{\n+  register float a __asm (\"xmm16\");\n+  register __m128 b __asm (\"xmm17\");\n+  a = x;\n+  asm volatile (\"\" : \"+v\" (a));\n+  float c = a;\n+  b = _mm_broadcast_ss (&c);\n+  asm volatile (\"\" : \"+v\" (b));\n+}\n+\n+/* { dg-final { scan-assembler \"vbroadcastss\\[^\\n\\r]*xmm1\\[67]\\[^\\n\\r]*xmm1\\[67]\" } } */"}]}