ool move_p, tree *spec_p, bool *trivial_p,\n       goto bad;\n     }\n \n-  if (constexpr_p && !DECL_DECLARED_CONSTEXPR_P (fn))\n-    *constexpr_p = false;\n+  if (constexpr_p)\n+    {\n+      /* If this is a specialization of a constexpr template, we need to\n+\t force the instantiation now so that we know whether or not it's\n+\t really constexpr.  */\n+      if (DECL_DECLARED_CONSTEXPR_P (fn) && DECL_TEMPLATE_INSTANTIATION (fn)\n+\t  && !DECL_TEMPLATE_INSTANTIATED (fn))\n+\tinstantiate_decl (fn, /*defer_ok*/false, /*expl_class*/false);\n+      if (!DECL_DECLARED_CONSTEXPR_P (fn))\n+\t*constexpr_p = false;\n+    }\n \n   return;\n "}, {"sha": "ed59f91eff34c65ac43c91f77101c52a9f0ddc42", "filename": "gcc/cp/optimize.c", "status": "modified", "additions": 1, "deletions": 0, "changes": 1, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Fcp%2Foptimize.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Fcp%2Foptimize.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Foptimize.c?ref=525c617d3b92c10f79d1ffc786fb87dbf0958f0b", "patch": "@@ -256,6 +256,7 @@ maybe_clone_body (tree fn)\n       /* Update CLONE's source position information to match FN's.  */\n       DECL_SOURCE_LOCATION (clone) = DECL_SOURCE_LOCATION (fn);\n       DECL_DECLARED_INLINE_P (clone) = DECL_DECLARED_INLINE_P (fn);\n+      DECL_DECLARED_CONSTEXPR_P (clone) = DECL_DECLARED_CONSTEXPR_P (fn);\n       DECL_COMDAT (clone) = DECL_COMDAT (fn);\n       DECL_WEAK (clone) = DECL_WEAK (fn);\n "}, {"sha": "9eff5ffcd2f530c5632d05a706dbc657c198558f", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=525c617d3b92c10f79d1ffc786fb87dbf0958f0b", "patch": "@@ -1,3 +1,7 @@\n+2011-02-20  Jason Merrill  <jason@redhat.com>\n+\n+\t* g++.dg/cpp0x/constexpr-synth1.C: New.\n+\n 2011-02-20  Nicola Pero  <nicola.pero@meta-innovation.com>\n \n \t* objc.dg/layout-2.m: New."}, {"sha": "9830939806b9de1ba3d14a7ab5efd405736b3cd0", "filename": "gcc/testsuite/g++.dg/cpp0x/constexpr-synth1.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-synth1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/525c617d3b92c10f79d1ffc786fb87dbf0958f0b/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-synth1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fconstexpr-synth1.C?ref=525c617d3b92c10f79d1ffc786fb87dbf0958f0b", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/46472\n+// { dg-options -std=c++0x }\n+\n+template<class T> struct A {\n+  T t;\n+  constexpr A(){}\n+};\n+\n+struct B\n+{\n+  A<int> a;\n+};\n+\n+B b;"}]}