 bar (float *);\n+\n+void\n+foo (void)\n+{\n+  int i;\n+  float f[3] = { 0.0f, 0.0f, 0.0f };\n+#pragma omp parallel for default(none) reduction(+:f[:3])\n+  for (i = 0; i < 1000; i++)\n+    {\n+      int j;\n+      float k[3] = { 0.25f, 0.5f, 0.75f };\n+      for (j = 0; j < 3; j++)\n+\tf[j] += k[j];\n+    }\n+  bar (f);\n+}"}]}