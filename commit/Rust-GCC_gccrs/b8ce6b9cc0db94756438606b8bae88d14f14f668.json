ntrinsics%2Fchmod.c?ref=b8ce6b9cc0db94756438606b8bae88d14f14f668", "patch": "@@ -459,25 +459,27 @@ chmod_func (char *name, char *mode, gfc_charlen_type name_len,\n \tif ((ugo[2] || honor_umask) && !rwxXstugo[8])\n \t  file_mode = (file_mode & ~(S_IROTH | S_IWOTH | S_IXOTH))\n \t\t      | (new_mode & (S_IROTH | S_IWOTH | S_IXOTH));\n+#ifndef __VXWORKS__\n \tif (is_dir && rwxXstugo[5])\n \t  file_mode |= S_ISVTX;\n \telse if (!is_dir)\n \t  file_mode &= ~S_ISVTX;\n+#endif\n #endif\n       }\n     else if (set_mode == 2)\n       {\n \t/* Clear '-'.  */\n \tfile_mode &= ~new_mode;\n-#ifndef __MINGW32__\n+#if !defined( __MINGW32__) && !defined (__VXWORKS__)\n \tif (rwxXstugo[5] || !is_dir)\n \t  file_mode &= ~S_ISVTX;\n #endif\n       }\n     else if (set_mode == 3)\n       {\n \tfile_mode |= new_mode;\n-#ifndef __MINGW32__\n+#if !defined (__MINGW32__) && !defined (__VXWORKS__)\n \tif (rwxXstugo[5] && is_dir)\n \t  file_mode |= S_ISVTX;\n \telse if (!is_dir)"}]}