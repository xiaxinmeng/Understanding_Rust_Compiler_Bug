 Weil  <janus@gcc.gnu.org>\n+\n+\tPR fortran/56968\n+\t* gfortran.dg/proc_ptr_41.f90: New.\n+\n 2013-04-16  Richard Biener  <rguenther@suse.de>\n \n \tPR tree-optimization/56756"}, {"sha": "7f50abab21a91a6ab416fb162969e8350b4075a4", "filename": "gcc/testsuite/gfortran.dg/proc_ptr_41.f90", "status": "added", "additions": 37, "deletions": 0, "changes": 37, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/bafa0782ad3ba8e5c92196b00d76b38d69e3e1e1/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_41.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/bafa0782ad3ba8e5c92196b00d76b38d69e3e1e1/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_41.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fproc_ptr_41.f90?ref=bafa0782ad3ba8e5c92196b00d76b38d69e3e1e1", "patch": "@@ -0,0 +1,37 @@\n+! { dg-do compile }\n+!\n+! PR 56968: [4.7/4.8/4.9 Regression] [F03] Issue with a procedure defined with a generic name returning procedure pointer\n+!\n+! Contributed by Samuel Debionne <samuel.debionne@ujf-grenoble.fr>\n+\n+module test\n+\n+  interface generic_name_get_proc_ptr\n+    module procedure specific_name_get_proc_ptr\n+  end interface\n+\n+  abstract interface\n+    double precision function foo(arg1)\n+      real, intent(in) :: arg1\n+    end function\n+  end interface\n+\n+contains\n+\n+  function specific_name_get_proc_ptr() result(res)\n+    procedure(foo), pointer :: res\n+  end function\n+\n+end module test\n+\n+program crash_test\n+    use :: test\n+\n+    procedure(foo), pointer :: ptr\n+\n+    ptr => specific_name_get_proc_ptr()\n+    ptr => generic_name_get_proc_ptr()\n+\n+end program\n+\n+! { dg-final { cleanup-modules \"test\" } }"}]}