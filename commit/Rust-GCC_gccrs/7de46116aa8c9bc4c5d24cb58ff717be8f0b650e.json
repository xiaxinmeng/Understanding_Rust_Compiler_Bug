ons": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7de46116aa8c9bc4c5d24cb58ff717be8f0b650e/gcc%2Ffortran%2Ftrans-openmp.cc", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7de46116aa8c9bc4c5d24cb58ff717be8f0b650e/gcc%2Ffortran%2Ftrans-openmp.cc", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Ftrans-openmp.cc?ref=7de46116aa8c9bc4c5d24cb58ff717be8f0b650e", "patch": "@@ -5998,7 +5998,7 @@ gfc_split_omp_clauses (gfc_code *code,\n       innermost = GFC_OMP_SPLIT_DO;\n       break;\n     case EXEC_OMP_MASKED_TASKLOOP:\n-      mask = GFC_OMP_SPLIT_MASKED | GFC_OMP_SPLIT_TASKLOOP;\n+      mask = GFC_OMP_MASK_MASKED | GFC_OMP_MASK_TASKLOOP;\n       innermost = GFC_OMP_SPLIT_TASKLOOP;\n       break;\n     case EXEC_OMP_MASTER_TASKLOOP:"}, {"sha": "5770c59b8309a675bd45f64c4911afdd7f52f50d", "filename": "gcc/testsuite/gfortran.dg/gomp/masked-taskloop.f90", "status": "added", "additions": 20, "deletions": 0, "changes": 20, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7de46116aa8c9bc4c5d24cb58ff717be8f0b650e/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fmasked-taskloop.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7de46116aa8c9bc4c5d24cb58ff717be8f0b650e/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fmasked-taskloop.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fmasked-taskloop.f90?ref=7de46116aa8c9bc4c5d24cb58ff717be8f0b650e", "patch": "@@ -0,0 +1,20 @@\n+! { dg-do compile }\n+! { dg-additional-options \"-fopenmp -fdump-tree-original\" }\n+\n+! There was a bug in the clause splitting for the \"masked taskloop\"\n+! combined directive that caused it to lose all the clauses.\n+\n+subroutine s1 (a1, a2)\n+  integer :: a1, a2\n+  integer :: i, j\n+\n+  !$omp masked taskloop collapse(2) grainsize(4)\n+  do i = 1, a1\n+    do j = 1, a2\n+    end do\n+  end do\n+\n+end subroutine\n+\n+! { dg-final { scan-tree-dump \"omp taskloop \\[^\\n\\r]*grainsize\\\\(4\\\\)\" \"original\" } }\n+! { dg-final { scan-tree-dump \"omp taskloop \\[^\\n\\r]*collapse\\\\(2\\\\)\" \"original\" } }"}]}