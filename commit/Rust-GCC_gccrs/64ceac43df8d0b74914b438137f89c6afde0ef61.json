.c?ref=64ceac43df8d0b74914b438137f89c6afde0ef61", "patch": "@@ -3552,7 +3552,7 @@ init_cumulative_args (CUMULATIVE_ARGS *cum,  /* Argument info to initialize */\n     {\n       /* If there are variable arguments, then we won't pass anything\n          in registers in 32-bit mode. */\n-      if (cum->maybe_vaarg)\n+      if (stdarg_p (fntype))\n \t{\n \t  cum->nregs = 0;\n \t  cum->sse_nregs = 0;"}, {"sha": "2332f6908f92af6d76a2bb20390ff98f9d54e07c", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/64ceac43df8d0b74914b438137f89c6afde0ef61/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/64ceac43df8d0b74914b438137f89c6afde0ef61/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=64ceac43df8d0b74914b438137f89c6afde0ef61", "patch": "@@ -1,3 +1,8 @@\n+2008-04-24  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR target/36015\n+\t* gcc.dg/pr36015.c: New test.\n+\n 2008-04-24  Francois-Xavier Coudert  <fxcoudert@gcc.gnu.org>\n \n \tPR rtl-optimization/36006"}, {"sha": "456ec6ad954ec6c3e2629a747fbcb3ec1000f808", "filename": "gcc/testsuite/gcc.dg/pr36015.c", "status": "added", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/64ceac43df8d0b74914b438137f89c6afde0ef61/gcc%2Ftestsuite%2Fgcc.dg%2Fpr36015.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/64ceac43df8d0b74914b438137f89c6afde0ef61/gcc%2Ftestsuite%2Fgcc.dg%2Fpr36015.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fpr36015.c?ref=64ceac43df8d0b74914b438137f89c6afde0ef61", "patch": "@@ -0,0 +1,22 @@\n+/* PR target/36015 */\n+/* { dg-do run } */\n+/* { dg-options \"-O0\" } */\n+/* { dg-options \"-O0 -mregparm=3\" { target { { i?86-*-* x86_64-*-* } && ilp32 } } } */\n+\n+static int test ();\n+\n+int\n+main (void)\n+{\n+  test (0, 1, 2, 3, 4, 5, 6, 7);\n+  return 0;\n+}\n+\n+static int\n+test (int a, int b, int c, int d, int e, int f, int g, int h)\n+{\n+  if (a != 0 || b != 1 || c != 2 || d != 3\n+      || e != 4 || f != 5 || g != 6 || h != 7)\n+    __builtin_abort ();\n+  return 0;\n+}"}]}