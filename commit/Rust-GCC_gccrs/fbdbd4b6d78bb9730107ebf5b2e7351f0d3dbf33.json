,0 +1,18 @@\n+! PR fortran/72744\n+! { dg-do compile }\n+! { dg-additional-options \"-Ofast\" }\n+\n+program pr72744\n+  integer, parameter :: n = 20\n+  integer :: i, z(n), h(n)\n+  z = [(i, i=1,n)]\n+  h = [(i, i=n,1,-1)]\n+  call sub (n, h)\n+  if ( any(h/=z) ) call abort\n+end\n+subroutine sub (n, x)\n+  integer :: n, x(n)\n+!$omp parallel\n+  x(:) = x(n:1:-1)\n+!$omp end parallel\n+end"}]}