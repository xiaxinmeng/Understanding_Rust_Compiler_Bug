+    {\n+      r = va_arg (b, int);\n+    }\n+  va_end (ap);\n+  return r;\n+}\n+\n+int\n+main ()\n+{\n+  if (foo (0) != 0 || foo (1, 7) != 7)\n+    __builtin_abort ();\n+}"}]}