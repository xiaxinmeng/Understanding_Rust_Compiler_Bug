lain, tree in_decl)\n \t\t\t= tsubst_constraint (constr, args, complain, in_decl);\n \t\t    else if (tree pl = CLASS_PLACEHOLDER_TEMPLATE (t))\n \t\t      {\n-\t\t\tif (DECL_TEMPLATE_TEMPLATE_PARM_P (pl))\n-\t\t\t  pl = tsubst (pl, args, complain, in_decl);\n+\t\t\tpl = tsubst_copy (pl, args, complain, in_decl);\n \t\t\tCLASS_PLACEHOLDER_TEMPLATE (r) = pl;\n \t\t      }\n \t\t  }"}, {"sha": "e8cdd8c710fa8a8b3e9e9cc34d95b7da7234e3b6", "filename": "gcc/testsuite/g++.dg/cpp1z/class-deduction50.C", "status": "added", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/d67b5d8d1db0b9dff3e123a8e2be31cd5634821a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1z%2Fclass-deduction50.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/d67b5d8d1db0b9dff3e123a8e2be31cd5634821a/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1z%2Fclass-deduction50.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp1z%2Fclass-deduction50.C?ref=d67b5d8d1db0b9dff3e123a8e2be31cd5634821a", "patch": "@@ -0,0 +1,22 @@\n+// PR c++/84355\n+// { dg-additional-options -std=c++17 }\n+\n+template <class, class> struct same;\n+template <class T> struct same<T,T> {};\n+\n+template<typename T> struct A\n+{\n+  template<class U> struct B\n+  {\n+    B(U);\n+  };\n+\n+  A() {\n+    B b(0);\n+    same<decltype(b),B<int>>{};\n+  }\n+};\n+\n+struct C {};\n+\n+A<C> a;"}]}