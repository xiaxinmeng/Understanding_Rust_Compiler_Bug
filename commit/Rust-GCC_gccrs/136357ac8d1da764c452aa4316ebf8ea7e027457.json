@ -16707,9 +16707,18 @@ cp_parser_direct_declarator (cp_parser* parser,\n \thandle_declarator:;\n \t  scope = get_scope_of_declarator (declarator);\n \t  if (scope)\n-\t    /* Any names that appear after the declarator-id for a\n-\t       member are looked up in the containing scope.  */\n-\t    pushed_scope = push_scope (scope);\n+\t    {\n+\t      /* Any names that appear after the declarator-id for a\n+\t\t member are looked up in the containing scope.  */\n+\t      if (at_function_scope_p ())\n+\t\t{\n+\t\t  /* But declarations with qualified-ids can't appear in a\n+\t\t     function.  */\n+\t\t  cp_parser_error (parser, \"qualified-id in declaration\");\n+\t\t  break;\n+\t\t}\n+\t      pushed_scope = push_scope (scope);\n+\t    }\n \t  parser->in_declarator_p = true;\n \t  if ((ctor_dtor_or_conv_p && *ctor_dtor_or_conv_p)\n \t      || (declarator && declarator->kind == cdk_id))"}, {"sha": "6ec76961013c436fc7906c7afa882edc0aa0959d", "filename": "gcc/testsuite/g++.dg/parse/typename7.C", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/136357ac8d1da764c452aa4316ebf8ea7e027457/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Ftypename7.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/136357ac8d1da764c452aa4316ebf8ea7e027457/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Ftypename7.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fparse%2Ftypename7.C?ref=136357ac8d1da764c452aa4316ebf8ea7e027457", "patch": "@@ -22,7 +22,7 @@ struct B\n     A().bar<typename T>(t); } // { dg-error \"expected|parse error|no matching\" }\n   // { dg-message \"candidate\" \"candidate note\" { target *-*-* } 22 }\n   void bad(T t) {\n-    B<typename T>::bar(t); } // { dg-error \"invalid|not a template\" }\n+    B<typename T>::bar(t); } // { dg-error \"invalid|qualified-id|not a template\" }\n };\n \n void baz()"}, {"sha": "8ec9e01d0de097229fb87b17927c30d1b040bdca", "filename": "gcc/testsuite/g++.dg/template/arrow2.C", "status": "added", "additions": 12, "deletions": 0, "changes": 12, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/136357ac8d1da764c452aa4316ebf8ea7e027457/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Farrow2.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/136357ac8d1da764c452aa4316ebf8ea7e027457/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Farrow2.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Ftemplate%2Farrow2.C?ref=136357ac8d1da764c452aa4316ebf8ea7e027457", "patch": "@@ -0,0 +1,12 @@\n+// PR c++/56639\n+\n+struct A {\n+  int i;\n+  static A* f();\n+};\n+\n+struct B {\n+  void g() {\n+    int (A::f()->i);\n+  }\n+};"}]}