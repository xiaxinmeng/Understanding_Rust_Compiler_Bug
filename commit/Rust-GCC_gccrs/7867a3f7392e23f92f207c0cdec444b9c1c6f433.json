-GCC/gccrs/raw/7867a3f7392e23f92f207c0cdec444b9c1c6f433/libstdc%2B%2B-v3%2Finclude%2Fext%2Fsso_string_base.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Finclude%2Fext%2Fsso_string_base.h?ref=7867a3f7392e23f92f207c0cdec444b9c1c6f433", "patch": "@@ -226,6 +226,10 @@ _GLIBCXX_BEGIN_NAMESPACE(__gnu_cxx)\n       void\n       _M_erase(size_type __pos, size_type __n);\n \n+      void\n+      _M_clear()\n+      { _M_set_length(0); }\n+\n       bool\n       _M_compare(const __sso_string_base&) const\n       { return false; }"}, {"sha": "d858949db467636fada95a03e2ccb4d227b88ac3", "filename": "libstdc++-v3/include/ext/vstring.h", "status": "modified", "additions": 1, "deletions": 1, "changes": 2, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7867a3f7392e23f92f207c0cdec444b9c1c6f433/libstdc%2B%2B-v3%2Finclude%2Fext%2Fvstring.h", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7867a3f7392e23f92f207c0cdec444b9c1c6f433/libstdc%2B%2B-v3%2Finclude%2Fext%2Fvstring.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Finclude%2Fext%2Fvstring.h?ref=7867a3f7392e23f92f207c0cdec444b9c1c6f433", "patch": "@@ -409,7 +409,7 @@ _GLIBCXX_BEGIN_NAMESPACE(__gnu_cxx)\n        */\n       void\n       clear()\n-      { this->_M_erase(size_type(0), this->size()); }\n+      { this->_M_clear(); }\n \n       /**\n        *  Returns true if the %string is empty.  Equivalent to *this == \"\"."}]}