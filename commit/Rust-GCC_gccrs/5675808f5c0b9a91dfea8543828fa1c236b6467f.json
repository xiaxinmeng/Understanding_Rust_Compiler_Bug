n+      if (!NULLPTR_TYPE_P (TREE_TYPE (expr)))\n \t{\n \t  if (complain & tf_error)\n \t    error (\"%qE is not a valid template argument for type %qT \""}, {"sha": "a34a6af73e9743af51703a404d66614fa310cd82", "filename": "gcc/testsuite/g++.dg/cpp0x/nullptr39.C", "status": "added", "additions": 15, "deletions": 0, "changes": 15, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5675808f5c0b9a91dfea8543828fa1c236b6467f/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr39.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5675808f5c0b9a91dfea8543828fa1c236b6467f/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr39.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnullptr39.C?ref=5675808f5c0b9a91dfea8543828fa1c236b6467f", "patch": "@@ -0,0 +1,15 @@\n+// PR c++/81671\n+// { dg-do compile { target c++11 } }\n+\n+namespace std { typedef decltype(nullptr) nullptr_t; }\n+\n+template<class R, class CB> struct Bar\n+{};\n+template<class R> struct Bar<R, std::nullptr_t>\n+{\n+    template<std::nullptr_t> struct Bind { constexpr static int const cb = 0; };\n+};\n+int foo()\n+{\n+  return Bar<int, decltype(nullptr)>::Bind<nullptr>::cb;\n+}"}]}