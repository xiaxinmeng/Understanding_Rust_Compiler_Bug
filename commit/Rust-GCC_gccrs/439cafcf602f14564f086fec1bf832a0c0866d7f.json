:= (1,2,3,4);\n+end;"}, {"sha": "774ffe5f785c8cea2a08effb7bf65ebb879514f3", "filename": "gcc/testsuite/gnat.dg/aliased1.adb", "status": "added", "additions": 34, "deletions": 0, "changes": 34, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/439cafcf602f14564f086fec1bf832a0c0866d7f/gcc%2Ftestsuite%2Fgnat.dg%2Faliased1.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/439cafcf602f14564f086fec1bf832a0c0866d7f/gcc%2Ftestsuite%2Fgnat.dg%2Faliased1.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Faliased1.adb?ref=439cafcf602f14564f086fec1bf832a0c0866d7f", "patch": "@@ -0,0 +1,34 @@\n+--  { dg-do compile }\n+--  { dg-options \"-gnatws\" }\n+\n+procedure aliased1 is\n+  \n+  type E is (One, Two);\n+  \n+  type R (D : E := One) is record\n+    case D is\n+      when One =>\n+         I1 : Integer;\n+         I2 : Integer;\n+      when Two =>\n+         B1 : Boolean;\n+    end case;\n+  end record;\n+  \n+  type Data_Type is record\n+    Data : R;\n+  end record;\n+  \n+  type Array_Type is array (Natural range <>) of Data_Type;\n+  \n+  function Get return Array_Type is\n+    Ret : Array_Type (1 .. 2);\n+  begin\n+    return Ret;\n+  end;\n+  \n+  Object : aliased Array_Type := Get;\n+\n+begin\n+  null;\n+end;"}, {"sha": "3bdc58ea529d1c7b2ce491a32ec0d468bc425721", "filename": "gcc/testsuite/gnat.dg/profile_warning.adb", "status": "added", "additions": 4, "deletions": 0, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/439cafcf602f14564f086fec1bf832a0c0866d7f/gcc%2Ftestsuite%2Fgnat.dg%2Fprofile_warning.adb", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/439cafcf602f14564f086fec1bf832a0c0866d7f/gcc%2Ftestsuite%2Fgnat.dg%2Fprofile_warning.adb", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fprofile_warning.adb?ref=439cafcf602f14564f086fec1bf832a0c0866d7f", "patch": "@@ -0,0 +1,4 @@\n+-- { dg-do compile }\n+\n+package body profile_warning is\n+