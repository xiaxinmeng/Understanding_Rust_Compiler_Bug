om/Rust-GCC/gccrs/blob/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Fcp%2Fpt.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Fcp%2Fpt.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Fpt.c?ref=1e5cdb9f896fb220b26fd2080406504c4badf5af", "patch": "@@ -26154,6 +26154,7 @@ instantiate_decl (tree d, bool defer_ok, bool expl_inst_class_mem_p)\n     }\n   else\n     {\n+      set_instantiating_module (d);\n       if (variable_template_p (gen_tmpl))\n \tnote_variable_template_instantiation (d);\n       instantiate_body (td, args, d, false);"}, {"sha": "b5e5a3fea5404e54c149220c0c8948a2a85add5a", "filename": "gcc/testsuite/g++.dg/modules/pr99377_a.H", "status": "added", "additions": 21, "deletions": 0, "changes": 21, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_a.H", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_a.H", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_a.H?ref=1e5cdb9f896fb220b26fd2080406504c4badf5af", "patch": "@@ -0,0 +1,21 @@\n+// PR 99377 failed to stream locally instantiated member\n+// link failure in function `Check(Widget<int> const&)':\n+// bug_c.ii:(.text._Z5CheckRK6WidgetIiE[_Z5CheckRK6WidgetIiE]+0x14): undefined reference to `Widget<int>::Second() const'\n+// { dg-module-do link }\n+// { dg-additional-options -fmodule-header }\n+// { dg-module-cmi {} }\n+template<typename>\n+struct Widget\n+{\n+  Widget (int) { }\n+\n+  bool First() const { return true; }\n+\n+  bool Second () const { return true;}\n+};\n+\n+inline void Frob (const Widget<int>& w) noexcept\n+{\n+  w.First ();\n+}\n+"}, {"sha": "77826379fc79fce5e7573a7a96de5c17b8e1c33b", "filename": "gcc/testsuite/g++.dg/modules/pr99377_b.C", "status": "added", "additions": 10, "deletions": 0, "changes": 10, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_b.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_b.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_b.C?ref=1e5cdb9f896fb220b26fd2080406504c4badf5af", "patch": "@@ -0,0 +1,10 @@\n+// { dg-additional-options -fmodules-ts }\n+export module Foo;\n+// { dg-module-cmi Foo }\n+import \"pr99377_a.H\";\n+\n+export inline bool Check (const Widget<int>& w)\n+{\n+  return w.Second ();\n+}\n+"}, {"sha": "287388fa6ddce0be34f4ec67a92cf1711dbd6bc6", "filename": "gcc/testsuite/g++.dg/modules/pr99377_c.C", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_c.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/1e5cdb9f896fb220b26fd2080406504c4badf5af/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_c.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fmodules%2Fpr99377_c.C?ref=1e5cdb9f896fb220b26fd2080406504c4badf5af", "patch": "@@ -0,0 +1,8 @@\n+// { dg-additional-options -fmodules-ts }\n+\n+import Foo;\n+\n+int main ()\n+{\n+  return Check (0) ? 0 : 1;\n+}"}]}