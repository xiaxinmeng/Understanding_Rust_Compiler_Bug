ngeLog?ref=cbd400b43e4068a886b7297e4da8599732ef6813", "patch": "@@ -1,3 +1,8 @@\n+2016-02-19  Richard Biener  <rguenther@suse.de>\n+\n+\tPR tree-optimization/69980\n+\t* gfortran.dg/vect/pr69980.f90: New testcase.\n+\n 2016-02-29  Eric Botcazou  <ebotcazou@adacore.com>\n \n \t* gcc.target/sparc/20160229-1.c: New test."}, {"sha": "8a20b983f1bf0b8a6e61768f81afacdaba767092", "filename": "gcc/testsuite/gfortran.dg/vect/pr69980.f90", "status": "added", "additions": 40, "deletions": 0, "changes": 40, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/cbd400b43e4068a886b7297e4da8599732ef6813/gcc%2Ftestsuite%2Fgfortran.dg%2Fvect%2Fpr69980.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cbd400b43e4068a886b7297e4da8599732ef6813/gcc%2Ftestsuite%2Fgfortran.dg%2Fvect%2Fpr69980.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fvect%2Fpr69980.f90?ref=cbd400b43e4068a886b7297e4da8599732ef6813", "patch": "@@ -0,0 +1,40 @@\n+! { dg-do run }\n+! { dg-additional-options \"-Ofast -fno-inline\" }\n+\n+subroutine check (a, b)\n+   real *8, intent(in) :: a(4), b(4)\n+\n+   IF (abs(a(1)-b(1)) > 1) THEN\n+      CALL ABORT\n+   END IF\n+end subroutine check\n+\n+program main\n+  real *8 :: mu(4,26), mumax(4), mumax2(4)\n+\n+  integer :: i, k\n+\n+  do k=1,26\n+     do i=1,4\n+        mu(i, k) = 4*(i-1) + k\n+     end do\n+  end do\n+\n+  mumax = 0; \n+  do k=1,26\n+     do i=1,3\n+        mumax(i) = max(mumax(i), mu(i,k)+mu(i,k))\n+     end do\n+  end do\n+\n+  mumax2 = 0;\n+  do i=1,3\n+     do k=1,26\n+        mumax2(i) = max(mumax2(i), mu(i,k)+mu(i,k))\n+     end do\n+  end do\n+\n+  CALL check (mumax, mumax2)\n+\n+  return\n+end program"}, {"sha": "d71384881b72a272ba510dd4237362e3e5c552b5", "filename": "gcc/tree-vect-slp.c", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/cbd400b43e4068a886b7297e4da8599732ef6813/gcc%2Ftree-vect-slp.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/cbd400b43e4068a886b7297e4da8599732ef6813/gcc%2Ftree-vect-slp.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftree-vect-slp.c?ref=cbd400b43e4068a886b7297e4da8599732ef6813", "patch": "@@ -1343,6 +1343,9 @@ vect_attempt_slp_rearrange_stmts (slp_instance slp_instn)\n \t  || (group_size == GROUP_SIZE (vinfo_for_stmt (first_stmt))\n \t      && GROUP_GAP (vinfo_for_stmt (first_stmt)) == 0))\n \tSLP_TREE_LOAD_PERMUTATION (node).release ();\n+      else\n+\tfor (j = 0; j < SLP_TREE_LOAD_PERMUTATION (node).length (); ++j)\n+\t  SLP_TREE_LOAD_PERMUTATION (node)[j] = j;\n     }\n \n   return true;"}]}