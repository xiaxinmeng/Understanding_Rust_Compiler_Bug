ead?ref=cd3b0fafaaebab56b17ec9f4fceff4c0f8e26aec", "patch": "@@ -135,15 +135,15 @@ namespace std\n     ~thread()\n     {\n       if (joinable())\n-\tdetach();\n+\tstd::terminate();\n     }\n \n     thread& operator=(const thread&) = delete;\n \n     thread& operator=(thread&& __t)\n     {\n       if (joinable())\n-\tdetach();\n+\tstd::terminate();\n       swap(__t);\n       return *this;\n     }"}]}