d1", "patch": "@@ -5320,7 +5320,7 @@ paired_expand_vector_init (rtx target, rtx vals)\n   for (i = 0; i < n_elts; ++i)\n     {\n       x = XVECEXP (vals, 0, i);\n-      if (!CONSTANT_P (x))\n+      if (!(CONST_SCALAR_INT_P (x) || CONST_DOUBLE_P (x) || CONST_FIXED_P (x)))\n \t++n_var;\n     }\n   if (n_var == 0)\n@@ -5472,7 +5472,7 @@ rs6000_expand_vector_init (rtx target, rtx vals)\n   for (i = 0; i < n_elts; ++i)\n     {\n       x = XVECEXP (vals, 0, i);\n-      if (!CONSTANT_P (x))\n+      if (!(CONST_SCALAR_INT_P (x) || CONST_DOUBLE_P (x) || CONST_FIXED_P (x)))\n \t++n_var, one_var = i;\n       else if (x != CONST0_RTX (inner_mode))\n \tall_const_zero = false;"}]}