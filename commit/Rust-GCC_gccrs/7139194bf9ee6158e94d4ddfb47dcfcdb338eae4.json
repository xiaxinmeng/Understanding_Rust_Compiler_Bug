f=7139194bf9ee6158e94d4ddfb47dcfcdb338eae4", "patch": "@@ -1643,10 +1643,19 @@ replace_dependent (slsr_cand_t c, enum tree_code cand_code)\n \n   basis = lookup_cand (c->basis);\n   basis_name = gimple_assign_lhs (basis->cand_stmt);\n-  incr_type = TREE_TYPE (gimple_assign_rhs1 (c->cand_stmt));\n-  code = PLUS_EXPR;\n+  if (cand_code == POINTER_PLUS_EXPR)\n+    {\n+      incr_type = sizetype;\n+      code = cand_code;\n+    }\n+  else\n+    {\n+      incr_type = TREE_TYPE (gimple_assign_rhs1 (c->cand_stmt));\n+      code = PLUS_EXPR;\n+    }\n \n-  if (bump.is_negative ())\n+  if (bump.is_negative ()\n+      && cand_code != POINTER_PLUS_EXPR)\n     {\n       code = MINUS_EXPR;\n       bump = -bump;"}, {"sha": "f10daeae50208b521b482b98872e287bb33af420", "filename": "gcc/testsuite/gcc.c-torture/compile/pr55350.c", "status": "added", "additions": 8, "deletions": 0, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7139194bf9ee6158e94d4ddfb47dcfcdb338eae4/gcc%2Ftestsuite%2Fgcc.c-torture%2Fcompile%2Fpr55350.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7139194bf9ee6158e94d4ddfb47dcfcdb338eae4/gcc%2Ftestsuite%2Fgcc.c-torture%2Fcompile%2Fpr55350.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.c-torture%2Fcompile%2Fpr55350.c?ref=7139194bf9ee6158e94d4ddfb47dcfcdb338eae4", "patch": "@@ -0,0 +1,8 @@\n+void\n+foo (__INTPTR_TYPE__ x, __INTPTR_TYPE__ y)\n+{\n+  int i;\n+  void **a = (void *)  (8UL * (x / 8UL));\n+  for (i = 0; i < x; i++)\n+    a[i] = (void *) y;\n+}"}]}