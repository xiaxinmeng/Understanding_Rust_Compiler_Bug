85-2.C?ref=ea229aa8a1bab0770a1fbd65bb577d2dc4925b69", "patch": "@@ -0,0 +1,20 @@\n+// PR c++/89285\n+// { dg-do compile { target c++14 } }\n+\n+struct A {\n+  int a {};\n+};\n+struct B {\n+  int b {};\n+  constexpr B (A *x) {\n+    int *c = &x->a;\n+    while (*c)\n+      c = reinterpret_cast<int *>((reinterpret_cast<char *>(c) + *c));\n+    *c = reinterpret_cast<char *>(this) - reinterpret_cast<char *>(c);\n+  }\n+};\n+struct C : A {\n+  B bar {this};\n+};\n+\n+C foo {};"}]}