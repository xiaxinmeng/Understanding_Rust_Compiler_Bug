st (tree type)\n {\n+  /* Look through typedefs.  */\n+  if (!TYPE_P (type))\n+    return false;\n+  type = TYPE_MAIN_VARIANT (type);\n   return (CLASS_TYPE_P (type)\n \t  && CP_TYPE_CONTEXT (type) == std_node\n \t  && strcmp (TYPE_NAME_STRING (type), \"initializer_list\") == 0);"}, {"sha": "6fc6b3bfdec5bfbff96f2785b6f2b7062f1b7561", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0060a10ae887561aef22299f92098d04aee1cf14/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0060a10ae887561aef22299f92098d04aee1cf14/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=0060a10ae887561aef22299f92098d04aee1cf14", "patch": "@@ -1,5 +1,8 @@\n 2010-07-06  Jason Merrill  <jason@redhat.com>\n \n+\tPR c++/44703\n+\t* g++.dg/cpp0x/initlist41.C: New.\n+\n \tPR c++/44778\n \t* g++.dg/template/ptrmem22.C: New.\n "}, {"sha": "b5385480c8964ee8ddf88e7dbc04c3346d52d347", "filename": "gcc/testsuite/g++.dg/cpp0x/initlist41.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0060a10ae887561aef22299f92098d04aee1cf14/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist41.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0060a10ae887561aef22299f92098d04aee1cf14/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist41.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Finitlist41.C?ref=0060a10ae887561aef22299f92098d04aee1cf14", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/44703\n+// { dg-options -std=c++0x }\n+\n+#include <initializer_list>\n+\n+typedef std::initializer_list<int> type ;\n+void f(type) {}\n+\n+int main()\n+{\n+//  error: could not convert '{1, 2, 3}' to 'type'\n+    f({1,2,3}) ;\n+}\n+"}]}