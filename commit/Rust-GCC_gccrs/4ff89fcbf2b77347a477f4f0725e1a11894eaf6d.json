ger :: lun\n+character(len=20) :: str\n+\n+! VALID Fortran 95:\n+open(unit=lun,file=str,delim='apostrophe',status='old')\n+inquire(lun, delim=str)\n+\n+! Fortran 2003:\n+write(*,*, delim='apostrophe') 'a' ! { dg-error \"Fortran 2003: DELIM= at .1. not allowed in Fortran 95\" }\n+end"}]}