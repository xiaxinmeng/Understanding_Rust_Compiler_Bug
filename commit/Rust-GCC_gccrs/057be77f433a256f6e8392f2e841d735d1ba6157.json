d", "additions": 6, "deletions": 1, "changes": 7, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/057be77f433a256f6e8392f2e841d735d1ba6157/gcc%2Ftestsuite%2Flib%2Ftarget-libpath.exp", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/057be77f433a256f6e8392f2e841d735d1ba6157/gcc%2Ftestsuite%2Flib%2Ftarget-libpath.exp", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Flib%2Ftarget-libpath.exp?ref=057be77f433a256f6e8392f2e841d735d1ba6157", "patch": "@@ -155,7 +155,12 @@ proc set_ld_library_path_env_vars { } {\n     setenv DYLD_LIBRARY_PATH \"$ld_library_path\"\n   }\n \n-  verbose -log \"set_ld_library_path_env_vars: ld_library_path=$ld_library_path\"\n+  verbose -log \"LD_LIBRARY_PATH=[getenv LD_LIBRARY_PATH]\"\n+  verbose -log \"LD_RUN_PATH=[getenv LD_RUN_PATH]\"\n+  verbose -log \"SHLIB_PATH=[getenv SHLIB_PATH]\"\n+  verbose -log \"LD_LIBRARY_PATH_32=[getenv LD_LIBRARY_PATH_32]\"\n+  verbose -log \"LD_LIBRARY_PATH_64=[getenv LD_LIBRARY_PATH_64]\"\n+  verbose -log \"DYLD_LIBRARY_PATH=[getenv DYLD_LIBRARY_PATH]\"\n }\n \n #######################################"}]}