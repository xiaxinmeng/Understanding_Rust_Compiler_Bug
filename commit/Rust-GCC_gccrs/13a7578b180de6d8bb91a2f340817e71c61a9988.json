, m, n, o;\n+static int g;\n+int\n+fn1 () {\n+    for (;; ++f)\n+      if (e)\n+\tbreak;\n+    return 0;\n+}\n+unsigned char fn2 ();\n+void\n+fn3 () {\n+lbl_220:\n+    if (j) {\n+lbl_221:\n+\tl = (g || b) <= fn1 ();\n+\tfor (;;) {\n+\t    g = 0;\n+\t    fn2 ();\n+\t    if (k)\n+\t      goto lbl_220;\n+\t    break;\n+\t}\n+\tif (l)\n+\t  goto lbl_221;\n+    }\n+}\n+unsigned char\n+fn2 () {\n+    o = d ? 0 : c;\n+    h = m | a % o != n;\n+    return i;\n+}"}, {"sha": "ad99884bfed23c1e464efee9d4fb0224ee55d558", "filename": "gcc/testsuite/gcc.dg/torture/pr52913.c", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/13a7578b180de6d8bb91a2f340817e71c61a9988/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr52913.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/13a7578b180de6d8bb91a2f340817e71c61a9988/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr52913.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Ftorture%2Fpr52913.c?ref=13a7578b180de6d8bb91a2f340817e71c61a9988", "patch": "@@ -0,0 +1,18 @@\n+/* { dg-do compile } */\n+\n+int a, b, c, d, e;\n+void\n+fn1 ()\n+{\n+lbl_101:\n+  e = 0;\n+lbl_274:\n+  for (c = 0; c < 1; c = a)\n+    if (d)\n+      if (b)\n+\tgoto lbl_101;\n+      else\n+\tbreak;\n+  d = 1;\n+  goto lbl_274;\n+}"}]}