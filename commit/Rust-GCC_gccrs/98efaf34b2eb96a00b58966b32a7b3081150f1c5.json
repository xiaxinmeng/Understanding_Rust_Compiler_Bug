ust-GCC/gccrs/contents/gcc%2Ffortran%2Ftrans-intrinsic.c?ref=98efaf34b2eb96a00b58966b32a7b3081150f1c5", "patch": "@@ -2840,6 +2840,8 @@ gfc_conv_associated (gfc_se *se, gfc_expr *expr)\n           gfc_conv_expr_lhs (&arg1se, arg1->expr);\n           tmp2 = gfc_conv_descriptor_data_get (arg1se.expr);\n         }\n+      gfc_add_block_to_block (&se->pre, &arg1se.pre);\n+      gfc_add_block_to_block (&se->post, &arg1se.post);\n       tmp = build2 (NE_EXPR, boolean_type_node, tmp2,\n \t\t    fold_convert (TREE_TYPE (tmp2), null_pointer_node));\n       se->expr = tmp;\n@@ -2863,6 +2865,8 @@ gfc_conv_associated (gfc_se *se, gfc_expr *expr)\n           gfc_conv_expr (&arg1se, arg1->expr);\n           arg2se.want_pointer = 1;\n           gfc_conv_expr (&arg2se, arg2->expr);\n+\t  gfc_add_block_to_block (&se->pre, &arg1se.pre);\n+\t  gfc_add_block_to_block (&se->post, &arg1se.post);\n           tmp = build2 (EQ_EXPR, boolean_type_node, arg1se.expr, arg2se.expr);\n           se->expr = tmp;\n         }"}, {"sha": "985d645556736feab87fd78955fa9f2f316c29e5", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/98efaf34b2eb96a00b58966b32a7b3081150f1c5/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/98efaf34b2eb96a00b58966b32a7b3081150f1c5/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=98efaf34b2eb96a00b58966b32a7b3081150f1c5", "patch": "@@ -1,3 +1,8 @@\n+2006-06-18  Francois-Xavier Coudert  <coudert@clipper.ens.fr>\n+\n+\tPR fortran/26801\n+\t* gfortran.dg/associated_4.f90: New test.\n+\n 2006-06-18  Jerry DeLisle  <jvdelisle@gcc.gnu.org>\n \n \tPR fortran/19310"}, {"sha": "dd4490b827cb7ef71a31e012c304ed3f90133690", "filename": "gcc/testsuite/gfortran.dg/associated_4.f90", "status": "added", "additions": 20, "deletions": 0, "changes": 20, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/98efaf34b2eb96a00b58966b32a7b3081150f1c5/gcc%2Ftestsuite%2Fgfortran.dg%2Fassociated_4.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/98efaf34b2eb96a00b58966b32a7b3081150f1c5/gcc%2Ftestsuite%2Fgfortran.dg%2Fassociated_4.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fassociated_4.f90?ref=98efaf34b2eb96a00b58966b32a7b3081150f1c5", "patch": "@@ -0,0 +1,20 @@\n+! { dg-do run }\n+! PR fortran/26801\n+  implicit none\n+\n+  integer :: i\n+  integer,target :: u\n+  logical :: l\n+  character(len=8) :: A\n+  type dt\n+    integer, pointer :: a => NULL()\n+  end type dt\n+  type(dt) :: obj(2)\n+\n+  i = 2\n+  l = associated(obj(i)%a)\n+  write(A,*) l\n+  l = associated(obj(i)%a,u)\n+  print *, l\n+  write(A,*) l\n+end"}]}