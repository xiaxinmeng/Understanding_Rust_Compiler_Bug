com/Rust-GCC/gccrs/raw/b39968989d6ae4289c01202c45268b5651d1c222/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr56052.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr56052.f90?ref=b39968989d6ae4289c01202c45268b5651d1c222", "patch": "@@ -0,0 +1,16 @@\n+! PR fortran/56052\n+! { dg-do compile }\n+! { dg-options \"-fopenmp\" }\n+\n+subroutine middle(args)\n+  type args_t\n+  end type\n+  type, extends(args_t) :: scan_args_t\n+  end type\n+  class(args_t),intent(inout) :: args\n+  !$omp single\n+    select type (args)\n+      type is (scan_args_t)\n+    end select\n+  !$omp end single\n+end subroutine middle"}]}