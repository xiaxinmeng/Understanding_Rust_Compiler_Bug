nit, bool init_const_expr_p,\n       tree d_init;\n       if (init == NULL_TREE)\n \t{\n+\t  if (DECL_TEMPLATE_INSTANTIATION (decl)\n+\t      && !DECL_TEMPLATE_INSTANTIATED (decl))\n+\t    {\n+\t      /* init is null because we're deferring instantiating the\n+\t\t initializer until we need it.  Well, we need it now.  */\n+\t      instantiate_decl (decl, /*defer_ok*/true, /*expl*/false);\n+\t      return;\n+\t    }\n+\n \t  error (\"declaration of %q#D has no initializer\", decl);\n \t  TREE_TYPE (decl) = error_mark_node;\n \t  return;"}, {"sha": "f4b29046d1d3b71b47a0c01af5c9d8dd3689f664", "filename": "gcc/testsuite/g++.dg/cpp0x/auto37.C", "status": "added", "additions": 14, "deletions": 0, "changes": 14, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b24cd3d1f8ab5ad45bafdfd4af0f5805c4788ca2/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fauto37.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b24cd3d1f8ab5ad45bafdfd4af0f5805c4788ca2/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fauto37.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fauto37.C?ref=b24cd3d1f8ab5ad45bafdfd4af0f5805c4788ca2", "patch": "@@ -0,0 +1,14 @@\n+// PR c++/55003\n+// { dg-do compile { target c++11 } }\n+\n+template<typename T>\n+struct A {\n+  static const auto t\n+    = (typename T::type)42;\n+};\n+\n+struct X {\n+  typedef int type;\n+};\n+\n+A<X> a;"}]}