n (sese_l scop) const\n   basic_block bb;\n   FOR_EACH_VEC_ELT (dom, i, bb)\n     {\n-      DEBUG_PRINT (dp << \"\\nVisiting bb_\" << bb->index);\n+      DEBUG_PRINT (dp << \"Visiting bb_\" << bb->index << \"\\n\");\n \n       /* We don't want to analyze any bb outside sese.  */\n       if (!dominated_by_p (CDI_POST_DOMINATORS, bb, exit_bb))\n \tcontinue;\n \n+      /* Basic blocks dominated by the scop->exit are not in the scop.  */\n+      if (bb != exit_bb && dominated_by_p (CDI_DOMINATORS, bb, exit_bb))\n+\tcontinue;\n+\n       /* The basic block should not be part of an irreducible loop.  */\n       if (bb->flags & BB_IRREDUCIBLE_LOOP)\n \t{"}, {"sha": "f107483de21f298ba6778d7a2ee42a8ef6e167d6", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a378e92255a31d26d0905e72de1abd91a161f758/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a378e92255a31d26d0905e72de1abd91a161f758/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=a378e92255a31d26d0905e72de1abd91a161f758", "patch": "@@ -1,3 +1,9 @@\n+2015-11-19  Aditya Kumar  <aditya.k7@samsung.com>\n+\t    Sebastian Pop  <s.pop@samsung.com>\n+\n+\tPR tree-optimization/68428\n+\t* gcc.dg/graphite/pr68428.c: New.\n+\n 2015-11-19  Aditya Kumar  <aditya.k7@samsung.com>\n \t    Sebastian Pop  <s.pop@samsung.com>\n "}, {"sha": "2dc63b70a557841689f1102c8e53bff096cb440c", "filename": "gcc/testsuite/gcc.dg/graphite/pr68428.c", "status": "added", "additions": 23, "deletions": 0, "changes": 23, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a378e92255a31d26d0905e72de1abd91a161f758/gcc%2Ftestsuite%2Fgcc.dg%2Fgraphite%2Fpr68428.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a378e92255a31d26d0905e72de1abd91a161f758/gcc%2Ftestsuite%2Fgcc.dg%2Fgraphite%2Fpr68428.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fgraphite%2Fpr68428.c?ref=a378e92255a31d26d0905e72de1abd91a161f758", "patch": "@@ -0,0 +1,23 @@\n+/* { dg-options \"-O2 -floop-nest-optimize\" } */\n+\n+int au[4] = { 0 };\n+\n+int\n+main(void)\n+{\n+  int dc;\n+  int m7;\n+  int lv;\n+  int f2;\n+  int uq[3] = { 1 };\n+  for (dc = 0; dc < 2; ++dc) {\n+    for (lv = 0; lv < 2; ++lv)\n+      for (m7 = 0; m7 < 3; ++m7) {\n+        if (uq[dc] == 0)\n+          continue;\n+        for (f2 = 0; f2 < 3; ++f2)\n+          au[dc+2] = uq[f2];\n+      }\n+  }\n+  return 0;\n+}"}]}