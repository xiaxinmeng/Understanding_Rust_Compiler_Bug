2Ftestsuite%2FChangeLog?ref=c873862c4f6f4a96a9e1e8dfd6540c0f11afb7c5", "patch": "@@ -1,3 +1,7 @@\n+2001-05-11  Mark Mitchell  <mark@codesourcery.com>\n+\n+\t* lib/objc.exp: Add -lposix4 on Solaris 2.8.\n+\n 2001-05-10  Joseph S. Myers  <jsm28@cam.ac.uk>\n \n \t* gcc.c-torture/compile/20010313-1.c: New test."}, {"sha": "fa13657f93c5c3674cd745e1fe2d0f1115d65de1", "filename": "gcc/testsuite/lib/objc.exp", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c873862c4f6f4a96a9e1e8dfd6540c0f11afb7c5/gcc%2Ftestsuite%2Flib%2Fobjc.exp", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c873862c4f6f4a96a9e1e8dfd6540c0f11afb7c5/gcc%2Ftestsuite%2Flib%2Fobjc.exp", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Flib%2Fobjc.exp?ref=c873862c4f6f4a96a9e1e8dfd6540c0f11afb7c5", "patch": "@@ -122,6 +122,12 @@ proc objc_target_compile { source dest type options } {\n     set ld_library_path \".\"\n     lappend options \"libs=-lobjc\"\n \n+    # On Solaris 2.8, we need to include libposix4 inorder to get\n+    # sched_get_priority_max and such.\n+    if [ishost \"sparc-sun-solaris2.8\"] {\n+\tlappend options \"libs=-lposix4\"\n+    }\n+\n     if { [target_info needs_status_wrapper]!=\"\" && [info exists gluefile] } {\n \tlappend options \"libs=${gluefile}\"\n \tlappend options \"ldflags=$wrap_flags\""}]}