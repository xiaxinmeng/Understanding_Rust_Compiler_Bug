%2Ffortran%2Fresolve.c?ref=582776eb1b62c32f5234566a01ea92247b7d6bcc", "patch": "@@ -11940,6 +11940,12 @@ gfc_resolve_code (gfc_code *code, gfc_namespace *ns)\n \n \t  if (resolve_ordinary_assign (code, ns))\n \t    {\n+\t      if (omp_workshare_flag)\n+\t\t{\n+\t\t  gfc_error (\"Expected intrinsic assignment in OMP WORKSHARE \"\n+\t\t\t     \"at %L\", &code->loc);\n+\t\t  break;\n+\t\t}\n \t      if (code->op == EXEC_COMPCALL)\n \t\tgoto compcall;\n \t      else"}, {"sha": "65d04c2b55d411fd7b63a1c76b47b70fa8934942", "filename": "gcc/testsuite/gfortran.dg/gomp/workshare-59.f90", "status": "added", "additions": 26, "deletions": 0, "changes": 26, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/582776eb1b62c32f5234566a01ea92247b7d6bcc/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fworkshare-59.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/582776eb1b62c32f5234566a01ea92247b7d6bcc/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fworkshare-59.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fworkshare-59.f90?ref=582776eb1b62c32f5234566a01ea92247b7d6bcc", "patch": "@@ -0,0 +1,26 @@\n+! PR fortran/100633\n+\n+module defined_assign\n+  interface assignment(=)\n+    module procedure work_assign\n+  end interface\n+\n+  contains\n+    subroutine work_assign(a,b)\n+      integer, intent(out) :: a\n+      logical, intent(in) :: b(:)\n+    end subroutine work_assign\n+end module defined_assign\n+\n+program omp_workshare\n+  use defined_assign\n+\n+  integer :: a\n+  logical :: l(10)\n+  l = .TRUE.\n+\n+  !$omp workshare\n+  a = l   ! { dg-error \"Expected intrinsic assignment in OMP WORKSHARE\" }\n+  !$omp end workshare\n+\n+end program omp_workshare"}]}