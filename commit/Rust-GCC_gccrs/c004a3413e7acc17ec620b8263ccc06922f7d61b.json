: "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ffortran%2Fdecl.c?ref=c004a3413e7acc17ec620b8263ccc06922f7d61b", "patch": "@@ -1718,7 +1718,7 @@ add_init_expr_to_sym (const char *name, gfc_expr **initp, locus *var_locus)\n \t\t    }\n \t\t  else if (init->expr_type == EXPR_ARRAY)\n \t\t    {\n-\t\t      if (init->ts.u.cl)\n+\t\t      if (init->ts.u.cl && init->ts.u.cl->length)\n \t\t\t{\n \t\t\t  const gfc_expr *length = init->ts.u.cl->length;\n \t\t\t  if (length->expr_type != EXPR_CONSTANT)"}, {"sha": "346cdc03110c607ce008ccd859f850ee09fabca6", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c004a3413e7acc17ec620b8263ccc06922f7d61b/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c004a3413e7acc17ec620b8263ccc06922f7d61b/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=c004a3413e7acc17ec620b8263ccc06922f7d61b", "patch": "@@ -1,3 +1,8 @@\n+2018-01-17  Harald Anlauf  <anlauf@gmx.de>\n+                         \n+\tPR fortran/83874\n+\t* gfortran.dg/pr83874.f90: New test.\n+\n 2018-01-18  Jakub Jelinek  <jakub@redhat.com>\n \n \tPR c++/83824"}, {"sha": "d1bdc97b783df439f8a6ad5fe83ecb5c687c53e2", "filename": "gcc/testsuite/gfortran.dg/pr83874.f90", "status": "added", "additions": 19, "deletions": 0, "changes": 19, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/c004a3413e7acc17ec620b8263ccc06922f7d61b/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr83874.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/c004a3413e7acc17ec620b8263ccc06922f7d61b/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr83874.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fpr83874.f90?ref=c004a3413e7acc17ec620b8263ccc06922f7d61b", "patch": "@@ -0,0 +1,19 @@\n+! { dg-do run }\n+! PR fortran/83874\n+! There was an ICE while initializing the character arrays\n+!\n+! Contributed by Harald Anlauf <anlauf@gmx.de>\n+!\n+program charinit\n+  implicit none\n+  type t\n+     character(len=1) :: name\n+  end type t\n+  type(t), parameter :: z(2)= [ t ('a'), t ('b') ]\n+  character(len=1), parameter :: names1(*) = z% name\n+  character(len=*), parameter :: names2(2) = z% name\n+  character(len=*), parameter :: names3(*) = z% name\n+  if (.not. (names1(1) == \"a\" .and. names1(2) == \"b\")) call abort ()\n+  if (.not. (names2(1) == \"a\" .and. names2(2) == \"b\")) call abort ()\n+  if (.not. (names3(1) == \"a\" .and. names3(2) == \"b\")) call abort ()\n+end program charinit"}]}