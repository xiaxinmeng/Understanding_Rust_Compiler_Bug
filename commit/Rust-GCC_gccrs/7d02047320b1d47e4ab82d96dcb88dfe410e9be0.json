gccrs/contents/libgomp%2Ftestsuite%2Flibgomp.oacc-c-c%2B%2B-common%2Fpr92854-1.c?ref=7d02047320b1d47e4ab82d96dcb88dfe410e9be0", "patch": "@@ -0,0 +1,31 @@\n+/* Verify that 'acc_unmap_data' unmaps even in presence of dynamic reference\n+   counts.  */\n+\n+/* { dg-skip-if \"\" { *-*-* } { \"*\" } { \"-DACC_MEM_SHARED=0\" } } */\n+\n+#include <assert.h>\n+#include <stdlib.h>\n+#include <openacc.h>\n+\n+int\n+main ()\n+{\n+  const int N = 180;\n+\n+  char *h = (char *) malloc (N);\n+  char *d = (char *) acc_malloc (N);\n+  if (!d)\n+    abort ();\n+  acc_map_data (h, d, N);\n+\n+  char *d_ = (char *) acc_create (h + 3, N - 77);\n+  assert (d_ == d + 3);\n+\n+  d_ = (char *) acc_create (h, N);\n+  assert (d_ == d);\n+\n+  acc_unmap_data (h);\n+  assert (!acc_is_present (h, N));\n+\n+  return 0;\n+}"}]}