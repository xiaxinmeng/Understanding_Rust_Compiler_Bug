 "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fparameter_unused.f90?ref=33c0c5e958f3aa199e8aba8ca3796464d3cf0354", "patch": "@@ -0,0 +1,9 @@\n+! { dg-do compile }\n+! { dg-options \"-Wunused\" }\n+!\n+! PR fortran/31129 - No warning on unused parameters\n+!\n+program fred\n+integer,parameter :: j = 9     ! { dg-warning \"unused parameter\" }\n+end\n+"}]}