      if (TREE_OPERAND (t1, 1) != TREE_OPERAND (t2, 1))\n-\treturn false;\n-      return cp_tree_equal (TREE_OPERAND (t1, 0), TREE_OPERAND (t2, 0));\n-\n     case PARM_DECL:\n       /* For comparing uses of parameters in late-specified return types\n \t with an out-of-class definition of the function, but can also come"}, {"sha": "d504954bc636c77e1dba2bf1e74aa51d6b7f9103", "filename": "gcc/testsuite/g++.dg/cpp0x/decltype-call4.C", "status": "added", "additions": 13, "deletions": 0, "changes": 13, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/5497bd2bb6410e58eff6f57e53700dec646b1315/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdecltype-call4.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/5497bd2bb6410e58eff6f57e53700dec646b1315/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdecltype-call4.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fdecltype-call4.C?ref=5497bd2bb6410e58eff6f57e53700dec646b1315", "patch": "@@ -0,0 +1,13 @@\n+// PR c++/81188\n+// { dg-do compile { target c++11 } }\n+\n+template <class F>\n+struct C {\n+  F fast(long i) const;\n+  auto operator[](long i) const -> decltype(this->fast(i));\n+};\n+\n+template <class F>\n+auto C<F>::operator[](long i) const -> decltype(this->fast(i)) {\n+  return fast(i);\n+}"}]}