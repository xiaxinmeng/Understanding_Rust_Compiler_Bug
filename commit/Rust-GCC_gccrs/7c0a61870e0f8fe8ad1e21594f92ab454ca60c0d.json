n-      adl_type (TREE_TYPE (expr));\n+      adl_type (unlowered_expr_type (expr));\n       return;\n     }\n "}, {"sha": "318caaaeb6576fa799fe7bffd792de65815ed156", "filename": "gcc/testsuite/g++.dg/overload/bit-field1.C", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7c0a61870e0f8fe8ad1e21594f92ab454ca60c0d/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Fbit-field1.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7c0a61870e0f8fe8ad1e21594f92ab454ca60c0d/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Fbit-field1.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Foverload%2Fbit-field1.C?ref=7c0a61870e0f8fe8ad1e21594f92ab454ca60c0d", "patch": "@@ -0,0 +1,18 @@\n+// PR c++/92859\n+// { dg-do compile { target c++11 } }\n+\n+void f(int) = delete;\n+\n+struct ES { \n+  enum E { v }; \n+  friend void f(E) { }\n+};\n+\n+struct S {\n+  ES::E e : 1; \n+};\n+\n+int main() {\n+  S s{}; \n+  f (s.e);\n+}"}]}