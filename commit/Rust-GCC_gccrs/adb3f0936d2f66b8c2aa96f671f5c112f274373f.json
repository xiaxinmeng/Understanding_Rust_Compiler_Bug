s/contents/gcc%2Ffortran%2Fopenmp.c?ref=adb3f0936d2f66b8c2aa96f671f5c112f274373f", "patch": "@@ -1573,6 +1573,7 @@ gfc_resolve_omp_directive (gfc_code *code, gfc_namespace *ns ATTRIBUTE_UNUSED)\n     case EXEC_OMP_PARALLEL_SECTIONS:\n     case EXEC_OMP_SECTIONS:\n     case EXEC_OMP_SINGLE:\n+    case EXEC_OMP_TASK:\n       if (code->ext.omp_clauses)\n \tresolve_omp_clauses (code);\n       break;"}, {"sha": "3cb2809056b13631ece174cfba809700020e7a07", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/adb3f0936d2f66b8c2aa96f671f5c112f274373f/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/adb3f0936d2f66b8c2aa96f671f5c112f274373f/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=adb3f0936d2f66b8c2aa96f671f5c112f274373f", "patch": "@@ -1,3 +1,8 @@\n+2011-02-26  Tobias Burnus  <burnus@net-b.de>\n+\n+\tPR fortran/47886\n+\t* gfortran.dg/gomp/task-1.f90: New.\n+\n 2011-02-25  Jie Zhang  <jie@codesourcery.com>\n \n \t* gcc.target/arm/neon-thumb2-move.c: Add"}, {"sha": "227902cbf5a9e6d9c15244a0bd1e4eb71f8abeb5", "filename": "gcc/testsuite/gfortran.dg/gomp/task-1.f90", "status": "added", "additions": 27, "deletions": 0, "changes": 27, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/adb3f0936d2f66b8c2aa96f671f5c112f274373f/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Ftask-1.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/adb3f0936d2f66b8c2aa96f671f5c112f274373f/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Ftask-1.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Ftask-1.f90?ref=adb3f0936d2f66b8c2aa96f671f5c112f274373f", "patch": "@@ -0,0 +1,27 @@\n+! { dg-do compile }\n+! { dg-options \"-fopenmp\" }\n+!\n+! PR fortran/47886\n+!\n+! Test case contributed by Bill Long\n+\n+!  derived from OpenMP test OMP3f/F03_2_7_1d.F90\n+program F03_2_7_1d\n+   use omp_lib\n+   implicit none\n+   integer, parameter :: NT = 4\n+   integer :: sum = 0\n+\n+   call omp_set_num_threads(NT); \n+\n+   !$omp parallel\n+   !$omp task if(omp_get_num_threads() > 0)\n+   !$omp atomic\n+      sum = sum + 1\n+   !$omp end task\n+   !$omp end parallel\n+   if (sum /= NT) then\n+      print *, \"FAIL - sum == \", sum, \" (expected \", NT, \")\"\n+      stop 1;\n+   end if\n+end program F03_2_7_1d"}]}