,0 +1,70 @@\n+// PR middle-end/36449\n+// { dg-do run }\n+// { dg-options \"-O3\" }\n+\n+extern \"C\" void exit (int);\n+extern \"C\" void abort ();\n+\n+struct R\n+{\n+  short a;\n+  short b;\n+};\n+\n+struct S\n+{\n+  R e;\n+  long f;\n+  long g;\n+};\n+\n+struct T\n+{\n+  short c;\n+  short d;\n+};\n+\n+struct U\n+{\n+  long h[0x1ffffff + 1];\n+  T i;\n+};\n+\n+U *j;\n+\n+void __attribute__((noinline))\n+bar ()\n+{\n+  exit (0);\n+}\n+\n+void __attribute__((noinline))\n+foo ()\n+{\n+  S s;\n+\n+  s.e.a = 36;\n+  s.e.b = 38;\n+  if (s.e.a == j->i.c && s.e.b == j->i.d)\n+    bar ();\n+}\n+\n+int\n+main ()\n+{\n+  try\n+    {\n+      j = new U;\n+    }\n+  catch (...)\n+    {\n+      return 0;\n+    }\n+  j->i.c = 36;\n+  j->i.d = 38;\n+  j->h[0] = 1;\n+  j->h[1] = 2;\n+  j->h[2] = 3;\n+  foo ();\n+  abort ();\n+}"}]}