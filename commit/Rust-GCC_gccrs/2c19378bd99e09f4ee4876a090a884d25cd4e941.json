thub.com/repos/Rust-GCC/gccrs/contents/gcc%2Fexplow.c?ref=2c19378bd99e09f4ee4876a090a884d25cd4e941", "patch": "@@ -132,7 +132,9 @@ plus_constant (machine_mode mode, rtx x, HOST_WIDE_INT c,\n \t{\n \t  tem = plus_constant (mode, get_pool_constant (XEXP (x, 0)), c);\n \t  tem = force_const_mem (GET_MODE (x), tem);\n-\t  if (memory_address_p (GET_MODE (tem), XEXP (tem, 0)))\n+\t  /* Targets may disallow some constants in the constant pool, thus\n+\t     force_const_mem may return NULL_RTX.  */\n+\t  if (tem && memory_address_p (GET_MODE (tem), XEXP (tem, 0)))\n \t    return tem;\n \t}\n       break;"}]}