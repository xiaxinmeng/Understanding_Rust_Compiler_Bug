antiate_class_template_1 (tree type)\n   push_deferring_access_checks (dk_no_deferred);\n \n   fn_context = decl_function_context (TYPE_MAIN_DECL (type));\n+  /* Also avoid push_to_top_level for a lambda in an NSDMI.  */\n+  if (!fn_context && LAMBDA_TYPE_P (type) && TYPE_CLASS_SCOPE_P (type))\n+    fn_context = error_mark_node;\n   if (!fn_context)\n     push_to_top_level ();\n   /* Use #pragma pack from the template context.  */\n@@ -12531,7 +12534,7 @@ tsubst_copy (tree t, tree args, tsubst_flags_t complain, tree in_decl)\n \t{\n \t  /* We get here for a use of 'this' in an NSDMI.  */\n \t  if (DECL_NAME (t) == this_identifier\n-\t      && at_function_scope_p ()\n+\t      && current_function_decl\n \t      && DECL_CONSTRUCTOR_P (current_function_decl))\n \t    return current_class_ptr;\n "}, {"sha": "98cdbce28b3ea7209a8c8f867976e653be6d51da", "filename": "gcc/testsuite/g++.dg/cpp0x/lambda/lambda-nsdmi6.C", "status": "added", "additions": 23, "deletions": 0, "changes": 23, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/0f5b01358db7cce3cc3d224edd63ef8e8c9838a9/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-nsdmi6.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/0f5b01358db7cce3cc3d224edd63ef8e8c9838a9/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-nsdmi6.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Flambda%2Flambda-nsdmi6.C?ref=0f5b01358db7cce3cc3d224edd63ef8e8c9838a9", "patch": "@@ -0,0 +1,23 @@\n+// PR c++/58761\n+// { dg-do compile { target c++11 } }\n+\n+template <class T>\n+struct X\n+{\n+  int x = 42;\n+  int y = [this](){return this->x;}();\n+};\n+\n+template <class T>\n+struct Y\n+{\n+  int x = 42;\n+  int y = [this](){return this->x;}();\n+  Y(int) {}\n+};\n+\n+int main()\n+{\n+  X<int> x;\n+  Y<int> y(42);\n+}"}]}