_url": "https://github.com/Rust-GCC/gccrs/raw/91929b4dee6788fdfbaee519272b8d027b3d47ba/gcc%2Fcp%2Fparser.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Fparser.c?ref=91929b4dee6788fdfbaee519272b8d027b3d47ba", "patch": "@@ -8602,8 +8602,11 @@ cp_parser_decltype (cp_parser *parser)\n   \n   /* Parse to the closing `)'.  */\n   if (!cp_parser_require (parser, CPP_CLOSE_PAREN, \"`)'\"))\n-    cp_parser_skip_to_closing_parenthesis (parser, true, false,\n-\t\t\t\t\t   /*consume_paren=*/true);\n+    {\n+      cp_parser_skip_to_closing_parenthesis (parser, true, false,\n+\t\t\t\t\t     /*consume_paren=*/true);\n+      return error_mark_node;\n+    }\n \n   return finish_decltype_type (expr, id_expression_or_member_access_p);\n }"}, {"sha": "1933792d7a342d638d0fe459086aca37075f8e78", "filename": "gcc/cp/semantics.c", "status": "modified", "additions": 2, "deletions": 1, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/91929b4dee6788fdfbaee519272b8d027b3d47ba/gcc%2Fcp%2Fsemantics.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/91929b4dee6788fdfbaee519272b8d027b3d47ba/gcc%2Fcp%2Fsemantics.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fcp%2Fsemantics.c?ref=91929b4dee6788fdfbaee519272b8d027b3d47ba", "patch": "@@ -4164,7 +4164,8 @@ finish_decltype_type (tree expr, bool id_expression_or_member_access_p)\n           break;\n \n         default:\n-          gcc_assert (TYPE_P (expr) || DECL_P (expr));\n+\t  gcc_assert (TYPE_P (expr) || DECL_P (expr)\n+\t\t      || TREE_CODE (expr) == SCOPE_REF);\n           error (\"argument to decltype must be an expression\");\n           return error_mark_node;\n         }"}, {"sha": "77b97d213fddc3ee95201f58a7aaa3c5a03f2e80", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/91929b4dee6788fdfbaee519272b8d027b3d47ba/gcc%2Ftestsuite%2FChangeLog", "r