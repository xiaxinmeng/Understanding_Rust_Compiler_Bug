_Iface_Test is\n+   protected body Buffer is\n+      procedure Dummy is begin null; end;\n+   end;\n+\n+   function First (Obj : Buffer) return Natural is\n+   begin\n+     return 0;\n+   end;\n+\n+   procedure Do_Test (Dummy : Natural; Item : Buffer)\n+   is\n+      Position1 : Natural := First (Item);\n+      Position2 : Natural := Item.First;   --  Problem here\n+   begin\n+      null;\n+   end;\n+end;"}, {"sha": "c172d7fa2b12d1fc6f270b6455a6512148d36ef2", "filename": "gcc/testsuite/gnat.dg/sync_iface_test.ads", "status": "added", "additions": 11, "deletions": 0, "changes": 11, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/978057627777b89b5b81d11fbca24a998212c199/gcc%2Ftestsuite%2Fgnat.dg%2Fsync_iface_test.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/978057627777b89b5b81d11fbca24a998212c199/gcc%2Ftestsuite%2Fgnat.dg%2Fsync_iface_test.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgnat.dg%2Fsync_iface_test.ads?ref=978057627777b89b5b81d11fbca24a998212c199", "patch": "@@ -0,0 +1,11 @@\n+package Sync_Iface_Test is\n+   type Iface is limited interface;\n+   function First (Obj : Iface) return Natural is abstract;\n+\n+   protected type Buffer is new Iface with\n+      procedure Dummy;\n+   end;\n+   overriding function First (Obj : Buffer) return Natural;\n+\n+   procedure Do_Test (Dummy : Natural; Item  : Buffer);\n+end;"}]}