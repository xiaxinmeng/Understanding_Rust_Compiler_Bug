ks (dk_no_deferred);\n \t  input_location = DECL_SOURCE_LOCATION (fn);\n \t  noex = tsubst_copy_and_build (DEFERRED_NOEXCEPT_PATTERN (noex),\n \t\t\t\t\tDEFERRED_NOEXCEPT_ARGS (noex),\n \t\t\t\t\ttf_warning_or_error, fn,\n \t\t\t\t\t/*function_p=*/false,\n \t\t\t\t\t/*integral_constant_expression_p=*/true);\n+\t  pop_deferring_access_checks ();\n \t  pop_access_scope (fn);\n \t  pop_tinst_level ();\n \t  spec = build_noexcept_spec (noex, tf_warning_or_error);"}, {"sha": "b86760292b35e54717afb1e9e0fb11f61640d4e4", "filename": "gcc/testsuite/g++.dg/cpp0x/noexcept20.C", "status": "added", "additions": 22, "deletions": 0, "changes": 22, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/904812578d14a5f06cd294b5398e8bb2a237c031/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnoexcept20.C", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/904812578d14a5f06cd294b5398e8bb2a237c031/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnoexcept20.C", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fg%2B%2B.dg%2Fcpp0x%2Fnoexcept20.C?ref=904812578d14a5f06cd294b5398e8bb2a237c031", "patch": "@@ -0,0 +1,22 @@\n+// PR c++/56071\n+// { dg-options -std=c++11 }\n+\n+class B\n+{\n+  template <typename T> friend struct A;\n+  B() {}\n+};\n+\n+template <typename T>\n+struct A\n+{\n+  A() noexcept(noexcept(B())) { }\n+};\n+\n+struct C\n+{\n+  C()\n+  {\n+    static_assert( !noexcept(A<int>()), \"\" );\n+  }\n+};"}]}