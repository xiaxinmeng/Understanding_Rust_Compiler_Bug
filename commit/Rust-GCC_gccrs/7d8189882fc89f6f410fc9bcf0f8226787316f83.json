xcept.h", "status": "modified", "additions": 25, "deletions": 0, "changes": 25, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7d8189882fc89f6f410fc9bcf0f8226787316f83/libstdc%2B%2B-v3%2Finclude%2Fbits%2Ffunctexcept.h", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7d8189882fc89f6f410fc9bcf0f8226787316f83/libstdc%2B%2B-v3%2Finclude%2Fbits%2Ffunctexcept.h", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/libstdc%2B%2B-v3%2Finclude%2Fbits%2Ffunctexcept.h?ref=7d8189882fc89f6f410fc9bcf0f8226787316f83", "patch": "@@ -43,6 +43,7 @@ namespace std _GLIBCXX_VISIBILITY(default)\n {\n _GLIBCXX_BEGIN_NAMESPACE_VERSION\n \n+#if _GLIBCXX_HOSTED\n   // Helper for exception objects in <except>\n   void\n   __throw_bad_exception(void) __attribute__((__noreturn__));\n@@ -112,6 +113,30 @@ _GLIBCXX_BEGIN_NAMESPACE_VERSION\n   void\n   __throw_bad_function_call() __attribute__((__noreturn__));\n \n+#else // ! HOSTED\n+\n+  __attribute__((__noreturn__)) inline void\n+  __throw_invalid_argument(const char*)\n+  { std::__terminate(); }\n+\n+  __attribute__((__noreturn__)) inline void\n+  __throw_out_of_range(const char*)\n+  { std::__terminate(); }\n+\n+  __attribute__((__noreturn__)) inline void\n+  __throw_out_of_range_fmt(const char*, ...)\n+  { std::__terminate(); }\n+\n+  __attribute__((__noreturn__)) inline void\n+  __throw_runtime_error(const char*)\n+  { std::__terminate(); }\n+\n+  __attribute__((__noreturn__)) inline void\n+  __throw_overflow_error(const char*)\n+  { std::__terminate(); }\n+\n+#endif // HOSTED\n+\n _GLIBCXX_END_NAMESPACE_VERSION\n } // namespace\n "}, {"sha": "1f3f68fefce29fea99bf0268daf88bd9e949f5a6", "filename": "libstdc++-v3/include/std/bitset", "status": "modified", "additions": 1, "deletions": 7, "changes": 8, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/7d8189882fc89f6f410fc9bcf0f8226787316f83/libstdc%2B%2B-v3%2Finclude%2Fstd%2Fbitset", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/7d8189882fc89f6f410fc9bcf0f8226787