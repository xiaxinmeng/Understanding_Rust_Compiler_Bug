tents/gcc%2Ffortran%2Fexpr.c?ref=b2890f04bdb5f579c901c49aa7f544601806061f", "patch": "@@ -2553,6 +2553,7 @@ gfc_check_pointer_assign (gfc_expr *lvalue, gfc_expr *rvalue)\n     return SUCCESS;\n \n   if (lvalue->ts.type == BT_CHARACTER\n+      && lvalue->ts.cl && rvalue->ts.cl\n       && lvalue->ts.cl->length && rvalue->ts.cl->length\n       && abs (gfc_dep_compare_expr (lvalue->ts.cl->length,\n \t\t\t\t    rvalue->ts.cl->length)) == 1)"}, {"sha": "577bdc9883c889516f63f8bcbf4947099aea246b", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 6, "deletions": 0, "changes": 6, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=b2890f04bdb5f579c901c49aa7f544601806061f", "patch": "@@ -1,3 +1,9 @@\n+2007-05-04  Tobias Burnus  <burnus@net-b.de>\n+\n+\tPR fortran/31803\n+\t* gfortran.dg/char_pointer_assign_3.f90: New test.\n+\t* gfortran.dg/char_result_2.f90: Re-enable test.\n+\n 2007-05-04  Tobias Burnus  <burnus@net-b.de>\n \n \tPR fortran/25071"}, {"sha": "21db2df14a8c55f441e7bcfe9a4dc23f5f36326c", "filename": "gcc/testsuite/gfortran.dg/char_pointer_assign_3.f90", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_pointer_assign_3.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_pointer_assign_3.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_pointer_assign_3.f90?ref=b2890f04bdb5f579c901c49aa7f544601806061f", "patch": "@@ -0,0 +1,18 @@\n+! { dg-do run }\n+! PR fortran/31803\n+! Assigning a substring to a pointer\n+\n+program test\n+  implicit none\n+  character (len = 7), target :: textt\n+  character (len = 7), pointer :: textp\n+  character (len = 5), pointer :: textp2\n+  textp => textt\n+  textp2 => textt(1:5)\n+  if(len(textp)  /= 7) call abort()\n+  if(len(textp2) /= 5) call abort()\n+  textp  = 'aaaaaaa'\n+  textp2 = 'bbbbbbb'\n+  if(textp  /= 'bbbbbaa') call abort()\n+  if(textp2 /= 'bbbbb') call abort()\n+end program test"}, {"sha": "4127ecf94e9f9847fae11a42709e20e9e5821dba", "filename": "gcc/testsuite/gfortran.dg/char_result_2.f90", "status": "modified", "additions": 2, "deletions": 2, "changes": 4, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_2.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/b2890f04bdb5f579c901c49aa7f544601806061f/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_2.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fchar_result_2.f90?ref=b2890f04bdb5f579c901c49aa7f544601806061f", "patch": "@@ -46,7 +46,7 @@ end function f2\n \n   a = 42\n   textp => textt\n-  ! textp2 => textt(1:50) ! needs fixed PR31803\n+  textp2 => textt(1:50)\n \n   call test (f1 (textp), 70)\n   call test (f2 (textp, textp), 95)\n@@ -55,7 +55,7 @@ end function f2\n   call test (f5 (textp), 140)\n   call test (f6 (textp), 29)\n \n-  ! call indirect (textp2) ! needs fixed PR31803\n+  call indirect (textp2)\n contains\n   function f3 (string)\n     integer, parameter :: l1 = 30"}]}