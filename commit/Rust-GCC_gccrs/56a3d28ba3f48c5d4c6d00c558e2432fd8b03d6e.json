sses.c?ref=56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e", "patch": "@@ -2812,6 +2812,12 @@ inline_matmul_assign (gfc_code **c, int *walk_subtrees,\n   if (in_where)\n     return 0;\n \n+  /* For now don't do anything in OpenMP workshare, it confuses\n+     its translation, which expects only the allowed statements in there.\n+     We should figure out how to parallelize this eventually.  */\n+  if (in_omp_workshare)\n+    return 0;\n+\n   expr1 = co->expr1;\n   expr2 = co->expr2;\n   if (expr2->expr_type != EXPR_FUNCTION"}, {"sha": "019087254e0fabf12b9ce72b0eba0273d162eda8", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e", "patch": "@@ -1,3 +1,8 @@\n+2016-05-11  Jakub Jelinek  <jakub@redhat.com>\n+\n+\tPR fortran/70855\n+\t* gfortran.dg/gomp/pr70855.f90: New test.\n+\n 2016-05-11  Richard Biener  <rguenther@suse.de>\n \n \tPR middle-end/71002"}, {"sha": "247e4edd6ffa45d1ca6df74e23faddab648a6b59", "filename": "gcc/testsuite/gfortran.dg/gomp/pr70855.f90", "status": "added", "additions": 18, "deletions": 0, "changes": 18, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr70855.f90", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr70855.f90", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgfortran.dg%2Fgomp%2Fpr70855.f90?ref=56a3d28ba3f48c5d4c6d00c558e2432fd8b03d6e", "patch": "@@ -0,0 +1,18 @@\n+! PR fortran/70855\n+! { dg-do compile }\n+! { dg-additional-options \"-O2\" }\n+\n+program pr70855\n+   integer, parameter :: m = 4\n+   integer, parameter :: n = 2\n+   real :: a(m,n)\n+   real :: x(n)\n+   real :: y(m)\n+   a = 1.0\n+   x = 1.0\n+!$omp parallel\n+!$omp workshare\n+   y(1:m) = matmul ( a(1:m,1:n), x(1:n) )\n+!$omp end workshare\n+!$omp end parallel\n+end program pr70855"}]}