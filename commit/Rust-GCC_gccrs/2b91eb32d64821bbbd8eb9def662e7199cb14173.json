: etime\n+    REAL(4) :: tarray(1:2)\n+    REAL(4) :: result\n+\n+    CALL etime(tarray, result)\n+END PROGRAM test_etime\n+\n+subroutine test_etime2\n+    IMPLICIT NONE\n+    INTRINSIC :: etime\n+    REAL(4) :: tarray(1:2)\n+    REAL(4) :: result\n+\n+    result = etime(tarray)\n+END subroutine test_etime2"}]}