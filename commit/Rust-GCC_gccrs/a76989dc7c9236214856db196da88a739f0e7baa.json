family%2Fc-common.c?ref=a76989dc7c9236214856db196da88a739f0e7baa", "patch": "@@ -11081,6 +11081,20 @@ get_atomic_generic_size (location_t loc, tree function,\n \t\t    function);\n \t  return 0;\n \t}\n+      else if (TYPE_SIZE_UNIT (TREE_TYPE (type))\n+\t       && TREE_CODE ((TYPE_SIZE_UNIT (TREE_TYPE (type))))\n+\t\t  != INTEGER_CST)\n+\t{\n+\t  error_at (loc, \"argument %d of %qE must be a pointer to a constant \"\n+\t\t    \"size type\", x + 1, function);\n+\t  return 0;\n+\t}\n+      else if (FUNCTION_POINTER_TYPE_P (type))\n+\t{\n+\t  error_at (loc, \"argument %d of %qE must not be a pointer to a \"\n+\t\t    \"function\", x + 1, function);\n+\t  return 0;\n+\t}\n       tree type_size = TYPE_SIZE_UNIT (TREE_TYPE (type));\n       size = type_size ? tree_to_uhwi (type_size) : 0;\n       if (size != size_0)"}, {"sha": "e37bff498e4d575873f90c8e05ac27fef5836cb7", "filename": "gcc/testsuite/ChangeLog", "status": "modified", "additions": 5, "deletions": 0, "changes": 5, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a76989dc7c9236214856db196da88a739f0e7baa/gcc%2Ftestsuite%2FChangeLog", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a76989dc7c9236214856db196da88a739f0e7baa/gcc%2Ftestsuite%2FChangeLog", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2FChangeLog?ref=a76989dc7c9236214856db196da88a739f0e7baa", "patch": "@@ -1,3 +1,8 @@\n+2016-08-18  Marek Polacek  <polacek@redhat.com>\n+\n+\tPR c/71514\n+\t* gcc.dg/pr71514.c: New test.\n+\n 2015-08-18  H.J. Lu  <hongjiu.lu@intel.com>\n \n \tPR target/72839"}, {"sha": "8bfa8051d0270afdb43b5e7b33ea71d1fec4ad73", "filename": "gcc/testsuite/gcc.dg/pr71514.c", "status": "added", "additions": 23, "deletions": 0, "changes": 23, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/a76989dc7c9236214856db196da88a739f0e7baa/gcc%2Ftestsuite%2Fgcc.dg%2Fpr71514.c", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/a76989dc7c9236214856db196da88a739f0e7baa/gcc%2Ftestsuite%2Fgcc.dg%2Fpr71514.c", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Ftestsuite%2Fgcc.dg%2Fpr71514.c?ref=a76989dc7c9236214856db196da88a739f0e7baa", "patch": "@@ -0,0 +1,23 @@\n+/* PR c/71514 */\n+/* { dg-do compile } */\n+/* { dg-options \"\" } */\n+\n+void\n+foo ()\n+{\n+}\n+\n+int a, b;\n+\n+void\n+fn1 (void)\n+{\n+  __atomic_exchange (&a, &foo, &b, __ATOMIC_RELAXED); /* { dg-error \"must not be a pointer to a function\" } */\n+}\n+\n+void\n+fn2 (int n)\n+{\n+  int arr[n];\n+  __atomic_exchange (&a, &arr, &b, __ATOMIC_RELAXED); /* { dg-error \"must be a pointer to a constant size type\" } */\n+}"}]}