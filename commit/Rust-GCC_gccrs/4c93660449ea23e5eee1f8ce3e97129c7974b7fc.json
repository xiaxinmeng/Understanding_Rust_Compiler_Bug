n+{\n+  int j1;\n+  ce[0] = 0;\n+  for (j1 = 0; j1 < 2; ++j1)\n+    for (o5 = 1; o5 >= 0; --o5)\n+      p7 += ce[o5];\n+  return 0;\n+}\n+\n+int du;\n+\n+int bar (void)\n+{\n+  int u7[2];\n+  int ar;\n+\n+  for (ar = 0; ar < 2; ++ar) {\n+    int xo;\n+\n+    for (xo = 0; xo < 2; ++xo) {\n+      du += u7[ar];\n+      u7[0] = 0;\n+    }\n+  }\n+\n+  return 0;\n+}"}]}