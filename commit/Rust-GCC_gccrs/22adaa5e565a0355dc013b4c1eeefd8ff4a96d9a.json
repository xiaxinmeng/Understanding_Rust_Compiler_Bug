 isa: sm_30 sm_35 sm_53 sm_70 sm_75 sm_80 >> $@\n+omp-device-properties-nvptx: $(srcdir)/config/nvptx/nvptx-sm.def\n+\t$(SHELL) $(srcdir)/config/nvptx/gen-omp-device-properties.sh \\\n+\t  \"$(srcdir)/config/nvptx\" > $@"}]}