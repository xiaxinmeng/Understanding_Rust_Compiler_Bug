l.adb?ref=ee7c961db9da34d3e437eba0088f7291a7a5dfb4", "patch": "@@ -13559,6 +13559,16 @@ package body Sem_Util is\n                              Is_Volatile_Full_Access (Etype (Entity (N)))));\n    end Is_Atomic_Or_VFA_Object;\n \n+   ----------------------\n+   -- Is_Attribute_Old --\n+   ----------------------\n+\n+   function Is_Attribute_Old (N : Node_Id) return Boolean is\n+   begin\n+      return Nkind (N) = N_Attribute_Reference\n+        and then Attribute_Name (N) = Name_Old;\n+   end Is_Attribute_Old;\n+\n    -------------------------\n    -- Is_Attribute_Result --\n    -------------------------"}, {"sha": "2d1bcf0519445a192e485d975b9c34dc9a43903d", "filename": "gcc/ada/sem_util.ads", "status": "modified", "additions": 3, "deletions": 0, "changes": 3, "blob_url": "https://github.com/Rust-GCC/gccrs/blob/ee7c961db9da34d3e437eba0088f7291a7a5dfb4/gcc%2Fada%2Fsem_util.ads", "raw_url": "https://github.com/Rust-GCC/gccrs/raw/ee7c961db9da34d3e437eba0088f7291a7a5dfb4/gcc%2Fada%2Fsem_util.ads", "contents_url": "https://api.github.com/repos/Rust-GCC/gccrs/contents/gcc%2Fada%2Fsem_util.ads?ref=ee7c961db9da34d3e437eba0088f7291a7a5dfb4", "patch": "@@ -1549,6 +1549,9 @@ package Sem_Util is\n    --  Determine whether arbitrary node N denotes a reference to an object\n    --  which is either atomic or Volatile_Full_Access.\n \n+   function Is_Attribute_Old (N : Node_Id) return Boolean;\n+   --  Determine whether node N denotes attribute 'Old\n+\n    function Is_Attribute_Result (N : Node_Id) return Boolean;\n    --  Determine whether node N denotes attribute 'Result\n "}]}